<script src="//js.3conline.com/bbs/pcauto/2013/js/auto_dealers_area.js" charset="utf-8"></script>
<script type="text/javascript">
var rid=getPCautoPriceArea();
var sgid= new Array(),sgNum=0,count=10;
var arr=new Array();
var getdataAd={
loadScript:function(sScriptSrc,callbackfunction){
var oHead = document.getElementsByTagName('head')[0]; 
if(oHead){ 
var oScript = document.createElement('script'); 
oScript.setAttribute('src',sScriptSrc); 
oScript.setAttribute('type','text/javascript'); 
var loadFunction = function(){ 
if (this.readyState == 'complete' || this.readyState == 'loaded'){ 
callbackfunction();  
} 
}; 
oScript.onreadystatechange = loadFunction; 
oScript.onload = callbackfunction; 
oHead.appendChild(oScript); 
} 
},
getCookie:function(name){
return unescape(document.cookie.replace(new RegExp(".*(?:^|; )"+name+"=([^;]*).*|.*"),"$1"));
},
getDataId:function(sgid){
var _this=this,
url="//price.pcauto.com.cn/dealer/interface/wap/get_wap_serial_region_hq.jsp?refresh=dsadas&sgid="+sgid+"&rid="+rid+"&callback=getAdData15";
_this.loadScript(url);
},
getData:function(sgid){
var _this=this;
var url_2="//price.pcauto.com.cn/dealer/interface/wap/get_wap_serial_region_hq.jsp?refresh=dsadas&sgid="+sgid+"&rid="+rid+"&callback=getAdData";
var url_3="//price.pcauto.com.cn/dealer/interface/order/order_info.jsp?m=getCountBySG&sgId="+sgid+"&callback=showCount";
_this.loadScript(url_3);
setTimeout(function(){_this.loadScript(url_2);},100)  
},
addEventAd:function(id){
var tid=id;
window.addEventListener("scroll", function() {
var dataRel=tid.getAttribute("data-rel");
if(tid.getBoundingClientRect().top <= 450 && dataRel!=1){
// 添加广告曝光量
var img = new Image;
img.src = "//count.pcauto.com.cn/count.php?channel=1098&screen="+screen.width+"*"+screen.height+"&refer="+encodeURIComponent(document.referrer)+"&anticache="+new Date().getTime()+"&url="+encodeURIComponent(location.href)+"&from=event";
tid.setAttribute("data-rel",1);
}
});
},
init:function(){
var _this=this;
var u=this.getCookie('u');
var url_1="//rs.pcauto.com.cn/recommend/online/wap/serialRecommend.do?cityId="+rid+"&encode=gbk&callback=getSgId";
this.loadScript(url_1);
setTimeout(function(){_this.getDataId(sgid[sgNum])},100)    
}
}
getdataAd.init();
function showCount(data){//获取领取礼包人数
if(data[0].result){
count=10+data[0].count;console.log(count);
}else{
count=10;
}
}
function getSgId(data){//获取车系id
var serials=data.serials;
for(var i=0;i<5;i++){
sgid.push(serials[i].id);console.log(serials[i].id);
}
}
function getAdData15(data){
var tid=document.getElementById("JpcautoAd");
if(sgNum<5){
if(data.result>0){
arr.push(sgid[sgNum]);
}else{
tid.style.display="none";
}
sgNum++;
getdataAd.getDataId(sgid[sgNum]);
}else if(sgNum==5 && arr.length>0){
var arrlen=arr.length,
arrNum=arrlen==1? 0:Math.floor(Math.random()*arrlen);
getdataAd.getData(arr[arrNum]);console.log("选中这个"+arr[arrNum]);
sgNum++;
}
}
function getAdData(data){//获取广告内容
var tid=document.getElementById("JpcautoAd");
if(data.result>0){
var arrdata=data.data, 	
len=arrdata.length,
num=len==1? 0:Math.floor(Math.random()*len),
cts=Math.abs(arrdata[num].endTime-arrdata[num].nowTime),
d=Math.floor(cts/(24*3600*1000))<1? 1:Math.floor(cts/(24*3600*1000));
str='<a href="'+arrdata[num].urlGift+'&ad=12391"> <span class="hot-icon">HOT</span> <img src="'+arrdata[num].bigThumb+'" alt="" class="adPic" />  <div class="adTxt"> <i class="tit">'+arrdata[num].name+'</i> <em class="other"> <i>'+count+'人领取了礼包&nbsp;&nbsp;剩<span class="font-red">'+d+'</span>天</i> <span class="adIcon">广告</span> </em> </div> </a>';
tid.innerHTML=str;
tid.style.display="block";
getdataAd.addEventAd(tid);
}else{
tid.style.display="none";
}
}
</script>
