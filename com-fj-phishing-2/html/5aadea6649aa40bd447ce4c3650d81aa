(function() {
if (window.location.search.indexOf("ToQingYingYong") != -1) {
if (window.sessionStorage) {
try {
sessionStorage.ToQingYingYong = true;
} catch (err) {
Cookie.set("ToQingYingYong", true);
}
} else {
Cookie.set("ToQingYingYong", true);
}
}
})();
/*判断localStorage或者cookie中是否有轻应用标识 , 如果有的话执行callback函数*/
function ToQingYingYong(fc) {
function callback() {
if (typeof(fc) != 'undefined') {
fc();
};
};
if (window.sessionStorage) {
try {
if (sessionStorage.ToQingYingYong) {
callback();
}
} catch (err) {
if (Cookie.get("ToQingYingYong")) {
callback();
}
}
} else {
if (Cookie.get("ToQingYingYong")) {
callback();
}
}
};
/*执行 callback函数*/
