<script>
    $(function () {
        $("div.pull-left a").click(function () {
            $(this).addClass("cur1").siblings().removeClass("cur1");
        }).hover(function () {
            $(this).addClass("cur");
        }, function () {
            $(this).removeClass("cur");
        });

        $(".WeChat").hover(function () {
            $("#wechat_QR_div").show();
        }, function () {
            $("#wechat_QR_div").hide();
