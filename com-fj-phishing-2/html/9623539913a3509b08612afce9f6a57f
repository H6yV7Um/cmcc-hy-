<script type="text/javascript">
<!--
    //弹出城市选择Table
	function showCityTable(){
	    document.getElementById("myTb").style.display="block";
	}
	//隐藏城市选择Table
	function hiddenCityTable(){
	    document.getElementById("myTb").style.display="none";
	}
	// 隐藏或显示Table
	function showORhiden() {
		if (document.getElementById("myTb").style.display == "block") {
			hiddenCityTable();
		} else {
			showCityTable();
		}
	}	
	//切换城市
	function changeCity(city){
		document.getElementById("currentSelCityId").value = city;
		if(document.getElementById("bond1").className=="bond02on"){
			document.getElementById("tag").value="2";
		}else{
			document.getElementById("tag").value="1";
		}
		document.getElementById("selectCityForm").submit();
	   //selectCityForm.submit();
	}
	
	//查看商户详情信息
	function searchDet(merBrandId,brandSubtypeName,merchantCity,id,flag){
		var url = encodeURI(encodeURI("?mock="+id+"&merBrandId="+merBrandId+"&brandSubtypeName="+brandSubtypeName+"&merchantCity="+merchantCity));
		if(flag == "merArging"){
			$("#searchDetForm").attr("action","/jsp/include/CN2/creditcard/merArgingBrandDetail.jsp"+url);
		}
		if(flag == "mer"){
			$("#searchDetForm").attr("action","/jsp/include/CN2/creditcard/merBrandDetail.jsp"+url);
		}
		$("#merBrandId").val(merBrandId);
		$("#brandSubtypeName").val(brandSubtypeName);
		$("#merchantCity").val(merchantCity);
		$("#mock").val(id);
		$("#searchDetForm").submit();
	}
	
	//表单提交
	function searchMerchantByTypeName(brandSubtypeName,flag) {

		if(flag == "mer"){
			$("#card_searchMerchantByTypeName").attr("action","/Channel/11608406");
		}
		if(flag == "arg"){
			$("#card_searchMerchantByTypeName").attr("action","/Channel/16217997");
		}
		$('#card_brandSubtypeName').attr("value",brandSubtypeName);
		$('#card_searchMerchantByTypeName').submit();
	 }
//-->
</script>

