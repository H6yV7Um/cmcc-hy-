<script type="text/javascript" src="/static/CN2/js/checkTipsWindow.js"></script>

<script type="text/javascript">

        function submitRequest(targetName){
        	document.submitForm.target = targetName;
            document.submitForm.submit();
       }

        //网银登录和网站登录窗口控制
        function signOnType(s){
        	    // 检查打开的窗口，并返回表单提交的target
            var targetName = checkTipsWindow.checkWin(s);
        	    // 如果targetName不为空就提交登录在线客服请求，否则不用提交
        	    if(!! targetName ) {
        	        submitRequest(targetName);
        	    }
        }

        </script>

<!-- 在线客服开始 -->
<div class="floatBar">
	
			
