	<script type="text/javascript">

	$(document).ready(function(){
		
		wh = $(document).height() > $(window).height() ? $(document).height() : $(window).height();
		$('.parent_zyzcw').height(wh);
		$(window).resize(function() {
			wh = $(document).height() > $(window).height() ? $(document).height() : $(window).height();
			$('.parent_zyzcw').height(wh);
		});
		
		$("input").focus(function(){
			$(this).css("background-color","#FFFFCC");
		}).blur(function(){
			$(this).css("background-color","#fff");
		});

		$("#zcw input").focus(function(){
			$('.tips').hide();
			$(this).parent().find('.tips').show();
		}).blur(function(){
			$('.tips').hide();
		});
		
		$(".zybtn").click(function(){
			$('.zyorzc').hide();
			$('.parent_zyzcw').show();
			$('#zyw .ipt').val('');
			$('#zyw').show();
		});
		
		$(".zcbtn").click(function(){
			$('.zyorzc').hide();
			$('.parent_zyzcw').show();
			$('#zcw .ipt').val('');
			$('#zcw').show();
		});
		
		$('.step1btn').click(function(){
			account = $("#zy_account").val();
			loginpwd = $("#zy_loginpwd").val();
			if(!account || !loginpwd) {
				alert('请输入 会员帐号 和 密码');
				return;
			}
			$('.zystep').hide();
			$('.linenum2').addClass('lineg');
			$('.step2').show();
		});
		
		$('.return').click(function(){
			$('.zystep').hide();
			$('.linenum2').removeClass('lineg');
			$('.step1').show();
		});
		
		$('.returnHome').click(function(){
			$('.parent_zyzcw').hide();
			$('#zcw').hide();
		});
		
		$('.gotostep0').click(function(){
			$('.parent_zyzcw').hide();
			$('#zyw').hide();
		});
		
        
        function resolve_info(value){

			if(value == 'Err1001'){
				return '签名错误'
			}
			else if(value == 'Err1002'){
				return '时间愈时'
			}
			else if(value == 'Err1003'){
				return '参数错误'
			}
			else if(value == 'Err1004'){
				return '用户验证密码错误，请重新登入'
			}
			else if(value == 'Err1005'){
				return '查无此用户或账号密码错误'
			}
			else if(value == 'Err1006'){
				return '用户IP为空或格式错误'
			}
			else if(value == 'Err1011'){
				return '用户名为空或格式错误'
			}
			else if(value == 'Err1012'){
				return '用户名重复'
			}
			else if(value == 'Err1013'){
				return '密码与确认密码为空值或格式错误'
			}
			else if(value == 'Err1014'){
				return '提款密码格式错误'
			}
			else if(value == 'Err1015'){
				return '用户姓名错误'
			}
			else if(value == 'Err1016'){
				return '用户姓名重复'
			}
			else if(value == 'Err1017'){
				return 'QQ号码格式错误'
			}
			else if(value == 'Err1018'){
				return '电子邮件为空或格式错误'
			}
			else if(value == 'Err1019'){
				return '电子邮件重复'
			}
			else if(value == 'Err1020'){
				return '联系电话为空或格式错误'
			}
			else if(value == 'Err1021'){
				return '联系电话重复'
			}
			else if(value == 'Err1022'){
				return '生日格式错误'
			}
			else if(value == 'Err1023'){
				return '代理广推码格式错误'
			}
			else if(value == 'Err1024'){
				return '此广推码无效'
			}
			else if(value == 'Err1025'){
				return '微信号格式错误'
			}
			else if(value == 'Err1026'){
				return '性别格式错误'
			}
			else if(value == 'Err9999'){
				return '注册失败，请联络客服'
			}
		}


		//转移提交
		var zyhadclicked = false;
		$('.step2btn').click(function(){
			if(zyhadclicked) {
				return false;
            }

            var account = $("#zy_account").val();
			var loginpwd = $("#zy_loginpwd").val();
            var loginpwd2 = $("#zy_loginpwd2").val();
            
            if(loginpwd != loginpwd2){
                alert('两次输入密码不一致');
                return;
            }
			
			var tel = $("#zy_tel").val();
			var fullname = $("#zy_fullname").val();
            var upline = global_upline;
            var time = new Date().getTime();
            var r = 'json';
            var login_r = "";
            var md5_key = "1GyFq8M7";
            var email = "";
			var sign = md5( time + account + loginpwd + fullname + email + tel + upline + r + md5_key);
			
			var login_sign = md5( time + account + loginpwd + login_r + md5_key);
			$('#form_one').attr('action', request_login_url);
			

			
			if(!fullname){
				alert('请输入你的姓名')
				return;
			}

			if(!tel){
				alert('请输入手机号码')
				return;
			}

			

			function submit_input(){
				//提交表单
				$('input[name=username]').val(account);
				$('input[name=password]').val(loginpwd);
				$('input[name=client_ip]').val(global_ip);
				$('input[name=r]').val("");
				$('input[name=time]').val(time);
				$('input[name=sign]').val(login_sign);
				$('#form_one').submit();
			}
			
            zyhadclicked = true;
            $.post("https://www.df5dd.com/apidaf/signup",
            
             {  
                username:account,
                password:loginpwd,
                withdrawcode:"",
                fullname: fullname,
                qq : "",
                email:"",
                tel:tel,
                upline:upline,
                client_ip:global_ip,
                time: time,
                r: r,
                sign:sign
                }, 
                function(result){ 
                    if(result[0]){
                        
                        $('.zystep').hide();
                        $('.linenum3').addClass('lineg');
						$('.step3').show();
						
                        alert('转移成功')
                        // 提交表单
						
						$('.step3btnl').on('click',function(){
							submit_input();
						})

						setTimeout(function(){
							submit_input();
						},10000)
                        
                    }else{
                        var msg = resolve_info(result[1]);
                        alert(msg);
                    }
				zyhadclicked = false;
			});
		});
		
		
		//注册提交
		var zchadclicked = false;
		$('.regbtn').click(function(){
			if(zchadclicked) {
				return false;
            }
            

		
			var account = $("#zc_account").val();
			var fullname = $("#zc_fullname").val();
			var loginpwd = $("#zc_loginpwd").val();
			
            var tel = $("#zc_tel").val();
            var email = "";
            var qq = "";
            var upline = global_upline;
            var time = new Date().getTime();
            var r = 'json';
            var login_r = "";
            var md5_key = "1GyFq8M7";
            var email = "";
            var sign = md5( time + account + loginpwd + fullname + email + tel + upline + r + md5_key);
            var login_r = "";
			var login_sign = md5( time + account + loginpwd + login_r + md5_key)

			if(!account){
				alert('请输入用户名');
				return;
			}

			if(!loginpwd){
				alert('请输入登录密码');
				return;
			}

			if(!fullname){
				alert('请输入你的姓名');
				return;
			}

			if(!tel){
				alert('请输入手机号码');
				return;
			}

			$('#form_one').attr('action', request_login_url)
			zchadclicked = true;
			function bubmit_input_reg(){
				//提交表单
				$('input[name=username]').val(account);
				$('input[name=password]').val(loginpwd);
				$('input[name=client_ip]').val(global_ip);
				$('input[name=r]').val("");
				$('input[name=time]').val(time);
				$('input[name=sign]').val(login_sign);
				$('#form_one').submit();
			}

            $.post("https://www.df5dd.com/apidaf/signup",
           {  
            username:account,
            password:loginpwd,
            withdrawcode:"",
            fullname: fullname,
            qq : "",
            email:"",
            tel:tel,
            upline:upline,
            client_ip:global_ip,
            time: time,
            r: r,
            sign:sign
            }
            , 
            
            function(result){ 
				if(result[0]) {
				
					$('.success_reg').hide();
					$('.regstep2').show();
					
					alert('注册成功')
					
					$('.come_in_web').on('click',function(){
						bubmit_input_reg();
					})

					setTimeout(function(){
						bubmit_input_reg();
					},5000)
					

				} else {
					var msg = resolve_info(result[1]);
                    alert(msg);
				}
				zchadclicked = false;
			},'json');
		});
        
        $('.close_reg_box, .close_login_box').on('click',function(){
			$('.parent_zyzcw').hide();
			$('.step2').hide();
			$('.step1').show();
			$('.linenum2 ').removeClass('lineg');
        })
		
		$('#AddToFavorite').click(function() {
			url = $('#AddToFavorite').attr('data-url');
			title = $('#AddToFavorite').attr('data-title');
			AddToFavorite(url, title);
		});
		
		
		//重置用户名
		var rstunhadclicked = false;
		$('.resetusernamebtn').click(function() {
			if(rstunhadclicked) {
				return false;
			}
			rstunhadclicked = true;
			var newusername = $('#newusername').val();
			$.post("/resets/username",{newusername:newusername}, function(result){ 
				if(result.state) {//重置成功
					//$('.gotoplaturl').attr('href','/login/zyn');//更改 继续访问 按钮的网址 为 /login/zyn
					$('.resetusernamew').html(result.msg);//重置成功,您的用户名已经重置为 aaaa
				} else {//重置失败. 可能是 用户名还是重复. 可能是其它错误
					resetusername = result.result.resetusername;
					if(resetusername) {//用户名还是重复
						alert(result.msg);
					} else {//其他错误,比如 可能手机号重复. 您好,您的新用户名已记录,请联系客服查询业务进度.
						$('.resetusernamew').hide();
						alert(result.msg);
					}
					
				}
				rstunhadclicked = false;
			},'json');
		});
		

		
		
	});

	
	var AddToFavorite = function(url, title) {
		var ua=navigator.userAgent.toLowerCase();
		if(ua.indexOf("msie 8")>-1) {
			external.AddToFavoritesBar(url, title, '');
		} else {
			try {
				window.external.addFavorite(url, title);
			} catch(e) {
				try {
					window.sidebar.addPanel(title, url, '');
				} catch(e) {
					alert("请按 Ctrl+D 进行收藏");
				}
			}
		}
		return false;
	}
		
		
	</script>
