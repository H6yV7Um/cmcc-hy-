<script type="text/javascript">
function cha1(url){
                 		document.getElementById('ilistmw1').setAttribute('class',url);
                            this.className=url;
	}
$(function(){
	$(".ilistmw").hover(function(){
		var idstr = $(this).attr("id");
		if(idstr == 'menulast'){
			$(this).addClass("menulastfocus");
		}else{
			$(this).addClass("menufocus");
		}
	},function(){
		var idstr = $(this).attr("id");
		if(idstr == 'menulast'){
			$(this).removeClass("menulastfocus");
		}else{
			$(this).removeClass("menufocus");
		}
	});
});
</script>
    
