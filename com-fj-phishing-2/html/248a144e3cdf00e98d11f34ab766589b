	function ie8_hack(className,className2){
		var control_width=document.body.offsetWidth>1210?'1201px':'975px';
		var control_width2=document.body.offsetWidth>1210?'900px':'700px';
		$(className).css("width",control_width);
		$(className2).css("width",control_width2);
	}
	$(document).ready(function(){
		ie8_hack(".control_w",".control_w2");
		$(window).resize(function(){
			ie8_hack(".control_w",".control_w2");
		})
	})
    </script>
