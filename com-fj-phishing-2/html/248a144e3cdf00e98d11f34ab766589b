<script type="text/javascript">
$(function(){
	index.init();
	
	var reg = new RegExp("(^|&)ref=([^&]*)(&|$)");
    var r = window.location.search.substr(1).match(reg);
    if (r!=null){ 
	    $.cookie('ref', unescape(r[2]), {expires: 1/(24*2), path: '/'});
    }
    
})
</script>








