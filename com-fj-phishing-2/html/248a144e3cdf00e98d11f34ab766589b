<script type="text/javascript">
function handle_login(callback_func){
    $('#err_msg').parent().hide();
    var username = $.trim($('#username').val()),
        pwd = $('#password').val();

    if(username == ""){
        $('#err_msg').text("用户名不能为空").show().parent().show();
        return;
    }

    if(pwd == ""){
        $('#err_msg').text("用户密码不能为空").show().parent().show();
        return;
    }
    var params = {
        'username': username,
        'password': pwd
    }
    $('#loading_msg').show();
    $('#hidden_iframe').unbind('load');
    $('#hidden_iframe').load(function() {
        $('#loading_msg').hide();
        var user = cookie_check_logined();
        if (user) {
            $('#non_logined').hide();
            $('#my_name').html(user);
            $('#logined').show();
            Popup.closeLogin();
             if(callback_func){callback_func();}
        }
        else {
            var code;
            try {
                code = login_err_code($('#hidden_iframe').prop('contentWindow').location.href);
            }
            catch (err) {
                login_redirect();
                return;
            }
            function login_redirect() {
                 $('#urs_login_form').attr('target', '_self');
                 $('#urs_login_form').find('input[name="url"]').val('http://bmall.163.com');
                 $('#urs_login_form').find('input[name="url2"]').remove();
                 $('#urs_login_form').submit();
            }
            var url;
            try {
                url = $('#hidden_iframe').prop('contentWindow').location.href;
            }
            catch (err) {
                login_redirect();
                return;
            }

            if (!url) {
                login_redirect();
                 return;
            }

            var code = login_err_code(url);
            var msg = code ? login_err_msg(code) : '内部错误，请稍后再试';
            $('#err_msg').text(msg).show().parent().show();
        }
    });
    $('#urs_login_form').submit();
}

function check_login(){
    return cookie_check_logined() != '';
}

function show_login(){
    Popup = new_popup();
    $('#password').val("");
    $('#err_msg').text("").parent().hide();
    Popup.showLogin();
    return;
}

$(function(){
    $('#password').bind('keypress', function(e){
        if (e.keyCode == 13){ 
            $('#login').click();
        }
    });
    //add by lmsh
    copy_username();
});

</script>

