//add by lmsh 用户名存取
function save_username(){
    var $username = $('#username').val();
    $.cookie("buyer_name",$username, {path: '/', expires:30});
}
function copy_username(){
	if($.cookie("buyer_name")){
		var $name = $.cookie("buyer_name")
		$('#username').val($name);
	}else{
		return;
	}	
}


//返回顶部
function gotoTop(){
	$("#gotoTop").click(function(){
		$('body,html').animate({scrollTop:0},400);
		return false;
	})
}
$(window).scroll(function(){
	if($(window).scrollTop()>400){
		$("#gotoTop").show();
	}else{
		$("#gotoTop").hide();
	}
})
