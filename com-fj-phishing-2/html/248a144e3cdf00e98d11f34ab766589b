    //搜索控制
    function search_ctrl(){
    	if(!$("#input_search").val()){
            $("#input_search").val("魔兽");
        };
        $("#input_search").blur(function(){
            if(!$("#input_search").val()){
                $("#input_search").val("魔兽");
            };
        })
        $("#input_search").focus(function(){
            if($("#input_search").val()=="魔兽"){
                $("#input_search").val("");
            };
        })
    }
    search_ctrl();

    //我的商城下拉
    $(".myshop_show").hover(function(){
        $(this).children("ul").show();
    },function(){
        $(this).children("ul").hide();
    });
    //购物车控制
    new_cart.ready();
});
</script>



