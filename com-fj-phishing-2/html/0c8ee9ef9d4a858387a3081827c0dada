                                                        $('#hdp button').eq(2).css('background','#6fb4bf');
                                                    break;
                                                    case "2":
                                                        $('#hdp a').eq(1).css('display','block'); 
                                                        $('#hdp h6').eq(1).css('display','block'); 
                                                        $('#hdp a').eq(0).css('display','none'); 
                                                        $('#hdp h6').eq(0).css('display','none'); 
                                                        $('#hdp a').eq(2).css('display','none'); 
                                                        $('#hdp h6').eq(2).css('display','none'); 
                                                        $('#hdp button').eq(1).css('background','#ff5d8f');
                                                        $('#hdp button').eq(0).css('background','#6fb4bf');
                                                        $('#hdp button').eq(2).css('background','#6fb4bf');
                                                    break;
                                                    case "3":
                                                        $('#hdp a').eq(2).css('display','block'); 
                                                        $('#hdp h6').eq(2).css('display','block'); 
                                                        $('#hdp a').eq(0).css('display','none'); 
                                                        $('#hdp h6').eq(0).css('display','none'); 
                                                        $('#hdp      a').eq(1).css('display','none'); 
                                                        $('#hdp h6').eq(1).css('display','none'); 
                                                        $('#hdp button').eq(2).css('background','#ff5d8f');
                                                        $('#hdp button').eq(0).css('background','#6fb4bf');
                                                        $('#hdp button').eq(1).css('background','#6fb4bf');
                                                    break;  
                                                }
                                                flag1=false;
                                                clearInterval(jsq);
                                            })        
                                            $('#hdp button').mouseout(function(){
                                                    if (!flag1){
                                                        js();
                                                    
                                                    } 
                                                    
                                                    })

                                                                                            
                                            
                                    })


								

                                </script></div><div class="h50"></div><ul><li><a href="http://tjoys.yejs.com.cn/h-pd-52-118_489.html" target="_blank" onclick="_czc.push(['_trackEvent', '广告', '点击', '首页微专题']);">我的环境我做主——幼儿园环境创设</a></li><li><a href="http://tjoys.yejs.com.cn/h-pd-38-118_489.html" target="_blank" onclick="_czc.push(['_trackEvent', '广告', '点击', '首页微专题']);">盘点新生入园前后的教师准备工作</a></li><li><a href="http://tjoys.yejs.com.cn/h-pd-70-118_489.html" target="_blank" onclick="_czc.push(['_trackEvent', '广告', '点击', '首页微专题']);">幼儿园冬季环境环创</a></li><li><a href="http://tjoys.yejs.com.cn/h-pd-75-118_489.html" target="_blank" onclick="_czc.push(['_trackEvent', '广告', '点击', '首页微专题']);">幼儿园里刮起民族风，最炫的！</a></li></ul><h5><a href="http://tjoys.yejs.com.cn/h-col-118.html" target="_blank" style="text-decoration:none;color: #F23579;font-size:12px;">&gt;&gt; 更多微专题</a></h5></div><div class="gbottom"></div></div><!--end m_left--><!--互动平台改为资讯--><div id="r_b_r" style="height:460px;"><div id="zixun" style="background:url(/Public/images/index/shouye_zixun.gif) no-repeat;"><h2>幼教资讯</h2><span><a href="/News"><img src="/Public/images/more.gif"></a></span><div id="xinxi"><a href="/News/article/id/61766.htm" target="_blank" onclick="_czc.push(['_trackEvent', '广告', '点击', '首页幼教资讯']);"><img src="./Public/backend/uploads/images/20180516_5afbd3241c7ae.png" width="80" height="80"></a><h3><a href="/News/article/id/61766.htm" target="_blank" onclick="_czc.push(['_trackEvent', '广告', '点击', '首页幼教资讯']);">专业升级的途径，别再错过了</a></h3><p>幼教从业者现在真的很辛苦，教育教学的同时，要应付各种检查、材料、...<span></span><a href="/News/article/id/61766.htm" target="_blank" onclick="_czc.push(['_trackEvent', '广告', '点击', '首页幼教资讯']);">查看全文</a></p></div><!--end xinxi--><div id="list"><ul><li><a href="/News/article/id/61452.htm" target="_blank">全景数学，颠覆传统教育的4大理由</a></li><li><a href="/News/article/id/60966.htm" target="_blank">园长邀请函•首批名师议题发布 丨...</a></li><li><a href="/News/article/id/59271.htm" target="_blank">2017全国家庭教育专业发展大会...</a></li><li><a href="/News/articl