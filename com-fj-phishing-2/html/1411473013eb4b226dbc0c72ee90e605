  <script type="application/javascript">
    (function () {
      //    全屏图片 路径
      var fullScreenImg = "https://gw.alicdn.com/tfs/TB1HJ9UuqmWBuNjy1XaXXXCbXXa-750-1300.jpg"
      //    缩略图图片 路径
      var miniImg = "//img.alicdn.com/tps/TB1OBH6NVXXXXaLXFXXXXXXXXXX-750-1080.jpg"
      //  是否使用钻展，0不使用，1使用
      var showZuan = 0

      var $viewport = document.querySelector('.viewport')
      var $dropUpBanner = document.querySelector('.drop-up-banner')
      var $img = document.querySelector('.drop-up-banner img')

      var timer = 3;

      var isClose = false
      var height = document.documentElement.clientHeight
      var width = document.documentElement.clientWidth
      var supportWebp = function (cb) {
        var img = new Image();
        img.onload = function () {
          cb(1)
        };
        img.onerror = function () {
          cb(0)
        };
        img.src = 'data:image/webp;base64,UklGRiQAAABXRUJQVlA4IBgAAAAwAQCdASoBAAEAAgA0JaQAA3AA/vuUAAA=';
      }
      var dropUp = {
        initializer: function () {
          var a = new Date().getTime() - localStorage.getItem('now')
          var leave1 = a % (24 * 3600 * 1000)    //计算天数后剩余的毫秒数
          var hours = Math.floor(leave1 / (3600 * 1000))
          if (!!localStorage.isFirst || hours >= 3) {
            // 手淘不开屏
            if(window.navigator && window.navigator.userAgent && /AliApp\((TB)\/([\d\.]+)\)/i.test(window.navigator.userAgent)){
              document.querySelector('.mini-banner').className += ' active'
            }else{
              this.preloadImg()
            }
          } else {
            document.querySelector('.mini-banner').className += ' active'
          }
          this.bindEvents()
          //                document.querySelectorAll('.clickUrl')[0].href=clickUrl
          //                document.querySelectorAll('.clickUrl')[1].href=clickUrl
        },
        initBanner: function (src) {
          $dropUpBanner.style.opacity = 1
          localStorage.setItem('isFirst', false)
        },
        initTimer: function () {
          var self = this;
          window.interval = setInterval(function () {
            timer--;
            document.querySelector('.timer-area .second').textContent = timer + 's'
            if (timer < 1) {
              clearInterval(window.interval)
              self.killBanner();
            }
          }, 1000)
        },
        killBanner: function () {
          clearInterval(window.interval)
          $dropUpBanner.style.opacity = 0
          setTimeout(function () {
            $dropUpBanner.style.height = 0
            document.querySelector('.mini-banner').className += ' active'
          }, 500)
        },
        bindEvents: function () {
          var self = this;
          document.querySelector('.timer-area').addEventListener('click', function () {
            self.killBanner()
          }, false)
        },
        preloadImg: function (config) {
          var preimg = new Image();
          var self = this
          $dropUpBanner.className += ' fullpage'
          $dropUpBanner.style.height = height + 'px'
          if (showZuan) {
            self.initBanner(preimg.src)
            document.querySelector('.zuanCode').style.display = 'block'
            self.initTimer(config)
          } else {
            document.querySelector('.clickUrl').style.display = 'block'
            $img.style['max-height'] = height + 'px'
            self.initBanner(preimg.src)
            preimg.onload = function () {
              //接下来可以使用图片了
              //do something here
              $img.src = preimg.src
              self.initTimer(config)
            };
          }

          supportWebp(function (webp) {
            var src = fullScreenImg + "_q90.jpg"
            if (webp == 1) {
              src += "_.webp";
            }
            preimg.src = src;
          })
        }
      };
      return dropUp.initializer();
    })()
  </script>
