        seajs.use('cellex-lazyload@0.0.1', function (Lazy) {
            window.MM_LAZY = new Lazy();
            window.MM_LAZY.add($('div[data-mxview]'), function (dom) {
                var id = dom.id;
                if (!id) {
                    id = Magix.guid('mx-');
                    dom.id = id;
                }
                var ownerId = dom.getAttribute('data-ownerid') || Magix.config('rootId');
                var owner = Magix.Vframe.get(ownerId);
                var path = dom.getAttribute('data-mxview');
                if (path && owner) {
                    owner.mountVframe(id, path);
                }
            })
        })
        // 参数
        window.initTrack('pid:mm_33231688_7050284_23466709,mock,debug,env,host,unid,hidebar,isout,clk1,smart,rb');
        // wakeup
        // if (location.search.indexOf('smart=1') > -1 && window.navigator && window.navigator.userAgent && !/AliApp\((TB)\/([\d\.]+)\)/i.test(window.navigator.userAgent) && !/AliApp\((TM)\/([\d\.]+)\)/i.test(window.navigator.userAgent)) {
        //     window.wakeup && window.wakeup(location.href)
        // }
        var hostname = location.hostname;
        var protocol = location.protocol;
        var isDaily = !!(location.search.indexOf('systype=waptest') > -1);
        var isPre = !!(location.search.indexOf('systype=wapa') > -1);
        var isHostnameHasSet = true;
        var frame_subDomain = 'm';
        function setMtopConfig() {
            var prefix;
            var subDomain;
            var mainDomain;
            if (hostname.indexOf('etao.com') > -1) {
                prefix = 'apie';
                subDomain = 'm';
                mainDomain = 'etao.com';
            } else if (hostname.indexOf('tmall.com') > -1) {
                // acs.m.tmall.com 不支持https
                // 故先统一设置成api
                prefix = 'api';
                subDomain = 'm';
                mainDomain = 'tmall.com';
            } else if (hostname.indexOf('taobao.com') > -1) {
                prefix = 'acs';
                subDomain = 'm';
                mainDomain = 'taobao.com';
            } else {
                isHostnameHasSet = false;
            }
            if (isDaily) {
                subDomain = 'waptest';
            }
            if (isPre) {
                subDomain = 'wapa';
            }
            frame_subDomain = subDomain;
            lib.mtop.config.mainDomain = mainDomain;
            lib.mtop.config.subDomain = subDomain;
            lib.mtop.config.prefix = prefix;
        }
        setMtopConfig();
    </script>


