<script>
$(function(){
    //加载更多
    var counter = 1;
    // dropload
    $('#home_beikao_load').dropload({
        scrollArea : window,
        loadDownFn : function(me){
            result = '';
            pageEnd = 667;
            $.ajax( {
                url:'/zajax/items/homemore.html',
                data:{
                    page : counter,
                    listType : 'channelsHomeList',
                    catIds : '20018,20019,20020,20021,20022,20023,20024,20026,20027,20028,20029,20030,20031,20032',
                    catId : '8',
                    type : 'ielts'
                },
                type:'post',
                success:function(data) {
                    result = data;
                    counter++;
                    $('.home_beikao-cont_load').append(result);
                    me.resetload();
                },
                error : function() {
                    alert("加载异常！");
                    me.resetload();
                }
            });
            if(counter >= pageEnd){
                // 锁定
                me.lock();
                // 无数据
                me.noData();
            }
            /*setTimeout(function(){
             $('.scroll_load_cont').append(result);
             // 每次数据加载完，必须重置
             me.resetload();
             },1000);*/
        }
    });
});
</script>
