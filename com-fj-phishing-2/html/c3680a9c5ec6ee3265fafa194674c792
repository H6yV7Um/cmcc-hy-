<script type="application/javascript">
    $(function () {
        // 点击关闭顶部广告start
        $('.ed_top_close').on('click',function(){
            $('.ed_top').addClass('dis_none');
            $('.swiper-slide1').addClass('dis_none');
            $('.mar_fix_top').removeClass('mar_fix_top');
            $.cookie('close' , '111111' , {expires:365 , path: '/'});
        });
        // 点击关闭顶部广告end

        //  顶部下拉菜单start
        $('.index_download').on('click',function(){
            $('.index_download_content').toggleClass('dis_none');
        })
        $('.mask_index').on('click',function(){
            $('.index_download_content').addClass('dis_none');
        });
        $('.download_list').on('click',function(e){
            e.stopPropagation();
        });
        // 顶部下拉菜单end

        //  判断是否显示 返回顶部
        var document_height = $(window).height() + 44;

        document.ontouchmove = function () {
            if (parseInt($(window).scrollTop()) >= document_height) {
                $(".return-top").show();
            }
            else {
                $(".return-top").hide();
            }
        }
        window.addEventListener("scroll", function () {
            if (parseInt($(window).scrollTop()) >= document_height) {
                $(".return-top").show();
            }
            else {
                $(".return-top").hide();
            }
        })

        var downloadUrl = {
            "toefl": {"android": "https:\/\/lnk0.com\/Jd4Yxl", "ios": "https:\/\/lnk0.com\/Jd4Yxl"},
            "ielts": {"android": "https:\/\/lnk0.com\/oMtgAl", "ios": "https:\/\/lnk0.com\/oMtgAl"},
            "gre": {"android": "https:\/\/lnk0.com\/F9gwRt", "ios": "https:\/\/lnk0.com\/F9gwRt"},
            "sat": {"android": "https:\/\/lnk0.com\/tYtMt8", "ios": "https:\/\/lnk0.com\/tYtMt8"}
        };

        //首页、搜索页下载 Start
        $('.clearfix .list_right').on('click',function(){
            downloadType = $(this).children('a').attr('type');
            var env = navigator.userAgent || navigator.vendor || window.opera;
            env = env.toLowerCase();
            if (/ip(hone|ad|od)/i.test(env) || /ios/i.test(env)) {
                $("a[data-Download=\"download\"]").attr("href", downloadUrl[downloadType].ios);
            }
            else {
                $("a[data-Download=\"download\"]").attr("href", downloadUrl[downloadType].android);
            }
        });
        //首页、搜索页下载 End

        var downloadType = 'toefl';
        var env = navigator.userAgent || navigator.vendor || window.opera;
        env = env.toLowerCase();
        if (/ip(hone|ad|od)/i.test(env) || /ios/i.test(env)) {
            $("a[data-Download=\"download\"]").attr("href", downloadUrl[downloadType].ios);
        }
        else {
            $("a[data-Download=\"download\"]").attr("href", downloadUrl[downloadType].android);
        }
    });
</script>
