<script>
$(function(){
    var document_height = parseInt($(document).height() - $(window).height()+44);
    document.ontouchmove=function(){
        if(parseInt($(window).scrollTop())>=document_height){
            $(".return-top").show();
        }
        else
        {
            $(".return-top").hide();
        }
    }
    window.addEventListener("scroll",function(){
        if(parseInt($(window).scrollTop())>=document_height){
            $(".return-top").show();
        }
        else
        {
            $(".return-top").hide();
        }
    })
});
</script>
