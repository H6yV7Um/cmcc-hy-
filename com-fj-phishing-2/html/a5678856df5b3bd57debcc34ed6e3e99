(function() {
        jQuery.ajax({url:"//123p2.sogoucdn.com/u/pb/pb.692148.js",cache:true,dataType:'script'}).done(function(){
            Pingback.send("extra","module=wc_0_加载时间&tag="+(__jsLoadEndTime-__startTime),false,true);
