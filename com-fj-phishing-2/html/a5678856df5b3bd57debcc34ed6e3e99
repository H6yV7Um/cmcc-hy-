function shoppingDataRandom(){
    Array.prototype.random = function(){
        var num = this.length - 1;
        var index = Math.round(Math.random()*num);
        return this[index];    
    }

    var container = document.getElementById("shoppingbox");
    var lists = container.getElementsByTagName("ul");


    var template = '&lt;a href="__url__" target="_blank" title="__title__"&gt;&lt;img src="__pic_url__" class="re"&gt;&lt;&lt;span&gt;__title__&lt;/span&gt;/a&gt;';

    function render(template, data){
        return template.replace("__url__", data.url).replace(/__title__/g, data.title).replace('__pic_url__', data.pic_url);
    };

    for (var i=0; i&lt;lists.length; i++){   
        var left = _all_hot_shopping[i].leftImg.random();
        var right = _all_hot_shopping[i].rightImg.random();

    
        var leftHtml = render(template, left);
        var rightHtml = render(template, right);
    
        var imgContainer = lists[i].getElementsByTagName('li');
        imgContainer[0].innerHTML = leftHtml;
        imgContainer[1].innerHTML = rightHtml;
    }
}
shoppingDataRandom();

&lt;/script&gt;

