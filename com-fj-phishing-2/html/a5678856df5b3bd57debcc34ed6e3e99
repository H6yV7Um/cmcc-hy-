        <script>
(function(w,d,js){
    function timing(){
        if (!performance){
            return -1;
        }

        var row = performance.getEntries();
        var data = null;
        for (var i=0; i<row.length; i++){
            if (row[i].name.indexOf(js) != -1){
                data = row[i];
                break;
            }
        } 

        if (!data){
            return -2;
        }

        return data.duration;
    }

    function refresh(js){
        var node = d.getElementsByTagName('script')[0];
        var j = d.createElement('script');
        j.src = '//hao.qq.com/js/' + js + '?id=JS_FAILOVER' + '&v=' + (+ new Date());
        node.parentNode.insertBefore(j, node);    
    }

    function cdncheck(){
        if(!w.haoqq){
            var all = document.getElementsByTagName('script');
            for(var i=0; i<all.length; i++){
                if (all[i].src && all[i].src.indexOf(js) != -1){
                    var jsname = encodeURIComponent(all[i].src);
                    break;
                }
            }
            
            var t = timing();
            var param = ['jsname='+jsname, 't='+t, 'r=' + (+new Date())];
            param = param.join('&');
            
            var img = new Image();
            img.src = "/img/check.gif?" + param;
            
            var jslist = ['main.minV5.js', 'pb.692148.js'];
            for(var j=0; j<jslist.length; j++){
                refresh(jslist[j]);
            }
        }
    }
    
    w.cdncheck = cdncheck;
})(window, document, 'main.minV5.js');

cdncheck();
</script>


