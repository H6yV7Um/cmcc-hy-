$(window).load(function(){
    $('#AppList img').each(function(){
        var height = $(this).height();
        if(height<80){
           $(this).css('height','80');
        }
    });
}); 
$("img.lazy").lazyload({
     threshold: 300,
     effect : "fadeIn",
     varHeight : "80"
 });
 $('#AppList ul li').on("touchstart", 
     function(){
       $(this).addClass("li-touchstart")
     }
 );
 $('#AppList ul li').on("touchend", 
     function(){
       $(this).removeClass("li-touchstart")
     }
 );
 $('#AppList ul li').click(function(){ 
     location.href = $(this).attr('data-url'); 
 });
function loadArticleList(page){
    var categoryPid = '';
    var cityId = '';
    $('#loadBtn').html('<i class="am-icon-spinner am-icon-pulse"></i>');
    $.get("/index/ajaxarticles",{page:page,categoryPid:categoryPid,cityId:cityId,htmlId:'Home'},function(data){
             $('.index-load-box').remove();
             $('#articleUl').append(data);
             $("img.lazy").lazyload({
                threshold: 300,
                varHeight : "80"
             });
         }
     )
} 
</script>
