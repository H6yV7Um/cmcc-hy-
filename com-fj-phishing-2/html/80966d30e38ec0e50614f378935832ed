var ndate=new Date(),yue=ndate.getMonth()+1,ri=ndate.getDate(),datajsonsrc="/json/n"+ndate.getFullYear()+"/y"+yue+"/r"+ri+".htm";
var syueri=""+(yue*100+ri);
if(syueri.length==3) syueri="0"+syueri;

$.get(datajsonsrc,function(data){
	var istart=data.indexOf('{'),ilast=data.lastIndexOf('}');
	if(istart>-1 && ilast>istart) data=data.substr(istart,ilast-istart+1);
	ThisDate=$.parseJSON(data);
	$("#topyueri").html(ThisDate.month+"月"+ThisDate.day+"日");
	$("#topxqnl").html("星期"+ThisDate.xingqi+"　农历"+ThisDate.yueri+"　"+ThisDate.forcejieri);
	$("#yidata").html(ThisDate.y || ThisDate.yi);	$("#jidata").html(ThisDate.j || ThisDate.ji);
});</script>


