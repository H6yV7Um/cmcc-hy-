                                    switch ($("#formCat").val()) {
                                    case "2":
                                        url = "/net/info/resultg.aspx?recruittype=1&keyword=" + encodeURIComponent(keyword.val());
                                        break;
                                    case "4":
                                        url = "/net/info/resultg.aspx?recruittype=2&keyword=" + encodeURIComponent(keyword.val());
                                        break;
                                    case "5":
                                        url = "/net/wcm/query.aspx?keyword=" + encodeURIComponent(keyword.val());
                                        break;
                                    case "1":
                                    default:
                                        url = "/net/wcm/query.aspx?catID=29&keyword=" + encodeURIComponent(keyword.val());
                                        break;
                                    }

                                    window.location.href = url;
                                }
