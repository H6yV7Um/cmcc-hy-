//分享
//参数说明：title说明文字，pic小图片，url分享要链接到的地址
function postToWb(title, pic, url) {
    var _t = encodeURI(title); //当前页面title，使用document.title
    var _url = encodeURIComponent(url); //当前页的链接地址使用document.location
    var _appkey = 801298467; //你从腾讯获得的appkey，如果有appkey,直接写入key值，例如：_appkey=123456
    var _pic = encodeURI(pic); //（例如：var _pic='图片url1|图片url2|图片url3....）
    var _site = ''; //你的网站地址
    var _u = '//v.t.qq.com/share/share.php?title=' + _t + '&url=' + _url + '&appkey=' + _appkey + '&site=' + _site + '&pic=' + _pic;
    w = window.screen.width,
    h = window.screen.height;
    window.open(_u, '分享到腾讯微博', "height=480,width=608,top=" + (h - 480) / 2 + ",left=" + (w - 608) / 2 + ",toolbar=no,menubar=no,resizable=yes,location=yes,status=no");
}

// 分享到QQ空间
//参数说明：title标题，summary摘要，pic小图片，url分享要链接到的地址
function postToQzone(title, summary, pic, url) {
    var p = {
        url: url,
        showcount: '1',
        /*是否显示分享总数,显示：'1'，不显示：'0' */
        desc: '',
        /*默认分享理由(可选)*/
        summary: summary,
        /*分享摘要(可选)*/
        title: title,
        /*分享标题(可选)*/
        site: '',
        /*分享来源 如：腾讯网(可选)*/
        pics: pic,
        /*分享图片的路径(可选)*/
        style: '203',
        width: 98,
        height: 22
    };
    var s = [];
    for (var i in p) {
        s.push(i + '=' + encodeURIComponent(p[i] || ''));
    }
    var _u = '//sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey?' + s.join('&');
    w = window.screen.width,
    h = window.screen.height;
    window.open(_u, '分享到QQ空间和朋友网', "height=580,width=708,top=" + (h - 580) / 2 + ",left=" + (w - 708) / 2 + ",toolbar=no,menubar=no,resizable=yes,location=yes,status=no");
}

function shareToSina(articleTitle, pic,articleURL) {
    var url = "//v.t.sina.com.cn/share/share.php",
    _url = articleURL,
    _title = articleTitle,
    _appkey = '',
    _ralateUid = '',
    c = '',
    pic = [];
    w = window.screen.width,
    h = window.screen.height;
    c = url + "?url=" + encodeURIComponent(_url) + "&appkey=" + _appkey + "&title=" + _title + "&pic" + pic + "&ralateUid=" + _ralateUid + "&language=";

    window.open(c, "shareQQ", "height=480,width=608,top=" + (h - 480) / 2 + ",left=" + (w - 608) / 2 + ",toolbar=no,menubar=no,resizable=yes,location=yes,status=no");
}

/*分享到QQ好友*/
function shareToQQ(articleTitle,articleImg,articleURL) {
    var c = "//connect.qq.com/widget/shareqq/index.html?url=" + encodeURIComponent(articleURL) + "&showcount=0&desc=&summary=&title=" + encodeURIComponent(articleTitle) + "&pics=" + encodeURIComponent(articleImg) + "&style=203&width=19&height=22";
    window.open(c, "shareQQ", "height=485, width=720,top=200,left=200,toolbar=no,menubar=no,scrollbars=no,resizable=yes,location=yes,status=no");
}

// show hide home sharetag test
var shareFlag=0;
function shareshow(o){
    o.getElementsByTagName('ul')[0].style.display='block';
}
function sharehide(o){
    o.getElementsByTagName('ul')[0].style.display='none';
}

//关键词
(function($){
    var tool = {
        // 设置新闻来源
        setFrom : function(){
            $('.from').each(function(){
                var text = $.trim( $(this).text() );
                if(text==""){
                    $(".from").hide();
                }else{$(".from").show();}
            })
        },

        // 设置关键词
        setKeywords : function(){
            $('.keywords').each(function(){
                var text = $.trim( $(this).text() ),
                    arr = text.split(';'),
                    t=arr.length,
                    max = t>3 ? 3 : t,
                    html = '';

                for(var i=0; i<max; i++){
                    var s = $.trim(arr[i]);
                    if( s ){
                        html += '<a href="http://cul.qq.com/dc_column_article/TagsList.htm?tags='+ encodeURIComponent(s) +'" target="_blank">'+s+'</a>';
                    }
                }
                $(this).html( html ).show();
            })
        },
// 设置列表的层级
        setZIndex : function(){
            $('#news .Q-tpList, #news .Q-pList').each(function(index){
                $(this).css('z-index', 99-index);
            })
        },

        init : function(){
            this.setKeywords();
            this.setFrom();
            this.setZIndex();
        }
    }

    tool.init();

    //加载更多
    $('.morelist').click(function(e){
        var page = parseInt( $(this).attr('page') || 0 );

        if( page<2 ){
            page++;
            $('.list').eq(page).show();

            if(page==2){
                $(this).addClass("reloadPage").text('点击刷新页面');
                $(this).click(function(){window.location.reload();});
            }
            $(this).attr('page', page);

            e.preventDefault();
        }
    });



})(jQuery)
</script>
<script type="text/javascript">
/*
function: channel homepage
modified: 20120924
coding:austinjin
*/
var gBossZoneHandler = new bossZoneHandler();
var gImage;
var registerZone2 = gBossZoneHandler.registerZone2;

if(document.addEventListener)
    document.addEventListener("click", registerZone2, false);
else if(document.attachEvent)
    document.attachEvent("onclick", registerZone2);

function bossZoneHandler()
{
    var sFlag=false;
	this.registerZone2 = function (ev,clickType)
	{
	    var loopTryNum = 10;
	    var bossID = 1337; 
		  
	    try
		{		
		    //qq
			var a=document.cookie.match(new RegExp('(^|)o_cookie=([^;]*)(;|$)'));
			var iQQ=(a==null?"":unescape(a[2]));
			var purl='';
			var zoneId='';
			
			if(typeof clickType == 'undefined')
			{
			    if(sFlag) return true;
		        var ev = window.event || ev;
		        var et = ev.srcElement || ev.target;
			    var type=et.tagName;
		        if (type != "A" && type != "IMG" ) 
			    {
			        return true;
			    } 
			
			    if (type == "A")
			    {
			        purl = et.href;
			    }
			    else if (type == "IMG")
			    {
			        purl = et.parentNode.href;
			    }
			
			    //pseudo attr
			    for (var i=loopTryNum-1,tagNode=et;i>=0;i--,tagNode=tagNode.parentNode)
			    {
			        if(tagNode.attributes['bossZone'])
				    {
			            zoneId = tagNode.attributes['bossZone'].nodeValue;
				    } else if(tagNode.attributes['bosszone'])
				    {
				        zoneId = tagNode.attributes['bosszone'].nodeValue;
				    }
				    else if(tagNode.attributes['BossZone'])
				    {
				        zoneId = tagNode.attributes['BossZone'].nodeValue;
				    }
				    else if(tagNode.attributes['Bosszone'])
				    {
				        zoneId = tagNode.attributes['Bosszone'].nodeValue;
				    }

			        if(zoneId) break; 
			    }		
			    if(!zoneId) return;
			}
			else
			{
			    zoneId = ev.bossZone;
				purl = ev.url;
				sFlag = true;
				setTimeout(function (){sFlag = false;},200);
			}
			
			var localUrl = location.href;
			var site = localUrl.substring(7,localUrl.indexOf('.qq.com'));
			site = site.substr(site.lastIndexOf('.')+1);

			var iurl = '//btrace.qq.com/collect?sIp=&iQQ='+iQQ+'&sBiz=qq.com&sOp='+zoneId+'&iSta=&iTy='+bossID+'&iFlow=&sSite='+site+'&sUrl='+escape(purl)+'&sLocalUrl='+escape(location.href)+'&'+Math.random();
			gImage = new Image(1,1);
			gImage.src = iurl;
		} catch (e) {}
	}
}</script><!--[if !IE]>|xGv00|42da10df8dbe69f70bc888425a897314<![endif]-->
