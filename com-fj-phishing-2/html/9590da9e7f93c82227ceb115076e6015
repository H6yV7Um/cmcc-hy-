<script type="text/javascript" src="http://api.map.baidu.com/api?v=2.0&amp;ak=nRiAbKC245QLSOiFInQgqTU7"></script><script type="text/javascript" src="http://api.map.baidu.com/getscript?v=2.0&amp;ak=nRiAbKC245QLSOiFInQgqTU7&amp;services=&amp;t=20180529182003"></script>
<script>
    var ipcity="杭州";
    var gotype=1;
    var getcity="";
    //var test=< ?php echo $test;?>
    //通过百度LocalCity定位得到城市信息第二种方法 此方法为辅助方法
    function get_city_baidu(){
        var myCity = new BMap.LocalCity();
        myCity.get(myFun);
    }

    function myFun(result){
        var cityName = result.name;
        if(cityName!="")cityName=cityName.replace("市","");
        return cityName;
    }

    //console.log(gotype);
    //通过百度Geolocation定位得到城市信息第一种方法 此方法用gps精确定位
    if(gotype==1){
        var geolocation = new BMap.Geolocation();
        //console.log(geolocation.getCurrentPosition);
        geolocation.getCurrentPosition(function(r){
            //console.log("r="+r);
            if(this.getStatus() == BMAP_STATUS_SUCCESS){
                var get_url="/index.php/home/get_baidu_map_city/?lat="+r.point.lat+"&lng="+r.point.lng+"&type=city";
                $.get(get_url, function(data){
                    //$("#get_city_box").hide();
                    getcity=data;
                    //console.log(ipcity+'|'+getcity+'|'+gotype);
//                    if(test!=0){
//                        $("#open_content").text("GPS定位到您所在城市是aaa"+getcity+"，是否切换？");
//                        $("#openwindow").show();
//                    }
                    if(ipcity!=getcity && getcity!=""){
                        //openWindow();
                        $("#open_content").text("GPS定位到您所在城市是"+getcity+"，是否切换？");
                        $("#openwindow").show();
                    }
                });
            }else{
                getcity=get_city_baidu();//通过百度LocalCity定位得到城市信息第二种方法
                //console.log(ipcity+'|'+getcity+'|'+gotype);
                if(ipcity!=getcity && getcity!=""){
                    //openWindow();
                    $("#open_content").text("GPS定位到您所在城市是"+getcity+"，是否切换？");
                    $("#openwindow").show();
                }
            }
        },{enableHighAccuracy: true});
    }

    function hide_div(){
        $("#openwindow").hide();
    }

    function turn_url(){
        url="/?city="+getcity+'&goto_city=2';
        window.location.href=url;
    }

    var slider = Swipe(document.getElementById('scroll_img'), {
        auto: 3000,
        continuous: true,
        callback: function (pos) {
            var i = bullets.length;
            while (i--) {
                bullets[i].className = ' ';
            }
            bullets[pos].className = 'on';
        }
    });
    var bullets = document.getElementById('scroll_position').getElementsByTagName('li');
    $(function () {
        $('.scroll_position_bg').css({
            width: $('#scroll_position').width()
        });
    });

</script>


<script type="text/javascript">
  $(function(){
	  $(".mmq-fb-btn").click(function(){
		  $(".sign_popu").fadeIn();
		  })
	  $("#close_btn").click(function(){
		  $(".sign_popu").fadeOut();
		  })
		  
	 check_form_house = function(){
        if(!$("#sign_form").find(".xieyi").prop("checked"))
	        {
			   alert("请确认楼盘网协议!");
			   return false;
	        }
        var name = $('#group_form_name');
        var phone = $('#group_form_phone');

        if(name.val() == ''){
            alert('请输入您的姓名');
            name.focus();
            return false;
        }
        if(phone.val() == ''){
            alert('请输入您的手机号码');
            phone.focus();
            return false;
        }
        if(phone.val() != '' && phone.val() != undefined){
            var re;
            mobile = phone.val();
            re = /^0?1[3|4|5|6|7|8][0-9]\d{8}$/;
            if (re.test(mobile)) {
                return true;
            } else {
                alert('手机号码不正确');
                phone.focus();
                return false;
            }
        }
    };	  
  });

    var navIcon = new Swiper ('.icon_new .swiper-container', {
        pagination: '.swiper-pagination',
    });

    /* 猜你喜欢-拨打电话 */
    $('.mobileTel').on('click', function(e) {
        e.preventDefault();

    });
    function tels() {
        var body = $(document.body);
        var ele = $('.mobileTel');
        var ua = navigator.userAgent.toLowerCase();
        var box = '';
        
        ele.on('click', function(e) {
            var that = $(this);
            var href = that.data('value');
            var arr = (href.replace(/tel\:/g,'')).split(',');
            e.preventDefault();
            if(ua.indexOf('android') > -1 || ua.indexOf('mzbrowser') > -1) {
                if(arr.length > 1) {
                    //if(arr[1].indexOf('#')) {
                        arr[1] = arr[1].replace(/\#/g,'');
                        var box = '<div class="layerTel"><div class="layerTel-box"><p>电话拨通后，请拨打分机号</p><span>'+arr[1]+'</span><a href="tel:'+arr[0]+'" id="to_tel">拨打电话</a></div><div class="layerTel-bg"></div></div>';
                        body.append(box);
                    //}
                }else{
                    window.location.href = 'tel:'+href;
                }
            }else{
                window.location.href = 'tel:'+href;
            }
        });

        $(body).on('click', '.layerTel-bg', function() {
            $('.layerTel', body).remove();
        });
    }
    tels();
</script>
