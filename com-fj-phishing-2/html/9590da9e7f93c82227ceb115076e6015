        wx.onMenuShareAppMessage({
            title: shareObj.title,
            desc: shareObj.desc,
            link: shareObj.link,
            /*imgUrl: "",*/
            imgUrl: shareObj.img,
            trigger: function (res) {
                /*不要尝试在trigger中使用ajax异步请求修改本次分享的内容，因为客户端分享操作是一个同步操作，这时候使用ajax的回包会还没有返回
                alert('用户点击发送给朋友');*/
            },
            success: function (res) {
                /*alert('已分享');*/
            },
            cancel: function (res) {
                /*alert('已取消');*/
            },
            fail: function (res) {
                console.log(JSON.stringify(res));
            }
        });

