            },
            success: function (res) {
                /*alert('已分享');*/
            },
            cancel: function (res) {
                /*alert('已取消');*/
            },
            fail: function (res) {
                /*alert(JSON.stringify(res));*/
            }
        });

        /*8.2 显示右上角菜单*/
        wx.showOptionMenu();

    });

    wx.error(function (res) {
        console.log(res.errMsg);
    });
</script>
