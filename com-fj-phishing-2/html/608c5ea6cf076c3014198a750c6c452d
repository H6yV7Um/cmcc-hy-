    if (!Validator.isPhone($("#liuliangPhone").val())) {
        $("#returnliluliangPhone").show();
        //$("#liuliangPhone").val("");
        $("#returnliluliangPhone").text("请填写正确的手机号！");
        $("#successText3").hide();
        return false;
    } else {
        $("#returnliluliangPhone").hide();
    }
    return true;
}
