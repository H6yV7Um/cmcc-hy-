<script type="text/javascript">
function editUamUserInfo(){
         $.ajax({
                   type: "GET",
                   url: "/dqmh/Uam.do?method=getUamUserInfo",   
                   dataType: "json",
                   async:false,
                   success: function(msg){
                      if(msg==null||(msg!=null&&(msg.userType=="201"||msg.userType=="202"||msg.userType=="203"))){
                                window.location.href = "/dqmh/ssoLink.do?method=linkTo&platNo=10012&toStUrl=http://zj.189.cn/zjpr/user/myOrder.html";
                          }else{
                                 var ss=new Date();
                                window.location.href = "/dqmh/userCenter/myOrderInfoList.do?method=listMyOrderInfo_new&isVs=yes&rand="+ss.getTime();
                          }	
                   },
                   error:function(msg){
                          window.location.href = "/dqmh/ssoLink.do?method=linkTo&platNo=10012&toStUrl=http://zj.189.cn/zjpr/user/myOrder.html";
                       }
          });
}
</script>
