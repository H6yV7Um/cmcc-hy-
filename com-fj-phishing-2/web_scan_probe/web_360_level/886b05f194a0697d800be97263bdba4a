<script type="text/javascript" src="/js/jquery.pagination.js"></script>
<script type="text/javascript" src="/js/jquery.pinwheel-0.1.0.js"></script>
<script>
var comment_token = '';
var isSendBtn = true;
$(document).ready(function(){
	ajax_get_comments(1,'m.yuerbao.com',4);
	$("#content").focus(function(){
		$("#comment_error").html("");
		$("#comment_error").css('color' ,'#FF0000');
	});
});	
//获取评论
function ajax_get_comments(pagenum,id_value,comment_type){
    if(comment_type == 4){
		id_value = encodeURIComponent(id_value);
	}
	if(!pagenum){
		pagenum = 1;
	}
  $.post("/comment/ajaxlist",{
		id_value:id_value,
		page:pagenum,
		comment_type:comment_type,
		yzm:"e482c1c51fbfe3c71cce75340a42d7ff",
		ignore:new Date().getTime()
		},
		function(data){
			comment_token = data.token;
			if($("#check_web_comment")){
				if(pagenum == 1){
					if(data.record_count > 0){
						if(comment_type == 4){
							$("#check_web_comment").html('查看此网站的评论');
						}else if(comment_type == 3){
							$("#check_web_comment").html('查看此漏洞的评论');
						}
					}else{
						if(comment_type == 4){
							$("#check_web_comment").html('对该网站进行评论');
						}else if(comment_type == 3){
							$("#check_web_comment").html('对该漏洞进行评论');
						}
					}
				}
			}
		var items_per_page = 20;
		$("#comment_totalnums_span").html(data.record_count);
		if(data.record_count == 0){
			return ;
			//data.record_count=1;
		}
		$("#pagination").pagination(data.record_count, {
			num_edge_entries: 2,
			num_display_entries: 5,
			prev_text:'<',
			next_text:'>',
			callback: pageselectCallback,
			items_per_page:items_per_page,
			current_page:(data.cur_page-1)
		});
		   
		var html_str='';    
		for(var i=0;i<data.comments.length;i++){
		    				html_str += '<dl class="plbg1 clearfix" id="dl_'+data.comments[i].comment_id+'"><dt><span class="louchen">'+data.comments[i].level+'楼</span><span style="line-height: 17px;padding-right: 10px;">'+data.comments[i].add_time+'</span><span style="font-weight: bold;color:#333;line-height: 17px">'+data.comments[i].user_name+'</span></dt>';			
						if(data.comments[i].isSiter == 1){
				html_str += '<dd><a ajax-href="/group/showuserpro/uid/'+data.comments[i].user_id+'/from/comment" class="loading_userpro"><img width="30" height="30" style="position: absolute; left: 10px; top: 6px;" src="'+data.comments[i].headlogo+'"></a><p>'+replaceTextarea1(data.comments[i].content)+'</p></dd></dl>';				
				
			}else{
				html_str += '<dd><img width="30" height="30" style="position: absolute; left: 10px; top: 6px;" src="'+data.comments[i].headlogo+'"><p>'+replaceTextarea1(data.comments[i].content)+'</p></dd></dl>';
			}
		}
	html_str +='';
	$("#comments").html(html_str);
	$('.loading_userpro').pinwheel();
	
	if(data.record_count <= items_per_page){
		$("#pagination").html("");
	}
},'json'
);
}

//分页回调函数

function pageselectCallback(page_index, jq){
	ajax_get_comments(page_index+1,'m.yuerbao.com',4);
	return false;
}
/*
function score(sco_action,id){
	$.post("/vul/score",{sco_action:sco_action,id:id},function(data){
		if(data && data.state == "ok"){
			if(data.action == "up"){
				$("#up_num").html(parseInt($("#up_num").html()) + 1);
			}else if(data.action == "down"){
				$("#down_num").html(parseInt($("#down_num").html()) + 1);
			}
		}else{
			alert(data.msg);
		}
	  },
	  "json");
}
