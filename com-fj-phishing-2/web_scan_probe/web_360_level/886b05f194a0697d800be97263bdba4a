	$("#more_ssl_table").css("display", "block");
	$("#more_ssl_a").hide();
}
function show_solutioncg(t) {
	if (t == 1) {
		show_solution("cg1");
	} else {
		show_solution("cg2");
	}
}
function to_good(){
	$.ajax({
		  url: "/group/good",
		  type:"POST",
		  dataType:"json",
		  data:"domain=m.yuerbao.com&token_good=eefbdc4e95d336cc75ad9323fd342a97&time=1529548024",
		  success: function(data){
				if(data.state == "yes"){
					good_tips_func();
					setTimeout(function(){$('#good_nums').html(parseInt($('#good_nums').html())+1);},900); 
					
					$('#good_img').attr("src","/img/group/v9-site-good-icon.png");
				}
		  }
	});	
	
}
function good_tips_func(){
	
	var anquanbi_obj = $('.anquanbi_tips');
	anquanbi_obj.html('+ '+1+'赞，+1 积分');
	anquanbi_obj.show().animate({
		top: "-20px",
		opacity: 1
	},
	"slow",
	function() {
		setTimeout(function() {
			anquanbi_obj.animate({
				top: "20px",
				opacity: 0
			},
			"slow")
		},
		900)
	});
}
