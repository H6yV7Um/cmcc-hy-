function getVulType(type)
{
	if(type==32)
	{
		return "该页面被挂马";
	}
	else if(type==8)
	{
		//return "该页面存在木马文件";
		return "该页面存在恶意下载链接";
	}	
	else if(type==16)
	{
		return "该页面可能存在钓鱼信息";
	}
	else if(type==48)
	{
		return "恶意下载";
	}
	else
	{
		return "其他";
	}
}
function getlinktype(type)
{
	if(type=="iframe")
	{
		return "<b style='color:red'>被植入不可见的恶意框架</b>";
	}
	else if(type=="script")
	{
		return "<b style='color:red'>被植入不可见的恶意脚本</b>";
	}
	else if(type=="exe")
	{
		return "<b style='color:red'>被植入木马文件</b>";
	}	
	else
	{
		return "<b style='color:red'>被植入恶意文件</b>";
	}
}
function to_tongjiweibo(){
      var leverl_t = $("#jg_tips").text();
	  var level = 0;
	  if(leverl_t == "高危"){
	  	 level = 3;
	  }else if(leverl_t == "严重"){
	     level = 2;
	  }else if(leverl_t == "安全"){
	     level = 1;
	  }else{
	  	 level = 0;
	  }
	  var trojan_text = $("#trojan_sec").text();
	  var tamper = 0;
	  var trojan = 0;
	  	  if(trojan_text == "被挂马" || trojan_text == "恶意下载"){
	  	trojan = 1
	  }
		$.ajax({
			  url: "/index/weibotongji",
			  type:"POST",
			  dataType:"json",
			  data:"url=m.yuerbao.com"+"&tamper="+tamper+"&trojan="+trojan+"&level="+level,
			  success: function(data){

			  }
		});

}
function to_tongjicheckwebsite(is_trojan){
      var referer = "";
      var leverl_t = $("#jg_tips").text();
	  var level = 0;
	  if(leverl_t == "高危"){
	  	 level = 3;
	  }else if(leverl_t == "严重"){
	     level = 2;
	  }else if(leverl_t == "安全"){
	     level = 1;
	  }else{
	  	 level = 0;
	  }
		$.ajax({
			  url: "/index/checkwebsitetongji",
			  type:"POST",
			  dataType:"json",
			  data:"url=m.yuerbao.com"+"&level="+level+"&referer="+referer+"&is_trojan="+is_trojan,
			  success: function(data){
				    
					var randdatas = data.result;
					var htmltext = "";
					if(randdatas.length > 0){
						htmltext += '<h3 class="zz-info">最新安全资讯</h3>';
						for(var i=0; i<randdatas.length; i++){
							var title = cutstr(randdatas[i].title, 23);	
							htmltext += '<dl class="clearfix"><dd><a style="color:#666" target="_blank" href="http://bobao.360.cn/news/detail/'+randdatas[i].id+'.html" >'+title+'</a></dd></dl>';
						}
						$("#bobao_div").html(htmltext);
					}


			  }
		});

}
function cutstr(str,len)
{
   var str_length = 0;
   var str_len = 0;
      str_cut = new String();
      str_len = str.length;
      for(var i = 0;i<str_len;i++)
     {
        a = str.charAt(i);
        str_length++;
        if(escape(a).length > 4)
        {
         
         str_length++;
         }
         str_cut = str_cut.concat(a);
         if(str_length>=len)
         {
         str_cut = str_cut.concat("...");
         return str_cut;
         }
    }
  
    if(str_length<len){
     return  str;
    }
}
function to_valideWebSite(){
        $("#top_loading").show();
		$.ajax({
			  url: "/domain/add2",
			  type:"POST",
			  dataType:"json",
			  data:"NewDomainBox=m.yuerbao.com&token_adddomain=23641d1a6a71ad98c0c93eb4cfb87442",
			  success: function(data){
			     if(data.state == "paiverify" || data.state == "already_add"){
				    window.location.href = "/manage/index/domainid/"+data.domainid;
				 }else{
				    $("#top_loading").hide();
				 	alert(data.message);
					if(data.token){
						window.location.reload();
					}
				 }
			  }
		});
}
function asyncGetTrojan()
{
	//var url = $("input[name='url']").attr("value");
	var url = 'm.yuerbao.com';
	if(url.length>0)
	{
		$.ajax({
			  url: "/index/gettrojan",
			  type:"POST",
			  dataType:"json",
			  data:"url="+url+"&token=a42903900454943adc4539a8301ebc2a&time=1529548024",
			  success: function(data){	
			        var type = "";
					var st = "";
					var sc = "";
					var ssc = "";
					var trojan_list = "";
					var isxujia = 0;
					var isguama = 0;
					var kx = 0;
					var kx_state = 0;
					var kx_url = '';
					var is_cuangai = 0;
					if(data.trojan){	
						type = data.trojan.type;
						st = data.trojan.st;
						sc = data.trojan.sc;
						ssc = data.trojan.ssc;
						trojan_list = data.trojan.list;
					}
					if(data.kx){
						kx_state = data.kx.state;
						kx_url = data.kx.url;
					}
					var kneturl = data.knet;
					if(kx_state == 1){
						if(kx_url.length > 0){
							$('#kx_dl').html('<dd><strong>网站认证：</strong></dd><dd><a href="'+kx_url+'" target="_blank" title="凭安认证"><img width="80" src="http://p8.qhimg.com/d/webscan/pingan_logo.jpg" border="0" /></a></dd>');
						}else{
							$('#kx_dl').html('<dd><strong>网站认证：</strong></dd><dd><img src="http://p8.qhimg.com/d/webscan/pingan_logo.jpg" border="0" /></dd>');
						}
						//$('#kx_dl').addClass('website-info');
					}
					if(kneturl){
						$('#kx_dl').html('<dd><strong>网站认证：</strong></dd><dd><a href="'+kneturl+'" target="_blank" title="中网认证"><img width="80" src="http://p6.qhimg.com/t01d09f3a6c0799f789.png" border="0" /></a></dd>');
						//$('#kx_dl').addClass('website-info');
					}
					if(type == 60 && st == 10 && sc == 115 && ssc == 1151){
						is_cuangai = 1;
						$("#cuangai_sec").attr("class","fc_dangerous");					
						$("#cuangai_sec").html("遭恶意篡改");
						
						$("#query").attr("src", "/img/zhiming.png?v=1");
						$("#jg_tips").text("高危");	
						$("#jg_tips").attr("class","jg_dangerous");	
						
						
						
						$("#diaoyu_sec").attr("class","fc_security");
						$("#diaoyu_sec").text("正常");	
						$("#report_diaoyu").html('<div class="gm_safe">　　正常</div>');						
						$("#report_diaoyu").show();	
					
						$("#trojan_sec").attr("class","fc_security");
						$("#trojan_sec").text("正常");	
						$("#report_gm").html('<div class="gm_safe">　　正常</div>');						
						$("#report_gm").show();	
						
						
							$("#report_cg").html('<div class="gm_info2" style="width:1295px">该网站多个子域名或页面存在异常的虚假或恶意内容，虚假和恶意的内容会诱骗访问者上当或使您的计算机感染木马或病毒，此种情况可能是因为网站被黑客入侵篡改，为了避免给您造成损失，请您谨慎访问。</div><div class="fast_conf" style="width:200px">如果您是站长，请走<a href="http://webscan.360.cn/p2p/index/domain/m.yuerbao.com" target="_blank">申诉通道</a></div>');
						
					}else if(type == 40 && st == 50){
						is_cuangai = 1;
						$("#cuangai_sec").attr("class","fc_dangerous");					
						$("#cuangai_sec").html("遭恶意篡改");
						
						$("#query").attr("src", "/img/zhiming.png?v=1");
