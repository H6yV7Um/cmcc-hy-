		});	
}
function show_no_validcheck(isxujia, isguama, score_value, scantime, is_domain_valid, isscan, isdomainsafe, iscuangai, isviolation) {
	if (is_domain_valid || isxujia || isguama || iscuangai) {
		if ( ($("#jg_tips").text().indexOf('高危') >=0 || $("#jg_tips").text().indexOf('严重') >=0)&& !0  && !parseInt(isxujia) && !parseInt(isguama) && !parseInt(iscuangai) && !parseInt(isviolation) && !0) {
			if ($("#vaildnosafedomain").html()) {
				$("#vaildnosafedomain").css("display", "block");
				$("#dabai_text_div").css('display' , 'none');
				$(".ld-main-list").html('<ul><li class="report_step4">&nbsp;&nbsp;站长正在努力修复网站漏洞，相关安全信息已经隐藏，天后公示安全得分</li></ul>');
			} else {
				$(".jg_showbox").css("display", "block");
				$(".jg_conlist").css("display", "block");
				$(".jg-bipin").css("display", "block");
			}
		} else {
			$(".jg_showbox").css("display", "block");
			$(".jg_conlist").css("display", "block");
			$(".jg-bipin").css("display", "block");
		}
		if (parseInt(isxujia) || parseInt(isguama) || parseInt(iscuangai) || parseInt(isviolation)) {
			$(".showlogin").click(function(){
				_gaq.push(['_trackPageview', '/checkwebsite/add_domain_guama']);
			}); 
		} else if (0) {
			$(".showlogin").click(function(){
				_gaq.push(['_trackPageview', '/checkwebsite/add_domain_black']);
			});
		} else if ( score_value >= 85 ) {
			$(".showlogin").click(function(){
				_gaq.push(['_trackPageview', '/checkwebsite/add_domain_veri_safe']);
			});
		} else if ( score_value < 85 ) {
			$(".showlogin").click(function(){
				_gaq.push(['_trackPageview', '/checkwebsite/add_domain_veri_nosafe']);
			});
		}
	} else if (isscan && isdomainsafe  && score_value>=80) {
		scantime = scantime.replace(/(前)/g, "");
		var str = "您的网站已经<strong class=\"red\">"+scantime+"</strong>没有检测了，上次安全得分"+score_value+"分，建议立即检测";
		var str2 = "暂时没有发现被黑客入侵的痕迹，建议立即进行安全评估，领先黑客一步";
		var str3 = "我是站长，我要立即检测";
		$(".scan-v1 dt").html(str);
		$(".scan-v1 dd").html(str2);
		$(".scan-tips span").html(str3);
		$(".scan-v1").css("display", "block");
		$(".scan-tips").css("display", "block");
		$("#dabai_text_div").css('display' , 'none');
		$(".jg_showbox").css("display", "none");
		$(".jg_conlist").css("display", "none");
		$(".jg-bipin").css("display", "none");
		$(".scan-tips-btn1").click(function() {
			_gaq.push(['_trackPageview', '/checkwebsite/add_domain_noveri_safe']);
		});
	} else if (isscan) {
		/*var str = "您的网站"+scantime+"安全得分<strong class=\"red\">"+score_value+"</strong>分，建议修复漏洞";
		var str2 = "您可以对网站漏洞进行一键修复，避免黑客入侵，保护网站安全";
		var str3 = "我是站长，查看检测结果";
		$(".scan-v1 dt").html(str);
		$(".scan-v1 dd").html(str2);
		$(".scan-tips span").html(str3);
		$(".scan-v1").css("display", "block");
		$(".scan-tips").css("display", "block");*/
		$(".jg_showbox").css("display", "block");
		$(".jg_conlist").css("display", "block");
		$(".jg-bipin").css("display", "block");
		$("#dabai_text_div").css('display' , 'block');
		
		$(".scan-tips-btn1").click(function() {
			_gaq.push(['_trackPageview', '/checkwebsite/add_domain_noveri_nosafe']);
		});
		$(".showlogin").click(function(){
				_gaq.push(['_trackPageview', '/checkwebsite/add_domain_noveri_nosafe']);
		});
	} else {
		$(".scan-unknown-tip").css("display", "block");
		$(".scan-v1").css("display", "block");	
		$("#scan-tips-1").css("display", "block");	
		
	}
}
