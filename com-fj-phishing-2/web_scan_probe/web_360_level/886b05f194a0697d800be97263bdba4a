	$("#add_btn").click(function(){
		var content = $.trim($("#content").val());
		var id_value = $("#id_value").val();
		var comment_type = $("#comment_type").val();
		$("#comment_error").css('color' ,'#FF0000');
		if(content == "我来点评..."){
			$("#comment_error").html("请输入评论内容!");
			$("#comment_error").show();
			$("#content").css('border-color' ,'#FF0000');
			return false;			
		}
		if(content == ""){
			$("#comment_error").html("请输入评论内容!");
			$("#comment_error").show();
			$("#content").css('border-color' ,'#FF0000');
			return false;
		}
		if(!isSendBtn){
			$("#comment_error").html("已经超出字数限制!");
			$("#comment_error").show();
			$("#content").css('border-color' ,'#FF0000');
			return false;			
		}
		if($('#yzm_comment').length > 0){
			var yzm = $('#yzm_comment').val();
			if(yzm == ''){
				$("#comment_error").html("请输入验证码!");
				$("#comment_error").show();

				return false;				
			}
		}
		$(this).attr('disabled',true).css("background-color",'#bbbbbb').css("border-color",'#bbbbbb');
		$.post("/comment/ajaxadd",{content:content,id_value:id_value,comment_type:comment_type,yzm:yzm,token:comment_token},function(data){
		if(data && data.state == "ok"){
			//alert("评论成功,等待管理员审核");
			ajax_get_comments(1,'m.yuerbao.com',4);
			$("#comment_error").css('color' ,'green');
			$("#comment_error").html("评论成功!");
			points_tips_func_comment();
			$("#content").val("");
			$("#comment_error").show();
			if($('#yzm_comment').length > 0){
				RefreshImage();
			}
			if($('#yzm_comment').length > 0){
				$('#yzm_comment').val('');
			}
			$("#add_btn").removeAttr("disabled").css("background-color",'#3FA156').css("border-color",'#1D7431');
			//$("#comment_error").fadeOut(4000);
		}else{
			//alert(data.msg);
			$("#comment_error").css('color' ,'#FF0000');
			$("#comment_error").html(data.msg);
			$("#comment_error").show();
			//$("#comment_error").fadeOut(4000);
			$("#add_btn").removeAttr("disabled").css("background-color",'#3FA156').css("border-color",'#1D7431');
		}
	  },
	  "json");

	});
</script>
<script>
$(function(){
    $("#content").focus(function(){
			$("#content").css('color' , '#333333');
			$("#content").css('border-color','#333333');
			if($("#content").val() == "我来点评..."){
				$("#content").val('');
			}
			to_limitInput();
		}).keyup(function(){
			to_limitInput();
		}).keydown(function(){
			to_limitInput();
		}).blur(function(){
			if($("#content").val() == ""){
				$("#content").css('color' , '#999999');
			}
		});
});
function to_limitInput(){
	var content = $("#content").val();
	content = $.trim(content);
    var maxNum = 120;
	var iLen = 0;		
	for (i = 0; i < content.length; i++) iLen += content.charAt(i).charCodeAt() > 255 ? 1 : 0.5;
	var retnums = Math.abs(maxNum - Math.floor(iLen));
	$("#inputnums").html(retnums);
	//if(retnums < 0)
	if((maxNum - Math.floor(iLen)) >= 0){
		isSendBtn = true;
		$("#inputnums_text").html('还可以输入');
		$("#inputnums").css('color' , '#666666');
	}else{
		$("#inputnums").css('color' , '#FF0000');
		$("#inputnums_text").html('已超出');
		isSendBtn = false;
	}
}
function to_deleteComment(id){
	if(id == "" || id == 0){
		return false;
	}
	if(!confirm('你确认要删除此楼评论吗?')){
		return false;
	}
	$.ajax({  	
			url : "/comment/delcommemt",
			type: 'POST',
			dataType:  'json',
			data:'id='+id,
			success: function(data){
				if(data.state == "yes"){
					$("#dl_"+id).remove();
				}else{
				
				}
			}
	});
}
</script>
<script type="text/javascript">
function RefreshImage()
{
	var imgsrc = $("#img11").attr("src");
	if(!imgsrc)
	{
		$("#img11").attr("src","/index/getcaptcha2");
	}
	
   var el =document.getElementById("img11");
   el.src=el.src+'?';  //这个特别重要
}
function replaceTextarea1(str){
	var reg=new RegExp("\n","g");
	str = str.replace(reg,"<br>");
	
	return str;
}
</script>
