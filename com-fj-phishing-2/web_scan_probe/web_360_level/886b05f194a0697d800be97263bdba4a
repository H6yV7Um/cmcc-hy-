					if(isxujia == 1 || isguama == 1){
						is_trojan = 1;
					}
					to_tongjicheckwebsite(is_trojan);
					//end 统计网站完全评级
					//begin 计算分数
																	
					
					getWebscore(isxujia , is_cuangai, '0' , isguama , '' , '' , '' , '');
					
					//end 计算分数	
							
					//begin 挂马列表
					var rowcontext = '<div style="margin-bottom: 8px;font-size:14px;color:#000000;border:1px solid #F0C36D;background-color:#F9EDBE;padding:0px 6px">如果您已经删除网站的木马或恶意内容，或者您的网站确实没有挂马或者不存在恶意内容，点击 <a  onclick="return hs.htmlExpand(this,{objectType:\'iframe\',width:420,height:230})" href="/manage/antitrojan/url/m.yuerbao.com">站长申诉通道</a> ; <br>注：根据近期站长反馈情况来看，很有可能是被黑客入侵导致挂马或钓鱼，推荐使用<a onClick="_gaq.push([\'_trackPageview\', \'/renjia/checkwebsite\']);" href="http://butian.360.cn" target="_blank">企业级安全服务</a></div>';	
					if(trojan_list != null && trojan_list.length > 0){
						var list = trojan_list;
						rowcontext+= '<table width="100%" border="0" cellpadding="0" cellspacing="1" bgcolor="#cccccc" style="table-layout:fixed;word-wrap:break-word;font-size:14px;"><tr bgcolor="#F2F2F2"><th width="15%">类型</th><th width="50%" height="25">被挂马的网址</th><th>详细说明</th></tr>';
						  
						for(var i=0; i<list.length; i++)
						{
							var row = list[i];
							if(!row)	break;
							rowcontext+='<tr bgcolor="#FFFFFF">';
							
							
							rowcontext+="<td>"+getVulType(row["type"])+"</td>";
							rowcontext += '<td height="25">'+row["url"]+"</td>";
							rowcontext+="<td><ul>";
							
							
							if(row["relation"])
							{	
								var linktype= '';
								var rellen = row["relation"].length;
								for(var j=0;j<rellen; j++)
								{
									if(row["relation"][j]["tag"])
									{
										linktype = getlinktype(row["relation"][j]["tag"]);
										rowcontext+="<li  class=\"g\">"+ linktype +"<br>" + row["relation"][j]["url"]+"</li>";
									}
								}
							}
							rowcontext+="</ul></td></tr>";
						}
						rowcontext += "</table>";
						$("#report_gm").html(rowcontext);						
						$("#report_gm").show();	
					}else{
						if((type == 60 && st == 20 && sc == 101)||(type == 60 && st == 20 && sc == 102)||(type == 60 && st == 20 && sc == 103)||(type == 70 && st == 0 && sc == 100)){
							var rowcontext_1 = '<div style="margin-bottom:8px;font-size:14px;color:#000000;border:1px solid #F0C36D;background-color:#F9EDBE;padding:0px 6px">如果您已经删除网站的木马或恶意内容，或者您的网站确实没有挂马或者不存在恶意内容，点击 <a  onclick="return hs.htmlExpand(this,{objectType:\'iframe\',width:420,height:230})" href="/manage/antitrojan/url/m.yuerbao.com">站长申诉通道</a> ; <br>注：根据近期站长反馈情况来看，很有可能是被黑客入侵导致挂马或钓鱼，推荐使用<a onClick="_gaq.push([\'_trackPageview\', \'/renjia/checkwebsite\']);" href="http://butian.360.cn" target="_blank">企业级安全服务</a></div>';	
							var high_text = getTrojanText(type , st , sc);
							var rowcontext = '<div class="gm_info2" >'+high_text+'</div>'+rowcontext_1;
							$("#report_gm").html(rowcontext);						
							$("#report_gm").show();						
						}
					}				
					//end   挂马列表		
			  }
		});
	}
					
}
function getTrojanText(type , st , sc){
	var text = "";
	if(type == 60){
		if(st == 10){
			if(sc == 100){
				text = "该网站推广的产品或其治疗范围属于国家明令禁止进行医疗广告的范畴，或使用了违禁内容或方式，请谨慎访问。";
			}else if(sc == 101){
				text = "该网站含有未经证实的金融证券相关内容，如果网站存在模拟登录、未经批准收取会员费等行为，会给您造成财产损失。";
			}else if(sc == 102){
				text = "该网站含有未经证实的彩票相关内容，如果网站未获经营许可而向您收取会员费、预测费、保密费等费用，会给您造成财产损失。";
			}else if(sc == 103){
				text = "该网站含有未经证实的境外博彩相关内容或广告链接，不可靠的博彩相关内容可能给您造成财产损失，请您谨慎访问。";
			}else if(sc == 104){
				text = "该网站含有未经证实的中奖活动内容，虚假的中奖活动网站会要求您支付中奖相关费用或输入账号密码，请您谨慎访问。";
			}else if(sc == 105){
				text = "该网站存在未经证实的用户登录框内容，通过未经证实的登录框登录，可能会导致账号信息被盗，请您谨慎访问。";
			}else if(sc == 106){
				text = "该网站存在未经证实的商品、商户、支付等相关信息，虚假交易信息可能会给您造成财产损失，请确认是否为官方网站。";
			}else if(sc == 107){
				text = "该网站含有未经证实的软件下载信息，下载带有木马的软件可能对您的电脑安全造成威胁，请您谨慎访问。";
			}else if(sc == 108){
				text = "该网站含有大量未经证实的广告信息，虚假广告可能给您造成财产损失，请您谨慎访问。";
			}else if(sc == 109){
				text = "该网站含有未经证实的弹窗广告信息，误点击模仿常用软件弹窗的内容可能会给您的电脑安全造成威胁，请您谨慎访问。";
			}else if(sc == 110){
				text = "该网站含有大量未经证实的信息，访问含有欺诈内容的网站可能会给您造成财产损失，请您谨慎访问。";
			}else if(sc == 111){
				text = "该网站可能盗用其他正规网站的标识，可能会给您带来损失，请谨慎访问。";
			}else if(sc == 112){
				text = "该网站含有大量未经证实的招聘信息，虚假招聘网站可能会让应聘者支付保证金、保密费、手续费等各种费用，请您谨慎访问。";
			}else if(sc == 113){
				text = "该网站所属学校不在教育部公布的全国普通高校、院校信息库中。未经教育部批准的学校属于虚假办学，请您谨慎访问。";
			}else if(sc == 114){
				text = "该网站不具备中国航空运输协会颁发的机票销售代理资质，假冒的机票销售网站诱骗访问者上当，骗取您的个人信息和钱财。";
			}else if(sc == 115){
				text = "该网站多个子域名或页面存在虚假或恶意内容，虚假和恶意的内容会诱骗访问者上当或使您的计算机感染木马或病毒，此种情况可能是因为网站被黑客入侵篡改，为了避免给您造成损失，360安全中心已经帮您拦截，请您谨慎访问。";
			}else if(sc == 116){
				text = "当前页面含有大量虚假信息，误信虚假电话和账号信息可能会给您造成财产损失，请您谨慎访问。";
			}else if(sc == 117){
				text = "当前页面是一个假冒的话费充值网站，在假冒的话费充值网站充值将会给您造成财产损失，请谨慎访问。";
			}else if(sc == 118){
				text = "当前页面是一个假冒的团购网站，在假冒的团购网站购物将会给您造成财产损失，请谨慎访问。";
			}else if(sc == 119){
				text = "当前页面是一个假冒的网游交易网站，在假冒的网游交易网站购买商品将会给您造成财产损失，请谨慎访问。";
			}else if(sc == 120){
				text = "此网站可能通过混淆其他正规网站的标识来误导用户，为了避免可能会给您带来的损失，请谨慎访问。";
			}
		
		}else if(st == 20){
			if(sc == 100 || sc == 101){
				text = "经360安全中心鉴定从此网站下载的文件包含木马病毒，为了让您的系统安全，建议您立即停止下载！";
			}else if(sc == 103){
				text = "该网站伪装成小说、色情或外挂网站传播木马，木马病毒侵入您的系统，可能会导致账号密码、私人数据等个人信息被窃取。";
			}else if(sc == 102){
				text = "最近360安全中心收到大量用户投诉，从该网站下载文件时被欺骗安装了木马等危险程序，造成主页被篡改、账户被盗等。";	
			}
		
		}
	}else if(type == 70){
		text = "该网站当前页面带有木马或病毒，木马或病毒会给您的电脑安全造成威胁，建议立即关闭此页面。";
	}
	
	return text;

}
function getsubdomains(){
    var url = "m.yuerbao.com";
	if(url == ""){
		return;
	}
	$.ajax({  	
			url : "/index/ajaxgetsubmains",
			type: 'POST',
			dataType:  'json',
			data:'url=m.yuerbao.com',
			success: function(data){
				if(data.state == "yes"){
					$("#subdomains").html('<a href="/sub/index/?url=m.yuerbao.com" title="子域名安全状况" onClick="_gaq.push([\'_trackPageview\', \'/checkwebsite/check_subdomain\']);">子域名安全状况</a>');
					$("#subdomains").css('marginLeft' , '80px');
				}else{
					$("#subdomains").html('');
				}
			}
	});	
}
function getsameip(){
    var url = "m.yuerbao.com";
	if(url == ""){
		return;
	}
	$.ajax({  	
			url : "/index/ajaxgetsameip",
			type: 'POST',
			dataType:  'json',
			data:'url=m.yuerbao.com',
			success: function(data){
				console.log(data);
				if(data.state == "yes"){
					$("#sameip").html('<a href="/sameip/index/?url=m.yuerbao.com" title="同主机域名安全状况" onClick="_gaq.push([\'_trackPageview\', \'/checkwebsite/sameip\']);">同主机网站安全状况</a>');
					$("#sameip").css('marginLeft' , '80px');
					if(data.sameip_tip){
					    $("#sameip_tips").attr("class","fc_dangerous");	
						$("#sameip_tips").html('<a class="loudong_red" href="javascript:" style="background:url(http://p7.qhimg.com/t0150911e10db7d7f8e.gif) no-repeat 62px 6px; display:block; width:100%;" onClick="to_bangzhu();_gaq.push([\'_trackPageview\', \'/checkwebsite/pangzhu_1\']);"" title="潜在风险">潜在风险 </a>');
					}else{
						$("#sameip_tips").html("正常");
						$("#sameip_tips").attr("class","fc_security");
					}
				}else{
					$("#sameip").html('');
					$("#sameip_tips").html("正常");
					$("#sameip_tips").attr("class","fc_security");	
				}
				ifsameip_end = 1;
			}
	});	
}
function getWebscore(isxujia,iscuangai,isviolation,isguama,high,mid,low,info){
    if(ifsameip_end == 0){
		setTimeout("getWebscore("+isxujia+","+iscuangai+","+isviolation+","+isguama+",'"+high+"','"+mid+"','"+low+"','"+info+"')" , 200);
		return false;
	}
	var pangzhu = 0;
	if(isxujia == 0 && iscuangai == 0 && isviolation == 0 && isguama == 0 && high == '' && mid == '' && low == '' && info == '' ){
		$(".scan-unknown-tip").css("display", "block");
		$(".scan-v1").css("display", "block");	
		$("#scan-tips-1").css("display", "block");
				$("#ldstate").html("&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;漏洞未知，只有认领了网站后才能进行漏洞检测，<a  href=\"javascript:void(0)\" onClick=\"_gaq.push(['_trackPageview', '/checkwebsite/add_domain_noveri_unknow']);to_valideWebSite()\">请点击这里</a>进行认领");
		$("#ldstate").attr("class", "gm_unknow_ld");
		/*$('#div_fenshu').html('<p style="font-weight: bold; font-size: 12px; margin-top: 50px;line-height:25px; text-indent:0">网站还未认领,，无法检测，安全得分未知</p>');
		$('.showlogin').html('我是站长，我要认领');*/
				//$('.showlogin').attr('href' , $('.showlogin').attr('href')+'&for=weizhi');
			}else{
		$.ajax({  	
				url : "/webscore/index",
				type: 'POST',
				dataType:  'json',
				data:'isxujia='+isxujia+'&iscuangai='+iscuangai+'&isviolation='+isviolation+'&isguama='+isguama+'&high='+high+'&mid='+mid+'&low='+low+'&info='+info+'&domain=m.yuerbao.com&pangzhu='+pangzhu,
				success: function(data){
					if(data.state == "yes"){
						/*var score = parseInt(data.total_score);
					    if(score >=90 && score <= 100){
							$("#site_score").css('color' , '#638726');
						}else if(score >= 80 && score < 90){
							$("#site_score").css('color' , '#FF9900');
						}else if(score >= 60 && score < 80){
							$("#site_score").css('color' , '#FC3B2C');
						}else if(score >= 0 && score < 60){
							$("#site_score").css('color' , '#F00000');
						}*/
						var total_score = parseInt(data.total_score);
						/*
						if($("#sameip_tips").text() != ""){
							if($("#sameip_tips").attr("class").toString() == 'fc_serious'){
								total_score = parseInt(total_score - 6);
								if(total_score < 0){
									total_score = 0;
								}
							}
						}*/
						$("#site_score").html(total_score);
						// begin 打败功能下线注释以下两行
						var save_site = false;
						
						if(save_site && isxujia ==0 && isguama ==0 && iscuangai == 0 && isviolation ==0 && high == 0 && mid ==0 && low == 0){
							var save_site_text = '该网站安全评分为...结果，不能代表最近的安全状况，建议';
														save_site_text += '<a class="showlogin" href="/index/loginbox/?domain=m.yuerbao.com" onclick="return hs.htmlExpand(this,{objectType:\'iframe\'})">重新检测</a>';		
							$(".jg-before3Month").html(save_site_text);
							if (isguama || isxujia ||  0) {
								$(".jg-before3Month").css('display' , 'block');
							}
						}else{
							$("#dabai_text").html(data.msg);
							if (isguama || isxujia ||  0) {
								$("#dabai_text_div").css('display' , 'block');
							}
						}
						
						score_rate = data.rate;
						score_value = data.total_score;
						if(score_value >= 90){
						  $(".start-5").show();
						}

						$("#share_sinaweibo").show();
						// end
						show_no_validcheck(isxujia, isguama, score_value, "...", 0, 0, 0, iscuangai, isviolation);
					}else{
						$("#site_score").html('0');
						show_no_validcheck(isxujia, isguama, score_value, "...", 0, 0, 0, iscuangai, isviolation);
					}
				}
		});	
		$("#ldstate").html("&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;正常");	
	}
}
function to_bangzhu(){
	$('#bangzhu_div').css('display','block');
}
