var str = {"state":"0","msg":"excessive number of calls"};
var temp = '';
for (var obj in str) temp += obj;
if (temp.indexOf("data") == -1) temp = '';
else {
	getid("jg_tips").innerHTML = str.msg;
	getid("site_score").innerHTML = str.data.score.score;
	getid("high").innerHTML = str.data.loudong.high == null ? "?": str.data.loudong.high;
	getid("mid").innerHTML = str.data.loudong.mid == null ? "?": str.data.loudong.mid;
	getid("warn").innerHTML = str.data.loudong.low == null ? "?": str.data.loudong.low;
	getid("tip").innerHTML = str.data.loudong.info == null ? "?": str.data.loudong.info;
	if (str.data.score.msg != '' && str.data.score.msg != null) {
		getid("dabai_text").innerHTML = filtermsg(str.data.score.msg)
	} else getid("dabai_text").innerHTML = "未知";
	if (str.data.loudong.high != 0 && str.data.loudong.high != null) {
		getid("loudong").innerHTML = "存在高危漏洞";
		getid("loudong").className = "fc_dangerous"
	} else if (str.data.loudong.mid != 0 && str.data.loudong.mid != null) {
		getid("loudong").innerHTML = "存在严重漏洞";
		getid("loudong").className = "fc_serious"
	} else if (str.data.loudong.low != 0 && str.data.loudong.low != null) {
		getid("loudong").innerHTML = "存在警告漏洞";
		getid("loudong").className = "fc_warning"
	} else if (str.data.loudong.info != 0 && str.data.loudong.info != null) {
		getid("loudong").innerHTML = "存在提示漏洞";
		getid("loudong").className = "fc_security"
	} else if (str.data.loudong.high == 0 && str.data.loudong.mid == 0 && str.data.loudong.low == 0 && str.data.loudong.info == 0) {
		getid("loudong").innerHTML = "无漏洞";
		getid("loudong").className = "fc_security";
