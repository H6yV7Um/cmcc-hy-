		getid("query").src = "http://css.tools.chinaz.com/tools/images/webscan/serious.png"
	} else if (str.webstate == 3) {
		getid("jg_tips").className = "jg_dangerous";
		getid("query").src = "http://css.tools.chinaz.com/tools/images/webscan/zhiming.png"
	} else if (str.webstate == 4) {
		getid("jg_tips").className = "jg_weizhi";
		getid("query").src = "http://css.tools.chinaz.com/tools/images/webscan/unknown.png"
	} else {
		getid("jg_tips").className = "jg_weizhi";
		getid("query").src = "http://css.tools.chinaz.com/tools/images/webscan/unknown.png"
	};
	if (str.data.loudong.high != 0 && str.data.loudong.high != null) getid("high").style.color = "#FC3B2C";
	if (str.data.loudong.mid != 0 && str.data.loudong.mid != null) getid("mid").style.color = "#FC3B2C";
	if (str.data.loudong.low != 0 && str.data.loudong.low != null) getid("warn").style.color = "#FC3B2C";
	if (str.data.loudong.info != 0 && str.data.loudong.info != null) getid("tip").style.color = "#FC3B2C";
	if (str.data.cuangai.level != 0) {
		getid("cuangai").innerHTML = "被篡改";
		getid("cuangai").className = "fc_dangerous"
	};
	if (str.data.xujia.level != 0) {
		getid("diaoyu_sec").innerHTML = "虚假或欺诈网站";
		getid("diaoyu_sec").className = "fc_dangerous"
	};
	if (str.data.guama.level != 0) {
		getid("trojan_sec").innerHTML = "挂马或恶意内容";
		getid("trojan_sec").className = "fc_dangerous"
	};
	if (str.data.google.level != 0) {
		getid("google_malware").innerHTML = "被屏蔽";
		getid("google_malware").style.color = "#FC3B2C"
	};
	if (str.state == 0) {
		getid("fail").style.display = "block"
	}
};
function filtermsg(msg) {
	return msg.replace(/(\d{1,3}%)/, "<span class=\"ratevul\">$1</span>")
}
$(function(){
    getSave();
})
function getSave(){
    jQuery.ajax({
        type: "post",
        url: "/GetSaveInfo.ashx",
        data: { host: 'http://login.10086.cn/' },
        beforeSend:function () {
            jQuery("#jstest").html("<div class=\"col-red tc\"><img src=\""+imgurlbase+"/public/loader.gif\" /></div>");
        },
        success: function (data) {
            jQuery("#jstest").html(data);
        },
        complete: function (XHR, TS) {
            XHR = null;
        }
    })
}
</script>



