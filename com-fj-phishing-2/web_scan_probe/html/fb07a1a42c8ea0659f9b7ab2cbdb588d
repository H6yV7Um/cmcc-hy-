//读取单个平台具体数据
function load_pfid() {
    var s = document.createElement('script'),
        s_tt = document.createElement('script'),
        url = 'http://www.uc.cn/product/product_choose_load_pack?callback=dataPush&product=ucbrowser&lang=chinese&bid=999&pfids=145,41,101,147,43',
        url_tt = 'http://www.uc.cn/product/product_choose_load_pack?callback=dataPushTT&product=ucnewsapp&lang=chinese&bid=999&pfids=190';
    s.src = url;
    s_tt.src = url_tt;
    document.body.appendChild(s);
    document.body.appendChild(s_tt);
}
load_pfid();

function dataPush(jsonData) {
    actDom("up_android",jsonData['pf-145'].split(' ')[0]);
    actDom("up_iphone",jsonData['pf-41'].split(' ')[0]);
    actDom("up_pc",jsonData['pf-101'].split(' ')[0]);

    if(parseInt(jsonData['pf-43'].split('-').join(''))<parseInt(jsonData['pf-147'].split('-').join(''))){
        actDom("up_pad",jsonData['pf-147'].split(' ')[0]);
    }else{
        actDom("up_pad",jsonData['pf-43'].split(' ')[0]);
    }
}
function dataPushTT(jsonData) {
    actDom("up_tt",jsonData['pf-190'].split(' ')[0]);
    actDom("up_tt_ios",jsonData['pf-190'].split(' ')[0]);
}

function actDom(id,time){
    var box=document.getElementById(id);
    if(box !== null) box.innerText=time+"更新";
    if(timeRange(time)){
        var subs=box.parentNode.children;
        subs[subs.length-1].style.display="block";
    }  
}
function timeRange(t){
    var d = new Date();
    var str = d.getFullYear()+"-"+(d.getMonth()+1)+"-"+d.getDate();
    var time=new Date(str).getTime()- 24 * 60 * 60 * 1000*2 ;
    var time1=new Date(t).getTime();
    if(time1>=time) return true;
    else return false;
}


function makeIe6(){
    var isIE=!!window.ActiveXObject;
    var isIE6=isIE&&!window.XMLHttpRequest;
    if(isIE6){
        $("#up_pad").parent().mouseenter(function(){
            $(this).addClass("plat-pad-sel");
        }).mouseleave(function(){
            $(this).removeClass("plat-pad-sel");
        });
        
    }
}

function estop(e){
        var e=arguments.callee.caller.arguments[0]||event;
    if (e && e.stopPropagation){
    //因此它支持W3C的stopPropagation()方法
       e.stopPropagation();
    }else{
    //否则，我们需要使用IE的方式来取消事件冒泡
       window.event.cancelBubble = true;
       return false;
    }
}
