var timeout = 400;
var closetimer = 0;
var ddmenuitem = 0;
var submenuitem = 0;

//语言
function lang_open(){
    lang_canceltimer();
    lang_close();
    ddmenuitem = $('#langlist').css('display', 'block');}

function lang_close(){if(ddmenuitem) ddmenuitem.css('display', 'none');}
function lang_timer(){closetimer = window.setTimeout(lang_close, timeout);}

function lang_canceltimer(){
    if(closetimer){
        window.clearTimeout(closetimer);
        closetimer = null;}}

$(document).ready(function(){
    $('#langcur').bind('mouseover', lang_open);
    $('#langcur').bind('mouseout',  lang_timer);});

document.onclick = lang_close;

