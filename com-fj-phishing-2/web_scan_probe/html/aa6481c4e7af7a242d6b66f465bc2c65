		<script type="text/javascript"> 
    document.onkeypress = keyPress;
	function keyPress(e){
		var currKey=0,e=e||event;
		currKey=e.keyCode||e.which||e.charCode;
		if (currKey==13) 
		{ 
			check();
		} 
	}
</script>

