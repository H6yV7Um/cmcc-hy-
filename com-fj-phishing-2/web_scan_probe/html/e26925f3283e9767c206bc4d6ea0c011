<script type="text/javascript">
    function ShowTab(index) {
        var imgs = document.getElementsByTagName("img");
        for (var i = 0; i < imgs.length; i++) {
            if (imgs[i].className == "navImg") {
                imgs[i].src = imgs[i].src.replace("-2", "");
            }
        }

        var divs = document.getElementsByTagName("div");
        for (var i = 0; i < divs.length; i++) {
            if (divs[i].className == "adTab show") {
                divs[i].className = "adTab hide";
                break;
            }
        }

        var topNavImg = document.getElementById("topNav_" + index);
        topNavImg.src = topNavImg.src.replace(".GIF", "-2.GIF");
        var bottomNavImg = document.getElementById("bottomNav_" + index);
        bottomNavImg.src = bottomNavImg.src.replace(".GIF", "-2.GIF");
        var tab = document.getElementById("tab_" + index);
        tab.className = "adTab show";
    }   
</script>
