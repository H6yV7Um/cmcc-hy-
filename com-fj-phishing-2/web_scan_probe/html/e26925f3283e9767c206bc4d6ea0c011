<script>
    function SubmitSearch() {
        var param = "";
        param += "&jobtype=" + escape($("#jobtype").val());
        param += "&releaseTime=" + $("#releaseTime").val();
        param += "&searchtype=" + $("#searchtype").val();
        param += "&keyword=" + encodeURIComponent($("#keyword").val() != "请输入关键字" ? $("#keyword").val() : "");
        param += "&sortby=updatetime&ascdesc=Desc";

        this.location.href = "/net/info/resultg.aspx?a=a&g=g" + param;
    }

    function SubmitIHRSearch() {
        var keyword = $("#keyword").val() != "请输入关键字" ? $("#keyword").val() : "";
    
        if (keyword) {
            this.location.href = "http://i.xmrc.com.cn/XMRCIntel/RecruitSearch.aspx?keyword=" + escape(keyword);
        } else {
            this.location.href = "http://i.xmrc.com.cn/";
        }
    }

   

    $(function() {
        
        $('#keyword').bind('keypress', function (event) {
            if (event.keyCode == "13") {
                window.event.keyCode = 0;
                SubmitSearch();
            }
        });

        
    });
</script>

