 <script type="text/javascript">
    function show (obj)
    {
        var img = document.getElementById ("showimg");
       
        img.style.display = "";
        img.style.position = "absolute";
        img.style.left = obj.offsetleft + obj.width;
    }
    function hide ()
    {
        document.getElementById ("showimg").style.display = "none";
    }
</script>
