    getpasswd = jQuery("[onclick='showWindow(\'login\', \'member.php?mod=logging&action=login&viewlostpw=1\')']");
    getpasswd.attr("onclick","showWindow('login', 'plugin.php?id=zhanmishu_sms:getpassword')");
})
