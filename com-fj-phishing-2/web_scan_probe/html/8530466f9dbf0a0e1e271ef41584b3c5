        <script>$(function(){
  
  //  try {
  // if (!localStorage.getItem('maskTime12')) {
  //    guideFn ()
  //    localStorage.setItem('maskTime12','on')
  //      } else {
  //          $('.guide').hide();
  //      }
  //} catch (e) {
  //  $('.guide1').hide();
  //}


        function guideFn (){
            var oMask=$('<div id="mask"></div>').css({'width':$(document).width(),'height':$(document).height()}).appendTo($('body'));
            var  oGuidePositon =   $('.guide_p'),
                    oBtnNext = $('.guide .btn-i-know'),
                    oGuide = $('.guide');
            oMask.css({'width':$(document).width(),'height':$(document).height()})
              
              

            oBtnNext.on('click',function(){
                var oThat =  oBtnNext.index(this);
                if(oThat < 3){ //导向个数
                    oGuide.hide().eq(oThat+1).show();
//                    $(window).scrollTop(oGuidePositon.eq(oThat+1).offset().top-100);
                    $('html, body').animate({ scrollTop: oGuidePositon.eq(oThat+1).offset().top-50}, 500);
                    return false;
                }else if (oThat == 3){
//                    $(window).scrollTop(0);
                    oGuide.hide()
                    $('html, body').animate({ scrollTop: 0}, 500);
                    oMask.remove();
                }
            })
        }

})</script> 
