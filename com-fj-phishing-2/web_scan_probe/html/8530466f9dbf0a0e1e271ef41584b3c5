var focusSwiper;
var adSwiper;
var treasureSwiper;
$(document).ready(function(){
   var longTime=200;
   var mytimer = null;
	
mytimer = window.setInterval(startRunFocus,longTime);

function startRunFocus(){
var swiperfocus=$('.swiper-focus');
if(swiperfocus && swiperfocus.length>0){
   window.clearInterval(mytimer);
   focusSwiper=new Swiper('.swiper-focus',{
			pagination:'.swiper-pagination-focus',
			paginationClickable: true,
			autoplay:4000,
			autoplayDisableOnInteraction : false,
                        parallax:true
		});
 }
}

 var adTimer = window.setInterval(startRunAd,longTime);
function startRunAd(){
var swiperad=$('.swiper-ad');
if(swiperad && swiperad.length>0){
   window.clearInterval(adTimer);
   adSwiper=new Swiper('.swiper-ad',{
			pagination:'.swiper-pagination-ad',
			paginationClickable: true,
                        parallax:true
  });
 }
}

var jingxuanTimer= window.setInterval(startRunJingxuan,longTime);
function startRunJingxuan() {
var treasure = $(".swiper-treasure");
if(treasure && treasure.length > 0) {
console.log("1111");
window.clearInterval(jingxuanTimer);
treasureSwiper=new Swiper('.swiper-treasure',{
			slidesPerView:3.2,
			freeMode:true,
			observer:true
		});
}
}

var guessTimer= window.setInterval(startRunGuess,longTime);
function startRunGuess() {
var treasure1 = $(".swiper-treasure1");
if(treasure1 && treasure1.length > 0) {
window.clearInterval(guessTimer);
var guess = new Swiper('.swiper-treasure1',{
			slidesPerView:3.2,
			freeMode:true,
			observer:true
		});
}
}

});

function setNav(tabName,tabValue) {
  _smqEvent('首页', '精选保贝',tabName);
  $('.treasure div.treasure_div').addClass('hide');
  $('.treasure-nav a').removeClass('active');
  $('.treasure div.treasure_'+tabValue).removeClass('hide');
  $('a[data-name='+tabValue+']').addClass('active');
var treasureSwiper=new Swiper('.'+tabValue,{
			slidesPerView:3.2,
			//spaceBetween:10,
			freeMode:true,
			observer:true
		});
}

