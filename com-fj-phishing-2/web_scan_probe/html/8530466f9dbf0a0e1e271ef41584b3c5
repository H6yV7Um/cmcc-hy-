      <script>
$(document).ready(function(){
    var condition = {"map":{"article_source_names":"顶部轮播","article_source_ids":"270","title_number":"14","abstrcation_number":"50","article_column":"9","more_word":"...","info_sort":"sort_num_asc","article_row":"1"}};
    var articleSourceIds = condition.map.article_source_ids;
    var url = "/TKSearch/ggonline/searchAdvertising";
    var articleColumn = condition.map.article_column;
    var infoSort= condition.map.info_sort;
    var infoSortType = infoSort.split('_')[2];

  function dataSortFunc (datas, type, infoSortType) {
    var newArr = null;
    if (infoSortType == 'asc') {
      newArr = datas.sort(function (pre, next) {
        if (type == 'sortNum') {
          return pre[type] - next[type]
        } else {
          return Date.parse(pre.articleHistory[type]) - Date.parse(next.articleHistory[type])
        }
      })
    } else {
      newArr = datas.sort(function (pre, next) {
        if (type == 'sortNum') {
          return next[type] - pre[type]
        } else {
          return Date.parse(next.articleHistory[type]) - Date.parse(pre.articleHistory[type]) 
        }
      })
    }
    return newArr;
  }

    $.ajax({
            url: url,
            type : "post",
            dataType: "json",
            contentType: "application/json",
            data: JSON.stringify({
                articleHistory: {
                  pageInfoId: articleSourceIds
                }
            }),
            success: function(data){
    var sortResult = null;
    switch(infoSort){
        case 'create_time_asc' || 'create_time_desc':
            sortResult = dataSortFunc(data.data,'createTime',infoSortType);
            break;
        case 'display_time_asc' || 'display_time_desc':
            sortResult = dataSortFunc(data.data,'displayTime',infoSortType);
            break;
        case 'sort_num_asc' || 'sort_num_desc':
            sortResult = dataSortFunc(data.data,'sortNum',infoSortType);
            break;
        default:
          sortResult = data.data;
    }
    for (var i = 0; i < sortResult.length; i++) {
      sortResult[i].setTop && sortResult.unshift(sortResult.splice(i, 1)[0]);
    }
    sortResult = sortResult.slice(0,articleColumn)
              var wapSwiper= "";
              sortResult.forEach(function(val){
                  var data = val.articleHistory;

                  wapSwiper += "<div class='swiper-slide'><a href='" + data.redirectUrl + "'><img src='" + data.advPicUrl + "' class='swiperImg' onclick='_smqEvent(\"首页\", \"轮播图\",\"+(article_index+1)+\")'/></a></div>";
              })
              $("#wapSwiper").html(wapSwiper)
       },
       error: function(){
             console.log("请求失败")
      }
    })
})

</script> 
