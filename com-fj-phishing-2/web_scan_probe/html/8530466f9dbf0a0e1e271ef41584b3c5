/*秒杀倒计时*/
function getCountDownTime(){
	var NowTime = new Date();
	var endDate = new Date(NowTime.getFullYear(), NowTime.getMonth(), NowTime.getDate(), "23", "59", "59");
	var leftTime = endDate.getTime() - NowTime.getTime();
	var leftsecond = parseInt(leftTime / 1000);
    var hour = Math.floor((leftsecond) / 3600) < 10 ? "0" + Math.floor((leftsecond) / 3600) : Math.floor((leftsecond) / 3600);
    var minute = Math.floor((leftsecond - hour * 3600) / 60) < 10 ? "0" + Math.floor((leftsecond - hour * 3600) / 60) : Math.floor((leftsecond - hour * 3600) / 60);
    var second = Math.floor(leftsecond - hour * 3600 - minute * 60) < 10 ? "0" + Math.floor(leftsecond - hour * 3600 - minute * 60) : Math.floor(leftsecond - hour * 3600 - minute * 60);
    $('#countdown').html('<span>'+hour+'</span>:<span>'+minute+'</span>:<span>'+second+'</span>');
}

function loginBack(backUrl) {
	getSingleLogin({
		loginSuccess:function(info){
			$('#btn_login').hide();
			location.href = backUrl;
		}
	});
}

/*判断登陆状态*/
function getloginstatus(locationUrl){
	var date = new Date();
    var url = wap_Url + "service/member?function_code=checkLoginStatus&" + date.getTime();
    $.ajax({  
        type:"post",
        url:url,  
        dataType:"json",
        error : function(e) {

        },
        success: function(data){
            if(data.result == '0000'){
            	// yesloginClick();
            	
				$('#loginpage').click(function(){
					//location.href = '//m.tk.cn/huiyuan.html';
                                        location.href = '//m.tk.cn/my_module/my_module.html';
				});

				$('#btn_login').addClass("login_head")
				$('#btn_login').text('');
				//$('#btn_login').attr('href',"//m.tk.cn/huiyuan.html");
				$('#btn_login').attr('href',"//m.tk.cn/my_module/my_module.html");
				if (locationUrl) {
					location.href = locationUrl;
				}
            }else{
				// notloginClick();
				
				$('#loginpage').click(function(){
					//location.href = '//m.tk.cn/person.html';
                                         location.href = '//m.tk.cn/my_module/my_module.html';
				});

				$('#btn_login').removeClass("login_head");
				$('#btn_register').text('注册');
				$('#btn_login').show();

				if (locationUrl) {
					loginBack(locationUrl);
				}
            }
        }
    });
}

