 <script>

 $("[_province]").each(function(index){
	 $(this).hide();
 })
 $("[_por]").click(function(){
		var por=$(this).attr('_por');
		$("[_province="+por+"]").each(function(index){
			$(this).toggle($(this).attr("display"));
		})
		my_scroll.refresh();
	})
$('[_city]').click(function (){
	var city=$(this).attr('_city');
	var pro=$(this).attr('_province');
	if(pro=='' || pro==null ||pro=='null' ||pro=='undefined' ||pro==undefined)pro=city;
	$('#form_province').val(pro);
	$('#form_city').val(city);
	$('#province').val(pro+'    '+city);
	$('#areae-content').removeClass('show');
})
