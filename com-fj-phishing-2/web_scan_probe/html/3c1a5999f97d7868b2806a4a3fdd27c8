 <script type="text/javascript">
 $("[_index_province]").each(function(index){
	 $(this).hide();
 })
 $("[_index_por]").click(function(){
		var por=$(this).attr('_index_por');
		$("[_index_province="+por+"]").each(function(index){
			$(this).toggle($(this).attr("display"));
		})
		my_scroll.refresh();
	})
	$('.J-dialogClose').click(function() {
    $('.dialog').removeClass('show');
    return false;
});
 </script>
<script src="http://src.aidiao.com/m/js/swipe.js"></script>
<script type="text/javascript">
		window.mySwipe = $('#slide').Swipe({
		     auto: 3000,
		  // stopPropagation: true,
		  	callback: function(idx, element) {
		  		var $dotsList = $(".trigger span");
		    	$dotsList.each(function(index){
		    		if(index === idx){
		    			$dotsList.each(function(){
		    				$(this).removeClass("current");
		    			});
		    			$(this).addClass("current");
		    		}
		    	});
		    	$(".slide-title").html($(element).find('img').attr('title'));
		  	},
		  // transitionEnd: function(index, element) {}
		}).data('Swipe');
		$city=$.fn.cookie('aidiao_location_city');
		$("#city_name").html($city);
</script>

</body></html>