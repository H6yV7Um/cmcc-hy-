 function is_weixin(){
		var ua = navigator.userAgent.toLowerCase();
		if(ua.match(/MicroMessenger/i)=="micromessenger") {
			return true;
	 	} else {
			return false;
		}
	}
	if(is_weixin()){
	 	$('#btn_login_form').after('<button class="btn-main" id="btns_weixin" style="width: 100%;background-color: #3EB447;margin-top: 50px;" onclick="LoginActionWind();">微信登录</button>');
	 	$("#btns_weixin").bind('click',function(){
	 		window.location.href="http://weixin.aidiao.com/reg/m_weixin/?m_url="+window.location.href;
	 	});
	 	$(".account #J-regNow").bind('click',function(){
