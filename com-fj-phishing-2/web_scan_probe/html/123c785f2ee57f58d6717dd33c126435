<script language="javascript">
function changeWlive(index)
{
	$$("wl_t" + index).className = "on";
	$$("wl_c" + index).style.display = "block";
	for(var i= 1;i<=4;i++)
	{
		index = (index+1)%5;
		$$("wl_t" + index).className = "";
		$$("wl_c" + index).style.display = "none";
	}
}
</script>

