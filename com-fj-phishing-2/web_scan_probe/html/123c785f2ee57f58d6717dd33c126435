<script type="text/javascript">
    function setTab(str, num){
        var span_len = document.getElementById('app_tab').getElementsByTagName('span').length;
        for(var i = 1; i <= span_len; i++) {
            var oSpan = document.getElementById(str + i);
            var oDiv = document.getElementById(str + "_" + i);
            if (i == num) {
                oSpan.className = 'app_on';
                oDiv.style.display = 'block';
            }
            else {
                oSpan.className = '';
                oDiv.style.display = 'none';
            }
        }
    }
</script>
