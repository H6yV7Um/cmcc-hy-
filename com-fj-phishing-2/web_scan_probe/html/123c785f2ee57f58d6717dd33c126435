<script type="text/javascript">
var vpicWidth  = [0,327];
var vtargetPos=0;
var vPhoto_Ctn = $$("v_photos_show");
var vPhotoTimer1,vPhotoTimer2;

var vcurPhotoIndex = 0;

function vchangePhotoPos(index)
{
	curPhotoIndex = index;
	vtargetPos = vpicWidth[index];
	vPhotoTimer1 = setInterval("vgoPhotoPos()",50);	
}

function vgoPhotoPos()
{
	var curPos = parseInt(vPhoto_Ctn.scrollLeft);
	if(curPos==vtargetPos) 
	{
		clearInterval(vPhotoTimer1);
		return;	
	}
	if(curPos<vtargetPos)
	{
		vPhoto_Ctn.scrollLeft = curPos+30;
	}
	else if(curPos>vtargetPos)
	{
		vPhoto_Ctn.scrollLeft = curPos-30;
	}
}

function vAutoScroll(){
	
	if( vcurPhotoIndex == 0 ){
		vcurPhotoIndex++;
	} else {
		vcurPhotoIndex--;
	}
	vchangePhotoPos(vcurPhotoIndex);	
	
}
vPhotoTimer2 = setInterval("vAutoScroll()",10000);
</script> 
