function checksearch()
{
	var key = $$("key").value.Trim();
	if(key=="")
	{
		$$("请输入搜索内容");
		return false;
	}
	$$("frmSearch").action = $$("ss_0").className=="b1" ? "http://search.7m.com.cn/search_gb.aspx":"http://search.7m.com.cn/bsearch_gb.aspx";
	return true;
		
}
