<script>
    (function(){
        var checkType=function (data) {
            return Object.prototype.toString.call(data).match(/^\[object\s?(\w+)\]$/i)[1];
        }
        /*获取熊猫足迹文章内容*/
        function getctwzwData(callback){
            $.ajax({
                type: "get",
                url: "http://cd.city.qq.com/interface/skanda/getlisttag?tid=315&page=1&size=3&rt=jsonp&callback=function",
                dataType: "jsonp",
                jsonp: "callback",
                success: callback,
                error: function(){
                    alert("数据提取失败，请刷新重试");
                }
            });
        }
        function getCInfo(msg){
            var article = msg.article;
            var ctwzwData = [];
            var artlen = article.length;
            for(var i = 0;i<artlen;i++){
                var tpli = article[i];
                var tplinfo = {
                    title:tpli.title,
                    url:tpli.url,
                    listimg:checkType(tpli.listimg )=='Array' ?tpli.listimg [0] : tpli.listimg ,
                    tags:tpli.tags,
                    intro:tpli.intro
                };
                ctwzwData.push(template("ctwzw-tpl",tplinfo));
            }
            return ctwzwData;
        }

        window["ctwzw"]=function(msg){
            var items = getCInfo(msg);
            //$("#ct1").empty().append(items[0]);
            //  $("#ct2").empty().append(items[1]);
            // $("#ct3").empty().append(items[2]);
        };
            getctwzwData(ctwzw);
        
    })();
