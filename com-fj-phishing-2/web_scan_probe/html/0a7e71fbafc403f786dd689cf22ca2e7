<script>
    (function(){
        var checkType=function (data) {
            return Object.prototype.toString.call(data).match(/^\[object\s?(\w+)\]$/i)[1];
        }
        /*获取熊猫足迹文章内容*/
        function getctwztData(callback){
            $.ajax({
                type: "get",
                url: "http://cd.city.qq.com/interface/skanda/getlisttag?tid=316&page=1&size=1&rt=jsonp&callback=function",
                dataType: "jsonp",
                jsonp: "callback",
                success: callback,
                error: function(){
                    alert("数据提取失败，请刷新重试");
                }
            });
        }

        window["ctwzt"]=function(msg){
            $("#cttu").empty();
            var article = msg.article;
            var ctwztData = [];
            var artlen = article.length;
            for(var i = 0;i<artlen;i++){
                var tpli = article[i];
                var tplinfo = {
                    title:tpli.title,
                    url:tpli.url,
                    listimg:checkType(tpli.listimg )=='Array' ?tpli.listimg [0] : tpli.listimg ,
                    tags:tpli.tags,
                    intro:tpli.intro
                };
                ctwztData.push(template("ctwzt-tpl",tplinfo));
            }
            $("#cttu").empty().append(ctwztData);
        };
        window["ctwztInit"] = function(msg){
            getctwztData(ctwzt);
        }
    })();
    //ctwztInit();
</script>
