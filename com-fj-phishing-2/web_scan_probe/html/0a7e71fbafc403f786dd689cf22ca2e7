<script>
    (function(){
        var checkType=function (data) {
            return Object.prototype.toString.call(data).match(/^\[object\s?(\w+)\]$/i)[1];
        }
        var currentPageNum=0,TotalPageNum,getItemsState2=true,loadTimes = 0,forum;
        /*获取文章内容*/

        function slide (){
            var scroIndex = 0,timer ;
            var picNum=$('.scrol-pic .scrol-pics').length;
            $(".slide-bar").css("width",(36*picNum )+"px");
            for(var i=0;i<4;i++){
                if(i==0){
                    $(".slide-bar").append("<a class='slide-bar-on' href='javascript:;'></a>");
                }else{
                    $(".slide-bar").append("<a href='javascript:;'></a>");
                }
            }
            function slider(index){
                $('.scrol-pic .scrol-pics').hide();
                $('.scrol-pic .scrol-pics').eq(index).show();
                $('.slide-bar a').removeClass('slide-bar-on');
                $('.slide-bar a').eq(index).addClass('slide-bar-on');
            }
            function nextClick (){
                if(scroIndex >= picNum-1){
                    scroIndex = 0;
                }else{
                    scroIndex++;
                }
                slider(scroIndex);
                return scroIndex;
            }
            function prevClick(){
                if(scroIndex <= 0){
                    scroIndex = picNum-1;
                }else{
                    scroIndex--;
                }
                slider(scroIndex);
                return scroIndex;
            }
            $('.slide-bar a').each(function(index){
                $(this).mouseover(function(){
                    scroIndex = index;
                    slider(scroIndex);
                });
            });
            $('.slide-bar a').hover(function(){
                clearInterval(timer);
            },function(){
                timer = setInterval(nextClick, 4000);
            });
            $('.pre').hover(function(){
                clearInterval(timer);
            },function(){
                timer = setInterval(nextClick, 4000);
            });
            $('.next').hover(function(){
                clearInterval(timer);
            },function(){
                timer = setInterval(nextClick, 4000);
            });
            $(".pre").click(function(){
                clearInterval(timer);
                prevClick();
                $(".pre").addClass("active");
                $(".next").removeClass("active");
            })
            $(".next").click(function(){
                clearInterval(timer);
                nextClick();
                $(".next").addClass("active");
                $(".pre").removeClass("active");
            });
        }
        function cutstr(str, len) {
            var str_length = 0;
            var str_len = 0;
            var str_cut = new String();
            str_len = str.length;
            for (var i = 0; i < str_len; i++) {
                a = str.charAt(i);
                str_length++;
                if (escape(a).length > 4) {
                    str_length++;
                }
                str_cut = str_cut.concat(a);
                if (str_length >= len) {
                    str_cut = str_cut.concat("...");
                    return str_cut;
                }
            }
            if (str_length < len) {
                return str;
            }
        }

        function ajaxData(url,data,callback){
            $.ajax({
                type: "get",
                data:data,
                url:url,
                dataType: "jsonp",
                jsonp: "callback",
                success: callback,
                error: function(){
                    alert("数据提取失败，请刷新重试");
                }
            });
        }
        var newSrc;
        function setImg(src){
            var oldSrc =src.split('/');
            for(var i = 0;i<oldSrc.length;i++){
                if(oldSrc[i] == "800"){
                    oldSrc[i] = "0";
                }
            }
            newSrc = oldSrc.join("/");
            return newSrc;
        }

        function getMInfo(msg){
            var article = msg.data.data;
            var ctwzwData = [];
            var artlen = article.length;
            if(!artlen)return false;
            for(var i = 0;i<artlen;i++){
                var tpli = article[i];
                var tplinfo = {
                    title:tpli.subject,
                    fid: tpli.fid,
                    url:"//panda.qq.com/fj/thread/"+tpli.tid,
                    headimg:setImg(tpli.headimg),
                    fname:setTag(tpli.fid)
                };
                ctwzwData.push(template("li-temp",tplinfo));
            }
            return ctwzwData;
        }

        function getTotalPageNum(total){
            return total > 27 ? (Math.ceil((total-27)/27) +1) : 1;
        }

        function paginationC(pageNum){
            var pN= pageNum+1;
            currentPageNum = pN;
            ajaxData("//panda.qq.com/fj/interface/topic/getRecThreads?s_code=",{page:currentPageNum,pagesize:27},pagisionData);
        }

        function setTag(id){
            switch (id){
                case "1000":
                    return tname = "吃货团";
                    break;
                case "1001":
                    return tname = "购物狂";
                    break;
                case "1002":
                    return tname = "搞装修";
                    break;
                case "1003":
                    return tname = "亲子团";
                    break;
                case "1004":
                    return tname = "来谈心";
                    break;
                case "1005":
                    return tname = "去旅游";
                    break;
                default:
                    return tname = "其他";
            }
        }

        function focusRender(msg,callback) {
            $(".scrol-main").empty();
            $(".slide-bar").empty();
            var article=msg.article;
            var focusArr=[];
            if(article.length>4){
                focusArr = article.slice(0,4);
            }
            else focusArr = article;
            for(var i in focusArr){
                var v=focusArr[i];
                var focusHimg = v['headimg'];
                var intro=[];
                var focusData={
                    articleurl:v['url'],
                    imgurl:checkType(focusHimg )=='Array' ?focusHimg[0] : focusHimg ,
                    title:v['title'],
                    intro:intro.join(' | ')
                };
                $('.scrol-main').append(template('banner-temp',focusData));
            }
            callback();
        }

        function createPager(msg) {
            if(!TotalPageNum){
                TotalPageNum=getTotalPageNum(parseInt(msg.data.total));
                $("#Pagination").pagination(TotalPageNum,{ show_if_single_page:false, callback:paginationC });
            }
        }
        window["pagisionData"]=function(msg){
            createPager(msg);
            var items=getMInfo(msg);
            $(".item1,.item2,.item3,.item4,.itemmore,#ct2,#cttu,#ct3").empty();
            for(var j in items){
                var imgS = items[j].split('alt')[0].split("src=")[1];
                if(imgS.length <= 5){
                    var tolen = items[j].length;
                    var firstS= items[j].slice(0,19);
                    var addOnly = firstS.concat(' only-word"');
                    items[j] = addOnly.concat( items[j].slice(20,tolen-1));
                }
                if(j<=5){ $(".item1").append(items[j]); $(".item1 .item-p").addClass("text-elli-multi"); }
                else if(j == 6){ $("#ct2").append(items[j]); $("#ct2 .rcm-item").addClass("ct-show");}
                else if(j>=7 && j<=12){ $(".item2").append(items[j]);$(".item2 .item-p").addClass("text-elli-multi"); }
                else if(j == 13){ $("#cttu").append(items[j]);$("#cttu .rcm-item").addClass("ct-show"); }
                else if(j>=14 && j<=19){ $(".item3").append(items[j]);$(".item3 .item-p").addClass("text-elli-multi"); }
                else if(j == 20){ $("#ct3").append(items[j]); $("#ct3 .rcm-item").addClass("ct-show");}
                else if(j>=21){ $(".item4").append(items[j]); $(".item4 .item-p").addClass("text-elli-multi");}
            }
            getItemsState2=true;
        };
        window["initM"] = function(){
            currentPageNum = 1;
            ajaxData("http://cd.city.qq.com/interface/skanda/getnew?cid=3019&page=1&rt=jsonp&callback=function&timeStamp="+new Date().getTime(), {size:4}, function(msg){
                focusRender(msg,slide);
            });
            ajaxData("//panda.qq.com/cd/interface/topic/getRecThreads?s_code=", {page:currentPageNum,pagesize:27}, pagisionData);
        }
    })();
    initM();
</script>
