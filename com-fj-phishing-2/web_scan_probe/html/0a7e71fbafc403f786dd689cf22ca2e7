<script>
    var subOut='<a href="javascript:void(0)" class="moreNav1" id="moreH">更多城市</a><div class="navmenu" id="navmenu" style="display: none;"><ul id="moreMenu"></ul></div>';
    var navName=[],navLink=[],subName=[],subLink=[];
    var str="",subStr="";
    var getNav = function(){
        var navLen = miniNavConf.length;
        for(var i=0;i<navLen;i++){
            if(miniNavConf[i].link == null){
                miniNavConf[i].link='javascript:void(0)';
            }
            str +='<li><a href="'+miniNavConf[i].link+'" target="_self">'+miniNavConf[i].name+'</a></li>';
            if(miniNavConf[i].subnav){
                var subLen = miniNavConf[i].subnav.length;
                for(var j=0;j<subLen;j++){
                    subStr+='<li><a href="'+miniNavConf[i].subnav[j].link+'" target="_self">'+miniNavConf[i].subnav[j].name+'</a></li>';
                    subName.push(miniNavConf[i].subnav[j].name);
                    subLink.push(miniNavConf[i].subnav[j].link);
                }
            }
        }
        document.getElementById("mini_linkbox").innerHTML =str;
    }
    getNav();
    window.onload=function(){
        var linkBox=document.getElementById("mini_linkbox");
        var liN=linkBox.getElementsByTagName("li");
        var panda = liN[liN.length-2];
        var moreN=liN[liN.length-1];
        panda.getElementsByTagName("a")[0].style.position="relative";
        var pandaStr='企鹅社区<img src="http://mat1.gtimg.com/fj/0426/sqlogo.png" alt="" style="position: absolute;width: 10px;height: auto; top:5px;right: 0px;">';
        panda.getElementsByTagName("a")[0].innerHTML=pandaStr;
        moreN.id="moreNav";moreN.className="moreNav";
        moreN.innerHTML=subOut;
        var moreMenu=document.getElementById("navmenu");
        var navMenu=document.getElementById("moreMenu");
        var moreH=document.getElementById("moreH");
        navMenu.innerHTML=subStr;
        moreN.onmouseover = function(){
            removeClass( moreH,"moreNav1");
            addClass( moreH,"moreNav2");
            moreMenu.style.display="block";
        };
        moreN.onmouseout = function(){
            addClass( moreH,"moreNav1" );
            removeClass( moreH,"moreNav2" );
            moreMenu.style.display="none";
        };
    };
    function hasClass( elements,cName ){
        return !!elements.className.match( new RegExp( "(\\s|^)" + cName + "(\\s|$)") );
    }
    function addClass( elements,cName ){
        if( !hasClass( elements,cName ) ){
            elements.className += " " + cName;
        }
    }
    function removeClass( elements,cName ){
        if( hasClass( elements,cName ) ){
            elements.className = elements.className.replace( new RegExp( "(\\s|^)" + cName + "(\\s|$)" )," " );
        }
    }
</script>
