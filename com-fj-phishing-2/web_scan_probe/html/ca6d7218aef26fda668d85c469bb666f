	    $(".top-menu ul li a").removeClass();
	    $(this).find("a").eq(0).addClass("active");
	},function(){
	    $(".top-menu ul li a").removeClass();
	    $(".top-menu ul li").eq(0).children("a").addClass("active");
	});
});

