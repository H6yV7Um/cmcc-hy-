<script>
    $(function () {
        //登录
        var $register = $('#register');
        var $loginBtn = $('#loginBtn');
        var $loginBoxPost = $('.login-box-post');
        $register.on('click', function () {
            $loginBoxPost.addClass('login-box-anim');
        });
        $loginBtn.on('click', function () {
            $loginBoxPost.removeClass('login-box-anim');
        });
    });
</script>











