<script type="text/javascript">
function checkfm(){
	if(document.getElementById("fm").query.value===""){
		alert("请输入要检索的内容！");
		document.getElementById("fm").query.focus();
		return(false);
	}
}

function tosubmit(){
  if(document.getElementById("fm").query.value===""){
		alert("请输入要检索的内容！");
		document.getElementById("fm").query.focus();
		return(false);
	}
  document.getElementById("fm").submit();
}

function searchKey(key){
  document.getElementById("fm").query.value=key;
  document.getElementById("fm").submit();
}
</script>

