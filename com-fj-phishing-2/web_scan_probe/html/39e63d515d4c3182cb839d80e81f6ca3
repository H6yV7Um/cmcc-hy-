<script type="text/javascript">
	    var finaIndex = 1;
	    var finaTime = setInterval("autoShow()", 2000);
	    var pause = false;
	    function autoShow() {
	       
	       	
	        if (finaIndex <= 4) {
	
	            if (document.getElementById("Tagadiv" + finaIndex))
	                changeFinanceMenu(document.getElementById("Tagadiv" + finaIndex), finaIndex);
	            finaIndex++;
	        }
	        else {
	            changeFinanceMenu(document.getElementById("Tagadiv1"), 1);
	            pause = true;
	            clearInterval(finaTime);
	       
	        }
	        
	    }
	function changeFinanceMenu(obj, index) {
	        finaIndex = index;
	        for (i = 1; i <= 4; i++) {
	            if (document.getElementById("Tagadiv" + i))
	                document.getElementById("Tagadiv" + i).style.display = "none";
					if (document.getElementById("taga" + i))
	               document.getElementById("taga"+i).className = "up";
	        }
			if (document.getElementById("taga" + index))
	               document.getElementById("taga"+index).className = "down";
	        if (document.getElementById("Tagadiv" + index))
	            document.getElementById("Tagadiv" + index).style.display = "block";
	    }
</script>
