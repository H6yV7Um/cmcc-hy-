    $(function(){
        setInterval("imgtabshowc()",3000);
    })
    var imgnumc=1;
    function imgtabshowc(){
        $('.imgtabc').hide();
        $('#imgtabc'+imgnumc).show();
        if(imgnumc==2){
            imgnumc=1;
        }else{
            imgnumc++;
        }
    }
</script>
