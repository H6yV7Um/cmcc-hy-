    $(document).ready(function(){
        var width=(document.body.offsetWidth-1000)/2-1;
        if(width>246){
            $(".qiaowang_top_layer").css({"width":"246px","right":"-246px"})
        }
        if(width<=246){
            $(".qiaowang_top_layer").css({"width":width+"px","right":-width+"px"})
        }
        if(width>450){
            $(".qw_banner_right").css({"width":"450px","right":"-450px"})
        }
        if(width<=450){
            $(".qw_banner_right").css({"width":width+"px","right":-width+"px"})
        }
    })

