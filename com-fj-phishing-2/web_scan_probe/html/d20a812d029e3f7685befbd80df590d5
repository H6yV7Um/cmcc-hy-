<script type="text/javascript">
    var leftSecond = 0;
    var bigeyeNow = 0;
    var bigeyeMax = 4;
    var bigeyeSi;
    var bigeyeSiSpeed = 3000;
    $(function () {
        //快捷购买
        abd("quickNav_1");
        $("#quickNav li").click(function () {
            if ($(this).index() != 0) {
                var ID = this.id;
                abd(ID);
            }
        });

        function abd(str) {
            $("#quickNav li").removeClass("now");
            $("#" + str).addClass("now");

            var num = str.split("_")[1];
            var ID = "#quickCon_" + num;
            $(".quickCon").hide();
            $(ID).show();
        }
        //大眼睛
        bigeyeMax = $(".bigeyeBoxLong").find("img").length;
        $(".bigeyeBoxLong").css("width", 745 * bigeyeMax);
        $(".bigeyeNav").css("width", 172 * bigeyeMax);
        $("#bigeyeNav a").click(function () {
            clearInterval(bigeyeSi);
            var n = $(this).attr("id").slice(2);
            bigeyeNow = n;
            bigeyeMove();
            bigeyeAuto();
        });
        function bigeyeMove() {
            $("#bigeyeNav a").removeClass("now");
            $("#bn" + bigeyeNow).addClass("now");
            $(".bigeyeBoxLong").animate({"marginLeft": bigeyeNow * -745}, {duration: 300});
            if (bigeyeNow > 3) {
                $(".bigeyeNav").animate({"marginLeft": (bigeyeNow - 3) * -172}, {duration: 300});
            } else {
                $(".bigeyeNav").animate({"marginLeft": 0}, {duration: 300});
            }
        }
        $("#bnavleft").click(function () {
            var bnavIndent = Math.floor($(".bigeyeNav").css("marginLeft").slice(0, -2));
            if (bnavIndent < 0) {
                $(".bigeyeNav").css({"marginLeft": bnavIndent + 172});
            }
        });
        $("#bnavright").click(function () {
            var bnavIndent = Math.floor($(".bigeyeNav").css("marginLeft").slice(0, -2));
            if (bnavIndent > ((bigeyeMax - 4) * -172)) {
                $(".bigeyeNav").css({"marginLeft": bnavIndent - 172});
            }
        });
        bigeyeSi = setInterval(function () {
            bigeyeNow++;
            if (bigeyeNow > (bigeyeMax - 1)) {
                bigeyeNow = 0;
            }
            bigeyeMove();
        }, bigeyeSiSpeed);
        $(".bigeyeBoxLong").mouseover(function () {
            clearInterval(bigeyeSi);
        });
        $(".bigeyeBoxLong").mouseout(function () {
            bigeyeAuto();
        });
        function bigeyeAuto() {
            bigeyeSi = setInterval(function () {
                bigeyeNow++;
                if (bigeyeNow > (bigeyeMax - 1)) {
                    bigeyeNow = 0;
                }
                bigeyeMove();
            }, bigeyeSiSpeed);
        }
        //获取抢购信息
        $.get(BASE_URL + '/index/rushlist', function (data) {
            if (data.success) {
                var rushId = data.rush_id;
                leftSecond = data.left_time;
                var items = data.items;
                var temp = 0;
                var step = 0;

                $("#rushItems").append('<ul></ul>');
                $.each(items, function (i, item) {
                    if (temp == 0) {
                        $("#rushItems ul").append('<li></li>');
                    }
                    if (temp < 2) {
                        var buyUrl = BASE_URL + '/rush/index/pid/' + item.pid + '/rush_id/' + rushId;
                        var remainNum = parseInt(item.allnum) <= 0 ? 99999 : parseInt(item.allnum) - parseInt(item.buynum);
                        remainNum = remainNum < 0 ? 0 : remainNum;
                        var sellover = '';
                        if (remainNum == 0) {
                            sellover = '<img style="width:240px; height:120px; display:block; border:none; top:0; left:0; position:absolute;" src="http://au.9you.com/images/shop48x34.png" />';
                        }
                        $("#rushItems li").eq(step).append(
                                '<dl class="hidetips">' +
                                '<dt><a href="' + buyUrl + '" target="_blank"><img src="' + item.pic + '"></a>' +
                                '<a href="' + buyUrl + '" target="_blank" class="msqg">马上抢购</a>' +
                                '</dt>' +
                                '<dd>' +
                                '<a href="' + buyUrl + '" target="_blank">' + item.name + '</a>' +
                                '<h6>价格：<span>' + item.price + '  M币</span></h6>' +
                                '<h6>剩余数量：<span>' + remainNum + '  件</span></h6>' +
                                //'<h6>所属专区:' + item.group + '</h6>' +
                                '</dd>' + sellover +
                                '</dl>');
                        if (++temp == 2) {
                            temp = 0;
                            step++;
                        }
                    }
                    $("#rushItems dl").hover(
                            function () {
                                $(this).removeClass('hidetips').addClass('showtips');
                            },
                            function () {
                                $(this).removeClass('showtips').addClass('hidetips');
                            }
                    )
                });
            }
            qinggouFun();
            leftTimeClock = window.setInterval(function () {
                ShowCountDown('divdown1');
            }, 1000);
        }, 'json');


    });
    function ShowCountDown(divname) {
        var cc = document.getElementById(divname);
        var hour1 = 0;
        var hour2 = 0;
        var minute1 = 0;
        var minute2 = 0;
        var second1 = 0;
        var second2 = 0;
        if (leftSecond > 0) {
            var hour = Math.floor(leftSecond / 3600);
            var minute = Math.floor((leftSecond - hour * 3600) / 60);
            var second = Math.floor(leftSecond - hour * 3600 - minute * 60);
            hour1 = Math.floor(hour / 10);
            hour2 = Math.floor(hour % 10);
            minute1 = Math.floor(minute / 10);
            minute2 = Math.floor(minute % 10);
            second1 = Math.floor(second / 10);
            second2 = Math.floor(second % 10);
            leftSecond--;
        } else {
            window.clearInterval(leftTimeClock);
        }
        cc.innerHTML = "<span>" + hour1 + "</span><span>" + hour2 + "</span>" + ":" + "<span>" + minute1 + "</span><span>" + minute2 + "</span>" + ":" + "<span>" + second1 + "</span><span>" + second2 + "</span>";
    }



    //抢购切换
    function qinggouFun() {
        currentLiIndex = 0;
        var leg = $(".productPan li").length;
        if (leg <= 0) {
            $(".panBuy .leftArrow").css("opacity", 0.5);
            $(".panBuy .rightArrow").css("opacity", 0.5);
            return;
        }
        var baseWNum = $(".productPan").width();
        $(".panBuy .leftArrow").css("opacity", 0.5);
        if (leg < 2) {
            $(".panBuy .rightArrow").css("opacity", 0.5);
        }
        $(".productPan ul").css("width", baseWNum * leg);
        for (var i = 0; i < leg; i++) {
            $(".productPan li").eq(i).css("margin-left", baseWNum * i);
        }
        $(".panBuy .leftArrow").click(function () {
            if (leg < 2)
                return;
            if (currentLiIndex <= 0)
                return;
            currentLiIndex--;
            if (currentLiIndex <= 0) {
                $(".panBuy .leftArrow").css("opacity", 0.5);
                $(".panBuy .rightArrow").css("opacity", 1);
            } else {
                $(".panBuy .leftArrow").css("opacity", 1);
            }
            movieFun(currentLiIndex);
        });
        $(".panBuy .rightArrow").click(function () {
            if (leg < 2)
                return;
            if (currentLiIndex >= (leg - 1))
                return;
            currentLiIndex++;
            if (currentLiIndex >= (leg - 1)) {
                $(".panBuy .rightArrow").css("opacity", 0.5);
                $(".panBuy .leftArrow").css("opacity", 1);
            } else {
                $(".panBuy .rightArrow").css("opacity", 1);
            }
            movieFun(currentLiIndex);
        });
        //移动控制
        function movieFun(nowNum) {
            var newSite = -baseWNum * nowNum;
            $(".productPan ul").animate({marginLeft: newSite}, {duration: 200});
        }
    }
    function pay(pkey, times, server, username, special, buytype) {

        var herf;
        herf = 'http://shop.9you.com/cart/buy?server_id=' + server +
                '&item_key[]=' + pkey + '&isGive=0&time=' + times + '&buytype=' + buytype + '&cart_type=mb&ups=0&special=' + special + "&getusername=" + username + "&getusername_r=" + username;


        $.fancybox.open({
            'href': herf,
            'type': 'iframe',
            'scrolling': 'no',
            'modal': false,
            'closeBtn': true,
            'autoSize': false,
            'fitToView': false,
            'helpers': {
                'overlay': {
                    'closeClick': false
                }
            },
            'width': 545,
            'height': 410,
            'padding': 0
        });
    }
    $(function () {
        $(".quickCon input[name='special']").blur(function () {

            var special = $(this).val();
            var parentId = $(this).parent().parent().attr("id");
            var bool = true;
            if (special && !/^[1-9]\d*$/.test(special)) {
                bool = false;
            } else if (special && parentId === "quickCon_1" && (special > 50000 || special < 100)) {
                bool = false;
            } else if (special && parentId === "quickCon_2" && (special > 40000 || special < 1)) {
                bool = false;
            } else if (special && parentId === "quickCon_3" && (special > 100000 || special < 1)) {
                bool = false;
            }
            if (!bool) {
                if (parentId === "quickCon_1") {
                    $("#quickCon_1 h6 font").html("请输入100-50000");
                    return;
                } else if (parentId === "quickCon_2") {
                    $("#quickCon_2 h6 font").html("请输入1-40000");
                    return;
                } else if (parentId === "quickCon_3") {
                    $("#quickCon_3 h6 font").html("请输入1-100000");
                    return;
                }
            }
            if (!special) {
                special = 0;
            }
            if (parentId === "quickCon_1") {
                $("#quickCon_1 h6 font").html(String(parseInt(special)) + "M币");
                return;
            } else if (parentId === "quickCon_2") {
                $("#quickCon_2 h6 font").html(String(parseInt(special) * 125) + "经验");
                return;
            } else if (parentId === "quickCon_3") {
                $("#quickCon_3 h6 font").html(String(parseInt(special)) + "情侣心");
                return;
            }
        });
        $(".quickCon a").on("click", function (event) {
            event.preventDefault();
            var parentId = $(this).parent().attr("id");
            if (parentId === "quickCon_1") {
                var server = $("#quickCon_1 input[name='server_id']").val();
                var username = $("#quickCon_1 input[name='username']").val();
                var special = $("#quickCon_1 input[name='special']").val();
                pay("OJxwfWsZ1135153983", 6, server, username, special, 'send');
                return;
            } else if (parentId === "quickCon_2") {
                var server = $("#quickCon_2 input[name='server_id']").val();
                var username = $("#quickCon_2 input[name='username']").val();
                var special = $("#quickCon_2 input[name='special']").val();
                pay("kBoubnG61322652566", 6, server, username, special, 'send');
                return;
            } else if (parentId === "quickCon_3") {
                var server = $("#quickCon_3 input[name='server_id']").val();
                var username = $("#quickCon_3 input[name='username']").val();
                var special = $("#quickCon_3 input[name='special']").val();
                pay("sfUZ06kQ1322652630", 6, server, username, special, 'buy');
                return;
            }
        });
    })
</script>
