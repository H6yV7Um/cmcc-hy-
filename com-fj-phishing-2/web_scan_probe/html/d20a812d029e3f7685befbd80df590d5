<script type="text/javascript">
    function give() {
        $.ajax({
            type: "get",
            url: "http://shop.9you.com/user/giveInfoMessage",
            data: "",
            dataType: "html",
            success: function(data) {
                if (isNaN(data)) {
                    return false;
                }
                $("#give").html(data);
            }
        });
    }
    give();
	
$(document).ready(function setLeftMenu(){
   $("#leftNavCan").mouseleave(function(){
	   $(".lm").hide();
	   
	   $("#leftNav a").removeClass("now");
	   	if($(this).attr("v") == 0){
			return false ;	
		}
	});
	$("#leftNav a").mouseover(function(){
		$("#leftNav a").removeClass("now");
		$(".lm").hide();
		if($(this).attr("v") == 0){
			return false ;	
		}else{
			$(this).addClass("now");
			$("._" + $(this).attr("v")).show();	
		}
		
	
  });
});
</script>





