<script language="javascript">
	function checksearch(obj){
		if(document.getElementById("keyword").value==''){
			alert('关键字不能为空!');
			return false;
		}
	}
	function displaymore(){
		document.getElementById("listselect").style.display="block";
		document.getElementById("showselect").className="onclick";
	}
	function hiddendiv(words){
		document.getElementById(words).style.display="none";
		document.getElementById("showselect").className="unclick";
	}
   function change_search_menu(id,words){

		document.getElementById('subbtn').value=words;
		document.getElementById('showselect').innerHTML = words;
		document.getElementById("showselect").className="unclick";
		document.getElementById("listselect").style.display="none";
		
		var alinks=document.getElementById('listselect').getElementsByTagName('a');
		for(var i=0;i<alinks.length;i++){
			alinks[i].className='';
		}								
		document.getElementById('tabmenu_'+String(id)).className='ck';
		if(id=='2'){
			document.getElementById('main_serach').action="http://www.cnena.com/buy/search.php?action=search&mid=1";
		}else if(id=='1'){
			document.getElementById('main_serach').action="http://www.cnena.com/sell/search.php?action=search&mid=1";
		}else if(id=='company'){
			document.getElementById('main_serach').action="http://www.cnena.com/hy/search.php?action=search";
		}else if(id=='jobs'){
			document.getElementById('main_serach').action="http://www.cnena.com/hr/search.php?action=search&mid=1";
		}else if(id=='zh'){
			document.getElementById('main_serach').action="http://www.cnena.com/showroom/search.php?action=search&mid=1";
		}else if(id=='news'){
			document.getElementById('main_serach').action="http://www.cnena.com/news/search.php?action=search";
		}
   }
</script>
            
            
