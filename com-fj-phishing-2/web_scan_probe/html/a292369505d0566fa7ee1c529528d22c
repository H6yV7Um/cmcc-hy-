<script>
$(function() {
var $pics = $('#picjs').find('.int_block');
$pics.each(function() {
var $pic = $(this), $cover = $pic.find('.cover'), height = $pic.height(), timer, enabled;

$cover.css({ top: height + 'px', display: 'block'});
$pic.hover(function() {
enabled = false;
timer = setTimeout(function() {
enabled = true;
$cover.css({top: height + 'px'}).stop(true, false).animate({top: '0'}, 'normal', 'easeInOutExpo');
}, 100)
}, function() {
if(timer) {
clearTimeout(timer);
timer = null;
}
if(enabled) {
$cover.stop(true, false).animate({top: '-' + height + 'px'}, 'normal', 'easeInOutExpo');
}
});
});
});
</script>

