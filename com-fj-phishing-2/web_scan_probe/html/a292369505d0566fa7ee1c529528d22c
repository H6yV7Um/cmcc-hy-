(function(){
var desc = $(".slide-desc-bd").find("p"), index;
$(".slide-bd").scrollable({
circular: true,
onSeek: function(event, tabIndex) {
if(index !== tabIndex) {
desc.addClass("hidden");
$(desc[tabIndex]).removeClass("hidden");
}
index = tabIndex;
}
}).navigator({
navi: ".slide-nav",
naviItem: 'li',
activeClass: 'slide-act'
}).autoscroll({interval: 5000});
})()
</script>
