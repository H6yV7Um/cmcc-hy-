var speed=400;
var step=2;

var mar=true,marquee0,marquee1,marquee2,marquee3;
function mar_load()
{
  marquee0=document.all('marquee0');
  marquee1=document.all('marquee1');
  marquee2=document.all('marquee2');
  if(marquee0&&marquee1&&marquee2)
  {
    if(!marquee0.length)
    {
      marquee0=new Array(marquee0);
      marquee1=new Array(marquee1);
      marquee2=new Array(marquee2);
    }
    for(var i=0;i<marquee0.length;i++)
    {
      marquee0[i].onmouseout=function()
      {
        mar=true;//setInterval(marquee,speed);
      }
      marquee0[i].onmouseover=function()
      {
        mar=false;//clearInterval(mar);
      }
      marquee1[i].innerHTML+=marquee1[i].innerHTML;
      marquee2[i].innerHTML=marquee1[i].innerHTML;
      mar=setInterval(marquee,speed);
    }
  }
}

function marquee()
{
  if(mar)
  {
  for(var i=0;i<marquee0.length;i++)
  {
    var sl=marquee0[i].scrollLeft;
    marquee0[i].scrollLeft+=step;
    if(sl==marquee0[i].scrollLeft)
    {
      marquee0[i].scrollLeft=step>0?0:marquee0[i].scrollWidth-parseInt(marquee0[i].style.width);
    }
  }
  }
}
window.attachEvent("onload",mar_load);

