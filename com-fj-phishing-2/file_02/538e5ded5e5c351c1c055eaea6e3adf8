var floatADbar_right=false,isCloseAD_right=false;
var timeoutid_right=null;
function close_ad_right()
{
    var img_ad_right=document.getElementById('img_ad_right');
    var sidebarad=document.getElementById('sidebar_ad_right');
    img_ad_right.style.display='none';
    sidebarad.style.display='block';
    floatADbar_right=true;
}
function show_ad_right()
{
    var img_ad_right=document.getElementById('img_ad_right');
    var sidebarad=document.getElementById('sidebar_ad_right');
    var closebtn=document.getElementById('ph_ad_right_bottom_close');
    img_ad_right.style.display=closebtn.style.display='block';
    sidebarad.style.display='none';
    floatADbar_right=false;
}
var ph_ad_right_float_parameter = {
    adWidth:84,
    //adWidth:90,
    //adHeight:330,
    adHeight:230,
    adLink: '/html/report/180120696-1.htm',

    adMediaUrl:'/res/Home/structure/180130066.jpg',

     isFlash:false,
    marginRight:0,
    marginBottom:20,
    maxHeight:document.body.scrollHeight
}
function time_show_right()
{
    timeoutid_right = setTimeout("show_ad_right()",500000000);
}
function time_hide_right()
{
    clearTimeout(timeoutid_right);
}
var ph_ad_right_float=
{
    render:function()
    {
        if(ph_ad_right_float_parameter.isFlash)
        {
            var _record_swf_click = escape(ph_ad_right_float_parameter.adLink);
            var _flashUrl = ph_ad_right_float_parameter.adMediaUrl;
            document.write('<div id=\"ph_ad_right_float_el\" style=\"position: absolute;clear:both; width:'+ph_ad_right_float_parameter.adWidth+'px; height:'+ph_ad_right_float_parameter.adHeight+'px; top: 350px; left:900px;VISIBILITY: visible; text-align:right; z-index: 9999\">');
            document.write('<div id=\"img_ad_right\" style=\"visibility:visible;\" >'); 
            document.write('<object classid=\"clsid:D27CDB6E-AE6D-11cf-96B8-444553540000\" id=\"flashad_right\" codebase=\"http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=7,0,19,0\" width=\"'+ph_ad_right_float_parameter.adWidth+'\" height=\"'+ph_ad_right_float_parameter.adHeight+'\">');
            document.write('<param name=\"movie\"  VALUE=\"' + _flashUrl + '?clickTag=' + _record_swf_click + '\" />');
            document.write('<param name="quality" value="high" />');
//          document.write('<param name="wmode" value="opaque" />');
            document.write('<embed src="'+_flashUrl+'" quality="high" pluginspage="http://www.macromedia.com/go/getflashplayer" type="application/x-shockwave-flash" width="'+ph_ad_right_float_parameter.adWidth+'" height="'+ph_ad_right_float_parameter.adHeight+'"></embed>');
            document.write('</object>');
            document.write('<div id=\"ph_ad_right_bottom_close\" style=\"position: absolute; width:100%; height:14px; text-align:right; line-height:14px;padding-top:1px;font-size:12px;  background:#ccc; bottom:-14px; right:0;VISIBILITY: visible; cursor:pointer; z-index: 10000\" onclick=\"close_ad_right()\">关闭 </div>');
            document.write('</div>');       
            document.write('<div style=\"visibility:visible; float:right;\" id=\"sidebar_ad_right\"><a target=\"_blank\" style=\"width:25px; height:210px; background:; display:block;\" onmouseover=\"time_show_right()\" onmouseout=\"time_hide_right()\"></a><a onclick=\"ph_ad_right_float.close()\" style=\" background:;height: 25px; width: 25px; cursor: pointer; display: block;\"></a></div>');       
            document.write('</div>');
        }
        else
        {
            document.write('<div id=\"ph_ad_right_float_el\" style=\"position: absolute;clear:both; width:'+ph_ad_right_float_parameter.adWidth+'px; height:'+ph_ad_right_float_parameter.adHeight+'px; top: 350px; left:900px;VISIBILITY: visible; text-align:right; z-index: 9999;\">');
            document.write('<div id=\"img_ad_right\" style=\"visibility:visible;\" >');
            document.write('<a href=\"'+ph_ad_right_float_parameter.adLink+'\" target=\"_blank\">');
            document.write('<img id=\"flashad_right\"   src=\"' + ph_ad_right_float_parameter.adMediaUrl + '\" border=\"0\" width=\"'+ph_ad_right_float_parameter.adWidth+'\" height=\"'+ph_ad_right_float_parameter.adHeight+'\" />');
            document.write('</a>');
            document.write('<div id=\"ph_ad_right_bottom_close\" style=\"position: absolute; width:100%; height:14px; text-align:right; line-height:14px;padding-top:1px;font-size:12px; background:#ccc; bottom:-14px; right:0;VISIBILITY: visible; cursor:pointer; z-index: 10000;\" onclick=\"close_ad_right()\">关闭 </div>');
            document.write('</div>');       
            document.write('<div style=\"visibility: visible; float:right;\" id=\"sidebar_ad_right\"><a target=\"_blank\" style=\"width:25px; height:'+(ph_ad_right_float_parameter.adHeight-20)+'px; background:; display:block;\" onmouseover=\"time_show_right()\" onmouseout=\"time_hide_right()\"></a><a onclick=\"ph_ad_right_float.close()\" style=\" background:;height: 20px; width: 25px; cursor: pointer; display: block;\"></a></div>');
            document.write('</div>');
        }
    },
    cal_pos:function()
    {
        var _top=_right=0;
        var doc=document;
        var docElement=document.body;//document.documentElement.clientWidth>0?document.documentElement:document.body;

        var el=doc.getElementById('ph_ad_right_float_el');
        _right=docElement.scrollLeft+docElement.clientWidth-(ph_ad_right_float_parameter.adWidth + ph_ad_right_float_parameter.marginRight);
        _top=docElement.scrollTop+docElement.clientHeight-(ph_ad_right_float_parameter.adHeight + ph_ad_right_float_parameter.marginBottom);
        el.style.left=_right+'px';

        el.style.top = document.getElementById('img_ad_right').style.top = document.getElementById('flashad_right').style.top = _top+'px';
        //New Code In Here
        var sad_top = docElement.scrollTop + docElement.clientHeight - el.offsetHeight - 20; 
        var imgad=doc.getElementById('img_ad_right');
        var sidebarad=doc.getElementById('sidebar_ad_right');
        var adclose=doc.getElementById('ph_ad_right_bottom_close');
        if(isCloseAD_right==false)
        {
        if((sad_top > (1600-134)) && (sad_top < 2334))
            {
            
                    imgad.style.display = adclose.style.display = "none";
                    sidebarad.style.display="block";
                
            }else
            {
                if(floatADbar_right==false)
                {
                    imgad.style.display= adclose.style.display  ="block";
                    sidebarad.style.display="none";
                }else
                {
                    imgad.style.display= adclose.style.display  ="none";
                    sidebarad.style.display="block";
                }
            }
        }
    },
    init:function(top,left)
    {
        this.render();
        this.cal_pos();
    },
    close:function()
    {
        var imgad=document.getElementById('img_ad_right');
        var sidebarad=document.getElementById('sidebar_ad_right');
        var adclose=document.getElementById('ph_ad_right_bottom_close');
        imgad.style.display= adclose.style.display  ='none';
        sidebarad.style.display='none';
        isCloseAD_right=true;
    }
}
