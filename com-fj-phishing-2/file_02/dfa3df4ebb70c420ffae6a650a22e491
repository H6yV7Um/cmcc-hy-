   <!-- <script src="/Public/home/src/js/jquery.min.js"></script>
    <script src="/Public/home/src/js/swiper.min.js"></script>
    <script src="/Public/home/src/js/jquery.lazyload.js"></script>
    <script src="/Public/home/src/js/jquery.sliphover.js"></script>
    <script src="/Public/home/src/js/plugins.js"></script>-->
    <script>
        $(function() {
            var $winH = $(window).height();
            var $headerTop = $(".header");
            var $helpUp = $(".help-up");
            $(window).scroll(function() {
                var $top = $(this).scrollTop();
                if ($top > 0) {
                    $headerTop.addClass("active");
                } else {
                    $headerTop.removeClass("active");
                }
                if ($top > $winH) {
                    $helpUp.addClass("active");
                } else {
                    $helpUp.removeClass("active")
                }
            });
            // 获得焦点
            $(".header-search").focusin(function() {
                $(this).addClass("active");
                $(".header-menu").addClass("hide");
                $(this).children(".select").removeClass("hide");
            });
            $(".header-main,.header-body").on("click", function(e) {
                if (e.target == $(this).get(0)) {
                    $(".header-search").removeClass("active");
                    $(".header-menu").removeClass("hide");
                    $(".header-search").children(".select").addClass("hide");
                }
            });


            // 返回顶部
            $(".help-up").on("click", function() {
                $("body,html").animate({
                    scrollTop: 0
                }, 1000);
            });
            // 选择栏-弹出下来菜单
            $(".select-text").on("click", function() {
                var $next = $(this).next("");
                if (!$next.hasClass("active")) {
                    $next.addClass("active");
                }
            });
            // 选择栏-选择内容
            $(".select-dropItem").on("click", function() {
                var txt = $(this).text();
                $(this).parent().parent().children(".select-text").text(txt);
                $(this).parent().removeClass("active");
            });
            // 新添加2017-9-15
            $(".select-dropMenu").mouseleave(function() {
                $(this).removeClass("active");
            });
            var mySwiper = new Swiper('.swiper-container', {
                loop: true,
                noSwiping: true,
                speed: 700,
                autoplay: 3000,
                autoplayDisableOnInteraction: false,
                //autoplay:true,//等同于以下设置
                pagination: '.swiper-pagination',
                nextButton: '.swiper-button-next',
                prevButton: '.swiper-button-prev',
            });
            // 切换
            $(".tab-link").hover(function() {
                var $tab = $(this).parent().parent();
                $(this).parent().children(".tab-link").removeClass("active");
                $(this).addClass("active");
                var index = $(this).index();
                var $items = $tab.find(".tab-item");
                $items.removeClass("active").eq(index).addClass("active");
            }, function() {
               // $(this).removeClass("active");
            })
            // 案例图片
            $('.paint').sliphover({
                backgroundColor: 'rgba(0,0,0,.5)'
            });
            // 首页－立即申请
            var indexFlag = true;
            $(".seek-btn").on("click", function() {
                var city = $(".seek-content .select-text").text().trim();
                var tel = $(".seek-input").val().trim();
                var telReg = /^1[34578]\d{9}$/;
                if (tel.length < 11) {
                    $.toast({
                        text: "请输入11位手机号"
                    })
                    return false;
                }
                if (telReg.test(tel) == false) {
                    $.toast({
                        text: "请输入有效手机号"
                    })
                    return false;
                }
                if (indexFlag) {
                    indexFlag = false;
                    var url="/ZxCompany/save_ment";
                    $.ajax({
                        url: url,
                        type: "post",
                        data: {
                            city: city,
                            member_tel: tel
                        },
                        success: function(res) {
                            //成功后
                            res = jQuery.parseJSON(res);
                            if (res.code == 200) {
                                $.toast({text: res.msg});
                            }
                        },
                        error: function() {
                            $.toast({
                                text: "出错了"
                            })
                        },
                        complete: function() {
                            indexFlag = true;
                        }
                    });
                }
            });


        });
    </script>


    
