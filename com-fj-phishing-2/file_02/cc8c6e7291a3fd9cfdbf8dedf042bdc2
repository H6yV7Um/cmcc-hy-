    	<script>
		window.baidu = new Array();
		function searchSuggest() {
			//document.getElementById('index-kw').focus();
			var head = document.getElementsByTagName("head")[0];
			var str = encodeURIComponent(document.getElementById('index-kw').value);
			var url = 'https://m.baidu.com/su?from=1017690a&wd=' + str;
			load_script(
					url,
					function() {
						window.baidu.sug = function(params) {
							var list = params.s;
							var ss = document
									.getElementById('suggest-content-id');
							ss.innerHTML = '';
							var div = '';
							if (list.length > 0) {
								document.getElementById('index-box').style.display = "block";
								for (i = 0; i <= list.length - 1 && i < 5; i++) {
									div += '<div class="sug"><a target="_top" onclick="hrefUrl(\''+list[i]+'\')">'
											+ list[i]
											+ '</a></div>';
								}
								ss.innerHTML += div;
							} else {
								hideSug();
							}
						};
					});
		}

		function hideSug() {
			document.getElementById('index-box').style.display = "none";
		}
		
		function hrefUrl(sw) {
			pLog(sw);
			window.top.location.href="https://m.baidu.com/s?word="+sw+"&from=1017690a";
		}

		function load_script(url, callback) {
			var head = document.getElementsByTagName('head')[0];
			var script = document.createElement('script');
			script.type = 'text/javascript';
			script.src = url;
			script.onload = script.onreadystatechange = function() {
				if ((!this.readyState || this.readyState === "loaded" || this.readyState === "complete")) {
					callback && callback();
					script.onload = script.onreadystatechange = null;
					if (head && script.parentNode) {
						head.removeChild(script);
					}
				}
			};
			head.insertBefore(script, head.firstChild);
		}

		//Mouse over function 
		function suggestOver(div_value) {
			div_value.className = 'suggest_link_over';
		}
		//Mouse out function 
		function suggestOut(div_value) {
			div_value.className = 'suggest_link';
		}
		//Click function 
		function setSearch(value) {
			document.getElementById('txtSearch').value = value;
			document.getElementById('search_suggest').innerHTML = '';
		}
		
		function pLog(sw){
			if(!isNullOrWhiteSpace(sw)){
				//渠道号代表不同的来源
				var map = {"channel":"1", "sw":sw};
				requestServer("/xmlkey/log/bds.do", map, null);
			}
		}
		
		function isNullOrWhiteSpace(param){
			if (param == null || param == undefined || param == '') {
				return true;
			}else{
				return false;
			}
		}
		
		// ajax请求
		function requestServer(url, params, callbackFun) {
			var xmlhttp;
			if (window.XMLHttpRequest) {
				// code for IE7+, Firefox, Chrome, Opera, Safari
				xmlhttp = new XMLHttpRequest();
			} else {
				// code for IE6, IE5
				xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
			}
			xmlhttp.onreadystatechange = function() {
				if (xmlhttp.readyState == XMLHttpRequest.DONE) {
					if (xmlhttp.status == 200) {
						if (callbackFun) {
							if (typeof xmlhttp.responseText !== "object") {
								callbackFun(JSON
										.parse(xmlhttp.responseText));
							} else {
								callbackFun(xmlhttp.responseText);
							}
						}
					} else if (xmlhttp.status == 400) {
					} else {
					}
				}
			}
			var paramsTemp = [];
			for ( var key in params) {
				var val = params[key];
				paramsTemp.push(key + "="
						+ (typeof val === "string" ? val : val + ""));
			}
			paramsTemp = paramsTemp.join("&");
			if (url.indexOf("=") > -1) {
				url += "&" + paramsTemp;
			} else {
				url += "?" + paramsTemp;
			}
			xmlhttp.open("GET", url, true);
			xmlhttp.send();
		}

	</script>
