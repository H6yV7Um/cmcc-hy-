        //列表展开收起
        function slideInit(){
            var slideH=$('.J-slide-main').height();
            $('.J-slide-main').data('height',slideH);
            $('.J-slide-main').data('vheight',$('.J-slide-main').offset().top);
            $('.J-slide-main').height(0);
            $("#menumain1").data('height',$("#menumain1").height());
            $("#menumain1").height(0);
            $('#kuzhan2').data('height',$('#kuzhan2').height());
            $('#mingzhanmenu').data('height',$('#mingzhanmenu').height());
            $('.J-header-ani').data('height',$('.J-header-ani').height());
            //$("#RefFrame").height($(window).height()-$("#RefFrame").offset().top);

            //$('.J-show-record').addClass('hide');
        }

        function showSlideMain($target){
        	//if($target.height()>30) return;
        	var tmp=Math.ceil(($('#kuzhan').children().length+1)/5)*72;
        	$target.height($target.data('height')+tmp);
        	$('.J-show-record').addClass('hide');
    		$('#mingzhanmenu').height($('#mingzhanmenu').data('height')+tmp);
        	$(".J-header-ani").animate({height: $('.J-header-ani').data('height')+tmp}, 400,function(){        		
        		//$('.J-slide-main').height($('.J-slide-main').data('height')+tmp);
        		//$('.J-hide-slide').show();
			});
        	
        }
        function hideSlideMain($target){    
        	$target.height(0);
    		$('.J-show-record').removeClass('hide');
    		$('#mingzhanmenu').height($('#mingzhanmenu').data('height'));
        	$(".J-header-ani").animate({height: $('.J-slide-main').offset().top-3}, 400,function(){
        		
        		//$('.J-slide-main').height($('.J-slide-main').height()-$target.data('vheight'));
        		//$('.J-hide-slide').hide();
			});
        }
		
