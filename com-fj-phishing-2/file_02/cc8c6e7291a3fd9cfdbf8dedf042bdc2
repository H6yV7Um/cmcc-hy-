            $(this).removeClass('J-collected').find('.collect').removeClass('v-hide').addClass('fadeInUpOut');
        });

        //点击分享
        $body.on('touchstart', '.J-share', function(event) {
            event.preventDefault();
            var $share=$('.share-detail');
            var $main=$('.share-detail-main');
            if($share.hasClass('show')){
                $share.addClass('fadeInLeft animated quicker').one(UTILS.animateEnd, function(event) {
                    event.stopPropagation();
                    $(this).removeClass('fadeInLeft animated quicker show');
                    $main.addClass('hide');
                });
            }else{
                $main.removeClass('hide');
                $share.css('opacity', '0');
                setTimeout(function(){
                    $share.addClass('fadeInRight animated quicker').one(UTILS.animateEnd, function(event) {
                        event.stopPropagation();
                        $(this).css('opacity', '1').removeClass('fadeInRight animated quicker');
                        $share.addClass('show');
                    });
                },100);
            }

        });

