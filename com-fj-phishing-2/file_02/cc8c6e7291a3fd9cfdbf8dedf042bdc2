		<script type="text/javascript">
			function getCookie(cname) {
			    var name = cname + "=";
			    var ca = document.cookie.split(';');
			    for(var i=0; i<ca.length; i++) {
			        var c = ca[i];
			        while (c.charAt(0)==' ') c = c.substring(1);
			        if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
			    }
			    return "";
			}
		
			var u = navigator.userAgent;
			var isIOS=!!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/);
			var isexpand=false;
			var isexpand1=false;
			var city="nanjing";
			var curlink="";
			var ctmp="|"+getCookie("kuzhan");
			
			//getLocation();

			function getLocation(){
				var options={
					enableHighAccuracy:true,
					maximumAge:1000
				}
				if(navigator.geolocation){
					navigator.geolocation.getCurrentPosition(onSuccess,onError,options);
				}else{
					getIPCity();
				}
			}

			function onSuccess(position){
				var lat=position.coords.latitude;
				var lon=position.coords.longitude;
				var point = new BMap.Point(lon, lat);    // 创建点坐标
				var gc = new BMap.Geocoder();
				gc.getLocation(point, function(rs){
					var addComp = rs.addressComponents;
					//alert(city);
					city = toPinyin(addComp.city.substring(0,addComp.city.length-1));
					citypro();
				});
			}

			function onError(error){
				switch(error.code){
					case 1:
						//alert("位置服务被拒绝");
						break;
					case 2:
						//alert("暂时获取不到位置信息");
						break;
					case 3:
						//alert("获取信息超时");
						break;
					case 4:
						//alert("未知错误");
						break;
				}
				getIPCity();
			}
			
			function getIPCity(){
				$.getScript("http://int.dpool.sina.com.cn/iplookup/iplookup.php?format=js",function(){
					city = toPinyin(remote_ip_info["city"]);
					citypro();
				});
			}

			function citypro(){
				//alert(city);
				$("#weath").html("<iframe width=\"122px\" height=\"25px\" scrolling=\"no\" frameborder=\"0\" allowtransparency=\"true\" src=\"http://i.tianqi.com/index.php?c=code&id=34&color=%23FFFFFF&icon=3&py="+city+"&wind=1&num=1\"></iframe>");
			}
		
			function expandmenu(){
				if(isexpand) {
					
					$("#menulist1").animate({height: 0}, 400,function(){
						document.getElementById('expandlink').innerHTML="更多";					
						isexpand=false;
						$("#menulist1").hide();
					});
				} else {
					$("#menulist1").show();
					$("#menulist1").animate({height: 100}, 400,function(){
						document.getElementById('expandlink').innerHTML="收回";
						isexpand=true;
						
					});
				}
			}
			
			function GetQueryString(name)
			{ 
				var reg = new RegExp("(^|&)"+ name +"=([^&]*)(&|$)");
				var r = window.location.search.substr(1).match(reg);
				if(r!=null)
					return  unescape(r[2]); 
				return null;
			}			
			
			if(GetQueryString("id")!=null) {
