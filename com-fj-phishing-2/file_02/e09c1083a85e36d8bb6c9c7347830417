<script type="text/javascript">
    var exhioldid = 1; //默认当前ID  
    var exhilen = 2; //共图片数
    function showexhi(exhiid) {
        for (i = 1; i <= exhilen; i++) {
            if (i == exhiid) { //document.getElementById("exhimenu"+i).style.background="#AAEEFA";
                document.getElementById("exhicon" + i).style.display = "block"; exhioldid = i;
            }
            else {//document.getElementById("exhimenu"+i).style.background="#fff";
                document.getElementById("exhicon" + i).style.display = "none";
            }
        } 
    }
    function exhiplay() {
        if (exhioldid == exhilen) { exhioldid = 1; }
        else { exhioldid++; }
        showexhi(exhioldid);
    }
</script>
