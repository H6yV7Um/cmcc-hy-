function ShowAllChannel(_this,id,bgleft,bgtop1,bgtop2){
	$(id).style.display="block";
	var bgpos1 = bgleft + " " + bgtop1;
	var bgpos2 = bgleft + " " + bgtop2;
	$(id).onmouseover=function(){this.style.display="block";_this.style.backgroundPosition=bgpos1;}
	$(id).onmouseout=function(){this.style.display="none";_this.style.backgroundPosition=bgpos2;}
	_this.style.backgroundPosition=bgpos1;
	_this.onmouseout=function(){$(id).style.display="none";_this.style.backgroundPosition=bgpos2;}
}
 
var adStr = $("adCtrl").getElementsByTagName("span")[0].innerHTML;

function ctrAd(){
	if(adStr.indexOf("1")!=-1){
		document.getElementById("Adv970_450").style.position="relative";
		endAdv("Adv970_450","span_Adv970_450",480);
		setTimeout(function(){$("Adv970_450").style.display="none";},12000)
	}
	if(adStr.indexOf("4")!=-1){
		endAdv("Adv970_450","span_Adv970_450",480);
		endAdv("Adv970_4502","span_Adv970_4502",480);
		setTimeout(function(){$("Adv970_450").style.display="none";},12000)
		setTimeout(function(){$("Adv970_4502").style.display="none";},16000)
	}
	if(adStr.indexOf("2")!=-1){
		if(adStr.indexOf("1")!=-1 || adStr.indexOf("4")!=-1){
			setTimeout("OptionMedia()",16000)
		}else{
			OptionMedia();
		}		
	}
	if(adStr.indexOf("3")!=-1){
		window.open("http://www.hc360.com/webad/ad/backindex.html","ad1110", "width=760,height=450,toolbar=no, menubar=no, scrollbars=no, resizable=no, location=no, status=no");
	}
}
function ctrAd2(){
	if(adStr.indexOf("1")!=-1){
		document.getElementById("Adv970_450").style.display="none";	
			
	}
	else if(adStr.indexOf("4")!=-1){
		document.getElementById("Adv970_450").style.display="none";	
		setTimeout(function(){$("Adv970_4502").style.display="none";},12000)
	}
}

function adReset(){
	//var loc = document.location+"";
	var loc = document.location.search;
	loc = loc.toLowerCase();	
	//alert(loc);
	switch (loc)
	{
	case '?hc360=pinpai':
		break;
	case '?urgestay=1':
		document.getElementById("displaylayout").innerHTML=document.getElementById("displaylayout").innerHTML+"欢迎首次访问慧聪网。慧聪网是国内领先的B2B网上贸易市场。<br />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;我们拥有覆盖70余行业的企业会员1200万，买家资源1120万。您可以免费拥有功能强大的网络商铺，24小时抢占商机！（本页只会在您初次访问时弹出一次）";
　　	document.getElementById("displaylayout").style.display="block";//显示 
		break;
	default:
		document.getElementById("displaylayout").style.display="none";//隐藏 
		ctrAd();
		//setTimeout(function(){ctrAd();},12000);
	}
}
