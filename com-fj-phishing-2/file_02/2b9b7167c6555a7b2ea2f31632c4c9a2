<script>

	function isNull( str ){
			if ( str == "")
					{return true;}
				var regu = "^[ ]+$";
				var re = new RegExp(regu);
				return re.test(str);
		}
		function check(){
			var keywd = document.getElementById("keyword").value;
			if (isNull( keywd )) {
				alert("有情提示：请输入搜索关键字！");
				document.getElementById("keyword").focus();
				return false;
			}else{
	  			return true;
	  		}

		}
		function submitForm() {
			if (check()) {
		  		var keywd = document.getElementById("keyword").value;
		  		window.location.href="http://www.bankcomm.com/BankCommSite/shtml/jyjr/cn/7848/list.shtml?word="+keywd;

	  		}
		}

//利率类型
var rateType1 = new Object({
	name:['人民币存款利率表','外币存款利率表','人民币贷款基准利率表','人民币贷款基础利率报价'],
	href:['http://www.bankcomm.com/BankCommSite/shtml/jyjr/cn/7158/7161/8086/8090/list.shtml?channelId=7158',
		'http://www.bankcomm.com/BankCommSite/shtml/jyjr/cn/7158/7161/8086/8089/list.shtml?channelId=7158',
		'http://www.bankcomm.com/BankCommSite/shtml/jyjr/cn/7158/7161/8086/8088/list.shtml?channelId=7158',
		'http://www.bankcomm.com/BankCommSite/shtml/jyjr/cn/7158/7161/8086/8087/list.shtml?channelId=7158']
});
var rateType2 = new Object({
	name:['新台币存放款利率','外币存款利率'],
	href:['http://www.bankcomm.com.tw/BankCommSite/shtml/tbfh/tw/14817/14826/list.shtml?channelId=14817',
		'http://www.bankcomm.com.tw/BankCommSite/shtml/tbfh/tw/14817/14825/list.shtml?channelId=14817']
});
var rateType3 = new Object({
	name:['澳门元存款利率','外币存款利率'],
	href:['http://www.bankcomm.com.mo/BankCommSite/shtml/mo/cn/9312/9351/list.shtml?channelId=9312',
		'http://www.bankcomm.com.mo/BankCommSite/shtml/mo/cn/9312/9350/list.shtml?channelId=9312']
});
var rateType4 = new Object({
	name:['港元存款','外币存款','零存整付'],
	href:['http://www.bankcomm.com.hk/tw/financialinfo/info0701.html',
		'http://www.bankcomm.com.hk/tw/financialinfo/info0702.html',
		'http://www.bankcomm.com.hk/tw/financialinfo/info0703.html']
});
//利率地区
var rateRegion = new Object({
	name:['境内','台北','澳门','香港'],
	rateType:[rateType1,rateType2,rateType3,rateType4]
});
/************************************************************************************************/

//外汇类型
//境内
