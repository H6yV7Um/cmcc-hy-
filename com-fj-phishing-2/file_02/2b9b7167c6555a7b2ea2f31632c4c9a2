<script type="text/javascript">
	var finance = {
		getValue: function(param){
			var args = [];
        	var item = '';

	        //正则表达式，匹配空白字符
	        var sRegs = /^\s*$/;
	        var sReg = /\s/g;
	        
	        //产品名称模糊查询关键字
	        var keyWord = $('.pic' + param + '-keyWord').val();
	        if(!sRegs.test(keyWord)){
	            //去除空白字符
	            if(sReg.test(keyWord)){
	                keyWord = encodeURI(keyWord.replace(sReg, ''));
	            }
	            item = 'keyWord=' + keyWord + '&';
	            args.push(item);
	        }

	        //活期or定期
	        item = 'tradeType=' + param;
	        args.push(item);

	        return args;
		}
	};

	$('.pic1-searchBtn').click(function(){
		//活期
		var args = finance.getValue(1);
		window.open('http://www.bankcomm.com/BankCommSite/shtml/jyjr/cn/7226/7266/7281/7282/list.shtml?channelId=7226&' + args.join(''),'_blank');
	});

	$('.pic2-searchBtn').click(function(){
		//定期
		var args = finance.getValue(2);
		window.open('http://www.bankcomm.com/BankCommSite/shtml/jyjr/cn/7226/7266/7281/7282/list.shtml?channelId=7226&' + args.join(''),'_blank');
	});

</script>
		
