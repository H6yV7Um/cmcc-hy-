 //截取字符串的方法
 function cutString(string, strLength) {
  var currentLength = 0;
  for (var i=0; i<=string.length; i++ ) {
   if (currentLength>strLength) {
    return string.substring(0,i-1)+"...";
   }
   var ch = string.charCodeAt(i);
   if (ch > 256) {
    currentLength++;
   }
   currentLength++;
  }
  return string;
 }
</script>
