/************************************************************************************************/

/*初始化存贷款利率相关数据*/
var box3 =  $(".float-box3 .select-box");
	//添加地区
	for(var i = 0; i< rateRegion.name.length; i++){
		$(box3).eq(0).find("ul").append('<li><a href="javascript:;" index="'+ i +'">'+ rateRegion.name[i] +'</a></li>');
	}
	$(box3).eq(0).find("ul li a:eq(0)").parents('ul').prev().text(rateRegion.name[0]);
	//添加类型 默认第一个
	for(var i = 0; i < rateRegion.rateType[0].name.length; i++){
		$(box3).eq(1).find("ul").append('<li><a href="javascript:;"   onclick="box2Click(this)"  _href="'+ rateRegion.rateType[0].href[i] +'">'+ rateRegion.rateType[0].name[i] +'</a></li>');
		if(i == 0){
			$(box3).eq(1).find("ul li a:eq(0)").parents('ul').prev().text(rateRegion.rateType[0].name[0]);
			$("#rate").attr("href",rateRegion.rateType[0].href[0]);
		}
	}
/************************************************************************************************/

/*存贷款利率地区选择*/
$(box3).eq(0).find("ul li a").click(function(){
	var span = $(this).parents('ul').prev();
	var text = $(this).text();//当前选择的内容
	span.text(text);
	var index = $(this).attr('index');//当前选择的下标
	$(box3).eq(1).find("ul").html("");//清空内容
	for(var i = 0;i < rateRegion.rateType[index].href.length;i++){//显示相应类型
		$(box3).eq(1).find("ul").append('<li><a href="javascript:;" onclick="box2Click(this)" index="'+ i +'" _href="'+ rateRegion.rateType[index].href[i] +'">'+ rateRegion.rateType[index].name[i] +'</a></li>');
		if(i == 0){
			$(box3).eq(1).find("ul li a:eq(0)").parents('ul').prev().text(rateRegion.rateType[index].name[0]);
			$("#rate").attr("href",rateRegion.rateType[index].href[0]);
		}
	}
});
/*存贷款利率类型选择
$(box3).eq(1).find("ul li a").live("click",function(){
	var span = $(this).parents('ul').prev();
	var text = $(this).text();//当前选择的内容
	var href = $(this).attr("_href");//当前连接
	span.text(text);
	$("#rate").attr("href",href);

});
*/
function box2Click(t){
	var span = $(t).parents('ul').prev();
	var text = $(t).text();//当前选择的内容
	var href = $(t).attr("_href");//当前连接
	span.text(text);
	$("#rate").attr("href",href);

};


/*初始化外汇牌价相关数据*/
var box2 =  $(".float-box2 .select-box");
	//添加地区
	for(var i = 0; i < forexRegion.name.length; i++){
		$(box2).eq(0).find("ul").append('<li><a index="'+ i +'" href="javascript:;">'+ forexRegion.name[i] +'</a></li>');
	}
	$(box2).eq(0).find("ul li a:eq(0)").parents('ul').prev().text(forexRegion.name[0]);
	//添加类型 默认第一个
	for(var i = 0; i < forexRegion.forexType[0].name.length; i++){
		$(box2).eq(1).find("ul").append('<li><a href="javascript:;"  onclick="box3Click(this)"  _href="'+ forexRegion.forexType[0].href[i] +'">'+ forexRegion.forexType[0].name[i] +'</a></li>');
		if(i == 0){
			$(box2).eq(1).find("ul li a:eq(0)").parents('ul').prev().text(forexRegion.forexType[0].name[0]);
			$("#forex").attr("href",forexRegion.forexType[0].href[0]);
		}
	}


/************************************************************************************************/


