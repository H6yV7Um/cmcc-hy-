function hide(sid){
 var light=document.getElementById(sid);
 light.style.display='none';
}
function submitform() {
           window.location="http://search.icbc-ltd.com/searchcenter/search.aspx?KeyWord="+escape(document.getElementById("KeyWord").value);
        }
