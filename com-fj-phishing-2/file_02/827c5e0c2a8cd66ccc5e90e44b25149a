        <script type="text/javascript">
            $(function () {
                $('#robotFloatTextQuestion').questionAutoComplete(
                    {
                        delay: 300,

                        appendTo: $('#rotbotFloatPlace'),

                        source:
                            function (request, response) {
                                $.ajax(
                                    {
                                        url: '/ICBCDynamicSite/Robot/RobotSuggest.ashx',
                                        dataType: 'text',
                                        timeout: 1000,
                                        data:
                                            {
                                                key: request.term
                                            },

                                        success:
                                            function (data) {
                                                var tips = data.split('#-_-#');

                                                if (tips != null && tips.length > 0 && tips[0] != '') {
                                                    response(tips);
                                                } else {
                                                    for (var i = 1; i < 99; i++) {
                                                        $(('#ui-id-' + i)).hide();
                                                    }
                                                }

                                                $('#robotAskTopTen').hide();
                                            }
                                    }
                                );
                            }
                    }
                )
            });
            </script>
        
