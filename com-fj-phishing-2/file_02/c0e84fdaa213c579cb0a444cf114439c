                // if(form.target == '_dialog_minicart') form.fireEvent('submit',e);
            } else if(Browser.ie6) {
                $$(spec_item.nocheckel).length && $$(spec_item.nocheckel).addClass('warn');
                Message.error('请选择规格:' + spec_item.nocheck.join(','));
            }
        });
      }catch(e){}
    });*/

    specdialog();
});
