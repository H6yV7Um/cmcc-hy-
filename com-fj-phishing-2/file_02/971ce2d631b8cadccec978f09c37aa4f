<script>
    $(function () {

        // category
        $('.js-category .category-item').hover(function () {
            var _showId = $(this).data('rel');
            $('.category-item-sub').hide();
            $('#' + _showId).show();
        }, function () {
            $('.category-item-sub').hide();
        });


        // banner
        if ($('.banner')[0] && $('.banner .bd li').length >= 2) {
            $('.banner').slide({
                titCell: ".hd ul",
                mainCell: '.bd ul',
                effect: 'left',
                autoPlay: true,
                autoPage: '<li>$</li>',
                trigger: 'click'
            });
        }

        // 推荐产品
        $(".pic-scroll").slide({
            titCell: ".hd ul",
            mainCell: ".bd ul",
            autoPage: true,
            effect: "left",
            autoPlay: 3000,
            vis: 5,
            prevCell: '.prev',
            nextCell: '.next',
            pnLoop: false
        });

        $('.pic-scroll .pic-list').find('li:nth-of-type(2n)').css({
            borderTopColor: '#00c0a5'
        });

        $('.pic-scroll .pic-list').find('li:nth-of-type(2n+3)').css({
            borderTopColor: '#2196f3'
        });
    });
</script>
