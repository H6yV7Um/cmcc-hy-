<script language="javascript">
function chkInfo()
{
	f1.MinAge.value=fnumh(f1.MinAge.value);
	if(trim(f1.MinAge.value,0)!='' && !isNum(trim(f1.MinAge.value,0)))
	{
		alert('年龄只能输入整数！');
		f1.MinAge.select();
		return false;
	}
	f1.MaxAge.value=fnumh(f1.MaxAge.value);
	if(trim(f1.MaxAge.value,0)!='' && !isNum(trim(f1.MaxAge.value,0)))
	{
		alert('年龄只能输入整数！');
		f1.MaxAge.select();
		return false;
	}
	f1.MinHeight.value=fnumh(f1.MinHeight.value);
	if(trim(f1.MinHeight.value,0)!='' && !isNum(trim(f1.MinHeight.value,0)))
	{
		alert('身高只能输入整数！');
		f1.MinHeight.select();
		return false;
	}
	f1.MaxHeight.value=fnumh(f1.MaxHeight.value);
	if(trim(f1.MaxHeight.value,0)!='' && !isNum(trim(f1.MaxHeight.value,0)))
	{
		alert('身高只能输入整数！');
		f1.MaxHeight.select();
		return false;
	}
	f1.submit();
}
