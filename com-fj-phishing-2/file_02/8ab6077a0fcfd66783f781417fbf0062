<script type="text/javascript">
    var getopenidfailedtime = parseInt('0');
    function IsWeiXin() {
        var ua = window.navigator.userAgent.toLowerCase();
        if (ua.match(/MicroMessenger/i) == 'micromessenger') {
            OCookie.SetCookie("__oran__weixin_browser", "1");
            //console.log("weixin:" + ua);
            return true;
        } else {
            OCookie.SetCookie("__oran__weixin_browser", "0");
            //console.log("wap:" + ua);
            return false;
        }
    }
    function Is1QianBao() {
        var ua = window.navigator.userAgent.toLowerCase();
        if (ua.indexOf('1qianbao') > 0) {
            OCookie.SetCookie("__oran__1qianbao_browser", "1");
            //console.log("weixin:" + ua);
            return true;
        } else {
            OCookie.SetCookie("__oran__1qianbao_browser", "0");
            //console.log("wap:" + ua);
            return false;
        }
    }
    Is1QianBao();
    function IsYingztApp() {
        var ua = window.navigator.userAgent.toLowerCase();
        if (ua.indexOf('yingztgoldapp') > -1 || ua.indexOf('yingztapp') > -1) {
            OCookie.SetCookie("__oran__yingztapp_browser", "1");
            //console.log("weixin:" + ua);
            return true;
        } else {
            OCookie.SetCookie("__oran__yingztapp_browser", "0");
            //console.log("wap:" + ua);
            return false;
        }
    }
    IsYingztApp();
    if (IsYingztApp()){
        document.write("<scr" + "ipt src=\"https://www.xiaoying.com/assets/app/js/release/model/goldNew/ctf.js\"></sc" + "ript>");
    }
    
</script>

