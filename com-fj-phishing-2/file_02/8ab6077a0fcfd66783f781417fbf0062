<script type="text/javascript">
    $(function () {
        $("#uc_footer_logout,.uc_footer_logout").click(function () {
            if (IsWeiXin() && false) {
                var $openid = OCookie.GetCookie("__oran__mem_weixin_openid");

                if ($openid == "" || $openid == null) {
                    location.href = "http://wx-uat.ctf.com.cn/zdfwxfwh/dsfapi/getRedirectUrl/175?redirectUrl=" + encodeURI("http://58.251.6.80/weixin/weixin_visit.aspx?method=3" + "&scopeType=1");
                }
                else {
                    location.href = "/weixin/weixin_visit.aspx?method=3&openid=" + $openid;
                }
            } else {
                ajax.UserLogout(function (data) {
                    window.location.href = "/message.aspx";
                });
            }
        });

        $("#uc_footer_login").click(function () {
            location.href = "/user/user_center.aspx";
        });

        $("#uc_footer_sigin").click(function () {
            if (location.href.indexOf("message.aspx") > 0) {
                location.href = "/user/login.aspx";
            }
            location.href = "/user/login.aspx?refurl=" + encodeURIComponent(location.href);
        });

        $("#uc_footer_sigup").click(function () {
            if (location.href.indexOf("message.aspx") > 0) {
                location.href = "/user/register_new.aspx";
            }
            location.href = "/user/register_new.aspx?refurl=" + encodeURIComponent(location.href.indexOf('message.aspx')>0?'/default.aspx':location.href);
        });

        var isGetVipNo = 'False';
        if (isGetVipNo == 'True') {
            ajax.getUserVipNo();//异步同步到CRM
        }
    });
</script>
                                      
