<script type="text/javascript">
    $(function () {
        if ('False' == "True" && '0' != '11') {
            $("#redPacketCount").html("0.00"+"元").css("font-size",".32rem")
        }
        $("#btn_hb").on("click", function () {
            var type = 1;//签到红包
            if('False' != "True"){
                window.location.href = "/user/login.aspx?refurl=/default.aspx";
                return false;
            }
            if ('0' == '11') {
                alert("免注册购物用户不能领取红包");
                return false;
            }
            ajax.GetRedPacket(type, 1,function (data) {
                if (data.state == "1") {
                    E.hideMask("loading...");
                    $("#redPacketMoney").html(data.amount+"元");
                    $("#pop-img_hb").css("visibility", "visible");
                    $(".bd_hb").css("opacity", "1");
                    $("#redPacketCount").html(data.redPacket_Left + "元");
                    $("#btn_hb").hide();
                    $("#btn_yqd").show();
                } else if (data.state == "-1") {
                    E.hideMask("loading...");
                    E.showToast(data.msg, "40%", "toast", 1250);
                } else if (data.state == "-3") {
                    E.hideMask("loading...");
                    E.showToast(data.msg, "40%", "toast", 1250);
                    $("#btn_hb").hide();
                    $("#btn_yqd").show();
                }
                else {
                    E.hideMask("loading...");
                    E.showToast(data.msg, "40%", "toast", 1250);
                }
            });
            //$("#pop-img_hb").css("visibility", "visible");
            //$(".bd_hb").css("opacity", "1");
            
        });
        $(".get_hb").on("click", function () {
            $("#pop-img_hb").css("visibility", "hidden");
            $(".bd_hb").css("opacity", "0");
        });
        $("#toHby1111").on("click", function () {
            if ("False" == "False") {
                E.showToast("活动暂未开始", "40%", "toast", 1250);
                return false;
            } else if ("False" == "True") {
                E.showToast("当前活动已参加，请留意下次活动", "40%", "toast", 1250);
                return false;
             }else{
                window.location.href = "/special/red_packet/";
            }
        })
    });
</script>


