            mc.get('pan').set({ direction: Hammer.DIRECTION_ALL });
            mc.on("pan", function (ev) {
                var Y = ev.deltaY;
                if (Y > 0) {
                    $('#E_PageNavContainer').show();
                } else if (Y < 0) {
                    $('#E_PageNavContainer').hide();
                }
            });
