<script type="text/javascript" src="/include/ocookie.js"></script>
<script type="text/javascript">
    /**清除历史隐藏or显示**/
    if ($("#history").children().length == 0) {
        $(".removeH").hide();
        $(".keywords").hide();
        $("#hot").show();
    } else {
        $(".removeH").show();
        $(".keywords").show();
    }
    /**搜索动作**/
    var search = function () {
        //alert(OUI.rgx.match(location.href,OUI.rgx.domain));
        var url = "/product/product_list.aspx?";
        var kw = $(".text.focus").val();
        if (kw.trim() != "") {
            url = url + "keywords=" + encodeURI(encodeURI(kw.trim()));
            ajax.AddHistoryKwds($(".text.focus").val(), url);
        } else {
            //默认跳转
            if ($(".text.focus").attr("placeholder") != "") {
                url = url + "keywords=" + encodeURI(encodeURI($(".text.focus").attr("placeholder")));
                ajax.AddHistoryKwds($(".text.focus").attr("placeholder"), url);
            }
            else {
                location.href = "/product/product_list.aspx";
            }
        }
    }
    /**确定搜索按钮点击事件**/
    $(".searchSure").on("click", function () {
        search();
    })

    /**点击搜索文本框事件**/
    $(".text.focus").on("click", function () {
        $(".i-sear").show();
    });
    /**点击历史关键词或热门关键词事件**/
    $(".sear-l>li").on("click", function () {
        //alert(OUI.rgx.match(location.href, OUI.rgx.domain));
        var url = "/product/product_list.aspx?";
        url = url + "keywords=" + encodeURI(encodeURI($(this).children().html()));
        ajax.AddHistoryKwds($(this).children().eq(0).html(), url);
    });

    /**点击清除历史关键词事件**/
    $(".removeH").on("click", function () {
        ajax.DelHistoryKwds($("#history"));
    })
    /**初始化搜索框**/
    $(function () {
        var keywords = decodeURI(decodeURI(OUI.getUrlParam("keywords")));
        //var keywords = OUI.getUrlParam("keywords");
        if (OUI.getUrlParam("keywords") != null) {
            $(".searchword").val(keywords);
        }
    });
    /**直接点击手机键盘中前往按钮，搜索**/
    function keyDown(e) {
        var keycode = e.which;
        if (parseInt(keycode) == 13) {
            search();
        }
    }
    document.onkeydown = keyDown;
    /**回删搜索框至无内容，隐藏x图标**/
    $("#search").find("input").on("input", function () {
        if ($(this).attr("value") == "") {
            $(".remove.r11").hide();
        } else {
            $(".remove.r11").show();
        }
    })

    //获取(_oran_isproduct_before_page）cookie，保存除了product_list的上一页
    var isProduct = OCookie.GetCookie("_oran_isproduct_before_page");
    if (!isProduct) {
        OCookie.SetCookie("_oran_isproduct_before_page", location.href);
    } else {
        var prevPage = document.referrer;
        if (parseInt(prevPage.indexOf("product_list")) < 0) {
            OCookie.SetCookie("_oran_isproduct_before_page", prevPage);
        }
    }
    $("#returnPrev").on("tap", function () {
        location.href = OCookie.GetCookie("_oran_isproduct_before_page");
        //history.go(-1);
    })


    //$(function () {
    //    if (window.history && window.history.pushState) {
    //        $(window).on('popstate', function () {
    //            var hashLocation = location.hash;
    //            var hashSplit = hashLocation.split("#!/");
    //            var hashName = hashSplit[1];
    //            if (hashName !== '') {
    //                var hash = window.location.hash;
    //                if (hash === '') {
    //                    location.href = location.href;
    //                }
    //            }
    //        });
    //    }
    //});
</script>

