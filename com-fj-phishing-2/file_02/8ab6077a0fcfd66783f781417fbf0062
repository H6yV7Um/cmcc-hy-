        });

        $(document).on('tap', 'a[data-target=\'menu\']', function (e) {
            //$("a").addClass("pointer-events");
            //$(".product .pro-inner a").addClass("pointer-events");
            //$("#mA").removeClass("pointer-events");
            //$("#tA").removeClass("pointer-events");
            //$("#pA").removeClass("pointer-events");
            //$(".navList a").removeClass("pointer-events");

            //$("a").css("pointer-events", "none");
            //$(".product .pro-inner a").css("pointer-events", "none");
            //$("#mA").css("pointer-events", "all");
            //$("#tA").css("pointer-events", "all");
            //$("#pA").css("pointer-events", "all");
            //$(".navList a").css("pointer-events", "all");

            CloseA();
        });

        $(document).on('tap', '#search', function (e) {
            //$("a").addClass("pointer-events");
            //$(".product .pro-inner a").addClass("pointer-events");
            //$("#mA").removeClass("pointer-events");
            //$("#tA").removeClass("pointer-events");
            //$("#pA").removeClass("pointer-events");
            //$(".navList a").removeClass("pointer-events");

            //$("a").css("pointer-events", "none");
            //$(".product .pro-inner a").css("pointer-events", "none");
            //$("#mA").css("pointer-events", "all");
            //$("#tA").css("pointer-events", "all");
            //$("#pA").css("pointer-events", "all");
            //$(".navList a").css("pointer-events", "all");

            CloseA();
        });

        $(document).on('tap', '#fu', function (e) {
            //$("a").addClass("pointer-events");
            //$(".product .pro-inner a").addClass("pointer-events");
            //$("#mA").removeClass("pointer-events");
            //$("#tA").removeClass("pointer-events");
            //$("#pA").removeClass("pointer-events");
            //$(".navList a").removeClass("pointer-events");

            //$("a").css("pointer-events", "none");
            //$(".product .pro-inner a").css("pointer-events", "none");
            //$("#mA").css("pointer-events", "all");
            //$("#tA").css("pointer-events", "all");
            //$("#pA").css("pointer-events", "all");
            //$(".navList a").css("pointer-events", "all");

            CloseA();
        });

        $(".pagenav").bind("touchstart", function () {
            CloseA();
        }).bind("touchend", function () {
            OpenA();
        });
    }

    function CloseA() {
        $("a").each(function () {
            if ($(this).attr("id") == "mA" || $(this).attr("id") == "tA" || $(this).attr("id") == "pA" || $(this).attr("id") == "filter" || $(this).parent().hasClass("navList")) {
                return true;
            }
            var href = $(this).attr("href");
            $(this).attr("href", "javascript:void(0)");
            $(this).attr("contextmenu", href);
        });
        $(".array.hs").css("pointer-events", "none");
    }

    function OpenA() {
        setTimeout(function () {
            $("a").each(function () {
                if ($(this).attr("id") == "mA" || $(this).attr("id") == "tA" || $(this).attr("id") == "pA" || $(this).attr("id") == "filter" || $(this).parent().hasClass("navList")) {
                    return true;
                }
                var href = $(this).attr("contextmenu");
                $(this).attr("href", href);
                $(this).removeAttr("contextmenu");
            });
        }, 320);
        $(".product .pro-inner a").removeClass("pointer-events");
        $(".array.hs").css("pointer-events", "all");
    }

</script>

