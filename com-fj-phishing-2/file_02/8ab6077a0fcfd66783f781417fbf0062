<script type="text/javascript">
    //$(function () {
    //    $('.nav_footer').css('height', $('.nav').height());
    //})
    var cart = OCookie.GetCookie("__oran__cart");
    if (cart == null || cart == "") {
        $("#acartempty").show();
        $("#acartfill").hide();
    } else {
        $("#acartempty").hide();
        $("#acartfill").show();
    }
    ajax.Fu(function (data) {
        if (data.msg.indexOf('allowSendInfoCoupon:1') > -1) {
            $(".nav5").addClass("nav51").removeClass("nav5");
        }
        if (data.msg.indexOf('hasNewCoupon:1') > -1) {
            $("#acenterempty").hide();
            $("#acenterfill").show();

        } else {
            $("#acenterempty").show();
            $("#acenterfill").hide();
        }
    });
    $('body,html').tap(function () {
        setTimeout(function () {
            var cart = OCookie.GetCookie("__oran__cart");
            if (cart == null || cart == "") {
                $("#acartempty").show();
                $("#acartfill").hide();
            } else {
                $("#acartempty").hide();
                $("#acartfill").show();
            }
            ajax.Fu(function (data) {
                if (data.msg.indexOf('allowSendInfoCoupon:1') > -1) {
                    $(".nav5").addClass("nav51").removeClass("nav5");
                }
                if (data.msg.indexOf('hasNewCoupon:1') > -1) {
                    $("#acenterempty").hide();
                    $("#acenterfill").show();
                } else {
                    $("#acenterempty").show();
                    $("#acenterfill").hide();
                }
            });
        }, 500)
    })
</script>
