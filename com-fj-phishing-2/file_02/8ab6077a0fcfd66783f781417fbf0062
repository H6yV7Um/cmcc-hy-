<script type="text/javascript">
    $(function () {
        //弹出弹框
        $('.LookMore').click(function () {
            $("#pop-img").show();
            $('.navigationbar').css("z-index", "1");
            $('.gotop').css("z-index", "1");
            //调整浮框的位置
            $('.goldleft').css('height', $('.todaygold').height() + 'px');
            $('.goldright').css('height', $('.todaygold').height() + 'px');
            $('.goldtop').css('height', ($('.pop-img').height() - $('.todaygold').height()) / 2 + 'px');
        });

        //隐藏弹框
        $(".goldtop,.goldleft,.goldright,.goldbuttom,.vote_btn").click(function () {
            $("#pop-img").hide();
            $('.navigationbar').css("z-index", "9999");
            $('.gotop').css("z-index", "99");
        });


    });
</script>
