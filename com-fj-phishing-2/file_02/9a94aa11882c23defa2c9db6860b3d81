<script>
	/*首页顶部图片轮播*/
	$("#slideBox").slide({
		mainCell:".slideBoxBd ul",
		titCell:".slideBoxHd ul",
		autoPlay:true,
		effect:'leftLoop',
		autoPage:"<li></li>"
	});
	//广播
	$(".broadcastBox").slide({mainCell:".broadcastBoxInner",autoPlay:true,effect:"leftMarquee",interTime:50,trigger:"click"});
	/*顶部客户端引流*/
	;(function(){
		if(!getCookie('isInstall')){
			$('.installClientBox').addClass('active');
			$('.lift-nav').addClass('withInstall');
			setCookie('isInstall','true',1);
			setTimeout(function(){
				$('.installClientBox').removeClass('active');
                $('.lift-nav').removeClass('withInstall');
			},5000)
		}
	}());
	// 关闭顶部客户端引流banner
	$('.installClientBox .close').click(function(){
		$('.installClientBox').removeClass('active');
	})

    /**
	 *
     */
    $('.awesome, .chase').on('click', '.tab-item', function () {
		var $self = $(this),
			$tab = $self.closest('.tab'),
			$ctt = $tab.siblings('.ctt'),
			$moreBtn = $tab.closest('.left-column').find('.more');
		$ctt.length === 0 && ($ctt = $tab.siblings('.ctt-column'));
		var $activeItem = $ctt.find('.seq' + ($self.index() + 1));
		if (!$self.hasClass('on')) {
            $tab.find('.on').removeClass('on');
            $self.addClass('on');
            $ctt.find('.on').removeClass('on');
            $activeItem.addClass('on');
            $moreBtn.attr('href', $self.attr('data-link'));
		}
	});
    $('.mainBody').on('mouseover', '.rank-ctt-item', function () {
        var $self = $(this),
			$rankCttBox = $self.parent();
        if (!$self.hasClass('on')) {
            $rankCttBox.find('.on').removeClass('on');
            $self.addClass('on');
		}
	});
    $('.comic, .cartoon').on('click', '.tab-item', function () {
        var $self = $(this),
			$tabBox = $self.parent(),
			$slideBox = $tabBox.siblings('.rank-slide-box');
        if (!$self.hasClass('on')) {
            $tabBox.find('.on').removeClass('on');
            $self.addClass('on');
            $slideBox.find('.rank-slide-item.on').removeClass('on');
            $slideBox.find('.rank-slide-item').eq($self.index()).addClass('on');
		}
	});
	(function () {
	    var historyJson = localStorage.getItem("historyList"),
            historyData = {},
			i = 0,
			len;
	    if (historyJson) {
            historyJson = JSON.parse(historyJson);
            len = historyJson.length;
            for (; i < len; i++) {
                historyData[historyJson[i].hwOpusId] = {
                    lastItemName: historyJson[i].lastItemName,
                    opusName: historyJson[i].opusName,
                    itemIdex: historyJson[i].itemIdex
				}
			}
			$('.js-start-opus').each(function (i, domEle) {
			    var $self = $(domEle),
					id = $self.attr('data-id'),
					href = '';
				switch ($self.attr('data-type')) {
					case '2':
					    href = id + '/chapter/' + (historyData[id] && historyData[id].itemIdex || 1) + '.html';
					    break;
                    case '4':
                        href = id + '/video/' + (historyData[id] && historyData[id].itemIdex || 1) + '.html';
                        break;
                    case '7':
                        href = id + '/fiction/' + (historyData[id] && historyData[id].itemIdex || 1) + '.html';
                        break;
                    case '8':
                        href = 'ugc/' + id + '.html';
                        break;
				}
                $self.attr('href', href);
			});
		}
	})();
	// 插码
	;(function(){
		var s=document.createElement("script"); s.async=true; s.src="/js/webtrends.load.js";    
		var s2=document.getElementsByTagName("script")[0]; s2.parentNode.insertBefore(s,s2);
	}());
</script>

