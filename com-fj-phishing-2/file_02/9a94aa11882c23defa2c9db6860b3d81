<script>
    $(".jsWwz").click(function(){
    	$(".jsWwzPopup").show();
    })
    $('.mgkf').click(function(){
      var seq, hash, msisdn, email,usertype,baseUrl,userId, isLogin = $('.userIsLogin ').val();
      $.ajax({
        url: $('.basePath').val() + 'common/getKefuParams.html',
        type: 'GET',
        dataType:"json",
        success: function(data){
        	// console.log(data)
          if(data.code == 0){
            seq = data.data.seq;
            hash = data.data.hash;
            usertype = data.data.usertype;
            msisdn = data.data.msisdn;
            email = data.data.email;
            userId = data.data.userId;
            baseUrl = 'http://kf.migu.cn/api/onlineWeb?appid=1501000001&channelId=1501&seq=' + seq + '&servicetype=5&usertype=' + usertype;
            if(msisdn){
              baseUrl = baseUrl + '&msisdn=' + msisdn;
            }
            if(email){
              baseUrl = baseUrl + '&email=' + email;
            }
            if(userId){
              baseUrl = baseUrl + '&userId=' + userId;
            }
            window.open(baseUrl + '&hash=' + hash);
            
          }else if(data.code == 1){
              common.popuTips("popularTips", "on", data.data.msg, 2000);
          }
        },
        error: function(){
        	common.popuTips("popularTips", "on", "网络异常，请稍后再试！", 2000);
        }
      })
    })

  /* 本地历史记录上传服务端 */
    function getLocal(){
        try{

            var local=localStorage.getItem("historyList"), getUrl="/home/saveHistory.html";
            if(local && local.length > 0){
                $.ajax({
                    url:getUrl,
                    type:"post",
                    data:{"syncList":local},
                    dataType:"json",
                    success:function(data){
                        
                    }
                })
            }
            console.log(local);
            
        }catch(e){
            
        }
    }

    
    /*服务端历史纪录存放到本地*/
var readHistory = function(){
    return {
        init:function(i){
            if(!sessionStorage.getItem("isLogin")){
                return false;
            }
            this.getHistory(i);
        },
        getHistory:function(type){
            var getUrl=$(".basePath").val()+"home/readHistory.html?pageNo=1&pageSize=40",opusType=type;
            $.ajax({
                url:getUrl,
                type:"POST",
                data:{"opusType":opusType},
                dataType:"json",
                success:function(data){
                    if(data.code==0 && data.data.total>0){
                        var list=data.data.list;
                        $.each(list,function(i){
                            if(this.opusType==2){
                                var thisType="/chapter/";
                            }else if(this.opusType==4){
                                var thisType="/video/";
                            }else if(this.opusType==7){
                                var thisType="/fiction/";
                            
                            }else if(this.opusType==8){
                                var thisType="/ugcvideo/";
                            }
                            var url=$(".basePath").val() + this.hwOpusId + thisType + this.index + ".html";
                            common.historyList.init(url, this.hwOpusId, this.hwItemId, this.totalTime, this.opusUrl,this.opusName,this.index,this.watchProgress,this.lastItemName,this.opusType,(this.updateFlg || 0),(this.validityFlg || 1),this.date, this.authorName)
                        });
                        if(window.location.href.indexOf("historyWatch.html") > -1){
	                        setTimeout(function(){
	                        	var start=new Date();  start.setHours(0);  start.setMinutes(0);  start.setSeconds(0);  start.setMilliseconds(0);  
	       						var todayStartTime=start.getTime(), todayEndTime = todayStartTime + 24 * 60 * 60 *1000,
	                        		getHistoryList = JSON.parse(localStorage.getItem("historyList")), liHtml='';
	                        		$.each(getHistoryList,function(i){
	                        			var thisTime =  this.watchTime, thisType = this.opusType;
	                        			if(thisType == 2 && thisTime < todayEndTime && thisTime > todayStartTime){
	                        				liHtml += '<li>'
												+ '<a target="_blank" data-ss="'+ this.type +'" data-hwid="'+ this.hwOpusId +'" data-opusid="" href="'+ $(".basePath").val() + this.hwOpusId +'/chapter/'+ this.itemIdex +'.html" title="'+ this.opusName +'">'
												+ '<div class="imgShow">'
												+ '<img src="'+ this.opusUrl +'" alt="'+ this.opusName +'"><i class="icon iconCheck"></i>'
												+ '<input type="checkbox" name="checkbox" class="checkStyle">'
												+ '</div>'
												+ '<h2>'+ this.opusName +'</h2>'
												+ '<p>观看至'+ this.itemIdex +'话</p>'
												+ '<p>'+ this.lastItemName +'</p>'
												+'</a></li>'
	                        			}
	                        		});
	                        		var todayText = $(".personCon span").text();
	                        		if(todayText.indexOf("今天") > -1){
	                        			$(".historyListTD").eq(0).html(liHtml);
	                        		}else{
	                        			var todayHtml = '<div class="timeTit timeTitTD" style="display:block"><span id="TD">今天</span></div><ul class="clearfix historyListTD">'+ liHtml +'</ul>';
	                        			$(".personCon").prepend(todayHtml);
	                        		}

	                        },50)
                        }
                    }
                }
            })
        }
    }
}();
var isLoginState = document.querySelector(".userIsLogin").value, isLoginStorage = sessionStorage.getItem("isLogin");
if(isLoginState == "true" && isLoginStorage != "isLogin"){
	getLocal();
	readHistory.init(2);
	readHistory.init(4);
	sessionStorage.setItem("isLogin","isLogin");
}

if(isLoginState == "false"){
	sessionStorage.setItem("isLogin","0");
}

</script>



