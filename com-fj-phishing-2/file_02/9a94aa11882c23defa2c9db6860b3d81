		}*/
	}
	(function () {
		var url = document.getElementById('urlLinkToUgcIndex').value;
		var url1 = document.getElementsByClassName('wwwUploadBaseUrl')[0].value + 'home/mySubmit.html';
		var loginState = document.getElementsByClassName('userIsLogin')[0].value;
		if (loginState == 'true') {
			url = 'http://dm.10086.cn/auth/login?display=phtml&needLogin=false&extension=platform::86_portal::04_companyID::02_accessMode::01&service=' + url;
			url1 = 'http://dm.10086.cn/auth/login?display=phtml&needLogin=false&extension=platform::86_portal::04_companyID::02_accessMode::01&service=' + url1;
		} else {
			url = decodeURIComponent(url);
		}
		var len = document.querySelectorAll('.headFixedLinkToUgcIndexBtn').length;
		var len1 = document.querySelectorAll('.manageVideoBtn').length;
		for(var i = 0; i < len; i++){
			document.querySelectorAll('.headFixedLinkToUgcIndexBtn')[i].href = url;
		}
		for(var i = 0; i < len1; i++){
			document.querySelectorAll('.manageVideoBtn')[i].href = url1;
		}
	})();

	var registeredBtnList = document.querySelectorAll(".registeredBtn");
	if(registeredBtnList.length > 0){
		for(var i=0; i<registeredBtnList.length; i++){
			var href = registeredBtnList[i].getAttribute("href") +"&relayState=86_04_phtml_"+window.location.href;
			registeredBtnList[i].setAttribute("href", href);

		}
	}
