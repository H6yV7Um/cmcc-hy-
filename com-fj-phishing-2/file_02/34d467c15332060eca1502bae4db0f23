<script language="JavaScript">
var _trackData = _trackData || [];
$(function(){
	var clock=setInterval(function(){
							var dom=$('.m_cla_right_cot div.right_top_cot')
							if(dom.length>0){
								for(var i=0;i<dom.length;i++){
									if(i%2==0){
										var nctag=$(dom[i]).attr("nctag");
										if(nctag==undefined){
											$(dom[i]).attr("nctag","nctag_index_div_RXB"+(i+1));
										}
									}
								}
								if(dom.length==10){
									window.clearInterval(clock);
								}
							}
						}, 1000);

});
</script>
