    $("#productboxinfo").html("<p id='productboxp'><span id='productboxspan'>Product</span>" + ProductFamilySelected + " <span id='expandproductspan'>(<a onclick='fnExpandProductSelection()'>change</a>)</span></p>");
}



        _gaq.push(['_setAccount', 'UA-55534-1']);
        _gaq.push(['_trackEvent', 'Quote Request - OPEN', window.location.href]);

