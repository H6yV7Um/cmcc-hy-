<script>    
$( document ).ready(function() {   
$("#intro").click(function() {
    $('html, body').animate({
        scrollTop: $("#intro-content").offset().top - 40
    }, 600);
	$("#ixon-emccd, #neo-scmos, #ikon-xl, #apogee-ccd, #decision-tree").removeClass("active");
	$("#intro").addClass("active");
});

$("#ixon-emccd").click(function() {
    $('html, body').animate({
        scrollTop: $("#ixon-emccd-content").offset().top - 40
    }, 600);
    $("#intro, #neo-scmos, #ikon-xl, #apogee-ccd, #decision-tree").removeClass("active");
	$("#ixon-emccd").addClass("active");
});

$("#neo-scmos").click(function() {
    $('html, body').animate({
        scrollTop: $("#neo-scmos-content").offset().top - 40
    }, 600);
    $("#into, #ixon-emccd, #ikon-xl, #apogee-ccd, #decision-tree").removeClass("active");
	$("#neo-scmos").addClass("active");
});

$("#ikon-xl").click(function () {
    $('html, body').animate({
        scrollTop: $("#ikon-xl-content").offset().top - 40
    }, 600);
    $("#into, #ixon-emccd, #neo-scnos, #apogee-ccd, #decision-tree").removeClass("active");
    $("#ikon-xl").addClass("active");
});

$("#apogee-ccd").click(function() {
    $('html, body').animate({
        scrollTop: $("#apogee-ccd-content").offset().top - 40
    }, 600);
    $("#into, #ixon-emccd, #neo-scmos, #ikon-xl, #decision-tree").removeClass("active");
	$("#apogee-ccd").addClass("active");
});

$("#decision-tree").click(function() {
    $('html, body').animate({
        scrollTop: $("#decision-tree-content").offset().top - 40
    }, 600);
    $("#intro, #ixon-emccd, #neo-scnos, #ikon-xl, #apogee-ccd").removeClass("active");
	$("#decision-tree").addClass("active");
});
});
</script>


