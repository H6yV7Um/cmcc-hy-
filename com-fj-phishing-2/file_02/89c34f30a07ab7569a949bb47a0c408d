<script type="text/javascript" language="javascript">

    $(document).ready(function () {
  
        var ProductFamilyID = document.getElementById('dnn_pricequote_DrpProductFamily2').value;
        var ProductModelId = document.getElementById('dnn_pricequote_DrpProductModel').value;

      
        if (ProductFamilyID == '0') {
            $('#dnn_pricequote_DrpProductFamily2').attr("disabled", "disabled");
         }

        if (ProductModelId == '0') {
            $('#dnn_pricequote_DrpProductModel').attr("disabled", "disabled");
        }

// below code appears here and in the BindDetails section down the page

        var ProductTypeDropDown = document.getElementById('dnn_pricequote_DrpProductType2');
        var ProductTypeSelected = ProductTypeDropDown.options[ProductTypeDropDown.selectedIndex].text;
        var ProductFamilyDropDown = document.getElementById('dnn_pricequote_DrpProductFamily2');
        var ProductFamilySelected = ProductFamilyDropDown.options[ProductFamilyDropDown.selectedIndex].text;
        var ProductModelDropDown = document.getElementById('dnn_pricequote_DrpProductModel');
        var ProductModelSelected = ProductModelDropDown.options[ProductModelDropDown.selectedIndex].text;
        var ProductFamilyID = document.getElementById('dnn_pricequote_DrpProductFamily2').value;
        var ProductModelId = document.getElementById('dnn_pricequote_DrpProductModel').value;

        $("#productboxinfo").html("<p id='productboxp'><span id='productboxspan'>Product</span>" + ProductModelSelected + " <span id='expandproductspan'>(<a onclick='fnExpandProductSelection()'>change</a>)</span></p>");

        if (ProductFamilyID == '0') {
            $("#productboxinfo").html("<p id='productboxp'><span id='productboxspan'>Product</span>None selected... <span id='expandproductspan'>(<a onclick='fnExpandProductSelection()'>change</a>)</span></p>");
        }

        if (ProductModelId == '0') {
            $("#productboxinfo").html("<p id='productboxp'><span id='productboxspan'>Product</span>" + ProductFamilySelected + " <span id='expandproductspan'>(<a onclick='fnExpandProductSelection()'>change</a>)</span></p>");
        }


        if (ProductModelSelected == 'Komet 7 and 7‑GLP') {
            $("#demorequestdiv").show();
        }


    });

	function postbackdata() {
	    document.getElementById('dnn_pricequote_hdnPostback').value = "FireSubmitEvent";
		__doPostBack("btnPopupSubmit");
		return true;
	}

	function fnCheckValidation() {
		postbackdata();
		return true;
	}

	function fnExpandProductSelection() {
	    $("#productboxes").show();
	    $("#productboxinfo").hide();
	        var currentquoteboxheight = $('.simplemodal-container').height();
	        var newquoteboxheight = currentquoteboxheight + 110;
	        $('.simplemodal-container').height(newquoteboxheight);
	}

  
    
</script>
<script type="text/javascript">
	function enableStates() {
		var country = $('.clscountry[id*="pricequote"]>option:selected').text();
		if (country == 'United States') {
		    $('.clsstate[id*="pricequote"]').removeAttr('disabled');
		    if ($('#statestogglediv').css('display') == 'none') {
		        var currentquoteboxheight = $('.simplemodal-container').height();
		        var newquoteboxheight = currentquoteboxheight + 48;
		        $('.simplemodal-container').height(newquoteboxheight);
		    }
            $('#statestogglediv').show();
		}
		else {
		    $('.clsstate[id*="pricequote"]').attr('disabled', 'true');
		    $('.clsstate[id*="pricequote"]').val('0');
		    if ($('#statestogglediv').css('display') == 'block') {
		        var currentquoteboxheight = $('.simplemodal-container').height();
		        var newquoteboxheight = currentquoteboxheight - 48;
		        $('.simplemodal-container').height(newquoteboxheight);
		    }
		    $('#statestogglediv').hide();
		}
	}

	function ProductModelGet() {
		var e = document.getElementById('dnn_pricequote_DrpProductFamily2').value;
		if (e == '0') {
			var listItems = "";
			listItems += "<option value='" + 0 + "'>" + "Please Select...." + "</option>";
			$("#dnn_pricequote_DrpProductModel").html(listItems);
			$('#dnn_pricequote_DrpProductModel').attr("disabled", "disabled");
		}
		else {
			$.ajax({
				type: "POST",
				url: "/ProductFilterService.asmx/ProductModel_Get",
				data: '{Value : "' + e + '"}',
				contentType: "application/json; charset=utf-8",
				dataType: "json",
				async: false,
				success: OnSuccess1,
				error: OnError
			});
		}
	}

	function OnSuccess1(data, status) {
		var listItems = "";
		listItems += "<option value='" + 0 + "'>" + "Please Select...." + "</option>";
		for (var i = 0; i < data.d.length; i++) {
			listItems += "<option value='" + data.d[i].ProductID + "'>" + data.d[i].Name + "</option>";
		}

		$("#dnn_pricequote_DrpProductModel").html(listItems);
		$('#dnn_pricequote_DrpProductModel').removeAttr('disabled');
	}


	function OnError(request, status, error) {
		//        alert("Error is " + error);
	}
   
   
</script>
<script language="javascript" type="text/javascript">

	function ProductFamilyGet() {
		var e = document.getElementById('dnn_pricequote_DrpProductType2').value;
		if (e == '0') {
			var listItems1 = "";
			listItems1 += "<option value='" + 0 + "'>" + "Please Select...." + "</option>";
			$("#dnn_pricequote_DrpProductFamily2").html(listItems1);
			$("#dnn_pricequote_DrpProductModel").html(listItems1);
			$('#dnn_pricequote_DrpProductFamily2').attr("disabled", "disabled");
			$('#dnn_pricequote_DrpProductModel').attr("disabled", "disabled");
		}
		else {
			$.ajax({
				type: "POST",
				url: "/ProductFilterService.asmx/ProductFamily_Get",
				data: '{Value : "' + e + '"}',
				contentType: "application/json; charset=utf-8",
				dataType: "json",
				async: false,
				success: OnSuccess,
				error: OnError
			});
		}
	}

	function OnSuccess(data, status) {
		var listItems = "";
		listItems += "<option value='" + 0 + "'>" + "Please Select...." + "</option>";
		for (var i = 0; i < data.d.length; i++) {
			listItems += "<option value='" + data.d[i].SeriesID + "'>" + data.d[i].Name + "</option>";
		}
		$('#dnn_pricequote_DrpProductFamily2').removeAttr('disabled');
		$("#dnn_pricequote_DrpProductFamily2").html(listItems);
	}


	function OnError(request, status, error) {
		// alert("Error is " + error);
	}
  
</script>
<script type="text/javascript">
	function GetHTMLContentForQuotation() {
		var Name = document.getElementById('dnn_pricequote_name').value;
		var Email = document.getElementById('dnn_pricequote_email').value;
		var Org = document.getElementById('dnn_pricequote_org').value;
		var Phone = document.getElementById('dnn_pricequote_phone').value;
		var Country = $("#dnn_pricequote_DrpCountry").find('option').filter(':selected').text();
		var ProductType = $("#dnn_pricequote_DrpProductType2").find('option').filter(':selected').text();
		var ProductFamily = $("#dnn_pricequote_DrpProductFamily2").find('option').filter(':selected').text();
		var ProductModel = $("#dnn_pricequote_DrpProductModel").find('option').filter(':selected').text();
		var Comment = document.getElementById('dnn_pricequote_TxtComments2').value;
		var CountryID = document.getElementById('dnn_pricequote_DrpCountry').value;
		var State = $("#dnn_pricequote_dropDownState").val() == "0"
			? ""
			: $("#dnn_pricequote_dropDownState").find('option').filter(':selected').text();
		var StateID = document.getElementById('dnn_pricequote_dropDownState').value;
		var SelectedTags = $('.clsselval').val();		
        var DemoRequested = "no";
        if ($('#demorequestchk').is(':checked')) {
            DemoRequested = "yes";
        }

        //google tracking data
        function _uGC(e, t, c) { if (!e || "" == e || !t || "" == t || !c || "" == c) return "-"; var u, m, n, r = "-"; return u = e.indexOf(t), n = t.indexOf("=") + 1, u > -1 && ((m = e.indexOf(c, u)) < 0 && (m = e.length), r = e.substring(u + n, m)), r } var pageTracker = _gat._getTracker("UA-1-1"); pageTracker._trackPageview(); var z = _uGC(document.cookie, "__utmz=", ";"), source = _uGC(z, "utmcsr=", "|"), medium = _uGC(z, "utmcmd=", "|"), term = _uGC(z, "utmctr=", "|"), content = _uGC(z, "utmcct=", "|"), campaign = _uGC(z, "utmccn=", "|"), gclid = _uGC(z, "utmgclid=", "|"); "-" != gclid && (source = "google", medium = "cpc"); var csegment = _uGC(document.cookie, "__utmv=", ";"); if ("-" != csegment) { var csegmentex = /[1-9]*?\.(.*)/; csegment = (csegment = csegment.match(csegmentex))[1] } else csegment = "(not set)"; var a = _uGC(document.cookie, "__utma=", ";"), aParts = a.split("."), nVisits = aParts[5];
        //google tracking data end
        //var googleinfo = " [END OF MESSAGE] Source: " + source + " | Medium: " + medium + " | Term: " + term + " | Content: " + content + " | Campaign: " + campaign + " | Segment: " + csegment + " | Number of visits: " + nVisits;

        var WebSource = source
        var WebMedium = medium
        var WebTerm = term
        var WebContent = content
        var WebCampaign = campaign
        var WebSegment = csegment
        var WebVisits = nVisits

        // google tracking data (utma cookie creation date)
        // function _uGC(e, t, c) { if (!e || "" == e || !t || "" == t || !c || "" == c) return "-"; var u, m, n, r = "-"; return u = e.indexOf(t), n = t.indexOf("=") + 1, u > -1 && ((m = e.indexOf(c, u)) < 0 && (m = e.length), r = e.substring(u + n, m)), r } var pageTracker = _gat._getTracker("UA-1-1"); pageTracker._trackPageview(); var z = _uGC(document.cookie, "__utma=", ";"), firstcreated = _uGC(z, "utmcsr=", "|"); 
        // end of google tracking data (utma cookie creation date)
        // var firstvisitdate = firstcreated;


       // var values = {};
       // var cookie = getCookie("__utma");
      //  if (cookie) {
       //     var z = cookie.split('.');
        //    var googledate1 = z[0];
        //    var googledate2 = z[1];
         //   var googledate3 = z[2];
         //   var googledate4 = z[3];
        //    var googledate5 = z[4];
      //  }

        $.ajax({
			type: "POST",
			url: "/ProductFilterService.asmx/QuotationRequest",
			data: '{Name : "' + Name + '",Email : "' + Email + '",Org : "' + Org + '",Phone : "' + Phone + '",Country : "' + Country + '",ProductType : "' + ProductType + '",ProductFamily: "' + ProductFamily + '",ProductModel: "' + ProductModel + '",Comment: "' + Comment + '",CountryID: "' + CountryID + '",SelectedVals: "' + SelectedTags + '",State: "' + State + '",StateID: "' + StateID + '",DemoRequested: "' + DemoRequested + '",WebSource: "' + source + '",WebMedium: "' + medium + '",WebTerm: "' + term + '",WebContent: "' + content + '",WebCampaign: "' + campaign + '",WebSegment: "' + csegment + '",WebVisits: "' + nVisits + '"}',
			contentType: "application/json; charset=utf-8",
			dataType: "json",
			success: OnSuccessGetHTMLContentForQuotation,
			error: OnError
		});
	}
	function OnSuccessGetHTMLContentForQuotation(data, status) {
	    document.getElementById("dnn_pricequote_BtnSendForm").style.background = '#5b8f22';
	    document.getElementById("dnn_pricequote_BtnSendForm").style.border = 'solid 1px green';
	    document.getElementById("dnn_pricequote_BtnSendForm").value = 'REQUEST HAS BEEN RECEIVED!';

       var Email = document.getElementById('dnn_pricequote_email').value;

		_gaq.push(['_setAccount', 'UA-55534-1']);
		_gaq.push(['_trackEvent', 'Quote Request - COMPLETED', Email, window.location.href]);
		_gaq.push(['_trackPageview', '/contact_us/thank_you/default.aspx?status=new']);

		// ensures the optimizely object is defined globally using
		window['optimizely'] = window['optimizely'] || [];

		// sends a tracking call to Optimizely for the given event name. 
		window.optimizely.push(["trackEvent", "QuoteSubmit"]);

	}


	function OnError(request, status, error) {
		//  alert("Error is " + error);
	}

