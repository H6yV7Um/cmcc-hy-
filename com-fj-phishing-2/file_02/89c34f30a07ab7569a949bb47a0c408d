<script type="text/javascript">

    $(window).load(function () {

        var BodyScript = $('.bodyscript').val();

        $('body').append(BodyScript);

        $('.search1').val('Search the site');
        $(".search1").focusin(function () {
            if ($('.search1').val() == 'Search the site') {
                $('.search1').val('');
                $('.search1').focus();
            }
        });

        $(".search1").focusout(function () {
            var value = $('.search1').val();

            if (value == 'Search the site') {
                $('.search1').blur();
            }
            else if (value == '') {
                $('.search1').val('Search the site');
                $('.search1').blur();
            }
            else {
                $('.search1').focus();
            }
        });
    });

</script>

