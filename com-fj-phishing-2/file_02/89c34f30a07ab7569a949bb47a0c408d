    <script type="text/javascript">
        if (typeof jQuery != 'undefined') {
            jQuery(document).ready(function ($) {
                var filetypes = /\.(zip|exe|pdf|doc*|xls*|ppt*|mp3)$/i;
                var baseHref = '';
                if (jQuery('base').attr('href') != undefined)
                    baseHref = jQuery('base').attr('href');
                jQuery('a').each(function () {
                    var href = jQuery(this).attr('href');
                    if (href && (href.match(/^https?\:/i)) && (!href.match(document.domain))) {
                        jQuery(this).click(function () {
                            var extLink = href.replace(/^https?\:\/\//i, '');
                            _gaq.push(['_trackEvent', 'External', 'Click', extLink]);
                            if (jQuery(this).attr('target') != undefined && jQuery(this).attr('target').toLowerCase() != '_blank') {
                                setTimeout(function () { location.href = href; }, 200);
                                return false;
                            }
                        });
                    }
                    else if (href && href.match(/^mailto\:/i)) {
                        jQuery(this).click(function () {
                            var mailLink = href.replace(/^mailto\:/i, '');
                            _gaq.push(['_trackEvent', 'Email', 'Click', mailLink]);
                        });
                    }
                    else if (href && href.match(filetypes)) {
                        jQuery(this).click(function () {
                            var extension = (/[.]/.exec(href)) ? /[^.]+$/.exec(href) : undefined;
                            var filePath = href;
                            _gaq.push(['_trackEvent', 'Main Content', 'Click-' + extension, filePath]);
                            _gaq.push(['_trackPageview', filePath]);
                            //                        if (jQuery(this).attr('target') != undefined && jQuery(this).attr('target').toLowerCase() != '_blank') {
                            //                            setTimeout(function () { location.href = baseHref + href; }, 200);
                            //                            return false;
                            //                        }
                        });
                    }
                });
            });
        }

        //$(function () { $("[id$='Username_registerLink']").attr("href", "http://www.andor.com/my/user/Account.aspx"); });
        // above script is to redirect View Profile link into MyAndor

    </script>
    <script type="text/javascript">

        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-55534-1']);
        _gaq.push(['_trackPageview']);

        (function () {
            var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'stats.g.doubleclick.net/dc.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        })();


        function ShowPdfModal(elem) {
            var url = $(elem).attr("pdfurl");
            $('#basic-modal-content-pdf-dynamic iframe')[0].src = url;
            $('#basic-modal-content-pdf-dynamic').css('height', $(window).height() * 0.8);
            $('#basic-modal-content-pdf-dynamic').css('width', $(window).width() * 0.8);
            _gaq.push(['_setAccount', 'UA-55534-1']);
            _gaq.push(['_trackEvent', 'Camera Filter Sidebar', 'Click-pdf', url]);
            _gaq.push(['_trackPageview', url]);


            $.ajax({
                type: "POST",
                url: "/ProductFilterService.asmx/LogPdfOpened",
                data: '{rawUrl : "' + url + '"}',
                contentType: "application/json; charset=utf-8",
                dataType: "json"
            });

            if (/Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent) && url) {
                window.open(url);
            }
            else {
                $('#basic-modal-content-pdf-dynamic').modal();
                $("#basic-modal-content-pdf-dynamic .basic-modal-open").click(function () { window.open($('#basic-modal-content-pdf-dynamic iframe')[0].src) });
                $("#basic-modal-content-pdf-dynamic .basic-modal-copy").zclip({
                    path: "/js1/zclip/ZeroClipboard.swf",
                    copy: function () { return $('#basic-modal-content-pdf-dynamic iframe')[0].src; }
                });
            }
            return false;
        }
    </script>  

