<script>

    $(window).scroll(function () {

        var scroll = $(window).scrollTop();
        if (scroll >= 400 && $("#plasma").hasClass("menuhighlight") == false) {

            setTimeout(function () { $("#plasma").addClass("menuselected") }, 1500);
            $("#plasma").addClass("menuhighlight");
            setTimeout(function () { $("#plasma").removeClass("menuselected") }, 2500);
            setTimeout(function () { $("#libs").addClass("menuselected") }, 2500);
            setTimeout(function () { $("#libs").removeClass("menuselected") }, 3500);
            setTimeout(function () { $("#combustion").addClass("menuselected") }, 3500);
            setTimeout(function () { $("#combustion").removeClass("menuselected") }, 4500);
            setTimeout(function () { $("#optics").addClass("menuselected") }, 4500);
            setTimeout(function () { $("#optics").removeClass("menuselected") }, 5500);
            setTimeout(function () { $("#luminescence").addClass("menuselected") }, 5500);
            setTimeout(function () { $("#luminescence").removeClass("menuselected") }, 6500);
        }

        if (scroll >= 1000 && $("#gating").hasClass("menuhighlight") == false) {

            setTimeout(function () { $("#gating").addClass("menuselected") }, 1500);
            $("#gating").addClass("menuhighlight");
            setTimeout(function () { $("#gating").removeClass("menuselected") }, 2500);
            setTimeout(function () { $("#intensify").addClass("menuselected") }, 2500);
            setTimeout(function () { $("#intensify").removeClass("menuselected") }, 3500);

        }


    }); 

</script>
    
