<script type="text/javascript">

	function BindDetails(e) {	    
        var settings = $(e).parent().find(".quoteSettings");
		if (settings.length > 0) {
			if (settings.attr("comments")) {
				$("#dnn_pricequote_TxtComments2").val(settings.attr("comments"));
			}
			if (settings.attr("productType")) {
				$('#dnn_pricequote_DrpProductType2').removeAttr('disabled');
				$("#dnn_pricequote_DrpProductType2").val(settings.attr("productType"));
				$("#dnn_pricequote_DrpProductType2").change();
			}
			if (settings.attr("productFamily")) {
				$('#dnn_pricequote_DrpProductFamily2').removeAttr('disabled');
				$("#dnn_pricequote_DrpProductFamily2").val(settings.attr("productFamily"));
				$("#dnn_pricequote_DrpProductFamily2").change();
			}
			if (settings.attr("productModel")) {
				$('#dnn_pricequote_DrpProductModel').removeAttr('disabled');
				$("#dnn_pricequote_DrpProductModel").val(settings.attr("productModel"));
			}
		}

		var pagename = $('.pagename').val();
		if (pagename == 'camera') {
			var camera_series_id = null;
			var product_id = null;
			camera_series_id = ($(e).next().next().val());
			camera_product_id = ($(e).next().next().next().val());
			if (camera_series_id != null) {
				$("#dnn_pricequote_DrpProductType2").val(1);
				$("#dnn_pricequote_DrpProductType2").change();
				$("#dnn_pricequote_DrpProductFamily2").val(camera_product_id);
				$("#dnn_pricequote_DrpProductFamily2").change();
				$("#dnn_pricequote_DrpProductModel").val(camera_series_id);
			}
		}

		if (pagename == 'spectrograph') {
			var spectrograph_series_id = null;
			var spectrograph_id = null;
			spectrograph_series_id = ($(e).next().next().val());
			spectrograph_product_id = ($(e).next().next().next().val());
			if (spectrograph_series_id != null) {
				$("#dnn_pricequote_DrpProductType2").val(2);
				$("#dnn_pricequote_DrpProductType2").change();
				$("#dnn_pricequote_DrpProductFamily2").val(spectrograph_product_id);
				$("#dnn_pricequote_DrpProductFamily2").change();
				$("#dnn_pricequote_DrpProductModel").val(spectrograph_series_id);
			}
			$('#dnn_pricequote_DrpProductModel').removeAttr('disabled');
			$("#dnn_pricequote_DrpProductFamily2").removeAttr('disabled');
		}

		if (pagename == 'system') {
			var system_series_id = null;
			var system_id = null;
			system_series_id = ($(e).next().next().val());
			system_product_id = ($(e).next().next().next().val());
			if (system_series_id != null) {
				$("#dnn_pricequote_DrpProductType2").val(3);
				$("#dnn_pricequote_DrpProductType2").change();
				$("#dnn_pricequote_DrpProductFamily2").val(system_product_id);
				$("#dnn_pricequote_DrpProductFamily2").change();
				$("#dnn_pricequote_DrpProductModel").val(system_series_id);
			} 
			$('#dnn_pricequote_DrpProductModel').removeAttr('disabled');
			$("#dnn_pricequote_DrpProductFamily2").removeAttr('disabled');
		}

		if (pagename == 'software') {
			var software_series_id = null;
			var software_id = null;
			software_series_id = ($(e).next().next().val());
			software_product_id = ($(e).next().next().next().val());
			if (software_series_id != null) {
				$("#dnn_pricequote_DrpProductType2").val(4);
				$("#dnn_pricequote_DrpProductType2").change();
				$("#dnn_pricequote_DrpProductFamily2").val(software_product_id);
				$("#dnn_pricequote_DrpProductFamily2").change();
				$("#dnn_pricequote_DrpProductModel").val(software_series_id);
			} 
			$('#dnn_pricequote_DrpProductModel').removeAttr('disabled');
			$("#dnn_pricequote_DrpProductFamily2").removeAttr('disabled');
		}

		if (pagename == 'cameradetails') {
			var spectrograph_series_id = null;
			var spectrograph_product_id = null;
			spectrograph_series_id = ($(e).next('.seriesid').val());
			spectrograph_product_id = ($(e).next('.seriesid').next('.productid').val());
			if (spectrograph_series_id != null) {
				$("#dnn_pricequote_DrpProductType2").val(1);
				$("#dnn_pricequote_DrpProductType2").change();
				$("#dnn_pricequote_DrpProductFamily2").val(spectrograph_series_id);
				$("#dnn_pricequote_DrpProductFamily2").change();
				$("#dnn_pricequote_DrpProductModel").val(spectrograph_product_id);
			}
		}
		if (pagename == 'spectrographdetails') {
			var spectrograph_series_id1 = null;
			var spectrograph_product_id1 = null;
			spectrograph_series_id1 = ($(e).next('.seriesid').val());
			spectrograph_product_id1 = ($(e).next('.seriesid').next('.productid').val());
			if (spectrograph_series_id1 != null) {
				$("#dnn_pricequote_DrpProductType2").val(2);
				$("#dnn_pricequote_DrpProductType2").change();
				$("#dnn_pricequote_DrpProductFamily2").val(spectrograph_series_id1);
				$("#dnn_pricequote_DrpProductFamily2").change();
				$("#dnn_pricequote_DrpProductModel").val(spectrograph_product_id1);
			}
		}

		if (pagename == 'systemdetails') {
			var spectrograph_series_id = null;
			var spectrograph_id = null;
			spectrograph_series_id = ($(e).next('.seriesid').val());
			spectrograph_product_id = ($(e).next('.seriesid').next('.productid').val());
			if (spectrograph_series_id != null) {
				$("#dnn_pricequote_DrpProductType2").val(3);
				$("#dnn_pricequote_DrpProductType2").change();
				$("#dnn_pricequote_DrpProductFamily2").val(spectrograph_series_id);
				$("#dnn_pricequote_DrpProductFamily2").change();
				$("#dnn_pricequote_DrpProductModel").val(spectrograph_product_id);
			}
		}

		if (pagename == 'softwaredetails') {
			var spectrograph_series_id = null;
			var spectrograph_id = null;
			spectrograph_series_id = ($(e).next('.seriesid').val());
			spectrograph_product_id = ($(e).next('.seriesid').next('.productid').val());
			if (spectrograph_series_id != null) {
				$("#dnn_pricequote_DrpProductType2").val(4);
				$("#dnn_pricequote_DrpProductType2").change();
				$("#dnn_pricequote_DrpProductFamily2").val(spectrograph_series_id);
				$("#dnn_pricequote_DrpProductFamily2").change();
				$("#dnn_pricequote_DrpProductModel").val(spectrograph_product_id);
			}
}

var ProductTypeDropDown = document.getElementById('dnn_pricequote_DrpProductType2');
var ProductTypeSelected = ProductTypeDropDown.options[ProductTypeDropDown.selectedIndex].text;
var ProductFamilyDropDown = document.getElementById('dnn_pricequote_DrpProductFamily2');
var ProductFamilySelected = ProductFamilyDropDown.options[ProductFamilyDropDown.selectedIndex].text;
var ProductModelDropDown = document.getElementById('dnn_pricequote_DrpProductModel');
var ProductModelSelected = ProductModelDropDown.options[ProductModelDropDown.selectedIndex].text;
var ProductFamilyID = document.getElementById('dnn_pricequote_DrpProductFamily2').value;
var ProductModelId = document.getElementById('dnn_pricequote_DrpProductModel').value;

