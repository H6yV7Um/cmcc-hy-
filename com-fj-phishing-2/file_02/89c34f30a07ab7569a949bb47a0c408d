      <script type="text/javascript">
          $(document).ready(function () {

              var CTAtitle = $("#CTAslider").find('h2').text();

              if (document.cookie.indexOf(CTAtitle) >= 0) {

              }
              else {

                  interval = setInterval(function () {
                      $('#CTAslider').animate({
                          bottom: 0
                      }, "slow");
                  }, 12000);

              }

              $("#CTAclose").click(function () {
                  $('#CTAslider').animate({
                      bottom: -225
                  }, "slow");
                  clearInterval(interval);

                  var CTAtitle = $("#CTAslider").find('h2').text();
                  if (document.cookie.indexOf(CTAtitle) >= 0) {

                  }
                  else {
                      document.cookie = CTAtitle + "=closed";
                  }


              });
          });

    </script>
    <script type="text/javascript">
        //for newsletter

        function validatefieldsNewsletter() {
            var txtname = $('.navNewsName').val();
            var txtemail = $('.navNewsEmail').val();
            var chkphysical = $('.navNewsPhysical').is(':checked') ? "Yes" : "No";
            var chklife = $('.navNewsLife').is(':checked') ? "Yes" : "No";
            var checked = $("#newsletterform input:checked").length > 0;
            $('#accept').click(function () {
                var permission = this.checked ? "Y" : "N";
                console.log(permission);
            });
            var returnval = true;
            if (txtname == '') {
                $('.navNewsName').css({ "border": "2px inset red" });
                return returnval = false;
            }
            else {
                $('.navNewsName').css({ "border": "2px inset white" });
            }
            if (txtemail == "") {
                $('.navNewsEmail').css({ "border": "2px inset red" });
                return returnval = false;
            }
            else {
                $('.navNewsEmail').css({ "border": "2px inset white" });
            }
            if (!checked) {
                $('#rfvNewsVersion').html('*');
                return returnval = false;
            }
            else {
                $('#rfvNewsVersion').html(' ');
            }
            var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
            if (reg.test(txtemail) == false) {
                $('.navNewsEmail').css({ "border": "2px inset red" });
                return returnval = false;
            }
            else {
                $('.navNewsEmail').css({ "border": "2px inset white" });

            }
            if (returnval == true) {
                $('.navNewsButton').css({ "background": "grey" });
                $('.navNewsButton').css({ "border": "solid 1px #4d4f53" });
                $('.navNewsButton').text('PLEASE WAIT...');
                SendMailCTANewsletter(txtname, txtemail, chkphysical, chklife);
            }
        }
        //for sending mail in case of newsletter
        function SendMailCTANewsletter(txtname, txtemail, chkphysical, chklife) {
            $.ajax({
                type: "POST",
                url: "/ProductFilterService.asmx/SendMailCTANewsletter",
                data: '{Name : "' + txtname + '",Email : "' + txtemail + '",Physical : "' + chkphysical + '",Life : "' + chklife + '"}',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: OnSuccessMsg,
                error: OnErrorMsg
            });
        }

        function OnSuccessMsg(data, status) {
            $('.navNewsButton').css({ "background": "#5B8F22" });
            $('.navNewsButton').css({ "border": "solid 1px #385815" });
            $('.navNewsButton').text('THANK YOU!');
            $('.navNewsName').val('');
            $('.navNewsEmail').val('');
            $('.navNewsOrg').val('');
            $('.navNewsTel').val('');
            $('.navNewsTime').val('');
            $('.navNewsSoftware').val('');

            //           set cookie 
            var CTAtitle = $("#CTAslider").find('h2').text();
            document.cookie = CTAtitle + "=submitted; max-age=63115200";

        }

        function OnErrorMsg(request, status, error) {
            //  alert("Error is " + error);
        }
    </script>





