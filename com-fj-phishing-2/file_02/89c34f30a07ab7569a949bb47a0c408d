	function validationcheck() {
	    
		var result = ValidateText('2', 'dnn_pricequote')
		if (result == true) {
		    document.getElementById("dnn_pricequote_BtnSendForm").disabled = true;
		    document.getElementById("dnn_pricequote_BtnSendForm").style.background = '#9a9b9c';
		    document.getElementById("dnn_pricequote_BtnSendForm").style.border = 'black';
            document.getElementById("dnn_pricequote_BtnSendForm").value = 'SENDING. PLEASE WAIT...';
            GetHTMLContentForQuotation();
	    	}
		else {
			return false;
		}
	}
