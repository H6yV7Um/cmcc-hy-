<script type="text/javascript">
  //搜索框热词默认词静态化数据暂时放在这里后续应该会调接口动态化
  var hotkey_list = [{"pc_url":"https://www.lemall.com/chaox/x50l.html","text":"X50L","phone_url_type":"1","m_url":"https://m.lemall.com/tv/chaox/x50l.html","phone_url":"https://m.lemall.com/tv/chaox/x50l.html"},{"pc_url":"http://www.lemall.com/tv.html","text":"超级电视","phone_url_type":"1","m_url":"http://m.lemall.com/huodong/tv3xh0929.html","phone_url":"http://m.lemall.com/huodong/tv3xh0929.html"},{"pc_url":"/product/search.html?level=2&cid2=20031","text":"耳机/音箱","phone_url_type":"1","m_url":"http://m.lemall.com/products/index/cid-20031.html","phone_url":"http://m.lemall.com/products/index/cid-20031.html"}];
  if(hotkey_list == null ||  hotkey_list.length ==  0 ) { hotkey_list =[{text:"",pc_url:""}];}
  var defaultkey_list =["新品电视","X43L","X40L"];
  var timeOutList = {};
  $(document).ready(function () {
      //为搜索框绑定自动补全事件
      $("#txt_company").autoComplete({ url:sendLink.mallcommon+"prouduct/getSuggest.jsonp", defaultData : defaultkey_list ,defaultKey :hotkey_list[0].text ,defaultUrl:hotkey_list[0].pc_url});
      $("#txt_company").val(hotkey_list[0].text);
      initHotKey();
  });
   //初始化热词数据
  function initHotKey(){
    if(hotkey_list){
          for (var i = 1; i < hotkey_list.length; i++) {
            $(".hotwords ").append("<a href='"+hotkey_list[i].pc_url+"' target='_blank'  onclick=\"javascript:letvAlysSendAction(34,83,'module=hot_search&click_area=text&action_code=0&position="+(i+1)+"&query="+hotkey_list[i].text+"');\" >"+hotkey_list[i].text+"</a>");
          };
    }
  }

    //顶托滑动效果
    function header_links(){
             $header_links=$("#header_links");
             $header_links.timer="";

             $header_links.find("li[type=slide]").mouseenter(function(event) {
                 var _this = this;
                  clearTimeout($header_links.timer);
                  $header_links.timer=setTimeout(function(){
                      $(_this).addClass('cur');
                      $(_this).find(".le-tips,dl").slideDown(150);
                  },250)
             }).mouseleave(function(event) {
                  clearTimeout($header_links.timer);
                  $(this).removeClass('cur');
                  $(this).find(".le-tips,dl").slideUp(100);
             })
    }
    header_links();
</script>
