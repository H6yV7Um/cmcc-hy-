    <script type="text/javascript">
    var globalHeadVar={
        m_url:"//m.lemall.com"//m站跳转链接
    };
    /**
     * pc转到m站的跳转
    */
    if ((typeof(globalHeadVar) != "undefined") && (globalHeadVar.m_url) && (globalHeadVar.m_url != "")) {
        var browser = {
            versions: function() {
                var a = navigator.userAgent;
                return {
                    mobile: !!a.match(/AppleWebKit.*Mobile.*/),
                    ios: !!a.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/),
                    android: -1 < a.indexOf("Android"),
                    iPhone: -1 < a.indexOf("iPhone"),
                    iPad: -1 < a.indexOf("iPad"),
                    webApp: -1 == a.indexOf("Safari"),
                    weixin: -1 < a.indexOf("MicroMessenger")
                }
            } ()
        };
        if (document.cookie.indexOf("COOKIE_USER_ID_FORWARD_WAP") == -1 && !browser.versions.iPad && (browser.versions.iPhone || browser.versions.mobile || browser.versions.android)) {
            window.location.href = globalHeadVar.m_url + location.search
        };
    }
