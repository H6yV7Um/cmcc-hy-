<script type="text/javascript">

//右侧飘窗显示隐藏方法
function showOrHide(parent,lis) {
				var $sidebar = $(parent);
				var $lis = $sidebar.find(lis);
				$sidebar.object="";

				$lis.mouseenter(function(event) {
					clearTimeout($sidebar.timer);
					$($sidebar.object).hide();
					$lis.removeClass('hover');
					$(this).addClass("hover");
					var id=$(this).find($(this).attr("object"));
					$(id).show();
				}).mouseleave(function(event) {
					$sidebar.object=$(this).attr("object");

					$sidebar.timer=setTimeout(function(){
						$lis.removeClass('hover');
						$($sidebar.object).hide()
					},300)
				});;
			}


/**
 *百度分享
 */
//构建百度分享URL
function createbdShareUrl(url){
	if(typeof url != "undefined" && url.length > 0){
		if(url.indexOf("http") >= 0){
			return url;
		}else{
          	return url.indexOf("/") == 0 ? ("http://" + window.location.host + url ) : ("http://" + window.location.host + url);
		}
	}
	return url;
}

var BDSHAREURL =   "/index.html?cps_id=u_p_sy";

if(BDSHAREURL.length > 0 ){
  BDSHAREURL = createbdShareUrl(BDSHAREURL);
}


window._bd_share_config={
	"common": {
		"bdSnsKey": {
		},
		"bdText" : "厉害了~@乐视商城 #超级电视#生态型电商2.0来啦！TA引领全球电商进入生态时代，TA带你享尽生态福利，TA……忍不住想要体验TA？猛戳链接→",
		"bdDesc" : "厉害了~@乐视商城 #超级电视#生态型电商2.0来啦！TA引领全球电商进入生态时代，TA带你享尽生态福利，TA……忍不住想要体验TA？猛戳链接→",
		"bdUrl" : BDSHAREURL,
		"bdPic" : "//img1-lemall.letvimg.com/file/20161223/default/5547235604044316",
		"bdSize": "32"
	},
	"share": {
	}
