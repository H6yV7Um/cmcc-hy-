<script>
(function() {
  var eDatetimes = document.querySelector('#J_mpHomeNoticeWrap').querySelectorAll('.mpHomeNoticeItem span[data-time]');
  if (!eDatetimes) {
    return;
  }

  var nowTime = +new Date();
  var newLimit = 60 * 60 * 24 * 30 * 1000;
  for (var i = 0, len = eDatetimes.length; i < len; i++) {
    var time = eDatetimes[i].getAttribute('data-time');
    if (time) {
      eDatetimes[i].innerText = [getDateString(+time), eDatetimes[i].innerText].join(' ');
      if (nowTime - time <= newLimit) {
        eDatetimes[i].parentNode.className += ' mpNew';
      }
    }
  }

  function getDateString(val) {
    var date = new Date(val);
    var year = date.getFullYear();
    var month = date.getMonth() + 1;
    var day = date.getDate();

    return ['[', month, '.', day, ']'].join('');
  }
})();
</script>


