<script type="text/javascript">
// 用URIBroker将URL替换成当前环境的真实URL
(function replaceServer() {
  function addParam(link, key, value) {
    var iHash = link.indexOf('#'),
      hash = iHash >= 0 ? link.substr(iHash, link.length) : '';

    link = link.replace(/#.*$/, '').replace(new RegExp('&?' + key + '=[^&]*'), '');

    return [
    link,
    link.indexOf('?') >= 0 ? '&' : '?',
    key + '=' + encodeURIComponent(value),
    hash
    ].join('');
  }

  if(window.GLOBAL && window.GLOBAL.system && window.jQuery){
    var sys = window.GLOBAL.system;

    var aNode, h, head, nodes = jQuery('.ent-uri-placeholder');
    nodes.shift = Array.prototype.shift;
    while(aNode = nodes.shift()){
      h = aNode.getAttribute('data-href') || aNode.href;
      if(h && (h = h.split('@')) && h.length > 1) {
        head = h.shift().split('/');
        head = head[head.length-1];
        if(head = sys[head]){
          if(head.substr(head.length-1) == '/') head = head.substr(head.length-1);
          h.unshift(head);
          aNode.href = h.join('');
          aNode.removeAttribute('data-href');
        }
      }

      if (h && jQuery(aNode).hasClass('_is_login')) {
        aNode.href = addParam(aNode.href, 'goto', sys['enterpriseportalServer'] + '/index.htm');
      }
    }
  }

  replaceServer.times = (replaceServer.times || 0) + 1;
  if (replaceServer.times < 30) {
    setTimeout(replaceServer, 1000);
  }
})();

//IE8
(function(){
  var noop = function(){};
  window.console = (window && window.console && window.console.log) ? window.console : {
  log: noop,
  warn: noop,
  error: noop,
  info: noop
  };
})();

// 黑条导航和二级导航的加载逻辑
seajs.use(['$',"enterpriseportal/globalNav2/2.0.1/globalNav"],function($,glb){
  var nav = new glb({
    useGoBackToTop:'YES',
    navType:'',
    role: 'visitor',
    session:'false',
    float:'false',
    baseURI:window.GLOBAL.system['enterpriseportalServer']
  });

  
  var sys = window.GLOBAL.system
  jQuery.ajax({
    type: "get",
    url: sys['enterpriseportalServer'] +"/portal/blackBarNav.json",
    dataType: "jsonp",
    jsonp: "jsonpCallback",
    jsonpCallback:"handle",
    success: function(res){
      // 灰度标示
      var productGray = res.productGray
      var menu = res.menu
      if (productGray === true) {
        var menus = []
        jQuery.each(menu, function(index, item) {
          menus.push('<li><a class="ent-uri-placeholder" target="_blank" href="' + item.url + '">'+ item.name + '</a></li>')
        })
        jQuery('#J_service .links').html(menus.join(''))
      }
    },
    error: function(){
      throw new Error('blackBarNav查询失败')
    }
  });
});

seajs.use(['arale/detector/1.3.0/detector'],function(d){
  var n,p,i=0,arr=[],innerArr;
  for(n in d){
    if(typeof d[n] == 'object'){
      innerArr = [];
      for(p in d[n]){
        innerArr.push(p + ' : ' + d[n][p]);
      }
      arr.push(innerArr);
    }
  }
});

// 企业版导航埋点数据
try {
  window.Tracker.click("entNavUnify_" + window.location.host);
} catch(e) {}
</script>
