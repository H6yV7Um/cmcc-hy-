                    //2016-03-14 交付文件数 1770821
                    var counts = Main.getcounts().value;
                    var num = (counts * 103 / 100000000).toFixed(2);
                    if (num.length>=4) {
                        num = num.substr(0,4);
                    }
                    $("#menoycount").html(num);
                    count_num(counts);
                    function count_num(t) {
                        var r = 200,
                        s = 0,
                        d = $("#numlist");
                        /^\d+$/.test(t) || (t = "0"),
                        t = String(t);
                        var o = t.split("");
                        d.empty();
                        for (var l = o.length - 1; l >= 0; l--) {
                            var c = parseInt(o[l]),
                            p = $('<div class="home_card"></div>');
                            d.prepend(p);
                            if (supportCss3('transition')) {
                                for (var u = 0; c >= u; u++) {
                                    var m = "mask-card";
                                    u == c ? m = "" : s++;
                                    var f = $('<div class="' + m + " home_card_" + u + '"> <div class="home_card_t"><div class="home_card_i"></div></div><div class="home_card_b" ><div class="home_card_i"></div></div></div>'),
                                    v = r + "ms",
                                    g = r * s + "ms",
                                    y = a(v, g);
                                    $.extend(y, {
                                        opacity: 1
                                    }),
                                    f.find(".home_card_t").css(y);
                                    var h = a(v, g);
                                    $.extend(h, i(0)),
                                    $.extend(h, {
                                        opacity: 1
                                    }),
                                    f.find(".home_card_b").css(h),
                                    //$(f).appendTo(p);
                                    $(p).prepend(f);
                                }
                                var e = {
                                    opacity: 0
                                };
                                setTimeout(function () {
                                    var e = {
                                        opacity: 0
                                    };
                                    $(".mask-card .home_card_t").css(e);
                                    var n = {
                                        opacity: 0
                                    };
                                    $.extend(n, i(180)),
                                    $(".mask-card .home_card_b").css(n)
                                }, r * s);
                            } else {
                                var f = $('<div class="home_card_' + c + '"> <div class="home_card_t"><div class="home_card_i"></div></div><div class="home_card_b" ><div class="home_card_i"></div></div></div>');
                                $(p).prepend(f);
                            }
                        }
                    }
                    function a(e, n) {
                        return {
                            transition: "all " + e + " linear " + n,
                            "-webkit-transition": "all " + e + " linear " + n,
                            "-moz-transition": "all " + e + " linear " + n,
                            "-ms-transition": "all " + e + " linear " + n,
                            "-o-transition": "all " + e + " linear " + n
                        }
                    }
                    function i(e) {
                        return {
                            transform: "rotateX(" + e + "deg)",
                            "-ms-transform": "rotateX(" + e + "deg)",
                            "-webkit-transform": "rotateX(" + e + "deg)",
                            "-o-transform": "rotateX(" + e + "deg)",
                            "-moz-transform": "rotateX(" + e + "deg)"
                        }
                    }
                    //判断是否支持css3
                    function supportCss3(style) {
                        var prefix = ['webkit', 'Moz', 'ms', 'o'],
                        i,
                        humpString = [],
                        htmlStyle = document.documentElement.style,
                        _toHumb = function (string) {
                            return string.replace(/-(\w)/g, function ($0, $1) {
                                return $1.toUpperCase();
                            });
                        };

                        for (i in prefix)
                            humpString.push(_toHumb(prefix[i] + '-' + style));

                        humpString.push(_toHumb(style));

                        for (i in humpString)
                            if (humpString[i] in htmlStyle) return true;

                        return false;
                    }


                </script>

            </div>
        </div>
        
