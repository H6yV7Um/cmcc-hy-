$(function(){

	$(".head-menu ul a").unbind().bind("mouseover",function(){

		$(this).parents("ul").find(".title_color").removeClass().addClass("text_color");

		$(this).removeClass().addClass("title_color title_font");

	}).bind("focus",function(){

		$(this).blur();

	});

	$(".head-menu ul").unbind().bind("mouseleave",function(){

		$(this).find(".title_color").removeClass().addClass("text_color");

		$(".head-menu ul a").eq($(".head-menu").attr("index")).removeClass().addClass("title_color title_font");

	});

	//$(".sub_content a").tip(2);

});

function toJobCenter(){

	var loginUser = parent.$("#p_userAccount").val();

	if(loginUser==''||loginUser==null){

		$.layer({

			dialog:{

				btns:2,

				btn:["是","否"],

				type:1,

				msg:"您尚未登录，是否登录？",

				"yes":function(index){

					layer.close(index);

					toLogin('getpersonalInfo.do');

				},

				"no":function(index){

					layer.close(index);

				}

			}

		});

	}else{

		window.location='getpersonalInfo.do';

	}

}

