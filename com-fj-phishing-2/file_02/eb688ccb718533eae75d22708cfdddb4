    $(function(){
        $('.ms_home_page_product_title_list_li').each(function(index){
            $('.ms_home_page_product_title_list_li').eq(index).click(function(){
                $(this).addClass('ms_home_page_product_title_list_li_active').siblings().removeClass('ms_home_page_product_title_list_li_active');
                $('.cardInfo_product_list_ul').eq(index).addClass('cardInfo_product_list_ul_show').siblings().removeClass('cardInfo_product_list_ul_show');
                $('#home_page_iframe').attr("height","");
                $('#iframe_card').attr("height",'');
                $('.detailBox').eq(index).addClass('cardInfo_product_title_nav_box_show').siblings().removeClass('cardInfo_product_title_nav_box_show');
            });
        });
    });
