<script>
  function search(){
	   	if(searchForm.searchValue.value=="" || searchForm.searchValue.value=="请输入关键字"){
	   		alert("关键字为必填项");
	   		searchForm.searchValue.focus();
	   		return;
	   	}else{
	   		searchForm.submit();
	    }
	}
  </script>
