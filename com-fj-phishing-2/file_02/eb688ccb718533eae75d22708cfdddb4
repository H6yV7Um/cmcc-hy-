    <script type="text/javascript">
	function step(){
			$('.ms_home_page_nav_list').removeClass('ms_home_page_nav_list_show');
				var taidf = $.PopupBox({
				type: 'fail',
			        headcont:'近期开放查询，敬请期待！',
			        headcontimgsrc:'/statics/CN/PC/images/close.png'
				});
				return;
	}





		function check(){
		$.ajax({

				
				url:rootPath + "/op_exchange_rate/islogined.gsp",
				type:"post",
				dataType:"json",
				success:function(r) {
					if(r.retCode == "0000"){
						$(".ms_home_page_header_li5_box").remove();
						var taidf = $.PopupBox({
							type: 'headConfirm',
							headcont:'您确定注销当前账号吗？',
							confirmBtn: '确定',
							cancelBtn:'取消',
						});
					}else {
						//var backimg=document.getElementById("loginicon").currentStyle.backgroundImage;
						var backimg=document.getElementById("loginicon");
						var imageUrl=window.getComputedStyle?window.getComputedStyle(backimg,null).backgroundImage:backimg.currentStyle.backgroundImage;
						//var imageUrl = $(".ms_home_page_header_li_a1").css("background");
						imageUrl = imageUrl.split("url(")[1].split(")")[0];
						if(imageUrl.indexOf("user1")!=-1){
							var taidf = $.PopupBox({
							type: 'error',
							headcont:'登录已失效，请重新登录',
							confirmBtn: '确定',
							cancelBtn:'返回首页'
							});
							return false;
						}else{
							$('.ms_home_page_header_li5_box').toggle();
						}
						
						
					}
				},
				error:function(r) {
					alert("连接失败");
				}
			});
        

    }
    </script>
	<script>
		//刷新验证码
		function changeImg(){
			$("#safeCodeImg_head").attr("src",rootPath+"opencard/safeCode.gsp?rd="+Math.random());//?rd="+new Date()
		}

</script>

	<script type="text/javascript">  



	//判断浏览器是否支持 placeholder属性  
	function isPlaceholder(){  
	    var input = document.createElement('input');  
	    return 'placeholder' in input;  
	}  
	  
	if (!isPlaceholder()) {//不支持placeholder 用jquery来完成  
	    $(document).ready(function() {  
	        if(!isPlaceholder()){  
	            $("input").not("input[type='password']").each(//把input绑定事件 排除password框  
	                function(){  
	                    if($(this).val()=="" && $(this).attr("placeholder")!=""){  
						$(this).css("color","#cccccc");
							$(this).css("height","34px;","line-height","34px;");
	                        $(this).val($(this).attr("placeholder"));  
	                        $(this).focus(function(){  
							$(this).css("color","#1a1a1a");
	                            if($(this).val()==$(this).attr("placeholder")) $(this).val("");  
	                        });  
	                        $(this).blur(function(){  
							if($(this).val()==""){
								$(this).css("color","#cccccc");
							}else{
								$(this).css("color","#1a1a1a");
							}
	                            if($(this).val()=="") $(this).val($(this).attr("placeholder"));  
	                        });  
	                    }  
	            });  

	            
	            //对password框的特殊处理1.创建一个text框 2获取焦点和失去焦点的时候切换  
	            var pwdField  = $("input[type=password]");  
	            var pwdVal = pwdField.attr('placeholder');  
	            pwdField.after('<input id="pwdPlaceholder" type="text" value='+pwdVal+' autocomplete="off" />');  
	            var pwdPlaceholder = $('#pwdPlaceholder');  

	            pwdPlaceholder.show();  
	            pwdField.hide();  

	            pwdPlaceholder.focus(function(){  
	                pwdPlaceholder.hide();  
	                pwdField.show();  
	                pwdField.focus();  
	            });  
	              
	            pwdField.blur(function(){  
	                if(pwdField.val() == '') {  
	                    pwdPlaceholder.show();  
	                    pwdField.hide();  
	                }  
	            });  
	              
	        }  
	    });  
	      
	}  
	</script>  
