  <script>
$(function(){
	index_scroll_02('#j_my_scroll2','.right_btn a','.left_btn a',158.5,6,'.index_scroll_li');
	function index_scroll_02(chang_obj,right_obj,left_obj,Swidth,i_num,num_obj){
		 $(left_obj).css('cursor','default');
		var len = $(chang_obj).find(num_obj).length;
		var index = 0 ,i=i_num; //index点击次数，i是一页li个数
		var Awidth = 0;	//不是一页倍数，点击滑动个数
		if(len < i+1 ){//隐藏
			$(right_obj).hide();
			 $(left_obj).hide();
	    }else{
				$(left_obj).removeClass('index_scroll_left_a_hover');
				$(right_obj).addClass('index_scroll_right_a_hover');
				$(right_obj).css('background','url(../v3/images/icon_right.png)');
				$(right_obj).css('cursor','pointer');
	    }
		   
	    $(right_obj).click(function(){//下一页
			  var Mwidth = len-(index+1)*i;
			
			  if(Mwidth>i) {
				 $(left_obj).css('background','url(../v3/images/icon_left.png)');
				 $(left_obj).css('cursor','pointer');
			 // $(right_obj).removeClass('index_scroll_right_a_hover');
				$(chang_obj).stop(true, false).animate({left: -(index+1)*Swidth*i+"px"},600);
				index++;
				} else {
				//$(left_obj).removeClass('index_scroll_left_a_hover');
				$(right_obj).removeClass('index_scroll_right_a_hover');
				$(right_obj).css('background','url(../v3/images/btn_right.png)');
				$(right_obj).css('cursor','default');
				Awidth = Mwidth;
				$(chang_obj).stop(true, false).animate({left:-index*Swidth*i-Swidth*Mwidth+"px"},600);
				$(left_obj).css('background','url(../v3/images/icon_left.png)');
				$(left_obj).css('cursor','pointer');
				}
	   
	    });
	    $(left_obj).click(function() { //上一页
			if(index==0) {
			    var Mwidth = len-i;
			   

				$(right_obj).addClass('index_scroll_right_a_hover');
				$(right_obj).css('background','url(../v3/images/icon_right.png)');
				$(right_obj).css('cursor','pointer');
			    $(left_obj).removeClass('index_scroll_left_a_hover');
				$(left_obj).css('background','url(../v3/images/btn_left.png)');
				$(left_obj).css('cursor','default');
				$(chang_obj).stop(true, false).animate({left: "0px"},600);
			}else {
				$(left_obj).addClass('index_scroll_left_a_hover');
				 $(left_obj).css('background','url(../v3/images/icon_left.png)');
				//$(right_obj).removeClass('index_scroll_right_a_hover');
				$(chang_obj).stop(true, false).animate({left: -(index-1)*Swidth-Swidth*Awidth+"px"},600);
				   index--;
			}

		 }); 
			  
		  
		  
		  
	}
		
		  

});
</script>
  

