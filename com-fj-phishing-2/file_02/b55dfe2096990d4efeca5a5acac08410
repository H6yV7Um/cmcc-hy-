	<script>
    function _sendGoldLog(goldId){
				//alert("埋点："+goldId);
                //增加黄金令箭埋点
                var img = new Image();
                var id = "_beacon_" + Math.random();
                var ksVersion='';
    			var KISSY = '';
                if(KISSY){
                    ksVersion = KISSY.version||'';
                }
                var uri =encodeURIComponent(window.location.href);
                window[id] = img; // 防止img变量被浏览器过早回收
                img.src = "//log.mmstat.com/" +goldId + "?type=9&_gm:id=mai-kissy-version&_r_=" + Math.random()+"&ks-version="+ksVersion+"&uri="+uri;
                img.onload = function () {
                    window[id] = null;
                }
            }
    </script>   
