function showMe(){
	if(""==""){
		alert("请先登录！");
	}else{
		var ON_OFF_box = $(".Msg_con_l .ON_OFF_box li span.pic_btn");
		for(var i=0;i<ON_OFF_box.length;i++){
			if(i==0){
				ON_OFF_box.eq(i).addClass("ON_btn").removeClass("OFF_btn");
				ON_OFF_box.eq(i).attr("checked","checked");
				$(".Msg_con_r .phone_text").eq(i).show();
			}else{
				ON_OFF_box.eq(i).addClass("OFF_btn").removeClass("ON_btn");
				$(".Msg_con_r .phone_text").eq(i).hide();
			}
		}

		$("input[name='recvMobile']").css("color","#ABABAB");
		$("#prompt_Msg").css("display","none");
		
		$.ajax({
		    type:"post",
			url: "/recommend/getWebMassageInfo.do",
			dataType:"json",
	  		data:{"type":7,"random":new Date().getTime()},
			success:function(data){
				
				for(var i=0;i<data.length;i++){
					$("#msg_"+i).html(data[i].urlDesc);
					$("#smsMsg_"+i).html(data[i].urlInfo);
					$("#smsHref_"+i).html(data[i].url);
					if(i==0){
						$("#smsHref").val(data[i].url);
					}
				}
				var recvMobileInput = $("input[name='recvMobile']");
				for(var i=0;i<recvMobileInput.length;i++){
					recvMobileInput.eq(i).val("请输入11位浙江移动手机号");
				}
				
				var CH = document.documentElement.clientHeight;
				var DEH = $(document).scrollTop();
				DEH = DEH + parseInt(CH/2);
				$("#Popup").css({"top":DEH+"px"});
				$("#shade,#Popup").show();
				
				$("#Popup .Msg_con_r").height( $("#Popup .Msg_con_l").height() );	
				$("#Popup").height( $("#Popup .Msg_con_l").height()+37 );
			},
			error:function(){
				alert("加载失败，请稍后重试。谢谢！");
			}
		}); 

	}
}

$(function(){
/* 弹出层 */
	var DEH = Math.max(document.documentElement.offsetHeight,document.body.offsetHeight);
	$("#shade").css({"height":DEH+"px","width":"100%","background":"white","opacity":"0.5","position":"absolute","left":"0","top":"0","z-index":"999"});
	$("#Popup .close_box").click(function(){
		    $("#shade,#Popup").hide();
		});	

	$(".Msg_con_l .ON_OFF_box li span.pic_btn").click(function(){
			$(this).addClass("ON_btn").removeClass("OFF_btn").parent().siblings("li").find("span.pic_btn").addClass("OFF_btn").removeClass("ON_btn");
			$(this).siblings("input:radio").attr("checked","checked");
			$(".Msg_con_r .phone_text").eq( $(this).parent("li").index() ).show().siblings(".phone_text").hide();

		//	$("#smsMsg").val($("#smsMsg_"+$(this).parent("li").index()).html());
			$("#smsHref").val($("#smsHref_"+$(this).parent("li").index()).html());
		});
		//showMe();
	  /*顶部分享开始*/
	  	$("#new_tjsyb").hover(function(){
		$(this).removeClass("new_tjsyb").addClass("new_tjsyb2");
		$(".new_tjfxbox").show();
		},function(){
		$(this).removeClass("new_tjsyb2").addClass("new_tjsyb");
		$(".new_tjfxbox").hide();	
		})
		$(".new_tjfxbox").hover(function(){
			$(this).show();
			$("#new_tjsyb").removeClass("new_tjsyb").addClass("new_tjsyb2");
			},function(){
			$(this).hide();
			$("#new_tjsyb").removeClass("new_tjsyb2").addClass("new_tjsyb");	
				});
	  /*顶部分享结束*/	
});

function sendTo(){

    var mbphno=/^(13[4-9])|(182)|(183)|(147)|(15[0-2])|(15[7-9])|(18[7-8])/;
    var num11=/^\d{11}$/;
	var recvMobileInput = $("input[name='recvMobile']");
	var recvMobile="";
	for(var i=0;i<recvMobileInput.length;i++){
		if(recvMobileInput.eq(i).val() != "" && recvMobileInput.eq(i).val() !="请输入11位浙江移动手机号"){
			if(!mbphno.test(recvMobileInput.eq(i).val()) || !num11.test(recvMobileInput.eq(i).val())){
				alert("请输入正确的浙江移动手机号码，谢谢！");
				return;
			}
			for(var j=i+1;j<recvMobileInput.length;j++){
				if(recvMobileInput.eq(i).val()==recvMobileInput.eq(j).val()){
					alert("提交号码有重复，请您核对后再次提交，谢谢！");
					return;
				}
			}
			if(recvMobile != ""){
				recvMobile+=";";
			}
			recvMobile+=recvMobileInput.eq(i).val();
		}
	}
	if(recvMobile.indexOf("")!=-1){
		alert("不能发送给自己，请确定发送号码，谢谢！");
		return;
	}
	var path = $("#smsHref").val();
	$.ajax({
	    type:"post",
	    url: "/recommend/messageToShare.do",
  		data:{"path":path,"type":7,"recvMobile":recvMobile,"random":new Date().getTime()},
		success:function(data){
  			if(data=="0"){
				alert("发送成功，谢谢！");
				$("#shade,#Popup").hide();
			}else if(data=="1"){
				alert("请先登录，谢谢！");
				$("#shade,#Popup").hide();
			}else if(data=="2"){
				alert("对不起，您今天已使用过短信分享，谢谢！");
				$("#shade,#Popup").hide();
			}else if(data=="3"){
				alert("不能将分享短信发送给自己，请重新确定分享手机号，谢谢！");
			}else if(data=="4"){
				alert("请输入正确的浙江移动手机号码，谢谢！");
			}else{
				alert("发送异常请重试！");
				$("#shade,#Popup").hide();
			}
		},
		error:function(){
			alert("加载失败，请稍后重试。谢谢！");
		}
	}); 
}

function prompt_Msg(){
	var recvMobileInput = $("input[name='recvMobile']");
	var bool = false;
	for(var i=0;i<recvMobileInput.length;i++){
		if(recvMobileInput.eq(i).val() != "" && recvMobileInput.eq(i).val() !="请输入11位浙江移动手机号"){
			for(var j=i+1;j<recvMobileInput.length;j++){
				if(recvMobileInput.eq(i).val()==recvMobileInput.eq(j).val()){
					bool = true;
				}
			}
		}
	}
	if(bool){
		$("#prompt_Msg").css("display","block");
	}else{
		$("#prompt_Msg").css("display","none");
	}
}

function changeColor(thisObj){
	if("请输入11位浙江移动手机号"==thisObj.value || ""==thisObj.value){
		thisObj.style.color="#ABABAB";
	}else{
		thisObj.style.color="#000000";
	}
}

</script>
