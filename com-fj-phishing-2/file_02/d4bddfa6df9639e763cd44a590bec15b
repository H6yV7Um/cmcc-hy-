		<script type="text/javascript">
	$(function() {
		// 上网伴侣发表意见
		var netBanlv = function() {
			//document.domain='10086.cn'; //网上营业厅 上线时需要设置此项解决跨域问题
			//$("body").append($("#f_banLv")); //使用CMS发布时需要移动层位置
			var f_banLv = $("#f_banLv"), banLv_btn = $("#banLv_btn"), banLv_infor = $("#banLv_infor"), N_banLv_close = $("#N_banLv_close"), f_banLv_close = $("#f_banLv_close"), f_banLv_fold = $("#f_banLv_fold"), N_tijiao = $("#N_tijiao"), tid, delayTime = 300;
			// 发表意见按钮鼠标悬停
			f_banLv.hover(function() {
				clearTimeout(tid);
				banLv_infor.slideDown(400);
			}, function() {
				tid = setTimeout(function() {
					banLv_infor.slideUp(150)
				}, delayTime);
			});
			banLv_btn.click(function() {
				$("#jianyiCon").val("");
				$("#banLv_pop").floatDiv( {
					width : 469,
					height : 423,
					close : '#N_banLv_close',
					drag : 'h4',
					ifScroll : false
				});
				if (!$("#newIframe").attr("src")) {
					document.domain = '10086.cn';
					var newIframeSrc = $("#newIframe").attr("datasrc");
					$("#newIframe").attr("src", newIframeSrc);
				}
			});
			// 关闭按钮鼠标悬停
			N_banLv_close.add('#f_banLv_close').hover(
					function() {
						$(this).addClass(
								this.id == 'N_banLv_close' ? "N_banLv_close_h"
										: "f_banLv_close_h");
					},
					function() {
						$(this).removeClass(
								this.id == 'N_banLv_close' ? "N_banLv_close_h"
										: "f_banLv_close_h");
					});
			// 点击折叠
			f_banLv_close.click(function() {
				f_banLv.hide();
				f_banLv_fold.floatDiv( {
					width : 25,
					height : 120,
					cover : false,
					level : "right",
					esc : false,
					blank : false,
					drag : 'aaa'
				});
			});

			f_banLv_fold.mouseover(function() {
				f_banLv_fold.hide();
				f_banLv.show();
			});

			f_banLv.floatDiv( {
				width : 123,
				height : 202,
				cover : false,
				level : "right",
				esc : false,
				blank : false,
				drag : 'aaa'
			});
		};

		if ($("#f_banLv").length) {
			netBanlv();
		}
		;

	});
	// 提供给框架内调用父层 关闭方法
	function closeTheDiv() {
		$("#banLv_pop").floatDiv( {
			onOff : false
		});
	};
</script>

