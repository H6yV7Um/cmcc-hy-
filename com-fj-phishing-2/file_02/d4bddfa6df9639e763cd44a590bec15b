<script type="text/javascript">
if( $.browser.msie && $.browser.version == '6.0' && $.browser.version == '7.0' && $.browser.version == '8.0'){
}else{
	$(function(){
		if($("#cookieDto").val() == 1){
		var cookieBusiness = GetBusinessCookie ("cookieBusiness");
		var url = window.location.href;
		if(cookieBusiness == null){
			var value = encodeURI("")+"^"+url+"|";
			setBusinessCookie("cookieBusiness",value);
		}else{
			var businesses = new Array();
			businesses = cookieBusiness.split("|");
			var flag = 0;
			if(businesses.length == 0){
				var value = encodeURI("")+"^"+url+"|";
				setBusinessCookie("cookieBusiness",value);
			}else{
				for(var i = 0; i < businesses.length; i++){
					var business = businesses[i].split("^");
					if(encodeURI("") == business[0]){
						flag = 1;
					}
				}
			}
			if(flag == 0 && businesses.length<7){
				var value = cookieBusiness+encodeURI("")+"^"+url+"|";
				setBusinessCookie("cookieBusiness",value);
			}else if(flag == 0 && businesses.length>=7){
				//var cookieBusiness = GetBusinessCookie ("cookieBusiness");
				//var businesses = new Array();
				//businesses = cookieBusiness.split("|");
				var start = cookieBusiness.indexOf("|");
				var end = cookieBusiness.length;
				var value = cookieBusiness.substring(start+1,end)+encodeURI("")+"^"+url+"|";
				setBusinessCookie("cookieBusiness",value);
			}
		}}
			
			function setBusinessCookie(name,value){
				expires = new Date();
				expires.setTime(expires.getTime() + (1000 * 86400 * 365));
				document.cookie = name + "=" + escape(value) + "; expires=" + expires.toGMTString() + "; path=/ ; domain=10086.cn";
			}
			
			function GetBusinessCookie (name ){
				cookie_name = name + "=";
				cookie_length = document.cookie.length;
				cookie_begin = 0;
				while (cookie_begin < cookie_length){
					value_begin = cookie_begin + cookie_name.length;
					if (document.cookie.substring(cookie_begin, value_begin) == cookie_name){
						var value_end = document.cookie.indexOf ( ";", value_begin);
						if (value_end == -1){
							value_end = cookie_length;
						}
						return unescape(document.cookie.substring(value_begin, value_end));
					}
					cookie_begin = document.cookie.indexOf ( " ", cookie_begin) + 1;
					if (cookie_begin == 0){
						break;
					}
				}
				return null;
			}
	});
}
</script>
 
