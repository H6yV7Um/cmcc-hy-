		function goPayPage(obj){
			//充值业务立即充值触发插码
			if(window.WebTrends){
				WebTrends.multiTrack('WT.si_n','ptcz','WT.si_x','czks');
			}
			if(document.getElementById("j-telnum").value == "请输入充值手机号码"){
				document.getElementById("j-telnum").value = "";
			}
			document.getElementById("zxczForm").submit();
		};
		function ckje(je){
			document.getElementById("total").value=je;
		}	
		
		
		function goPayPage1(obj){
		
		if(window.WebTrends){
		
				WebTrends.multiTrack('WT.si_n','ptcz','WT.si_x','czks');
			}
			var tel =document.getElementById("j-telnum").value;
			
			if(document.getElementById("j-telnum").value == "请输入充值手机号码"){
			
				document.getElementById("j-telnum").value = "";
				
			}
			var amount=document.getElementById("total").value;
			
			window.location.href="http://shop.10086.cn/i/?f=rechargecredit&mobileNo="+tel+"&amount="+amount;
			
		
		}	
		
</script>
<script type="text/javascript">
$(function(){
$(".zj-yh-pic").hide();
$("#loading,#zj-zhezhao").show();
$.ajax({
		url:"http://service.zj.10086.cn/index.do",
		dataType:"json",
		type:"post",
		success:function(data){
		if(data.isLogin==1){
		$(".zj-yh-pic").show();
		$("#loading,#zj-zhezhao").hide();
		}else{
		$(".zj-yh-xx").html("<p style='font-size:15px;'>欢迎您:"+data.userName+"</p> <p>话费余额:"+data.NowuseAmount+"<span style='color:#fd0172;'>元</span></p>");
		$("#loading,#zj-zhezhao").hide();
		$(".zj-ldyh-pic").show();
		}
		},
		error:function() {
			alert("系统繁忙，请稍后再试");
		}
	});

});
		function ckje(je){
			document.getElementById("total").value=je;
		}	
</script>


	
