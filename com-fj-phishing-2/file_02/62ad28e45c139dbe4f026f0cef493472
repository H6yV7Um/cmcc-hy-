<script type="text/javascript">
$(function (){  
/*---------------------------------------------*/   
   //搜索文本框获得焦点事件处理
	$("#search_text").focus(function(){
	  $(this).val("").css({"color":"#000"}); 
	  });
   
  //标签选项切换
  if($('.tabs span')){
     $('.tabs span').click(function(){
	          $(this).parent().find('span').removeClass("default"); 
	          $(this).parent().parent().parent().find('.tab').removeClass("default"); 
	          $(this).addClass("default"); 						  
	          var ss = $(this).attr("rel");
	          $(this).parent().parent().parent().find(".tab."+ ss).addClass("default"); 
		  });
  };
  //
   $('h4#info span').click(function(){
	          $(this).parent().find('span').removeClass("default"); 
	          $(this).parent().parent().parent().find('.layer').removeClass("default"); 
	          $(this).addClass("default"); 						  
	          var ss = $(this).attr("rel");
			  $('.jhnews_content .more').hide();
			  $('#' + ss + "_a").show();
	          $(this).parent().parent().parent().find(".layer."+ ss).addClass("default"); 
		  });

	//锚点
	var _anchor = "null";	
	if(_anchor=="null" ){
		var href = window.location.href;
		var index = href.indexOf("anchor=");
		if(index>-1){
			_anchor = href.substring(index+7);
		}
	}
	if(_anchor != null && _anchor != "null"){
		$("h4#info span").each(function(){
			if($(this).attr("rel") == _anchor){
				$(this).click();
				this.scrollIntoView();
				return;
			}
		});
	}
		
	var scrollId = null;
	$(window).scroll(function(){
		clearTimeout(scrollId);
		var _top = 12 + Math.max(document.documentElement.scrollTop,document.body.scrollTop);	
		scrollId = setTimeout(function(){
			$('#flowAdv').animate({top:_top},500);
		},100);
	});
	
/*---------------------------------------------------------------------*/
});
</script>

<!-- 漂浮广告  -->
<style type="text/css">
img{border:0;}
