<script type="text/javascript">
	
	
		var newMaskWidth = document.body.scrollWidth;//遮罩层宽度
		var newMaskHeight = document.body.scrollHeight;//遮罩层高度

		//显示提示层方法
		function showDiv() {
			var target = document.getElementById("picDiv");
			target.style.display = "block";
		}
		
		
		
		openDiv('newDiv');
		
		function openDiv(newDivID) {
			var newMaskID = "mask"; //遮罩层id
			//mask遮罩层  
			var newMask = document.createElement("div");//创建遮罩层
			newMask.id = newMaskID;//设置遮罩层id
			newMask.style.position = "absolute";//遮罩层位置
			newMask.style.zIndex = "900";//遮罩层zIndex
			newMask.style.width = newMaskWidth + "px";//设置遮罩层宽度
			newMask.style.height = newMaskHeight + "px";//设置遮罩层高度
			newMask.style.top = "0px";//设置遮罩层于上边距离
			newMask.style.left = "0px";//设置遮罩层左边距离
			newMask.style.filter = "alpha(opacity=40)";//遮罩层透明度IE
			newMask.style.opacity = "0.50";//遮罩层透明度FF
			newMask.style.background = 'black';
			document.body.appendChild(newMask);//遮罩层添加到DOM中

		}
		$(function() {
			var height = $(".HomeTitle").height();
			var width = $(".HomeTitle").width();
			var top = (document.body.clientHeight - height) / 2;
			var left = ( document.body.clientWidth- width) / 2;
			if(navigator.appVersion.match(/MSIE 7.0/)){
				$(".close").css('top','180px');
				top=0;
			}
			$(".HomeTitle").css({
				"top" : top,
				"left" : left
			});
			
			
			$(".close,.nextSee").click(function() {
				$(".HomeTitle").hide();
				$("#newDiv").hide();
				$("#mask").hide();
			});
			showDiv();
		});


		
	</script><div id="mask" style="position: absolute; z-index: 900; width: 1124px; height: 1615px; top: 0px; left: 0px; opacity: 0.5; background-color: black; background-position: initial initial; background-repeat: initial initial;"></div>



</body></html>