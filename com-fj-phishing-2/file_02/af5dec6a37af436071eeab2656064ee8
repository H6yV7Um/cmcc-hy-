<script src="/javascripts/lib/jquery.js?276c87"></script>
<script>
    function isMobile() {
        return document.documentElement.offsetWidth < 880;
    }
    var userScrolled = false;
    function resize(ele, cb) {

        var html = document.documentElement;
        var screenWidth = html.offsetWidth;
        var screenHeight = html.offsetHeight * 0.75;
        var videoRatio = 1280 / 720;

        var $video = $(ele);

        function videoEnd() {
            $(ele).hide();
        }

        function videoStart() {
            $(ele).show();
        }

        if (!userScrolled) {

            if (screenWidth / screenHeight > videoRatio) {
                $video.css('width', '100%');
                $video.css('height', 'auto');
            } else {
                $video.css('height', '100%');
                $video.css('width', 'auto');
            }
        }
        cb && cb($video, screenWidth, screenHeight);
    }
    function resizeAll() {
        resize('#video', function ($video, screenWidth, screenHeight) {
            var $videoCtrl = $video[0];
            if ((screenWidth / screenHeight < 1) || screenWidth < 880) {
                $videoCtrl.pause();
                $video.hide();
            } else {
                if ($videoCtrl.paused) {
                    $video.show();
                    $videoCtrl.play()
                }
            }
        });
    }

    resizeAll();

    if (((navigator.userAgent.toUpperCase().search('MSIE') > -1 || navigator.userAgent.toUpperCase().search('SE ') > -1) && !isMobile()) || isMobile()) {
        //sogou
        $('#video').hide();
        $('#round-pics').show();
        var picIndex = 1;
        var classes = ['zoomOutFadeOut', 'zoomInFadeOut', 'zoomInFadeOut', 'zoomOutFadeOut', 'zoomOutFadeOut'];
        $('.pic' + (picIndex)).addClass(classes[picIndex - 1]);
        setInterval(function () {
            $('.pic' + (picIndex)).removeClass(classes[picIndex - 1])
            picIndex = (picIndex % 5) + 1;
            console.log(picIndex);
            $('.pic' + (picIndex)).addClass(classes[picIndex - 1])
        }, 3500);
    }

    if(/Mac OS/.test(navigator.userAgent) && isMobile()){
        $('#general-feature').hide();
        $('#ios-feature').show();
        $('#mobile-download').attr("href","https://itunes.apple.com/app/apple-store/id439638720?pt=69276&ct=website&mt=8");
    }else{
        $('#ios-feature').hide();
    }
</script>
