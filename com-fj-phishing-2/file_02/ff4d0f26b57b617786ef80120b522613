      var currentNav;
      var moveTime = 300;

      E.domReady(function() {
        //初始化下划线的位置
        A($$(".nav-link ul li")).each(function(item, index) {      
            //获取当前current的li标签
            if(Node(item).hasClass("current")) {
              currentNav = item;
              $("navLine").setStyle("left", Node(item).getPosition()["left"] + "px");
              Node($("navLine")).removeClass("fn-hide");
            };
        })

        Loader.use('arale.fx', function() {
          var anim1 = new Animator({
            duration : moveTime,
            interval : 20,
            onComplete : function() {
              isPlay = false;
            }
          });

          //mouseover
          E.delegate($('link'), "mouseover", function(target, e) {
            var rt = e.relatedTarget;
            if(!rt || $E._isInDomChain(rt, target, $('link')))
              return;

            if(Node(target).attr("id") == "link-about" || Node(target).attr("id") == "link-partner") {
              //setTimeout(function() {
                Node($(target).query(".link-dropdown")[0]).removeClass("fn-hide");
                //}, 25);
            }

            Node(target).addClass("hover");

            if(!(Node(target).hasClass("current"))) {
              anim1.addCSSMotion($("navLine"), "left:" + Node(target).getPosition()["left"] + "px");
              anim1.play();
            }
          }, "li");
          //mouseout
          E.delegate($('link'), "mouseout", function(target, e) {
            var rt = e.relatedTarget;
            if(!rt || $E._isInDomChain(rt, target, $('link')))
              return;

            if(Node(target).attr("id") == "link-about" || Node(target).attr("id") == "link-partner") {
              Node($(target).query(".link-dropdown")[0]).addClass("fn-hide");
            }

            Node(target).removeClass("hover");

            if(!(Node(target).hasClass("current"))) {
              anim1.addCSSMotion($("navLine"), "left:" + Node(currentNav).getPosition()["left"] + "px");
              anim1.play();
            }
          }, "li");
        });
      })
