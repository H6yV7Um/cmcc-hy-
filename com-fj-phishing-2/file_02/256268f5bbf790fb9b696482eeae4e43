<script type="text/javascript">
var hasSync = false;
var animate = {
	nodeid : "#animate"
	, timer : null
	, show : function () {
		if(hasSync) return;
		hasSync = true;
		animate.changeMaxImage();
		$(animate.nodeid).animate({"height": "202px"}, 600);
		/*
		$(animate.nodeid).slideDown(1000, function() {
			$(this).height(200);
		});
		*/
		animate.startTimer();
	}
	, hide : function () {
		$(animate.nodeid).animate({"height": "40px"}, 600);
		/*
		$(animate.nodeid).slideUp(1000, function() {
			$(this).height(40);
			$(this).show();
		});
		*/
		animate.changeMiniImage();
		animate.stopTimer();
		hasSync = false;
		
	}
	, startTimer : function () {
		this.timer = setInterval(this.hide, 3000);
	}
	, stopTimer : function () {
		clearInterval(this.timer);
	}
	,changeMiniImage : function () {
		$("#_topimage").attr("src", "images/jcdckmini.jpg");
	}
	,changeMaxImage : function () {
		$("#_topimage").attr("src", "images/jcdck2.0.jpg");
	}
}
$(document).ready(function () {
	animate.show();
});
$('#animate').mouseout(function() {
	//animate.startTimer();
	animate.timer = setInterval(animate.hide, 1000);
});
$('#animate').mouseover(function() {
	animate.stopTimer();
	animate.show();
	animate.stopTimer();
});
</script>
