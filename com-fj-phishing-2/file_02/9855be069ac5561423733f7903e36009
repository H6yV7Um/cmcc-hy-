    <script type="text/javascript">
    	// 登录点击事件
       	function  loginClick() {
       		// 获取当前页面url
       	  	var state=document.location.href;
       	 	location.href="/ucenter/user!login.action?state="+state;
		}
       	//获取购物车数量
       	function countCart(){
       		$.ajax({
	   		type:"get",
	   		url:CONTEXT_PATH +"/order/shoppingCart!countCart.action",
	   		async:true,
	   		cache:false,
	   		success:function(res){
                if(!res.successSign){
	   				alert("服务器错误！");
	   				return;
	   			}
                $(".shopCar").html(res.result);
	   		},
	   		error:function(res){
	   			console.log(res.message)
	   		}
	    });
       	}
	   	countCart();
      </script>
    
