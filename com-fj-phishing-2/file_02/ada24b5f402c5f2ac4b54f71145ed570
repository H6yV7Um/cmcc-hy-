<script type="text/javascript">
            function logout() {
            var msg = "您真的确定要退出吗？";
            if (confirm(msg)==true){
                $.ajax({ 
                    url: "/index.php/Home/Login/logout.html",
                    type:'get',
                    success: function(data){
                        if(data == 1){
                            window.location.href="/index.php/Home/Index/index.html";
                            location.reload();
                        }
                    }
                });
            }else{
                return false;
                }
            }
        </script>
