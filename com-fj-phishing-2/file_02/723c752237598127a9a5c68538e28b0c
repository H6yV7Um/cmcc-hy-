<script type="text/javascript">
        $(function(){
            $("#flhmbk>li").mouseover(function(){
                var _index = $(this).index();
                $("#pfxbk").stop().animate({"left":(_index*90)+"px"},250);
                $("#ipaovhid>.hmactke:eq("+_index+")").addClass("display_block").siblings(".hmactke").removeClass("display_block");
            });
            $(".hptitle .hpnav").mouseover(function(){
                var _index = $(this).index();
                /*$("#jintou").stop().animate({"left":(_index*82.5)+"px"},250);*/
                $(this).addClass('active').siblings('.hpnav').removeClass('active');
                $("#hplist .hpmain:eq("+_index+")").addClass("display_block").siblings(".hpmain").removeClass("display_block");
            });
            $(".ipright .listbox span").click(function(){
                var _index = $(this).index(),
                        obj = $(this).closest('.ipright');
                $("#jintou").stop().animate({"left":(_index*82.5)+"px"},250);
                obj.find(".gbrtitle .listbox span").removeClass("active");
                $(this).addClass("active");

                obj.find(".gbrbox:eq("+_index+")").addClass("display_block").siblings(".gbrbox").removeClass("display_block");
            });

            /*专刊*/
            var ml = 0,
                    lWidth = 255,/*列表里的每一个内容的宽度*/
                    num = 4,/*列表每一排显示多少个内容*/
                    mObj = $('#vrBox'),
                    lLength = mObj.find('a').length;
            if(lLength <= num){
                $('.vrDown,.vrNext').hide();
            }else{
                $('.vrDown').hide();
            }
            $('.vrNext').click(function(){
                var sNum = lLength - (ml + num);
                if(sNum >= num){
                    ml += num;
                }else if(sNum > 0 && sNum < num){
                    ml += sNum;
                }
                if(ml + num >= lLength){
                    $(this).fadeOut();
                    $('.vrDown').fadeIn();
                }else if(ml > 0){
                    $('.vrDown').fadeIn();
                }
                mObj.animate({'left':(ml * lWidth) * -1 + 'px'});
            });
            $('.vrDown').click(function(){
                if(ml >= num){
                    ml -= num;
                }else if(ml > 0 && ml < num){
                    ml -= ml;
                }
                if(ml <= 0){
                    $(this).fadeOut();
                    $('.vrNext').fadeIn();
                }else if(ml + num < lLength){
                    $('.vrNext').fadeIn();
                }
                mObj.animate({'left':(ml * lWidth) * -1 + 'px'});
            });

            /*孕育指南*/
            $('.selectDate .text').html(dateFormat(new Date(),'type1'));
            $('.guideDate').attr('value',new Date());
            $('.selectDate').change(function(){
                $(this).find('.text').html($(this).val());
                $(this).next().attr('value',$(this).attr('data-value'));
            });
        })
    </script>
