        $(".tempen>a").hover(function(){
            var idex = $(this).index();
            var iprent = $(this).parents('.zsyyrt').find('.pageSosmp');
            $(this).addClass("on").siblings().removeClass("on");
            iprent.find('.itemcell').eq(idex).removeClass("hide").siblings().addClass("hide");
        })
        //孕育百科 切换效果
        $(".hovitem>a").hover(function(){
            var idex = $(this).index();
            $(this).addClass("on").siblings().removeClass("on");
            liscgo = $(this).parent().find('.liscgo');
            liscgo.stop().animate({
                'left':(idex*60)+'px'
            },300);
            var iprent = $(this).parent().next('.bakswth');
            iprent.find('ul.baklist').eq(idex).removeClass('hide').siblings().addClass('hide');
        })
        //豆妈工坊
        $(".prSwith>a").hover(function(){
            var idex = $(this).index();
            $(this).addClass("on").siblings().removeClass("on");
            var iprent = $(".prswall");
            iprent.find('.calitem').eq(idex).removeClass('hide').siblings().addClass('hide');

