<script type="text/javascript">
    $(function(){
        var clickWPA = function(buttonId, wpaIframeId) {
            $(buttonId).on('click', function(){
                var $iframe = $(wpaIframeId),
                    $iframeWin = $iframe[0].contentWindow,
                    $doc = $($iframeWin.document);

                $doc.find('[data-event]')[0].click();
            });
        };

        clickWPA('#wpa_qq', '#qidian_wpa_2852062940_35');
        clickWPA('#wpa_adn', '#qidian_wpa_2852062940_36');
        clickWPA('#wpa_wx', '#qidian_wpa_2852062940_37');
    });
</script>

