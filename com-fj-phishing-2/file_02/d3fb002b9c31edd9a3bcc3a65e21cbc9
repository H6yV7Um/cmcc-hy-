        $(document).ready(function(){
        	jQuery.post("/mall/asyreq/asyHotKeyList.jhtml",{},function(msg,data){
        		if(msg.result.length!=0){
        			$(msg.result).each(function(i,value){
        				if(value.hotKey!=null){
        				if(value.linkAddress!=null){
        					var a = $("<a target=\"_blank\" href=\""+value.linkAddress+"\">"+value.hotKey+"</a>");
        					}else { 
        					 var a = $("<a href=\"javascript:hotkeyLabel('"+value.hotKey+"')\">"+value.hotKey+"</a>");
        					}
        					jQuery("#hotKey span").append(a);
        				}
        			});
        		}
        	},"json");
        });
