    <script>

        //计算器
        function loanCalculateSubmit() {
            $("#loanCalculate").off();
            var postdata = {
                Amount: $("#loanAmountForCalculate").val(),
                Rate: $("#loanRateForCalculate").val(),
                Term: $("#loanTermForCalculate").val(),
                TermType: $('input:radio[name="loanTermTypeForCalculate"]:checked').val(),
                
            };
            $.ajax("/CalculatInterest",
            {
                type: "post",
                data: postdata,
                dataType: "json",
                cache: false,
                
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    if (textStatus == "timeout" || textStatus == "error") {
                        $.jBox.tip("请求超时，请检查网络！");
                    } else {
                        $.jBox.tip("系统错误，请刷新重试！");
                    }
                    $("#loanCalculate").bind("click", loanCalculateSubmit);
                },
                success: function (data) {
                    if (data.success == false) {
                        $.jBox.tip(data.data);
                    } else {
                        var re = data.data.split('|');
                        $("#calculateResultSpan1").html(re[0]);
                        $("#calculateResultSpan2").html(re[1]);
                        $("#calculateResultSpan3").html(re[2]);
                        $("#liEachMonthEarning").show();
                        $(".ca-count-result").show();
                    }
                    $("#loanCalculate").bind("click", loanCalculateSubmit);
                }
            });
        }

        function claimsLoanCalculateSubmit() {
            $("#claimsLoanCalculate").off();
            var postdata = {
                Amount: $("#claimsLoanAmountForCalculate").val(),
                Rate: $("#claimsLoanRateForCalculate").val(),
                Term: $("#claimsLoanTermForCalculate").val(),
                
            };
            $.ajax("/CalculatInterest",
            {
                type: "post",
                data: postdata,
                dataType: "json",
                cache: false,
                
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    if (textStatus == "timeout" || textStatus == "error") {
                        $.jBox.tip("请求超时，请检查网络！");
                    } else {
                        $.jBox.tip("系统错误，请刷新重试！");
                    }
                    $("#claimsLoanCalculate").bind("click", claimsLoanCalculateSubmit);
                },
                success: function (data) {
                    if (data.success == false) {
                        $.jBox.tip(data.data);
                    } else {
                        var re = data.data.split('|');
                        $("#calculateResultSpan1").html(re[0]);
                        $("#calculateResultSpan2").html(re[1]);
                        $("#liEachMonthEarning").hide();
                        $(".ca-count-result").show();
                    }
                    $("#claimsLoanCalculate").bind("click", claimsLoanCalculateSubmit);
                }
            });
        }
    </script>
    <script>
        function showdiv() {
            document.getElementById("bg-1").style.display = "block";
            document.getElementById("show-1").style.display = "block";
        }
        function hidediv() {
            document.getElementById("bg-1").style.display = 'none';
            document.getElementById("show-1").style.display = 'none';
        }

    </script>
    <script>
        $(function () {
            //鼠标放微信icon图标
            $(".top-weixin-icon a,.t-wechat-bg").mouseover(function () {
                $(".t-wechat-bg").css("display", "block");
            });
            $(".top-weixin-icon a,.t-wechat-bg").mouseout(function () {
                $(".t-wechat-bg").css("display", "none");
            });
            $(".footer-weixin").mouseover(function () {
                $(".b-wechat-bg").css("display", "block");
            });
            $(".footer-weixin").mouseout(function () {
                $(".b-wechat-bg").css("display", "none");
            });
        });
    </script>

