<script src="/Content/Scripts/home/home.js"></script>
<script type="text/javascript">
    $(function () {
        var expanded = true;
        var cookie_val = getCookie("risk");
        if (cookie_val != undefined) {
            document.cookie = "risk=true";
            $('.main1').css('left', '-282px');
            $(".strndgea_barset").show();
        }
        else {
            $('.main1').css('left', '0px');
            //   $(".strndgea_barset").show();
             expanded = false;
        }



        $('.barset').click(function () {
            if (expanded) {
                $('.main1').animate({ left: '0' }, 500);
                $('.v').css('background-position', '-25px 0px');
                $(".strndgea_barset").hide();
            } else {
                $('.main1').animate({ left: '-282px' }, 100);
                $('.barset').css('background-position', '-0px 0px');
                $(".strndgea_barset").show();
                document.cookie = "risk=true";
            }
            expanded = !expanded;
        });
    });
</script>

