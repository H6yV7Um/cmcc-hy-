    <script>
        $(function () {
            var BarName = "首页";
            //页面选择效果
            $(".fa-menu-li a").each(function () {
                $(".fa-menu-li").remove("on-fa-menu-li");
                if ($(this).html() == BarName) {
                    //  alert($(this).html());
                    $(this).parent().addClass("on-fa-menu-li");
                    if (BarName == "我要投资") {
                        $(this).parent().addClass("fa-son-menu-li-fff");
                    }
                    return;
                }
            });
            //我要 投资单独加样式
            $(".fa-son-menu-li").mouseover(function () {
                $(this).css({ "border-bottom-left-radius": "0px", "border-bottom-right-radius": "0px", });
            });
            $(".fa-son-menu-li").mouseout(function () {
                $(this).css({ "border-bottom-left-radius": "5px", "border-bottom-right-radius": "5px", });
            });


            
        });

        function layers(con) {
            layer.confirm(con, {
                btn: ['确定']
            });


