					"<input name=\"input\" type=\"button\" class=\"btn_blue\" onclick=\"window.open(\'"+insProPageURL+"\',\'_blank\')\" value=\""+insProBuy+"\" style=\"position: absolute;bottom: 6px;\"></p></div></div></li>\n");
				}
			}
			$("#by_slids").Xslider({
			unitdisplayed:1,
			numtoMove:1,
			unitlen:960,
			autoscroll:null
			});
		}
	});	
