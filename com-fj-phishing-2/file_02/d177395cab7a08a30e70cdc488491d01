<script type="text/javascript">
$(function(){			
	$.ajax({
		type: "post",
		cache:false,
		url: "/tran/WCCMainPlatV5?CCB_IBSVersion=V5&isAjaxRequest=true&SERVLET_NAME=WCCMainPlatV5&TXCODE=NBX001",
		/*data: {"insCoID":insCoID,"cvrID":cvrID},*/
		dataType: "json",
		success: function (data) {
			var list=data;
			var valelist=list;
			//var output = data;
			//var valelist=output.list;
			var c=valelist.length;
			if(valelist.length>0){
				for(var i=valelist.length-1;i>=0;i--){
					var Cvr_PD_Cgy_ID =valelist[i].Cvr_PD_Cgy_ID;
					var Ins_Co_ID =valelist[i].Ins_Co_ID;
					var insProBuy =valelist[i].insProBuy;
					var insProPageURL =valelist[i].insProPageURL;
					var insProName =valelist[i].insProName;
