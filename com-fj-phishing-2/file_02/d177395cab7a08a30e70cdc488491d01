						insProPicURL="";
					}
					var Ins_PD_Trl_Cd=valelist[i].Ins_PD_Trl_Cd;
					var Intfc_Tpl_TpCd=valelist[i].Intfc_Tpl_TpCd;
					if(insProBuy=="1"){
						insProBuy="立即投保"
					}else{
						insProBuy="查看详情"
					}

