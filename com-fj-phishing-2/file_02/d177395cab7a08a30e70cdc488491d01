				var firIndex = firCount+1;
				gethtml+='<li><a class="myTab1_a nav0'+firIndex+'">'+firName+'</a><div class="nav_insur_sec_boxBig boxBig"><div class="nav_insur_sec_box"><h4 class="sec01_box01">险种分类</h4>';
	
				if(firChild){
					$.each(firChild, function(secCount,secValue){				
						var secName = secValue.Cvr_PD_Cgy_Nm;
						var secId = secValue.Cvr_PD_Cgy_ID;
						gethtml+='<a href="/cn/insurance/guide/chanpinsousuo.html?Cvr_PD_Cgy_ID='+secId+'" target="_blank">'+secName+'</a>';
					});
				}
				
				gethtml+='</div></div></li>';
			});
			$("#myTab1").append(gethtml);	
		}
	});
</script> 
