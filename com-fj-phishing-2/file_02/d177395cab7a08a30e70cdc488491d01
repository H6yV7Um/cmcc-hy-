<script type="text/javascript">
	function select_init(default_select){
		$(".bd .select_list .select_ul").append($("#account_query .select_list .select_ul ul"));
		$("#finance_today_order").remove();
		var $select_li=$(".bd .select_list .select_ul ul li :eq("+default_select+")");
		$(".bd .select_list .select_value").text($select_li.text());
		var query_url = $("#input_"+$select_li.attr("id")).val();
		$("#btn_query_now").attr("hrefUrl",query_url);
	}
	select_init(0);
	var code="";
	var str = '03';
	$(function(){
		$.ajax({
			type: "post",
			cache:false,
			url: "/cc_webtran/queryFinanceProdList.gsp?jsoncallback=?",
			data: {"queryForm.saleStatus":str,"pageSize":1},
			dataType: "jsonp",
			success: function(data){
				if(data != null && data != ""){
					data = data.ProdList;
					if(data.length > 0){
						code=data[0].code;
					}else{
						$("#account_query").css({display:"block",height:"0"});
					}						
				}else{
					$("#account_query").css({display:"block",height:"0"});
				}
			}
		});
	});
	$(".select_list .select_ul ul li").click(function(){
		var query_url = $("#input_"+$(this).attr("id")).val();
		$("#btn_query_now").attr('hrefUrl',query_url);
	});
	function btnQueryNow(obj){
		if($("#btn_query_now").attr("class") == "gray" || code == null){
			return;
		}else{
			var hrefUrl = $("#btn_query_now").attr('hrefUrl');
			if(hrefUrl != null && hrefUrl != ""){
				window.open(hrefUrl);
			}
		}
	}
</script>
				

					
