        	 var t = document.documentElement.scrollTop || document.body.scrollTop;
			 var th = document.getElementById("topbox").offsetHeight;
			 var no = 13;
			 if (t > no + th) {
			 	document.getElementById("sgbox").style = "position:fixed;top:0px;left:0;width:100%;z-index:10;margin:0;padding-top:20px;border:1px solid #ddd;background:#f3f3f3;opacity: .97;filter: alpha(opacity=97); ";
