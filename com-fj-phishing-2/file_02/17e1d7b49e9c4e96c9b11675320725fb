	<script type="text/javascript">
		/** 主banner **/
		TouchSlide({ 
			slideCell:"#focus",
			titCell:".hd ul", //开启自动分页 autoPage:true ，此时设置 titCell 为导航元素包裹层
			mainCell:".bd ul", 
			effect:"leftLoop", 
			autoPlay:true,//自动播放
			interTime:3500,
			autoPage:true, //自动分页
			switchLoad:"_src" //切换加载，真实图片路径为"_src" 
		});
		/** 商城banner **/
		TouchSlide({ 
			slideCell:"#pro_focus",
			titCell:".hd ul", //开启自动分页 autoPage:true ，此时设置 titCell 为导航元素包裹层
			mainCell:".bd ul", 
			effect:"left", 
			autoPlay:true,//自动播放
			interTime:3500,
			autoPage:true //自动分页
		});
		
		$(document).ready(function(){
			auto_size();
		});
		
		$(window).resize(function(){
			auto_size();
		});
		
		/*
		* banner图自适应
		*/
		function auto_size(){
			var autoHeight = Math.ceil($(window).width()/2.237762237762238);
			if(autoHeight<143){autoHeight=143}
			if(autoHeight>286){autoHeight=286}
			$("#focus").height(autoHeight);
		};
		
	</script>

		<!-- 底部JS执行区域 结束 -->
