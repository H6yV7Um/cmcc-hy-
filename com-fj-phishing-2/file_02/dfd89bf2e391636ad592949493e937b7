$(document).ready(function(){
	$(".all_kes,.all_kes_menu").mouseleave(function(){
       $(".all_kes_menu").hide();
});
	$(".all_kes_menu").mouseleave(function(){
       $(".all_kes_menu").hide();
});
	$(".all_kes").mouseenter(function(){
       $(".all_kes_menu").show();
});
	$(".all_kes_menu").mouseenter(function(){
       $(".all_kes_menu").show();
});
	$("#msg_key").focus(function(){
		var test1 = $(this).val();
		if(test1 == "请输入关键词"){
			$(this).val("")
		}
	});
	$("#msg_key").blur(function(){
		var test1 = $(this).val();
		if(test1 == ""){
			$(this).val("请输入关键词")
		}
	});

})
</script>
