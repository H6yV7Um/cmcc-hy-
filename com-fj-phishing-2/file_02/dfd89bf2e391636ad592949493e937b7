<script>

$(document).ready(function(){

  $.post("/User/is_login", function(data){

      if(data.uid>0)
        {
	    $("#denglu").show();
	    $("#weidenglu").hide();
	    $("#uname").html(data.username);
        }else{
	    $("#weidenglu").show();
	    $("#denglu").hide();
        }
     },'json');

});
</script>


<script language="javascript">
$(document).ready(function(){
	var menuYloc = $("#erweima").offset().top;
    var yloc = menuYloc + 145;
	var xloc=yloc+145;
    $('#wedma').css({top:menuYloc+"px"});
    $('#erweima').css({top:yloc+"px"});
    $('#three').css({top:xloc+"px"});
	$(window).scroll(function (){
	    var offsetTop = 145 + menuYloc + $(window).scrollTop() +"px";
        var offsetweb = menuYloc + $(window).scrollTop() +"px";
		var offsetweb2 = xloc + $(window).scrollTop() +"px";
        $("#erweima").animate({top : offsetTop },{ duration:600 , queue:false });
        $("#wedma").animate({top : offsetweb },{ duration:600 , queue:false });
		$("#three").animate({top : offsetweb2 },{ duration:600 , queue:false });
	    });
	});
</script>
