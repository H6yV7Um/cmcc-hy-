<script type="text/javascript">
$(document).ready(function(){
	$(".toggle_click,.down").click(function(){
	    $(".FanK_position").toggle();
	  })
  $("#feedbackbtn").click(function(){
    var feedbackemail=$('#input_mail').val();
    var feedback=$("#feedback").val();
    var feedbackname=$("#feedbackname").val();
    if($.trim(feedback)==''){
      alert("反馈内容不能为空");
    }else{
      $.post("/Feedback",{feedback:feedback,feedbackemail:feedbackemail,feedbackname:feedbackname},function(data){
        if(data.status){
         $('#input_mail').val('');
         $("#feedback").val('');
         $("#feedbackname").val('');
          $(".FanK_position").toggle(1000);
         alert('我们已收到您的反馈。谢谢!');
        }else{
          alert('提交失败');
        }
      },"json");
    }
  })
});
</script>
