var romClass = "";
function romClassName(){
	var romStyle = ["qrcodeStyle1","qrcodeStyle2","qrcodeStyle3","qrcodeStyle4","qrcodeStyle5","qrcodeStyle6"];

		romClass = romStyle[Math.floor(Math.random()*romStyle.length)];

	return romClass;
}
romClass = romClassName();
