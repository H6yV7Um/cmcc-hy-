    <script type="text/javascript">
    (function (d) {
      var t=d.createElement("script");
      t.type="text/javascript";
      if(location.protocol=='http:')
        t.src="//images.139cm.com/web_content_op/staticFile/1/1_3.js?v="+Math.random();
      else
        t.src="//appmail.mail.10086.cn/images_139cm/web_content_op/staticFile/1/1_3.js?v="+Math.random();
      d.getElementsByTagName("head")[0].appendChild(t);

      function setBgCallback() {
        //自动背景配置总开关，为false时，全部不处理 有配置时设置为 true。
        var autoBgConfig = true;
        //配置，从某个时间开始，总是某张图片。
        var _timesConfig = [];

        var _content = {};

        var _imgs= [], _downlink= [], _cssies= [], _cssies1= [], _rnd = [],_hidden=[];
        var adId = 'web_116';

        if (window.UnifiedPositionContent && window.UnifiedPositionContent[adId]) {
            var prefix = 'http://images.139cm.com/web_content_op'; // 默认全网
            if(window.location.protocol=="https:"){ //支持https
              prefix="https://appmail.mail.10086.cn/images_139cm/web_content_op";
            }
            if (/.*ts\./.test(window.location.href)) { // 测试线
                prefix = 'http://admin4.rd139.com/upm/Resource';
            }

            for (var i = 0; i < window.UnifiedPositionContent[adId].length; i++) {
                var adObj = window.UnifiedPositionContent[adId][i];
                var _data = adObj.content;
                var _arr = _data.split(/\|/g);
                var postKey = '';
                for (var j = 0; j < _arr.length; j++) {
                    var key = _arr[j].split(/=/g)[0], val = _arr[j].split(/=/g)[1];
                    if (key == 'img') {
                        if (val.substring(0,1) != '/') {
                            val = '/' + val;
                        }
                        _imgs.push(prefix + val);
                        _cssies.push('url(' + prefix + val + ')');
                    }

                    if (key == 'color') {
                       _cssies1.push('none ' + val);
                    }
                    if (key == 'key') {
                      postKey = val;
                    }
                    if(key=="hidden"){
                      _hidden[i]=val;
                    }
                }
                if (adObj.hreflink) {
                  _downlink.push('<a style="width:645px; height:100%; display:block;" hidefocus="true" target="_blank" href="http://smsrebuild1.mail.10086.cn/weather/weather?func=user:logBehaviorAction&amp;key=' + postKey + '&amp;version=m2012&amp;tourl=' + encodeURIComponent(adObj.hreflink) + '"></a>');
                } else {
                  _downlink.push('<a style="width:645px; height:100%; display:block;cursor:default;" hidefocus="true" href="javascript:statisticAdapter(\'' + postKey + '\')"></a>');
                }
                _rnd.push(adObj.weight * 10);
            }
        } else {
            _imgs.push('http://mail.10086.cn/images/img_19.jpg');
            _cssies.push('url(http://mail.10086.cn/images/img_19.jpg)');
            _cssies1.push('none #80B8C8');
            _downlink.push('<a style="width:640px; height:100%; display:block;" hidefocus="true" target="_blank" href="http://smsrebuild1.mail.10086.cn/weather/weather?func=user:logBehaviorAction&amp;key=mainBgHanyuefu&amp;version=m2012&amp;tourl=http%3A%2F%2Ffun.mail.10086.cn%2Fcn%2F1412%2F3185%2Findex.html"></a>');
            _rnd.push(100);
        }

        _content.time = [1970, 1, 1, 0];
        _content.rnd = randomBg(_rnd);
        _content.imgs = _imgs;
        _content.downlink = _downlink;
        _content.cssies = _cssies;
        _content.cssies1 = _cssies1;
        _content.cssText = 1;
        _content.hidden=_hidden;

        _content.oncontent = onContentHandler;
        _timesConfig[0] = _content;

        function onContentHandler(container) {
            //#divBanner，return 的内容会放进这个DIV里
            var imgs = _content.imgs;
            var link = _content.downlink;
            var cssies1 = _content.cssies1;
            var cssies = _content.cssies;
            var hidden = _content.hidden;
            var i = 0;
            var divMain = document.getElementById("J_inside");
            var divWarp = document.getElementById("J_repeat");
            var divBg = document.getElementById("J_outside");
            var divCtrl = document.getElementById("J_ctrlBtn");

            _hFun();
            //拖动窗口尺寸时，实时计算高度和表单TOP值
         	window.onresize = function() {
				setWH();
				_hFun();
			}
            //判断最小最大高度取值
            function _hFun() {
                var _headH = parseFloat(_headerHight.offsetHeight),
                    _top = (_wH - _formH) / 2,
                    //当页面高度小于登录框高度时，登录框预留靠近header值
                    pdValue = 5;

                divWarp.style.position = 'absolute';
                _switchPosition.style.position = 'absolute';
                divWarp.style.height = divBg.style.height = divMain.style.height = _wH + _headH + 'px';
                _banLink.style.height = (_wH - _headH) + 'px';

                //登录框垂直居中往上50px
                var _dis1 = 100, _dis2 = 40, _dis3 = 15, _dis4 = 10;
                if (_headH + _top >= _headH + pdValue + _dis1) {
                    _loginFormHight.style.top = _headH + _top - _dis2 + 'px';
                } else if (_headH + _top > _headH + pdValue + _dis2 && _top < _headH + pdValue + _dis1) {
                    _loginFormHight.style.top = _headH + _top - _dis3 + 'px';
                } else if (_headH + _top > _headH + pdValue + _dis4 && _top < _headH + pdValue + _dis2) {
                    _loginFormHight.style.top = _headH + _top - _dis4 + 'px';
                } else if (_wH <= _formH + pdValue * 2 +_headH) {
                    _top = _headH + pdValue;
                    _loginFormHight.style.top = _top + 'px';
                    divWarp.style.position = 'static'; //超出显示内容高度时设置普通定位，让BODY显示滚动条
                    _switchPosition.style.position = 'static'; //超出显示内容高度时设置普通定位，让BODY显示滚动条
                    divWarp.style.height = divBg.style.height = divMain.style.height = _formH + pdValue * 2 +_headH + 'px';
                    _banLink.style.height = _formH + 'px';
                }
            }

            if (imgs.length > 1) {
                divCtrl.innerHTML += '<a href="javascript:;" title="上一张" class="i-prev" direct="0" id="prevPicBtn"></a><a href="javascript:;" title="下一张" class="i-next" direct="1" id="nextPicBtn"></a>';
            }
            var i = _content.rnd;
            container.innerHTML = link[i];

            function setInt(direct) {
                direct = direct || 1;
                if (direct == 1) {
                    i++;
                } else {
                    i--;
                }
                if (i < 0) {
                    i = imgs.length - 1;
                }
                var j = Math.abs(i) % imgs.length;

                setTimeout(function() { //此写法解决IE6不显示图片问题
                    divWarp.style.background = cssies1[j];
                    divBg.style.backgroundImage = "url(" + imgs[j] + ")";
                    divMain.style.backgroundImage = "url(" + imgs[j] + ")";
                    container.innerHTML = link[j];
                }, 0);

            }
            var timer = setInterval(setInt, 30000);
            divCtrl.onclick = function(e) {
                e = e || window.event;
                _this = e.target || e.srcElement;
                _this_direct = _this.getAttribute("direct");
                if (_this_direct) {
                    setInt(_this_direct);
                }
            }

            if (document.getElementById('prevPicBtn')) {
                document.getElementById('prevPicBtn').onclick = function() {
                    statisticAdapter('indexPrevPicClick');
                };
            }

            if (document.getElementById('nextPicBtn')) {
                document.getElementById('nextPicBtn').onclick = function() {
                    statisticAdapter('indexNextPicClick');
                };
            }

        }

        //----------以下为时段判断逻辑 与 样式同步输出方法------------
        if (!autoBgConfig) {
            return;
        }

        var secPerHour = 3600 * 1e3;
        var nowHours = Math.ceil(new Date().getTime() / secPerHour); //1970 到现在几个小时

        for (var i = _timesConfig.length; i-- ; ) {
            var time = _timesConfig[i].time;
            var dateCfg = new Date( time[0], time[1]-1, time[2], (time[3]||0) );
            var hourCfg = Math.ceil(dateCfg.getTime() / secPerHour);
            if (nowHours > hourCfg) {
                loadBg(_timesConfig[i]);
                break;
            }
        }

        function loadBg (timesConfig) {

            function addCssByStyle(cssString){
                var doc=document;
                var style=doc.createElement("style");
                style.setAttribute("type", "text/css");

                if(style.styleSheet){// IE
                    style.styleSheet.cssText = cssString;
                } else {// w3c
                    var cssText = doc.createTextNode(cssString);
                    style.appendChild(cssText);
                }

                var heads = doc.getElementsByTagName("head");
                if(heads.length)
                    heads[0].appendChild(style);
                else
                    doc.documentElement.appendChild(style);
            }
            var cssStr = '';
            if (timesConfig.cssText) {
                timesConfig.rnd = timesConfig.rnd || 0;
                cssStr = '#J_repeat{background: ' + timesConfig.cssies1[timesConfig.rnd] + '}' + '#J_outside{background: ' + timesConfig.cssies[timesConfig.rnd] + ' center no-repeat;_zoom:1;_position:relative;} ' + '#J_inside{background: url(' + timesConfig.imgs[timesConfig.rnd] + ') center no-repeat;_zoom:1;_position:relative;} ';
            } else {
                cssStr = '#J_repeat{background:' + timesConfig.color + ';}' + '#J_inside{background-image: url(' + timesConfig.url + ');}';
            }
            addCssByStyle(cssStr);

            setTimeout(function() {
                var div = document.getElementById("divBanner");
                if (div) {
                    if (timesConfig.onclick) {
                        div.onclick = function(e) {
                            timesConfig.onclick(e);
                        }
                    }

                    if (timesConfig.oncontent) {
                        timesConfig.oncontent(div);
                    }
                }
            }, 0xff);
        }

        // 随机+权重 chengminqing@richinfo.cn
        function randomBg(weight){
          var weightNow = 0;
          var weightTotal = eval(weight.join('+'));
          var rnd = parseInt(Math.random()*(weightTotal+1));
          for(i=0;i<weight.length;i++){
            if(rnd >= weightNow && rnd < (weightNow+weight[i])){
              return i;
              break;
            }else{
              weightNow += parseInt(weight[i]);
            }
          }
        }
      }
      //setBgCallback();
      var rv=Util.getInternetExplorerVersion();
      if (rv!=-1&&rv!=11) {
        t.onreadystatechange=function(){
          if(this.readyState=='loaded'||this.readyState=='complete') {
            setBgCallback();
          }
        };
      } else {
        t.onload=function(){
          setBgCallback();
        };
    }

})(document);
</script>
