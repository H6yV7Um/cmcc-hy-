 <script type="text/javascript">
	
	//新需求为了追加插码,新定义的function
	function commonHelpUrl(helpCenterUrlId,helpCenterMenuName,firstMenuName){
		//运营监控系统插码  首页的页尾内容插码
		eventVesopera('JFPC_TOP_YWDH_'+helpCenterMenuName);
		if("新手上路"==firstMenuName){
			//JF_FOOT_XS_N
			addEventCode('JF_FOOT_XS_'+helpCenterMenuName);
			}
		else if("配送方式"==firstMenuName){
			//JF_FOOT_PS_N
			addEventCode('JF_FOOT_PS_'+helpCenterMenuName);
			}
		else if("售后服务"==firstMenuName){
			//JF_FOOT_SH_N
			addEventCode('JF_FOOT_SH_'+helpCenterMenuName);
			}
		else if("积分计划"==firstMenuName){
			//JF_FOOT_JH_N
			addEventCode('JF_FOOT_JH_'+helpCenterMenuName);
		}

		var helpCenterUrlStr="";
		if("积分网站地图"==helpCenterMenuName||"100000000000004l"==helpCenterUrlId){
			helpCenterUrlStr='http://jf.10086.cn/help/sitemap.html';
		}
		else{
			helpCenterUrlStr='http://jf.10086.cn/help/'+helpCenterUrlId+'.html';
		}
		windowLocationHref(helpCenterUrlStr);
	}
 </script>
