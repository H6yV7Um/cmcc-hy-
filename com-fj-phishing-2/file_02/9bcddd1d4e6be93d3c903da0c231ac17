/**另外一种换一批算法,通过一个指定大小的数组，来存储数据.最原始的设计是
 *全局变量i=0，开始从源数组取值，都索引==数组最大值
 * i=0;复原，从源数组索引0，开始新的循环。
 *同时额定大小的临时数组，每次循环，只允许记录4或者8个。记录满了就break 循环。
 *在下一次调用方法时候，i作为全局变量，会自动增加索引下标，一直到最大索引，然后置0。
 *var i=8：原因是第一次循环不需要去取数据存放到临时数组做处理。直接起始值从
 */
 var fineI=10; //标志json数组的索引
 var fineversionMax=10;  //临时数组容量定义
function initChangeArray(){  
	 var a= new Array(fineversionMax);  
    var jsonArrayLen=fineJson.length;
	var j = 0;
	while(true)
	{
		if(fineI == jsonArrayLen)
		{
			fineI = 0;
		}			
		a[j] = fineJson[fineI];  //从json数组里面，复制数据到临时数组，临时数组每次都是额定大小
		j++;
		fineI++;
		if(j == fineversionMax)  //临到数组复制到最大量的时候(每次展示的数量),while循环退出
		{
			j = 0;
			break;   
		}			
	}  
	return a; 
	
}
/**此方式用于换一批功能，所有的图片循环依次补足8位；采用替换标签属性方式*/
 function changeNewFineRecomonShow(){
	//增加插码 
	 addEventCode('JF_JPTJ_HH'); 	 
      var changeFine= initChangeArray(); //versionMax全局变量，每个模块展示图片的常量定义
      for(var m=0;m<fineversionMax;m++){ 
            var tempImgUrl= changeFine[m].m_back.RECOMON_IMG_URL;  
            var tempHref=changeFine[m].m_back.RECOMON_HREF;
            var tempName=changeFine[m].m_back.RECOMON_NAME;
           //通过id定位，并更改的属性值,在标签里的id索引是从1开始的。故m+1
           var finerecomonPath='';
           tempImgUrl=finerecomonPath+fineRecomonPicHome+tempImgUrl;
           
           $("#fineurl"+(m+1)).attr("value",tempHref); //每次循环取值的时候，将value属性值先更改，保证fineRecomonUrlJump()能够取到变换后的值
      	   $("#fineurl"+(m+1)).attr("href","javascript:fineRecomonUrlJump('"+(m+1)+"','"+tempHref+"','"+tempName+"')"); 
      	   $("#fineimgUrl"+(m+1)).attr("src",tempImgUrl);
      	   $("#fineimgUrl"+(m+1)).attr("alt",tempImgUrl);  
          }     
	 } 
//精品推荐跳转到function ,return fasle是专门用于a标签的,保证href和onclick只执行一个方法
 function fineRecomonUrlJump(Id,recomonHref,recomonName){ 
 	//增加插码 ,多按钮时候，需要体现不同按钮值
 	var N=recomonName;
 	addEventCode('JF_JPTJ_'+N); 
 	//运营监控系统插码
 	eventVesopera('JFPC_TOP_JPTJ_'+N);
 	clickUrl=$("#fineurl"+Id).attr("value");
  //  window.location.href=recomonHref;
 	 windowLocationHref(clickUrl);//以新窗口形式打开
 }

 </script>


