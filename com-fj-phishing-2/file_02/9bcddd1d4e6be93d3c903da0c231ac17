 <script type="text/javascript">
 var actIndex = '';
 $(document).ready(function(){
	 slide('slide', 'li', 'curr');
	 $.ajax({
		type:"post",
		url:basePath + "/portal/lottery/web/LotteryDrawAction?action=getFreeReceiveInfo",
		dataType:"json",
		success:function(data){
			if("" !== actIndex){//如果“0元送流量”活动在轮播图中已经配置执行下列逻辑
				if(data.isLogin=="1" && data.isActivityEnd=="0" && data.isCanReceive=="1" && actIndex=="0"){
					$("#campus").show();
					$("#slide_controls").show();
				}else if(data.isLogin=="1" && data.isActivityEnd=="0" && data.isCanReceive=="1" && actIndex!="0"){
					$("#campus").hide();
					$("#slide_controls").show();
			    }else{
			    	$("#slide_controls li:last").remove();
			    	$("#slide_controls").show();
					$("#campus").remove();
					if(actIndex == "0"){
						$("#slide_items li:first").show();
					}
				}
			}else{//没有活动 展示...
				$("#slide_controls").show();
			}
		}
	 });
 });
 
 function eventadd(url){
	 if(url.indexOf("/topics/campus_act.html")>=0){
		 addEventCode('XYYX_PC_HOME_banner1');
	 }else if(url.indexOf("/topics/cuUtcti_lottery.html")>=0){
		 addEventCode('XYYX_PC_HOME_banner2');
	 }else{
		 addEventCode('JF_AD');
	 }
	//运营监控平台插码
	eventVesopera('JFPC_TOP_BANNER')
 };
//为了处理 没有超链接的数据，这种极端特殊情况，不允许跳转 ，写js来中转处理.window.open(intentUrl,"_blank")这种方式才能新页面打开

function windowLocationHref(intentUrl){
	if(intentUrl!=null&&$.trim(intentUrl)!= ""){//将页面以新窗口打开
		window.open(intentUrl,"_blank");
	} 
}
 
 </script>
