<script type="text/javascript">



/**点击事件，根据传递过来的索引，获取url+索引 ，这个id下的name的值
 * 点击九宫格，根据type类型，进行页面跳转或者样式的滑动处理
 * 滑动处理，之前是任意一个<li>都支持。现在改为。只有礼品的type=1的，才绑定滑动事件
 * 同时在public.js中不再做滑动处理
 * 并且根据这个方法也动态控制所有绑定了礼品的专区展示效果
 */
function o2oInfoShow(Id,o2obandCounts){	
	//固定的拼接出 <a>标签的ID
    var o2oType=$("#url"+Id).attr("name"); //o2oType 1是礼品绑定，2是专区绑定    
    //增加插码 ,多按钮时候，需要体现不同按钮值
	var N=$("#o2odiv"+Id).text();
	addEventCode('JF_O2O_'+N); 
	eventVesopera('JFPC_TOP_O2O_'+N);
	//运营监控插码
    if(o2oType=='2'){
         var o2oContent=$("#url"+Id).attr("value");  //此时取出的是专题连接地址
         windowLocationHref(o2oContent);
        }      
	else{ 
		  //当时礼品类型时候，九宫格绑定滑动效果;绑定了礼品的专区展示顺序问题，前提是要求运营保证前面配置的是礼品专区，后面配置的是专题类型专区，不能出现交叉
		   var indexLi,_this;
	      //divBand  eventListShow 调用该方法的时候，全部隐藏，然后根据规则进行数据隐藏和展示
	      //O2O展示礼品绑定div部分,当页面选中id>=4(索引),就按照如下方式倒着展示4个div
	       $("[id^=divBand]").hide();
		   $("[id^=eventListShow]").hide();
           //针对绑定礼品专区小于=4的。根据绑定礼品专区总数做特殊处理
	       
	       if(o2obandCounts>0&&o2obandCounts<4&&Id<4){  //绑定了礼品的专区总数小于4,且点击的索引也小于4,则按照总数循环设置选择效果
	    	   for(var g=0;g<=o2obandCounts-1;g++){
		    	   document.getElementById("divBand"+g).style.display="block";
			       document.getElementById("eventListShow"+g).style.display="block";
			   }      

		    }

		   if(Id<4&&o2obandCounts>=4){ //绑定了礼品的专区总数大于等于4，但是当前点击的id索引小于4，需要展示前4个div
		       for(var g=0;g<4;g++){
		    	   document.getElementById("divBand"+g).style.display="block";
			       document.getElementById("eventListShow"+g).style.display="block";
			   }     
		   } 
		   
		   if(o2obandCounts>=4&&Id>=4){//绑定了礼品的专区总数大于等于4,当前点击索引>=4
			   for(var k=Id;k>Id-4;k--){
				   document.getElementById("divBand"+k).style.display="block";
			       document.getElementById("eventListShow"+k).style.display="block";
				   }
			   }      

		 //单独给li绑定滑动效果,只有当o2oType=='1'，商品绑定的才有效果  
		 $("#lio2o"+Id).bind('click',function(){
			    _this=$(this);
		        indexLi=$('.listIm li').index(_this); 
		        $(this).closest('.listIm').slideUp(300).siblings('.event').fadeIn(300);
		        $('.UL1 li').eq(indexLi).addClass('active').siblings('li').removeClass('active');           
		        $('.event_main .event_list').eq(indexLi).show().siblings('.event_list').hide();
			 });	
		    $('.Img').bind('click',function(){
		        $(this).closest('.event').hide(300).siblings('.listIm').slideDown(300);
		    });   
		}      
}
/**点击兑换功能时候，传入select标签的id --select0 select1 select 2 select3，
 * 标记每个“选择礼品”的select标签。进行选择项获取，然后跳转到指定的礼品详情页
 */
function  businessDo(selectId){
	var options=$("#select"+selectId+"  option:selected"); //获取选中的项的值
	var wareId=options.val();
	var wareConst='';         
	wareConst=wareConst+"/ware/";   //这个是根据工具类UrlUtil.wareUrl()方法写的常量地址
	var wareUrl=wareConst+wareId+".html";
	windowLocationHref(wareUrl);  	  
}
//关闭礼品div
function closeBrandDiv(){     
	document.getElementById("event").style.display="none";
}
   
//查看全部礼品，通过select的标签位置，定位当前是在哪个选择div下,然后直接取4块固定区域O2O专区索引对应的名称
function showAllWareInfos(selectId){
	var pathContentURL='';
	var intentUrlForO2oWareShow=pathContentURL+"/portal/ware/web/SearchWareAction?action=searchWareInfo";
	var divO2oName=$("#divBand"+selectId).text();
	var divO2oNomalName=$("#divBand"+selectId).attr("name"); //获取O2O的全名，展示的区域是经过的字符截取的
	//获取对应的索引下标位置的select option里面的全部值  $(#selectID option).each();
	var forSearchPame="";
	$("#select"+selectId +"  option").each(function(){
		 var $option =$(this);
         var value=$option.val();
         forSearchPame=forSearchPame+value+",";
		});
	forSearchPame=forSearchPame.substring(0,forSearchPame.lastIndexOf(","));
	//name转码处理
	divO2oNomalName=encodeURIComponent(encodeURIComponent(divO2oNomalName));
	var parmes="&o2on="+divO2oNomalName+"&wi="+forSearchPame;
	//跳转到搜索页面   
	windowLocationHref(intentUrlForO2oWareShow+parmes);
}

</script>
   <!-- O2O区域 -->
	</div>
    <!--右侧登录end-->
</div>
