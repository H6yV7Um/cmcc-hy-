<script type="text/javaScript">
function sendAndImgSign(type){
	var url = '';
	switch(type){
	 case 'MOB'://运营监控平台插码
	        url="http://www.10086.cn/";
			eventVesopera('JFPC_TOP_MOB');
	 		break;
	 case 'HJF'://和积分按钮
	 		url=""+"/index.html";
			addEventCode('JF_HJF');
			//运营监控平台插码
	 		eventVesopera('JFPC_TOP_HJF');
	 		break;
	}
	window.location.href=url;
}
</script>
    
    
      
    
    	
    	
