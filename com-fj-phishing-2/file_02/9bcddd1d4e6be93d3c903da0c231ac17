<script type="text/javaScript">
var bjfindex = "1";
function searchByScore(str,indexNo){
	var startValue; 
	var endValue;	
	if (escape(str).indexOf("%u") < 0){
		//没有汉字
		startValue = str.split("-")[0];
		endValue = str.split("-")[1];
	}else{
		//有汉字
		startValue = str.substring(0,escape(str).indexOf("%u"));
		endValue = "999999";
	}
	//增加插码 JF_FZLL_N
	addEventCode('JF_FZLL_'+str);
	//运营监控系统插码
	eventVesopera('JFPC_TOP_FZH_'+str);
	//var url = "/portal/ware/web/SearchWareAction?action=searchWareInfo&s="+startValue+"-"+endValue+"&rfet=sortWayChange";
	var url = "/searchScore/"+indexNo+".html";
	if(bjfindex == 0){
		window.location.href=url;
	}else{
		window.open(url);
	}
}
</script>

