            <a class="dispalyblock returntop_icon" href="#returnTop"></a>
            <a class="dispalyblock ftitle stitle returntop_icon_on" href="#returnTop" onclick="addEventCodeXS()" style="display: none;"></a>
        </li>
    </ul>
</div>
<!--左侧浮动导航end-->
<script type="text/javascript">
var contextPath='';
function myInfoFuc(navInfoUrl,navName){
	var targetNavUrl=contextPath+navInfoUrl;
	//插码
	addEventCode('JF_YYQ_'+navName);
	//运营监控系统插码  首页右侧导航
	eventVesopera('JFPC_TOP_TAG_'+navName);
	window.location.href=targetNavUrl;
}
function addEventCodeXS(){
	addEventCode('JF_YYQ_XS');
	//运营监控系统插码  首页右侧导航向上
	eventVesopera('JFPC_TOP_TAG_TOP');
}
</script>
