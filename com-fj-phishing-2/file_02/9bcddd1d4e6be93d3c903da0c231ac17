      <div class="index_ad_btmWrap">
           
      </div>      
      <div class="index_ad_btmImg">
                  
      </div>
</div>
<script type="text/javascript">
var delayShutDown='0';
$(document).ready(function(){	
	if("0"!=delayShutDown){ //当获取的延迟时间不为0，才有延迟的效果
		$("#adPcBotom").fadeOut(parseInt(delayShutDown));//页尾广告，初次加载延迟关闭
	}
});
function closeBotomAd(){
	$("#adPcBotom").hide();  
}
function eventVesoperaBP(url){
	//运营监控系统插码  首页页尾广告插码
	 eventVesopera('JFPC_TOP_AD_BOTTLE');
	 windowLocationHref(url);
}
 
</script>
