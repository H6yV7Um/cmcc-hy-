<script type="text/javascript">
var floorInfoJson='[{"floorSort":"0","floorName":"移动自有","isHaveAd":"0"},{"floorSort":"1","floorName":"聚便利","isHaveAd":"1"},{"floorSort":"2","floorName":"享品质","isHaveAd":"1"},{"floorSort":"3","floorName":"爱生活","isHaveAd":"0"},{"floorSort":"4","floorName":"品时尚","isHaveAd":"0"}]';
var footerHeigth=0;//底部高度
var adSumHeight=0;//广告总高度
var floorBaseSumHeight=0;//楼层基本信息总高度
var foorSumHeight=0;//楼层模块总高度(基本信息+广告)
var aboveFloorHeight=0;//楼层模块上方的总高度
var floorArray=eval(floorInfoJson);
var contextPath='';
var wareConstant='ware';
$(function(){
	if(floorInfoJson!=null&&floorInfoJson!="null"&&floorArray.length>0){
		var adCount=0;
		for(var i=0;i<floorArray.length;i++){
	          if("1"==floorArray[i].isHaveAd){
	        	  adCount=adCount+1;
	              } 
			} 
		 footerHeigth=$(".footer").height()+30;//底部高度（30是Margin值）
		 adSumHeight=adCount*110;//广告总高度（110是广告高度+margin值）
		 floorBaseSumHeight=floorArray.length*588;//楼层基本信息总高度（588楼层基本信息高度+margin值）
		 foorSumHeight=adSumHeight+floorBaseSumHeight;//楼层总高度
		 aboveFloorHeight=document.body.scrollHeight- Number(footerHeigth+foorSumHeight);//楼层上面的总高度（上面不确定的高度比较多不好计算。故采用这种方式算出）
		}

	 $("div[name='MobileADIV']").each(function(){
		var divId=$(this).attr("id");
		var divNum=divId.substring(divId.lastIndexOf("_")+1);
		var floorNum = parseInt(divNum)+1;
		clickFuns("MobileA_"+divNum,"active","MobileA_main_"+divNum);
	    function clickFuns(nav,navStyle,content){
	    	  var floorName= $("#"+nav).find("i").eq(1).text();
	    	 $("#"+nav).find("li[name='floorTab']").click(function(){
		      $("#"+nav).find("li[name='floorTab']").removeClass(navStyle);
	            $(this).addClass(navStyle);
	            var index = $(this).index();
	            if(index==0){
	            	addEventCode('JF_LC_'+floorNum+'_RXTJ');
		            }else{
		            	addEventCode('JF_LC_'+floorNum+'_XPTJ');
			            }
	            $("#"+content).children().hide();
	            $("#"+content).children("div").eq(index).show();
	        });
	            
	    }
	});
     
});

/*
 * 楼层导航根据滚动条控制显示效果
 */
$(window).scroll( function() {
	if(floorInfoJson!=null&&floorInfoJson!="null"&&floorArray.length>0){
		var currentFloor=0;//当前楼层高度
		var nextFloor=0;//下一楼层高度
	    var sropTop=$(document).scrollTop();
	    sropTop=sropTop+900;
	    for(var j=0;j<floorArray.length;j++){
	    	if(j<floorArray.length-1){
	    		 if("1"==floorArray[j].isHaveAd){
	        		 currentFloor=currentFloor+588+110;//588是楼层基本信息的高度加上margin值  110是广告的高度加上margin值
	    		 }else{
	    			 currentFloor=currentFloor+588;
	        		 }
	        		 if("1"==floorArray[j+1].isHaveAd){
	        			 nextFloor=currentFloor+588+110;
	            		 }else{
	            			 nextFloor=currentFloor+588;
	                		 }
	        		 if(Number(sropTop)>Number(aboveFloorHeight+currentFloor)&&Number(nextFloor+aboveFloorHeight)>Number(sropTop)){
	            		 $("#floorSort_"+j).hide();
	            		 $("#floorName_"+j).removeClass('ctitle').find('i').html(floorArray[j].floorName);
	            		 $("#floorName_"+j).show();
	            		 }else{
	            			 $("#floorSort_"+j).show();
	            			 $("#floorName_"+j).hide();
	            			 $("#floorName_"+j).addClass('ctitle');
	                		 } 	
	        		 
	        	}else{
	        		 if("1"==floorArray[j].isHaveAd){
	            		 currentFloor=currentFloor+588+110;
	        		 }else{
	        			 currentFloor=currentFloor+588;
	            		 }
	        		 if(Number(sropTop)>Number(aboveFloorHeight+currentFloor)){
	            		 $("#floorSort_"+j).hide();
	            		 $("#floorName_"+j).removeClass('ctitle').find('i').html(floorArray[j].floorName);
	            		 $("#floorName_"+j).show();
	            		 }else{
	            			 $("#floorSort_"+j).show();
	            			 $("#floorName_"+j).hide();
	            			 $("#floorName_"+j).addClass('ctitle');
	                		 }
	            	}
	    	
	        }
		}
	
	} );

//新增子类目的插码function,楼层主图和默认页签下的商品都没有名称，固不增加插码53行onclick=addEventCode('JF_LC_PIC_')
//跳转方式改为打开新窗口
function floorSubKindInsertCode(floorSubKindWareId,floorSubName,floor){
	///mKind/.html 子类目原始跳转链接
	addEventCode('JF_LC_'+floor+'_DH_'+floorSubName);
	//运营监控系统插码  //点击主图下标签内容方法
	eventVesopera('JFPC_TOP_FLOOR'+floor+'_TAG_'+floorSubName);
	var floorSubKindHref=contextPath+'/mKind/'+floorSubKindWareId+'.html';
	windowLocationHref(floorSubKindHref);
}
//新增商品页面的插码function ,改造之前的href跳转,跳转方式改为打开新窗口
function productTabInsertCode(wareId,floor,index,name){
  ///ware/.html   商品页签下礼品原始跳转
  addEventCode('JF_LC_'+floor+'_XPTJ_'+index);
	//运营监控系统添加插码  //点击楼层里格内容的方法
	eventVesopera('JFPC_TOP_FLOOR'+floor+'_'+name);
  var productTabHrefStr=contextPath+'/'+wareConstant+'/'+wareId+'.html'; 
  windowLocationHref(productTabHrefStr);
}
//楼层主图跳转方式，打开新窗口
function floorMainImg(url,floor){
	addEventCode('JF_LC_'+floor+'_L1');
	//运营监控系统插码  //点击楼层主图方法
	eventVesopera('JFPC_TOP_FLOOR'+floor+'_ADIMG');
	windowLocationHref(url);
}
function adHrefFuc(navInfoUrl,navName){
	//插码
	addEventCode('JF_LCGG_'+navName);
	//运营监控系统插码 //点击楼层底下的广告方法
	eventVesopera('JFPC_TOP_ADFL_'+navName);
	windowLocationHref(navInfoUrl);
}
//默认页签中部正品部分插码
function floorDefaultTab(url,floor,index,name){
	addEventCode('JF_LC_'+floor+'_RXTJ_'+index);
	//运营监控系统添加插码  //点击楼层里格内容的方法
	eventVesopera('JFPC_TOP_FLOOR'+floor+'_'+name);
	windowLocationHref(url);
}
//默认页签右侧图片区域插码
function floorDefaultTabAd(url){
	addEventCode('JF_LC_YPIC_'+url);
	windowLocationHref(url);
}
</script>

<!--楼层end-->  

