<script type="text/javaScript">
    $('#campus_tips').hide();
    $('#insertCode').click(function(){
    	addEventCode('XYYX_PC_LOGON_new_Y');//点击领取 的插码 
    })
    $('#closeTips').click(function(){
    	addEventCode('XYYX_PC_LOGON_new_N');//点击关闭  的插码 
    })
	var contextPath = "";
	var basePath="";
	var imgPath="/pic/ware/";
	var isShow = "Y";
	
	var brand_gotone = "0";
	var brand_mzone = "2";
	var ware_show_img_px_110 = "_110.";
	var brand_easyown = "1";
	var MOBILE_LEVEL_DIAMOND="301";
	var MOBILE_LEVEL_GOLD="302";
	var MOBILE_LEVEL_SILVER="303";
	var PAY_TYPE_INTEGRAL="01";
	
	var user_login_status_login = "USER_LOGIN_STATUS_LOGIN";
	var user_login_status_not_login = "USER_LOGIN_STATUS_NOT_LOGIN";
	var searchAction = contextPath + "/portal/ware/web/SearchWareAction?action=searchWareInfo";
	var checkUserLogin = basePath + "/portal/order/web/UserOrderAction?action=checkUserLogin";

	$(function(){
		if("Y" == isShow){
	        
	        $("#wareName").autocomplete(contextPath + "/portal/ware/web/SearchWareAction?action=getSuggest", {
	    		scroll: false,
	    		selectFirst: false,
	    		parse: function(data) {
	    			var parsed = [];
	    			var docs = eval('(' + data + ')');
	    			for (var i=0; i<docs.length; i++) {
	    				var row = docs[i].word.split("|");
	    				parsed[parsed.length] = {
	    					data: row,
	    					value: row[0],
	    					result: row[0],
	    					oriData: docs[i]
	    				};
	    			}
	    			return parsed;
	    		}
	    	});
	        
	        //用于统计提示关键字点击率
	        $("#wareName").result(function(event, data, formatted) {
	        	if (data) {
	        		var termId = data.id;
	        		if (termId) {
	        			$.ajax({
	        				url: contextPath + "/portal/ware/web/SearchWareAction?action=incrHitCount",
	        				type: 'POST',
	        				data: '&tid='+termId,
	        				dataType: 'json',
	        				async: false
	   	       			});
	        		}
	        		doSearch();
	        	}
	        });
	        
	        $(window).resize(function() {
	        	var width = $("input.ac_input").width();
	        	if (width < 300) {
	        		//提示框有1px的边框
	        		width = $("div.search-in").width() -2;
	        	} else {
	        		//宽度+34，是因为输入框左侧有个34px的padding
	        		width = $("#wareName").width() + 34;
	        	}
	        	
	        	$("div.ac_results").width(width);
	        	
	        	var offset = $("div.search-in").offset();
	        	var top = offset.top;
	        	var left = offset.left;
	        	$("div.ac_results").css("left", left);
	        	$("div.ac_results").css("top", top + $("div.search-in").height());
	        });
		}
	});

	/*为此function增加一级二级三级目录名称，作为入参用于插码按钮值区分记录*
	 *此方法在一级目录，传入一级目录对应的4个参数；二级目录时候传入一级二级对应参数，三级目录时候传入一二三级目录对应参数。
	 */
	function searchNaviWare(onecontent,onetype,oneid,oneName,twocontent,twotype,twoid,twoName,thrcontent,thrtype,thrid,threeName){
		var type = "";
		var hreftmp = "";
		if(typeof(thrid) != 'undefined'){
			//hreftmp = "&onli="+oneid+"&twli="+twoid+"&thli="+thrid;
			hreftmp = oneid+"_"+twoid+"_"+thrid;
			if(thrtype==3){
				hreftmp = thrcontent;
				type = 3;
			}
			//增加插码功能
			if(typeof(threeName)!="undefined"){				
				addEventCode('JF_LPFL_'+threeName);
				//运营监控平台插码  三级类目点击事件
		 		eventVesopera('JFPC_TOP_CATA03_'+threeName);
			}
			
		}else if(typeof(twoid) != 'undefined'){
			//hreftmp = "&onli="+oneid+"&twli="+twoid;
			hreftmp = oneid+"_"+twoid+"_0";
			if(twotype==3){
				hreftmp = twocontent;
				type = 3;
			}
			//增加插码功能
			if(typeof(twoName)!="undefined"){				
				addEventCode('JF_LPFL_'+twoName);
				//运营监控平台插码   二级类目点击事件
		 		eventVesopera('JFPC_TOP_CATA02_'+twoName);
			}
		}else{
			//hreftmp = "&onli="+oneid;
			hreftmp = oneid+"_0_0";
			if(onetype==3){
				hreftmp = onecontent;
				type = 3;
			}
			//增加插码功能
			if(typeof(oneName)!="undefined"){				
				addEventCode('JF_LPFL_'+oneName);
				//运营监控平台插码   一级类目点击事件 
		 		eventVesopera('JFPC_TOP_CATA01_'+oneName);
			}
		}
		
		var url = "";
		if(type==3){
			url = hreftmp;
		}else {
			//url = ""+"/portal/ware/web/SearchWareAction?action=searchWareInfo"+hreftmp;
			url = "/category/"+hreftmp+".html";
		}
		windowLocationHref(url);
	} 
	 
	function sendHotSearchSignForAnd(name,url){
		//运营监控平台插码
		eventVesopera('JFPC_TOP_HJF_'+name);
		if(name=='和·优选'){
			addEventCode('JF_HOT_SS_和·优选');
		}
		window.location.href=url;
	}

	  //为了增加插码，对首页 头部页签部分 重新抽象出的function navInfoFuc(navInfoUrl,navName)
	function navInfoFuc(navInfoUrl,navName){
		var targetNavUrl=contextPath+navInfoUrl;
		//插码
		addEventCode('JF_NAV_'+navName);
		//运营监控系统插码
		eventVesopera('JFPC_TOP_NAVI_'+navName);
		window.location.href=targetNavUrl;
	}
	function goCart(){
		//运营监控系统插码
		eventVesopera('JF_CARS');
		window.location.href="/portal/order/web/ShoppingCartAction?action=showUserShopInfo";
	}
	
