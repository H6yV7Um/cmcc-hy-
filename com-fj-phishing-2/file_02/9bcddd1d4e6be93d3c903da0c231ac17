<script>
    function signIn() {
        addEventCode('JF_LOGIN');
        window.location.href = '/portal/user/web/UserLoginAction?action=goAuthLoginPage&targetUrl=';
    }

    function loginHead() {
        addEventCode('JF_LOGIN_HEAD');
        window.location.href = '/portal/user/web/UserOrderQueryAction?action=myDefaultAccount';
    }

    function loginValue() {
        addEventCode('JF_LOGIN_VALUE');
        window.location.href = '/portal/user/web/UserOrderQueryAction?action=myDefaultAccount';
    }

    $(function(){
       if (isUserLogin) {
           $('.beforeLogin').hide();
           $('.afterLogin').show();
           $('#userPhoneNum').text(userPhoneNum);
           $('#starCite').addClass("star" + starCiteLevel);
           $('#totalScore').html(totalScore);
       }
    });
</script>
        







