 <script type="text/javascript">
 var FinetotalSize='5';
 //json前台数据处理使用
 var fineText='[{"m_type":{},"m_isDeleted":false,"m_back":{"DOWN_DATE":{"nanos":0},"SORT":"1","UP_DATE":{"nanos":0},"FINE_RECOMON_ID":100000000000001,"RECOMON_IMG_URL":"/recommen/1501665082422.jpg","CREATE_DATE":{"nanos":0},"RECOMON_NAME":"mall","STATUS":"0","IS_HOME_SHOW":"1","MODIFY_DATE":{"nanos":0},"RECOMON_HREF":"http://jf.10086.cn/topics/jfspecial_04.html"}},{"m_type":{},"m_isDeleted":false,"m_back":{"DOWN_DATE":{"nanos":0},"SORT":"2","UP_DATE":{"nanos":0},"FINE_RECOMON_ID":100000000001601,"RECOMON_IMG_URL":"/recommen/1510308389653.jpg","CREATE_DATE":{"nanos":0},"RECOMON_NAME":"维士十字","STATUS":"0","MODIFY_DATE":{"nanos":0},"RECOMON_HREF":"http://jf.10086.cn/topics/jfspecial_03.html"}},{"m_type":{},"m_isDeleted":false,"m_back":{"DOWN_DATE":{"nanos":0},"SORT":"3","UP_DATE":{"nanos":0},"FINE_RECOMON_ID":100000000001602,"RECOMON_IMG_URL":"/recommen/1501665179108.jpg","CREATE_DATE":{"nanos":0},"RECOMON_NAME":"被窝窝","STATUS":"0","MODIFY_DATE":{"nanos":0},"RECOMON_HREF":"http://club.10086.cn"}},{"m_type":{},"m_isDeleted":false,"m_back":{"DOWN_DATE":{"nanos":0},"SORT":"4","UP_DATE":{"nanos":0},"FINE_RECOMON_ID":100000000001603,"RECOMON_IMG_URL":"/recommen/1501665463310.jpg","CREATE_DATE":{"nanos":0},"RECOMON_NAME":"爱仕达","STATUS":"0","MODIFY_DATE":{"nanos":0},"RECOMON_HREF":"http://jf.10086.cn/topics/jfspecial_08.html"}},{"m_type":{},"m_isDeleted":false,"m_back":{"DOWN_DATE":{"nanos":0},"SORT":"5","UP_DATE":{"nanos":0},"FINE_RECOMON_ID":100000000001604,"RECOMON_IMG_URL":"/recommen/1501665604329.jpg","CREATE_DATE":{"nanos":0},"RECOMON_NAME":"陶云","STATUS":"0","MODIFY_DATE":{"nanos":0},"RECOMON_HREF":"http://jf.10086.cn/topics/jfspecial_07.html"}}]'; 
 var fineJson=eval(fineText);
 var fineRecomonSize='5';
 var fromFirst=false;      
 var tempFineRecomons='[Lngves.asiainfo.core.floor.bo.FineRecomonBean;@f15799';
 var fineRecomonPicHome='/pic/home';
 $(document).ready(function() {
		if(fineRecomonSize>10){   
