<script type="text/javaScript">

var ad1_DelayShutTime='0';
var ad2_DelayShutTime='0';
var ad1_div_flag='null';
var ad2_div_flag='null';
$(document).ready(function(){	
	initFrontAd();	
});

//新增门户通栏广告1 2的延迟效果方法
function initFrontAd(){
//因为不能确定，首页展示的div是通栏1 还是通栏2 ，或者还是 都没有有效数据， 所以在java脚本处理的时候对div的id进行取值处理。如果结果集有通栏1的一些标志，通栏1 div的id就会记录。没有的话 值就是null。这样就可以判断是否结果集有通栏1 的div布局了
	
	if(null!=ad1_DelayShutTime&&"0"!=ad1_DelayShutTime){ //当获取的延迟时间不为0，才有延迟的效果
		if(null!=ad1_div_flag&&"adContent1"==ad1_div_flag){
			$("#adContent1").fadeOut(parseInt(ad1_DelayShutTime));
		}
	    if(null!=ad1_div_flag&&"adImg"==ad1_div_flag){
	    	$("#adImg").fadeOut(parseInt(ad1_DelayShutTime));
		   }	
	}   
	if(null!=ad2_DelayShutTime&&"0"!=ad2_DelayShutTime){ 
		if(null!=ad2_div_flag&&"adContent2"==ad2_div_flag){  //在延迟时间不为0，且div标记变量是记录的通栏2 div的id值。对通栏2 div 进行隐藏操作
			$("#adContent2").fadeOut(parseInt(ad2_DelayShutTime));
		}		
	}  
}
function sendTopAdUrlAndInsertCode(intentUrl){
	//页首浮动广告插码标志
	addEventCode('JF_HEAD_BAN');
	//运营监控系统插码  页头通栏广告插码
	eventVesopera('JFPC_TOP_AD_TL'+index);
	var topADurl=intentUrl;
	windowLocationHref(intentUrl);//将页首广告以新窗口打开
}

</script>



