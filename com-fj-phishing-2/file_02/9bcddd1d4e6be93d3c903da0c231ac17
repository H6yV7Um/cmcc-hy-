 <script type="text/javascript">
 var brandUrlConst='/brand/';
 var picUrlConst='/pic/brand/';
 var brandText='[]';
 var brandjson=eval(brandText);  
 var fromFirst=false; 
 var brandRecomonJsonSize='0';
 var brandi=5;    //原始json数组默认开始索引,全局变量
 var versionMax=5;//全局默认数组大小定义
 $(document).ready(function() {
		if(brandRecomonJsonSize>5&&brandRecomonJsonSize<10||brandRecomonJsonSize>10&&brandRecomonJsonSize<15||brandRecomonJsonSize>15){
			document.getElementById("changeLabel").style.display="block";
			document.getElementById("changeId").style.display="block";			  
			} 
		//全局变量下标索引，根据总数和需要展现的页面行数进行首次更改。
	    var jsonArrayLen=brandjson.length;    
	    if(jsonArrayLen>5&&jsonArrayLen<10){  //一行5个图片，循环
	    	 brandi=5;
		     versionMax=5;
		     } 
	    if(jsonArrayLen>10&&jsonArrayLen<15){ //不足三行记录,只展示两行-10个
	    	brandi=10;     
	        versionMax=10;//数组大小两行记录总数：10即可
	     } 
	    if(jsonArrayLen>15){//品牌推荐总数大于3行记录15，只展示三行，换一批从15开始，数组大小15一批
	    	brandi=15;
	    	versionMax=15;
	     }
            
		});
 /**此方式用于换一批功能，所有的图片循环依次补足动态的数组大小位；采用替换标签属性方式*/
 function changeNewBrandShow(){
	//增加插码 
	 addEventCode('JF_PPTJ_HH'); 
	//运营监控系统插码
	 eventVesopera('JFPC_TOP_PPTJ_CHANGE');
	 var brand= new Array(versionMax); //versionMax全局变量，每个模块展示图片的常量定义
	 brand=initBrandChangeArray();
     for(var k=0;k<versionMax;k++){
         var wareID= brand[k].m_back.WARE_BRAND_ID;
         var wareName=brand[k].m_back.WARE_BRAND_NAME; 
         var imgName= brand[k].m_back.WARE_BRAND_LOG_FILE;
         var tempHrefUrl =brandUrlConst+wareID+".html";  //   /portal_web/brand/100000000040450.html   
         var tempPicSrc=picUrlConst+imgName;
         //执行换一批功能的时候，去掉onclick属性，设置href的js方法。跳转到搜索列表(function 方法参数一定要有'','') 
         $("#brandurl"+(k)).removeAttr('onclick');   
         $("#brandurl"+(k)).attr("title",wareName);
       	 $("#brandurl"+(k)).attr("href","javascript:brandRecomonForSearch('"+wareName+"','"+wareID+"')"); 
   	     $("#brandimgUrl"+(k)).attr("src",tempPicSrc);  //图片处理
   	     $("#brandimgUrl"+(k)).attr("alt",wareName);  
   	     
       }   
	 }
function initBrandChangeArray(){	
    var jsonArrayLen=brandjson.length;    
    var a= new Array(versionMax); 
	var j = 0;
	while(true)
	{
		if(brandi == jsonArrayLen)
		{
			brandi = 0;
		}			
		a[j] = brandjson[brandi];  //从json数组里面，复制数据到临时数组，临时数组每次都是额定大小
		j++;
		brandi++;
		if(j == versionMax)  //临到数组复制到最大量的时候(每次展示的数量),while循环退出
		{
			j = 0;
			break;   
		}			
	}  
	return a;	
} 
   
//品牌推荐跳转到搜索页面function
function brandRecomonForSearch(brandName,brandId){ 
	//增加插码 ,多按钮时候，需要体现不同按钮值
	var N=brandName;
	addEventCode('JF_PPTJ_'+N); 
	//运营监控系统插码
	eventVesopera('JFPC_TOP_PPTJ_'+N);
	var bn=encodeURIComponent(encodeURIComponent(brandName));
	var pathContenxBrand=''; 
	var brandForSearchUrl=pathContenxBrand+"/brand/"+brandId+".html";
	windowLocationHref(brandForSearchUrl);
}

 </script>
 
<!-- 品牌推荐end -->

