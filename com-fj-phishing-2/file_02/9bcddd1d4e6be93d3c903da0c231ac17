<script type="text/javascript">   
function  moreFunction(){
	var moreUrl=''+'/common/newsList.jsp';
	addEventCode('JF_GG_MORE');
	//运营监控平台插码
	eventVesopera('JFPC_TOP_GG_MORE');
	windowLocationHref(moreUrl);
}
function detailNoticeUrl(bulletinId){
	//增加插码 
	addEventCode('JF_GG');
	//运营监控平台插码
	eventVesopera('JFPC_TOP_GG_'+bulletinId);
	var noticeConstant='notice';
	var detailNoticeUrl=''+'/'+noticeConstant+'/'+bulletinId+'.html';
	windowLocationHref(detailNoticeUrl);
}

</script>

        











   








