<script type="text/javascript">
$(function(){
	var isTimeRange = "false";
	if(isTimeRange == "true"){
		//获取cookie中的值判断是否是第一次进入
		var firstIn = $.cookie('first_in');
		//如果是第一次进入
		if(!firstIn){
			//向cookie中塞值
			$.cookie('first_in', true, { expires: 55 , path: '/'}); 
			//页面显示
			$(".AdvAll").show();
			$(".bg").show();
			//十秒钟广告关闭
			tenSecClo();
		}else{
			//广告框关闭
			$(".AdvAll").hide();
			$(".bg").hide();
		}	
	}
	
})
//跳转至兑换页
function goExchange(){
	window.location.href="/topics/cuUtcti_iteralxcane.html";
}
//十秒钟关闭页面弹层广告
function tenSecClo(){
	var second = 0;
    setInterval(function(){
   		  second++;
	      if(second >= 10){
	   	   	closeAdv();
	      }
    }, 1000);
}
//页面关闭按钮
$("#Advclose").click(function(){
	closeAdv();
});
//执行关闭
function closeAdv(){
	  var height = $("#Advindex").height();
      $("#Advindex").animate({top:-height-34},500);
      $("#Advclose").hide();
      $("#bg").hide();
}
</script>





























