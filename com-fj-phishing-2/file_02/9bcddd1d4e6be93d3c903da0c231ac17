<script type="text/javaScript">

/*为此function增加一级二级三级目录名称，作为入参用于插码按钮值区分记录*
 *此方法在一级目录，传入一级目录对应的4个参数；二级目录时候传入一级二级对应参数，三级目录时候传入一二三级目录对应参数。
 */
function searchNaviWare(onecontent,onetype,oneid,oneName,twocontent,twotype,twoid,twoName,thrcontent,thrtype,thrid,threeName){
	var type = "";
	var hreftmp = "";
	if(typeof(thrid) != 'undefined'){
		//hreftmp = "&onli="+oneid+"&twli="+twoid+"&thli="+thrid;
		hreftmp = oneid+"_"+twoid+"_"+thrid;
		if(thrtype==3){
			hreftmp = thrcontent;
			type = 3;
		}
		//增加插码功能
		if(typeof(threeName)!="undefined"){				
			addEventCode('JF_LPFL_'+threeName);
			//运营监控平台插码
	 		eventVesopera('JFPC_TOP_CATA03_'+threeName);
		}
	}else if(typeof(twoid) != 'undefined'){
		//hreftmp = "&onli="+oneid+"&twli="+twoid;
		hreftmp = oneid+"_"+twoid+"_0";
		if(twotype==3){
			hreftmp = twocontent;
			type = 3;
		}
		//增加插码功能
		if(typeof(twoName)!="undefined"){				
			addEventCode('JF_LPFL_'+twoName);
			//运营监控平台插码
	 		eventVesopera('JFPC_TOP_CATA02_'+twoName);
		}
	}else{
		//hreftmp = "&onli="+oneid;
		hreftmp = oneid+"_0_0";
		if(onetype==3){
			hreftmp = onecontent;
			type = 3;
		}
		//增加插码功能
		if(typeof(oneName)!="undefined"){				
			addEventCode('JF_LPFL_'+oneName);
			//运营监控平台插码
	 		eventVesopera('JFPC_TOP_CATA01_'+oneName);
		}
	}

	
	var url = "";
	if(type==3){
		url = hreftmp;
	}else {
		//url = ""+"/portal/ware/web/SearchWareAction?action=searchWareInfo"+hreftmp;
		url = "/category/"+hreftmp+".html";
	}
	windowLocationHref(url);
}

</script>
	<!--全部商品分类end-->
