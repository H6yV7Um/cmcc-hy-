<script type="text/javaScript">
	var user_login_status_login = "USER_LOGIN_STATUS_LOGIN";
	var user_login_status_not_login = "USER_LOGIN_STATUS_NOT_LOGIN";
    var ssoCheckUrl = "/portal/user/web/UserLoginAction?action=checkRedisUserInfo";
    var toSsoCheckUrl = "/portal/user/web/UserLoginAction?action=toSsoCheckUrl";
	if(typeof basePath =="undefined"){
		window.basePath="";
	}
	function headTopNavFuc(navInfoUrl,navName){
		var navUrl=window.basePath+navInfoUrl;
		if('TC'==navName){//如果是退出就把这个cookie里的内容删除了
			$.cookie("UASC",'',{path:'/'})
		}else{
			//插码
			addEventCode('JF_DBDH_'+navName);
			//运营监控平台插码
			eventVesopera('JFPC_TOP_'+navName);
		}
		window.location.href=navUrl;
	}
	function netFunction(navInfoUrl){
		//插码
		addEventCode('JF_DBDH_WZDH');
		window.open(navInfoUrl);
	}
	function userHelpFuc(navInfoUrl){
		var targetNavUrl=window.basePath+navInfoUrl;
		//插码
		addEventCode('JF_DBDH_KHFW');
		window.open(targetNavUrl);
	}
	function phoneFuc(navInfoUrl){
		//插码
		addEventCode('JF_DBDH_SJB');
		window.open(navInfoUrl);
	}
    var is_login = $.cookie('is_login');
    if (!!is_login){
        $.post(ssoCheckUrl, function (data) {
            data = eval('(' + data + ')');            
            if (data.isLogin == "N") {
            	var url = window.location.href;
                url = url.replace(location.protocol + "//" + location.host,"");
                window.location.href = toSsoCheckUrl + "&targetUrl=" + url
            }
        });
    }

</script>

