<script type="text/javascript" src="https://s.56qq.com/staticBase/dist/20160622/js/lib/lib.js"></script>
<script type="text/javascript">
	$(function(){
		var index = {
				init: function(){
					this._road();
					this._event();
					this._render();
				},

				_event: function(){
					var target = $('#JS_code');
					var wrap = $('#JS_warp');
					var footer = $('#JS_code_footer');

					$('#JS_code_close').on('click', function(){
						target.hide();
					})

					$('#JS_header_nav, #JS_go_login').on('click', 'a', function(){
						target.show();
					})

					$(window).on('resize', function(){
						var h = $(this).height();
						if(h <= 752){
							footer.addClass('pos');
						}else{
							footer.removeClass('pos');
						}
					});
				},

				_render: function(){
					var h = $(window).height();
					var footer = $('#JS_code_footer');
					if(h <= 752){
						footer.addClass('pos');
					}else{
						footer.removeClass('pos');
					}
				},

				_road : function() {
		            var speed = 50,
		                road1 = document.getElementById('js_road1'),
		                road2 = document.getElementById('js_road2'),
		                roadBox = document.getElementById('road_box');

		            if(!road1 || !road2){
		            	return;
		            }
		            
		            road2.innerHTML = road1.innerHTML;
		            function Marquee() {
		                if (roadBox.scrollLeft >= road1.scrollWidth) {
		                    roadBox.scrollLeft = 0;
		                }
		                else {
		                    roadBox.scrollLeft++;
		                }
		            }

		            var MyMar = setInterval(Marquee, speed);
		            roadBox.onmouseover = function () {
		                clearInterval(MyMar);
		            };
		            roadBox.onmouseout = function () {
		                MyMar = setInterval(Marquee, speed);
		            };
		        }
			}
		index.init();
	})
</script>
</body></html>