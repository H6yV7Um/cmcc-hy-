<script type="text/javascript">
    var tabIndex;

    function SelectMenu(index) {
 
        for (i = 0; i <= 8; i++) 
        {
            if (i == index){
				if (document.getElementById("SecMenu" + i) && document.getElementById("SecMenu" + i).style.display != "block"){
					document.getElementById("SecMenu" + i).style.display = "block";
					
					if(i == 0){
						document.getElementById("Menu_" + i).className = "FstMenu_on";
					}else{
						document.getElementById("Menu_" + i).className = "Menu_on";  
					}
				}
            }else{ 
	            if (document.getElementById("SecMenu" + i) && document.getElementById("SecMenu" + i).style.display != "none"){
	                document.getElementById("SecMenu" + i).style.display = "none";
	                if(i == 0 ){
	                	document.getElementById("Menu_" + i).className = "FstMenu_off";
	                }else{
	                	document.getElementById("Menu_" + i).className = "Menu_off";
	                }
	               
	            }
	             
            }

         }

    
        if(index == 0) {
            document.getElementById("SecMenu0").style.display = "block";
        }else{
        	document.getElementById("SecMenu0").style.display = "none";
        }
        
    }
    
    function HideAllMenu() 
    {
        for (i = 1; i <= 8; i++) {
            //if (i == tabIndex)
              //  continue;
            if (document.getElementById("SecMenu" + i) && document.getElementById("SecMenu" + i).style.display == "block")
                document.getElementById("SecMenu" + i).style.display = "none";
            if (document.getElementById("Menu_" + i) && document.getElementById("Menu_" + i).className != "Menu_off")
                document.getElementById("Menu_" + i).className = "Menu_off";
        }
         if (document.getElementById("SecMenu" + tabIndex))
            document.getElementById("SecMenu" + tabIndex).style.display = "block";
         else
            document.getElementById("SecMenu1").style.display = "block";
        if (document.getElementById("Menu_" + tabIndex) && document.getElementById("Menu_" + tabIndex).className != "Menu_on") 
            document.getElementById("Menu_" + tabIndex).className = "Menu_on";
    }
      
    function HideMenu(e, subMenuElementID)
    {
        if(!isMouseToSubMenu(e, subMenuElementID))
        HideAllMenu();
    } 
    
    function HideSubMenu(e, handler)
    {
        if(isMouseLeaveOrEnter(e, handler))
        {
        HideAllMenu();
        }
    } 
    

    function isMouseLeaveOrEnter(e, handler)
    {
      if (e.type != 'mouseout' && e.type != 'mouseover') return false;
       var reltg = e.relatedTarget ? e.relatedTarget : e.type == 'mouseout' ? e.toElement : e.fromElement;
       while (reltg && reltg != handler)
                 reltg = reltg.parentNode;
       return (reltg != handler);
    }

    function isMouseToSubMenu(e, subMenuElementID)
    {
     if (e.type != 'mouseout')
        return false;
     var reltg = e.relatedTarget ? e.relatedTarget : e.toElement;
     while(reltg && reltg.id != subMenuElementID)
        reltg = reltg.parentNode;
       
      return reltg;
    }

</script>

