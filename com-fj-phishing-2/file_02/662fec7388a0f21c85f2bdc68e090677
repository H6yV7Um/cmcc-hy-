<script type="text/javascript">
function secBoard(toId){
	window.location.href="/rio/index.do?action=openHome&toId="+toId+"&token=1515479598551";	
	}
	function loadLogin(){
		var url="/rio/index.do?action=loadLogin&token=1515479598551";
　　		var xmlHttp=this.createXmlHttpRequest();
        xmlHttp.open("POST",url, true);
        xmlHttp.setRequestHeader("Content-Type", "application/x-www-form-urlencoded;charset=utf-8");
		xmlHttp.send("");
		xmlHttp.onreadystatechange=processResponse;
		function processResponse(){
			if(xmlHttp.readyState==4){ 
				if(xmlHttp.status==200){ 
					var msg=xmlHttp.responseText;
					var ifLogin = msg.substring(0,1);
					var end = msg.length;
					document.getElementById("frm").innerHTML = msg.substring(1,end);
					if(ifLogin == "n"){
						showImg();
					}
				}
			}
		}
	}
	function showImg(){
		var timer = window.setTimeout("showImg()",2000);
		document.getElementById("randomImg").src="/rio/image.do";
		window.clearTimeout(timer);
	}
</script>
