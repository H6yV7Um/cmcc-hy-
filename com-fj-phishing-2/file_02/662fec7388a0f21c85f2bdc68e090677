<script type="text/javascript">
	function search_(searchType){
		var deptID = document.getElementById("hs(deptId)").value;
        var engageTYPE = document.getElementById("hs(engageType)").value;
        if ((deptID == null || deptID == "") && (engageTYPE == null || engageTYPE == "")) {
            alert("招聘机构、招聘类别不能同时为空！");
            return false;
        }
        window.location.href='/prs/jobDeptSearch.do?action=quickSearch&engageType='+engageTYPE+'&deptId='+deptID+'&searchType='+searchType+'&token=1515479598566';
	}
 	//弹出密码发送方式选择框
    function showFindPassModle() {
        window.location.href='/rio/showFindPass.do';
    }
    
    function login() {
        var userName;
        var userPwd;
        var validCode;
        var href;

        userName = document.getElementById("userName").value;
        userPwd = document.getElementById("userPwd").value;
        validCode = document.getElementById("validCode").value;
        if (userName == null || userName == "") {
            alert("请输入用户名！");
            return false;
        }

        if (userPwd == null || userPwd == "") {
            alert("请输入密码！");
            return false;
        }
        if (validCode == null || validCode == "")
        {
            alert("请输入校验码");
            return false;
        }
		var result = hex_md5(userPwd);
        document.getElementById("userPwd").value = result;
		//RSAEncode();

        href = "/rio/logon.do?action=newLoginFromHome";
        href = href + "&userName=" + userName;
        href = href + "&userPwd=" + result;
        href = href + "&validCode=" + validCode+"&token=1515479598566";

		var url=href;
　　		var xmlHttp=this.createXmlHttpRequest();
        xmlHttp.open("POST",url, true);
        xmlHttp.setRequestHeader("Content-Type", "application/x-www-form-urlencoded;charset=utf-8");
		xmlHttp.send();
		xmlHttp.onreadystatechange=processResponse;
		function processResponse(){
			if(xmlHttp.readyState==4){ 
				if(xmlHttp.status==200){ 
					var msg=xmlHttp.responseText;
					var ifLogin = msg.substring(0,1);
					var end = msg.length;
					if(ifLogin==1 || ifLogin==2 || ifLogin==3){
						window.location.href='/rio/logonjump.do?action=loginjump&clue='+msg;
					}
					else if(ifLogin==4){
						var info = msg.substring(1);
						alert(info);
						window.location.href='/rio/enterLogon.do?';
					}
					//if(msg!=1 && msg!=2 && msg!=3)
					else if(ifLogin == "n"){
						showImg();
					}
					else {
						document.getElementById("frm").innerHTML = msg.substring(1,end);
					}
				}
			}
		}
//        location.href = href;
//        document.frm.submit();
    }
    function DoKeyEnter()
    {
        if (window.event.keyCode == 13)  //Enter
        {
            login();
            window.event.returnValue = false;
        }
    }
    function showCurrentUsers() {
        lastTd.innerHTML = "";
    }
    function showLogonUsers() {
        window.open("/rio/logonUsers.do?action=getLogonUser&token=1515479598566", "", "");
    }
    
    function frmSubmit() {
        var date1;
        var date = new Date();
        var date2;
        switch (document.getElementById("daterange").value) {
            case "1":
            {
                date2 = date.getYear() + "-" + (date.getMonth() + 1) + "-" + date.getDate();
                date.setDate(parseInt(today.getDate().toString()) - 3);
                date1 = date.getYear() + "-" + (date.getMonth() + 1) + "-" + date.getDate();
                break;
            }
            case "2":
            {
                date2 = date.getYear() + "-" + (date.getMonth() + 1) + "-" + date.getDate();
                date.setDate(parseInt(today.getDate().toString()) - 7);
                date1 = date.getYear() + "-" + (date.getMonth() + 1) + "-" + date.getDate();
                break;
            }
            case "3":
            {
                date2 = date.getYear() + "-" + (date.getMonth() + 1) + "-" + date.getDate();
                date.setDate(parseInt(today.getDate().toString()) - 30);
                date1 = date.getYear() + "-" + (date.getMonth() + 1) + "-" + date.getDate();
                break;
            }
            case "4":
            {
                date2 = date.getYear() + "-" + (date.getMonth() + 1) + "-" + date.getDate();
                date.setDate(parseInt(today.getDate().toString()) - 90);
                date1 = date.getYear() + "-" + (date.getMonth() + 1) + "-" + date.getDate();
                break;
            }
        }
        document.getElementById("hs(issueTimeBegin)").value = date1;
        document.getElementById("hs(issueTimeEnd)").value = date2;

//        document.frm.submit();
        href = "/rio/jobListByBank.do?action=listJobByBank&token=1515479598566";
        href = href + "&hs(issueTimeBegin)=" + document.getElementById("hs(issueTimeBegin)").value;
        href = href + "&hs(issueTimeEnd)=" + document.getElementById("hs(issueTimeEnd)").value;

        location.href = href;
    } 
</script>
<script language="javascript" type="text/javascript">
	var player1 = new player('player');
</script> <!--/*焦点图切换JS结束*/-->
	
