<script type="text/javascript">
//提交表单
function per_submit()
{ 
  var flag = true;
  if(document.getElementById('contentKey').value=="")
  {
  alert("搜索关键词不能未空！");
  	flag = false;
  	return false;	
  }
  var a = areaServer;
  var arrMg =new Array();
  arrMg=a.split('|');
 var showContent = document.getElementById('contentKey').value;  
  for (var i = 0; i < arrMg.length-1; i++) 
  {
    var r = arrMg[i];
    if(showContent.indexOf(r)!=-1)
    {
    flag = false;
    }
  }
 if(flag == true){ 
 return true;
}
else
{
  alert("搜索关键词包含敏感词！");
  return false;
}
} 
</script>

