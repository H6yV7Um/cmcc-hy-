					<script>
						//语言切换
						function changeLang(value){
							var url = document.location.href;
							url = Com_SetUrlParameter(url, "lang", value);
							url = Com_SetUrlParameter(url, "username", document.getElementsByName("j_username")[0].value);
							location.href = url;
						}
					</script>
				</td>
			</tr>
		
		
