f;border-color:#ccc}.ui-order-dialog .ui-dialog-footer button:active,.ui-order-dialog .ui-dialog-footer button:focus,.ui-order-dialog .ui-dialog-footer button:hover{color:#333;background-color:#ebebeb;border-color:#adadad}.ui-order-dialog .ui-dialog-footer button:active{background-image:none}.ui-order-dialog .ui-dialog-footer button[disabled],.ui-order-dialog .ui-dialog-footer button[disabled]:active,.ui-order-dialog .ui-dialog-footer button[disabled]:focus,.ui-order-dialog .ui-dialog-footer button[disabled]:hover{background-color:#fff;border-color:#ccc}.ui-order-dialog .ui-dialog-footer button.ui-dialog-autofocus{color:#fff;background-color:#428bca;border-color:#357ebd}.ui-order-dialog .ui-dialog-footer button.ui-dialog-autofocus:active,.ui-order-dialog .ui-dialog-footer button.ui-dialog-autofocus:focus,.ui-order-dialog .ui-dialog-footer button.ui-dialog-autofocus:hover{color:#fff;background-color:#3276b1;border-color:#285e8e}.ui-order-dialog .ui-dialog-footer button.ui-dialog-autofocus:active{background-image:none}.ui-order-dialog .ui-popup-top-left .ui-dialog,.ui-order-dialog .ui-popup-top-right .ui-dialog,.ui-order-dialog .ui-popup-top .ui-dialog{top:-8px}.ui-order-dialog .ui-popup-bottom-left .ui-dialog,.ui-order-dialog .ui-popup-bottom-right .ui-dialog,.ui-order-dialog .ui-popup-bottom .ui-dialog{top:8px}.ui-order-dialog .ui-popup-left-bottom .ui-dialog,.ui-order-dialog .ui-popup-left-top .ui-dialog,.ui-order-dialog .ui-popup-left .ui-dialog{left:-8px}.ui-order-dialog .ui-popup-right-bottom .ui-dialog,.ui-order-dialog .ui-popup-right-top .ui-dialog,.ui-order-dialog .ui-popup-right .ui-dialog{left:8px}.ui-order-dialog .ui-dialog-arrow-a,.ui-order-dialog .ui-dialog-arrow-b{position:absolute;display:none;width:0;height:0;overflow:hidden;_color:#ff3fff;_filter:chroma(color=#FF3FFF);border:8px dashed transparent}.ui-order-dialog .ui-popup-follow .ui-dialog-arrow-a,.ui-order-dialog .ui-popup-follow .ui-dialog-arrow-b{display:block}.ui-order-dialog .ui-popup-top-left