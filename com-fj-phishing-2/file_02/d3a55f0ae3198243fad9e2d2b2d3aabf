<script>

	function getRecommendTask(){
		var timestamp = Date.parse(new Date());
		var url = "http://1.game.qq.com/app/login/index?t=" + timestamp+'&g_tk=' + getCSRFToken();
		loadScript(url,function(loaded){
			if(!loaded||!isObject(window["login_index"])){
				alert("亲爱的玩家，当前网络繁忙，请稍后再试");
				return;
			}			
			if(typeof(login_index.jData.r_task) != "undefined"){//推荐任务
				$("#rtaskImg").attr("src",login_index.jData.r_task[0].sTaskPic);		
				$("#rtaskName").html(login_index.jData.r_task[0].sTaskName);
			}									
		});		
	}
	function fetchLoginData(){
		var timestamp = Date.parse(new Date());
		var url = "http://1.game.qq.com/app/login/index?t=" + timestamp+'&g_tk=' + getCSRFToken();
		loadScript(url,function(loaded){
				if(!loaded||!isObject(window["login_index"])){
					alert("亲爱的玩家，当前网络繁忙，请稍后再试");
					return;
				}
				
				if(typeof(login_index.jData.game) != "undefined"){//我的游戏
					//console.log(login_index.jData.game);
					need("util.template",function(template){
						var t = template.parseDOMTemplate("tRecentGameList_template");
						g("tRecentGameList_content").innerHTML = t.process(login_index.jData.game);
					});
				}	

				if(typeof(login_index.jData.task) != "undefined"){//我的任务

					if(login_index.jData.task.list.length == 0){										
						$("#taskBox").hide();
						$("#unloginTaskBox").show();
						return;
					}  	
						
					need("util.template",function(template){
						
						var t = template.parseDOMTemplate("tMyTask_template");
						g("tMyTask_content").innerHTML = t.process(login_index.jData.task);
					
					});	

															
				}	
				
				if(typeof(login_index.jData.r_task) != "undefined"){//推荐任务
					need("util.template",function(template){						

						var t = template.parseDOMTemplate("rTask_template2");
						g("taskBox2").innerHTML = t.process(login_index.jData.r_task);
						
					});
				}

				
				$("#iMyTaskNum").html(login_index.jData.task.count);
					
									
			});
	}
	   
	
	milo.ready(function() {

		getRecommendTask();
		
		need("biz.login",function(LoginManager){
			LoginManager.init({
				needReloadPage:true
			});
			LoginManager.checkLogin(function(){//已登录
				LoginManager.getNickName(function(loginInfo){						
					g("login_nickname_span").innerHTML = filterXSS(loginInfo.nickName);
					//console.log(LoginManager.getUserUin());				
				});
				LoginManager.getUserFace(function(faceInfo){
					$("#login_qq_face").attr("src",faceInfo.userFace);
				});
				fetchLoginData();

				/**Scott added in 2015-06-01**/
				$("#rollbox ul li a").each(function(){//广告位添加qq号码后缀信息
					var sHref = $(this).attr("href");
					var reg = RegExp("qq=","g");
					var sNewHref = sHref.replace(reg,"qq="+LoginManager.getUserUin());
					$(this).attr('href',sNewHref);
				});
				
				/**Scott added in 2015-06-01**/
				//loginDisplayData();              
			});
		});	

		milo.addEvent(g("dologin"), "click", function() {
			need("biz.login",function(LoginManager){
				LoginManager.init({
					needReloadPage:true
				});
				LoginManager.login();
			});
			return false;
		});
		
		
		milo.addEvent(g("dologout"), "click", function() {
			need("biz.login",function(LoginManager){
				
				LoginManager.logout();
				unloginDisplayData();
			});
			return false;
		});

		
	});	

	
	
	
</script>

<script>
function gameHover(num){
	var token;
	$("#gameBox .ctn"+num+" li:gt(2)").hover(function(){
		$(this).addClass("hover");
		token = $(this);
	},function(){
		$(this).removeClass("hover");
		if(token!=null){token.removeClass("hover");}
		token = $(this);
	});
}
gameHover(1);

function setTab(index,name){
	$("#"+name+" .tab a").removeClass("curr");
	$("#"+name+" .ctn").hide();
	$("#"+name+" .tab a:eq("+(index-1)+")").addClass("curr");
	$("#"+name).find(".ctn"+index).show();
	if(name=="gameBox"){gameHover(index);}
}

function hoverEvent(objId){
	var token = $("#"+objId).find(".hover");
	$("#"+objId+" li").hover(function(){
		token.removeClass("hover");
		$(this).addClass("hover");
		token = $(this);
	},function(){
		$(this).removeClass("hover");
		if($("#"+objId+" li").find(".hover").length==0){
			token.addClass("hover");
		}
	});
}

hoverEvent("gameList");
hoverEvent("pkgList");
hoverEvent("pkgList2");
$(".g-ls-jp li").hover(function(){
	$(this).find("b,.btn-bar").show();
},function(){
	$(this).find("b,.btn-bar").hide();
});

$(".g-ls-e li").hover(function(){
	$(this).find(".tit-bar").animate({top:'126px'});
},function(){
	$(this).find(".tit-bar").animate({top:'84px'});
});

function TGDialogS(e){
    need("biz.dialog-min",function(Dialog){
        Dialog.show({
            id:e,
            bgcolor:'#000', //弹出"遮罩"的颜色，格式为"#FF6600"，可修改，默认为"#fff"
            opacity:50      //弹出"遮罩"的透明度，格式为｛10-100｝，可选
        });
    });
}
function closeDialog(){
    need("biz.dialog-min",function(Dialog){
        Dialog.hide();
    });
}

function AutoScroll(obj){
    $(obj).find("ul:first").animate({
        marginTop:"-16px"
    },500,function(){
        $(this).css({marginTop:"0px"}).find("li:first").appendTo(this);
    });
}

$(".menu dl").each(function(i){$(this).hover(function(){$(this).find("dt a").addClass("i-curr");},function(){$(this).find("dt a").removeClass("i-curr");});});

if($("body").width()<=980){
	$("#gg-box").width(1200);
	$("#rollbox li").width(1200);
}
$(window).resize(function(){
	var w = $("body").width();
	$("#gg-box").width(w);
	$("#rollbox li").width(w);
	if(w>1200){
		$("#gg-box").width(w);
		$("#rollbox li").width(w);
	}else{
		$("#gg-box").width(1200);
		$("#rollbox li").width(1200);
	}
	
});
</script>


