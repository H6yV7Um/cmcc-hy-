		$(function() {
			$(".add-favor").click(function() {
				otoPage.message("收藏失败,请使用Ctrl+D进行添加");
			});
			//显示左侧主菜单
			if ($("input#home").length > 0) {
				$("#category").addClass("now");
			} else {
				$("#category").on("mouseenter", function() {
					var $this = $(this);
					setTimeout(function() {
						$this.addClass("now");
					}, 200);
				});
				$("#category").on("mouseleave", function() {
					var $this = $(this);
					setTimeout(function() {
						$this.removeClass("now");
					}, 200);
				});
			}

		});
	</script>

