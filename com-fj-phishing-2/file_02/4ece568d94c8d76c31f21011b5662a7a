<script>
    function bindover() {
        if ($('.bql').length != 0) {
            $('.bql .qlm').each(function(idx, ele) {
                var subs = $(this).find('.qlw');
                if (subs.length != 0) {
                    subs.data('width', subs.width()).css({
                        width: 0,
                        visibility: 'visible'
                    });
                    $(this).hover(
                        function(e) {
                            subs.stop();
                            subs.animate({
                                width: subs.data('width')
                            }, 100);
                        },
                        function(e) {
                            subs.stop();
                            subs.animate({
                                width: '0'
                            }, 100);
                        }
                    );
                }
            });
            $('.bql .btp').click(function(e) {
                e.preventDefault();
                $('html,body').animate({
                    scrollTop: 0
                });
            });
        } else {
            setTimeout(bindover, 300);
        }
    }

    //处理商务通无轨迹
    function routebreak() {
        var v = ["http://www.akbchina.com/swt", "/swt", "/swt/", "/swt/?pos=r", "/swt/?pos=m", "/swt/?pos=f1", "http://www.akbchina.com/swt/", "/Include/swt/", "http://www.akbchina.com/Include/swt/", "/Include/swt", "http://www.akbchina.com/Include/swt", "/include/swt/", "/include/swt", "http://www.akbchina.com/include/swt/", "http://www.akbchina.com/include/swt", "http://webim.h.qiao.baidu.com//im/index?siteid=3690332&ucid=5169774", "http://webim.h.qiao.baidu.com//im/index?siteid=3690332&ucid=5169774"];
        for (var i = 0; i < v.length; i++) {
            $("[href='" + v[i] + "']").attr({
                "href": "javascript:void(0)",
                "target": "_self"
            }).click(function() {
                openZoosUrl();
                LR_HideInvite();
                return false;
            });
        }
    }


    (function loadGuide() {
        if (typeof jQuery == 'undefined') {

            setTimeout(loadGuide, 500);
        } else {
            bindover();
