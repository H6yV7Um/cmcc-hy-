//二级菜单显示效果
var jq = jQuery.noConflict();
jq(
function(){
jq(".head-logo-nav li").hover(
  function () {
    jq(this).find('a:first').addClass("selected");
    jq(this).find('dl').show(); 
  },
  function () {
      jq(this).find('a:first').removeClass("selected");
    jq(this).find('dl').hide(); 
  });});
