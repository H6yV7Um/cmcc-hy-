<link href="/css/global.css" rel="stylesheet" type="text/css">


<script type="text/javascript">
var jq = jQuery.noConflict();
<!--定义验证码路径 -->
var contextPath="";
<!--定义jquery validate -->
jq().ready(function() {
	jq("#loginForm").validate({
		 submitHandler: function(form) {
var username =  jq("#loginUserName").val();
   var password = jq('#loginPassword').val();
	jq.ajaxSetup({async:false});
	jq.post("/checker.do?method=encrypt",{key:username,key1:password},function(data){
			if (data){
				var datastring = data.split(".");
				jq("#loginUserName").attr("value",datastring[0]);
			}
			else{
				alert("数据错误！");
			}
			});
   jq('#loginPassword').val(MD5(password));
            form.submit();
        },
		rules:{
			loginUserName:{
				required: true
			},
			loginPassword:{
				required: true
			},
			verifyCode:{
				required: true,
				remote: "/checker.do?method=checkVerifyCode&verifyCodeName=verifyCode"
			}
		},
		messages: {
	        loginUserName: {
	            required: "请输入用户名"
	        },
	        loginPassword:{
	        	required:"请输入密码"
	        },
	        verifyCode:{
	        	required: "请输入验证码",
	        	remote: jq.format("验证码错误")
	        }
        },
        errorPlacement: function(error, element) {
			element.parent().next().remove();
			error.appendTo(element.parent());
		}
	});
});

</script>
<!--弹出登录页-->
