	<script language="javascript">  				
	function sytab(id,index){ 
	var i=0
	for (i=0;i<5;i++)
	{
		if (index == i) {	
			document.getElementById(id+i).style.display="block";
			document.getElementById(id+i+'tab').className="active"
		}else{
			document.getElementById(id+i).style.display="none";				
			document.getElementById(id+i+'tab').className=""
		}
	}	
	} 
	</script>
