$('#jToolbar .skinOpen').prepend('<a id="js_open_change_style" href="javascript:void(0);">换肤</a><span>|</span>');

new PageStyle({
    settingStyleCallback: function (data) {
       $('#js_image_from').text(data.source).parent().show();
    }
});
//搜索框
$('body').commonFixed({
  eleScrollLeft: false,
  ele: '.chinaso-search-box',
  eleTopNum: 0,
  eleAddClass: 'curJs',
  eleZindex: '9999',
  eleAddClassPar: 'search-jsFixedPar'
});
//tab导航
!function () {
  var setting = {
    eleScrollLeft: true,
    ele: '.qieHuanWrap-v3',
    eleTopNum: 52,
    eleAddClass: 'curJs',
    eleZindex: '9999',
    eleAddClassPar: 'qieHuanWrap-jsFixedPar'
  };
  if($.cookie('__WWW_CARD__DISSTATUS__') === 'true'){
    setting.eleHolderHeight = 40;
    setting.elesOffsetTop = 339;
  }
  $('body').commonFixed(setting);
}();


// Statistics
(function() {
  var log = document.createElement("script");
  log.src = "//n2.static.pg0.cn/www/common/base/js/statistics.js";
  var s = document.getElementsByTagName("script")[0]; 
  s.parentNode.insertBefore(log, s);
})();

// Statistics Bridge
(function () {
  var $form = $('#flpage');
  var $input = $form.find('#q');
  $form.on('submit', function () {
    if (window.StatLog) {
      window.StatLog.search($input[0], '5993a950ec6e0f76098ffa6c');
    }
  }).on('click', 'li', function () {
    if (window.StatLog) {
      window.StatLog.search($input[0], '5993a950ec6e0f76098ffa6c');
    }
  });
})();
$(function(){
  //添加公共链接
$('#jToolbarHomepage').after('<span>|</span><a href="http://promotion.chinaso.com/xwdt/detail/20171110/1000200032903301510306338629848974_1.html" style="color:#e3000b" target="_blank" >中国搜索公告</a>');
})
</script>

