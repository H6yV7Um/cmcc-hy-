<script>
   
	$(".change li").hover(function(){
		var id = $(this).attr("id");
		$(this).parents(".wall2").next(".secmenu").find("nav").siblings().hide();
		$("#child"+id).show();
	});
	$(".secmenu nav").hover(function(){		
		var svn = $(this).attr("svn");
		$("#"+svn).addClass("on");
	},function(){
		var svn = $(this).attr("svn");
		$("#"+svn).removeClass("on");
		$(this).hide();
	});
</script>
