<script type="text/javascript">
    // 顶部幻灯
    new Swiper(".j-slideBox", {
        pagination: ".swiper-pagination",
        paginationClickable: ".swiper-pagination",
        nextButton: ".swiper-button-next",
        prevButton: ".swiper-button-prev",
        spaceBetween: 30,
        effect: "fade",
        // autoplay:3000,
        speed:1500,
        loop:true,
        onSlideChangeStart:function(swiper){
            var activitySwiper=swiper.slides[swiper.activeIndex];
            for(var i=0,n=swiper.slides.length;i<n;i++){
                if(i!=swiper.activeIndex){
                    $(swiper.slides[i]).removeClass("show");
                }
            }
            setTimeout(function(){
                $(activitySwiper).addClass("show");
            },800)
        }
    });
    var $indexBanner=$(".m-index-banner");
    var $slideBoxBtn=$(".j-slideBox .btn");
    $indexBanner.on("mouseenter",function(){
        $slideBoxBtn.removeClass("hide");
    })
    $indexBanner.on("mouseleave",function(){
        $slideBoxBtn.addClass("hide");
    })
    // 游戏专区盒子
    var $gameBtns=$(".j-game-btns");
    var $gameBtnBg=$gameBtns.find(".btn-bg");
    $gameBtns.on("mouseenter",".btn",function(){
        var $this=$(this);
        var offset=$this.position();
        $this.addClass("sel").siblings().removeClass("sel");;
        $gameBtnBg.css({top:offset.top});
    })
    // 游戏新闻
    new Swiper(".j-news-banner", {
        pagination: ".swiper-pagination",
        paginationClickable: ".swiper-pagination",
        nextButton: ".swiper-button-next",
        prevButton: ".swiper-button-prev",
        spaceBetween: 30,
        loop:true
    });
    // 活动banner

    // 游戏资讯
    new Swiper(".j-slideNews", {
        pagination: ".swiper-pagination",
        paginationClickable: ".swiper-pagination",
        nextButton: ".swiper-button-next",
        prevButton: ".swiper-button-prev",
        spaceBetween: 23,
        slidesPerView:3,
        slidesPerGroup:3,
        loop:true,
        autoplay:4000,
        speed:1300,
    });
    // 游戏
    var gameSlideNav=new Swiper(".j-gameNav", {
        slidesPerView: "auto",
        paginationClickable: true,
        spaceBetween: 20,
        loop:true
    });
    $(".j-gameNav-next").on("click",function(){
        gameSlideNav.slideNext();
    })
</script>

