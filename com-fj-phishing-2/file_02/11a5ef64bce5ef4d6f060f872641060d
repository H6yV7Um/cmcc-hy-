<a href="http://www.jxmall.com/promotion/20171128180642/0.html" id="headbanner" class="head_ad" target="_blank"><img alt="精品农家菜" src="http://www.jxmall.com/pic/advert/2017/12/1512096661611.jpg"></a>
<script type="text/javascript">
$(function(){
$.ajax({
	url:path + '/index/index/loadHeadBanner.jhtml',
	cache:false, 
	success:function(msg){
		var headbanner = $('#headbanner');
		if(msg.result==false){
			headbanner.remove();
		} else {
			var img = '';
			if(msg.advance.type=='1'){
				if(msg.advance.spareOpenModel == '10'){
					headbanner.attr('target','_blank');
				}
				if(msg.advance.spareHttpAddr.indexOf("http")>-1){
					headbanner.attr('href',msg.advance.spareHttpAddr);
				} else {
					headbanner.attr('href',path + '/' + msg.advance.spareHttpAddr);
				}
				img = '<img alt="'+msg.advance.spareTitle+'" src="'+resPath+'/'+msg.advance.sparePictureFile+'"/>';	
			}else{
				if(msg.advance.openModel == '10'){
					headbanner.attr('target','_blank');
				}
				if(msg.advance.httpAddr.indexOf("http")>-1){
					headbanner.attr('href',msg.advance.httpAddr);
				} else {
					headbanner.attr('href',path + '/' + msg.advance.httpAddr);
				}
				img = '<img alt="'+msg.advance.title+'" src="'+resPath+'/'+msg.advance.pictureFile+'"/>';	
			}
			headbanner.html(img);
		}
	}
});
});
</script>
