<script>
/*登录*/
$(".nav_last_login").bind('click',function(){
	$(".tnav_ul").toggle();
	$(this).toggleClass("change_bbg");
});
/*底部图标变化*/
$(".hzimgshow img").hover(function(){
	var nSrc=$(this).attr('data');
    var oSrc=$(this).attr('src');
	$(this).attr('src',nSrc);
	$(this).attr('data',oSrc);
},function(){
	var oSrc=$(this).attr('data');
    var nSrc=$(this).attr('src');
	$(this).attr('src',oSrc);
	$(this).attr('data',nSrc);
});
</script>


