	$(".quote-notice").hover(function(){                               // 维修师抢单通知
		var _this = $(this);
		var type  = _this.attr('data-type');
		$.post('/Index/loadUserOffer', {'type':type}, function(d){
			var data = JSON.parse(d);
			if(data || data != ''){
				if(type == 'repair'){
					var html = '';
					for(var a = 0;a < data.length;a++){
						html += '<li>';
						html += '	<a href="http://www.caobao.com/offer/'+data[a]['order_id']+'-'+data[a]['shop_id']+'.html" class="clearfix">';
