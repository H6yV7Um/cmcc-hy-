						html += '		</div>';
						html += '		<div class="content">';
						html += '			<div class="user-need">';
						if(data[a]['order_type'] == 1){
							html += '			<p>用户<span>'+data[a]['uname']+'</span>发表了一条维修订单</p>';
						}else{
							html += '			<p>用户<span>'+data[a]['uname']+'</span>发表了一条回收订单</p>';
						}
						html += '			</div>';
						html += '			<div class="need-detial clearfix">';
						html += '				<p>【详情】'+data[a]['details']+'</p>';
						html += '				<time>'+data[a]['show_time']+'</time>';
						html += '			</div>';
						html += '		</div>';
						html += '	</a>';
						html += '</li>';
					}
				}else{
					var html = '';
					for(var a = 0;a < data.length;a++){
						var url = "http://www.caobao.com/repair/server/id/"+data[a]['order_id']+".html";
						if(data[a]['order_type'] == 2){
							var url = "http://www.caobao.com/recovery/server/id/"+data[a]['order_id']+".html";
						}
						html += '<li>';
						html += '	<a href="'+url+'" class="clearfix">';
