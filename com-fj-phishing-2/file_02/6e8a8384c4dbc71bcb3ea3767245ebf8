							var x_ckj = '',
								_titles='';

							$.ajax({
								type: "get",
								url: "http://finance.qq.com/crossapi/finance/15365_v1.html",
								dataType: "jsonp",
								jsonp: "callback",
								jsonpCallback: "crossCallback_15365_v1",
								success: function(data) {
									
									for(var t = 0; t < data.data.length; t++) {
										var sy= decodeURIComponent(data.data[t].title);
										_titles = sy.length > 20 ? sy.substr(0, 18)+'...' : sy;
										if(t == 0) {
											x_ckj += '<li class="current"><h3 class="bg0"><a href="' + data.data[t].url + '" target="_blank">' + _titles+ '</a></h3>';
											x_ckj += '<div class="picbox"><a href="' + data.data[t].url + '" target="_blank">' + '<img alt="' + _titles + '" src="' + data.data[t].image + '">';
											x_ckj += ' </a></div></li>'
										} else {
											x_ckj += '<li><h3 class="bg0"><a href="' + data.data[t].url + '" target="_blank">' + _titles + '</a></h3>';
											x_ckj += '<div class="picbox"><a href="' + data.data[t].url + '" target="_blank">' + '<img alt="' + _titles + '" src="' + data.data[t].image + '">';
											x_ckj += ' </a></div></li>'
										}

										$('.x_ckj').html(decodeURIComponent(x_ckj));
									}
								}
							});
