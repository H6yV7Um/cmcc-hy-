		<script type="text/javascript">
			function focus_a_dot() {
				var dot_html = '',
					$focus = $('#focus_01'),
					$item = $focus.find('.item'),
					$dot = $focus.find('.focus_dot');
				for(var i = 0, len = $item.length; i < len; i++) {
