<script type="text/javascript">
	(function($){
	/*
	direction: 方向 up left right down
	speed: 滚动速度
	gap: 间隔滚动时间 ms
	*/
	$.fn.hScroll = function(options){
		var _default = {
				direction : 'left',
				speed : 100,
				gap : 500
			},
			$this = $(this),
			li_width = [],
			ul_width = 0,
			timer = null;

		$.extend(_default, options);

		init();
		
		function init(){
			var $ul = $this.children('ul'),
				$li = $ul.find('li'),
				width_s = 0;

			$li.each(function(){
				var w = $(this).outerWidth(true);
				width_s += w;
				li_width.push( w );
			});
			width_s += 2;
			$ul.css('width', width_s);
			ul_width = width_s;

			// 只有内容宽度超过显示的宽度时，才进行轮播
			if( ul_width > $this.width() ){
				$this.html( '<div class="clearfix cons" style="width:'+ (width_s*2) +'px">'+ $ul[0].outerHTML + $ul[0].outerHTML +'</div>' );
				start();
				addEvent();
			}
		}

		function scroll(){
			var $cons = $this.children('.cons');

			var left = parseInt( $cons.css('left') );

			if( left <= 0-ul_width ){
				$cons.css('left', 0);
			}else{
				left--;
				$cons.css('left', left+'px');
			}
		}

		function start(){
			stop();
			timer = setInterval(scroll, 26);
		}

		function stop(){
			clearInterval( timer );
		}

		function addEvent(){
			$this.on('mouseenter', function(){
				stop();
			}).on('mouseleave', function(){
				start();
			})
		}
	}
})(jQuery);

$(function(){
	'use strict';
	// author: skeetershi
	
	var tool = {
		// 设置z-index值
		setZIndex : function(){
			$('.list').find('.item').each(function(index){
				$(this).css('z-index', 1010-index);
			})
			$('.list').find('.Q-tpList, .Q-pList').each(function(index){
				$(this).css('z-index', 500-index);
			})
		},

		// 获取滚动数据
		getScrollData : function(){
			$.ajax({
				url: 'http://roll.finance.qq.com/interface/roll.php?site=finance&page=1&mode=1&of=json',
				dataType: 'json',
				type: 'get'
			}).done(function(result){
				console.log( result );
			})
		},

		// 获取lighting数据
		getLightData : function(){
			$.ajax({
				url: 'http://finance.qq.com/crossapi/finance/6798.html',
				dataType: 'jsonp',
				type: 'get',
				jsonpCallback: 'success_crossapicallback_6798'
			}).done(function(result){
				if( result.code==='0' ){
					var list = result.itemContent.dataList,
						html = '';

					for(var i=0; i<4; i++){
						var cls = i==0 ? 'current' : '',
							item = list[i],
							title = decodeURIComponent(item.title),
							_title = title.length > 20 ? title.substr(0, 18)+'...' : title;
						html += '<li class="'+cls+'">\
									<h3 class="bg0">\
										<a href="'+ decodeURIComponent(item.link) +'" target="_blank" title="'+title+'">'+ _title +'</a>\
									</h3>\
									<div class="picbox">\
										<a href="'+ decodeURIComponent(item.link) +'" target="_blank">\
											<img alt="'+ decodeURIComponent(item.title) +'" src="'+ decodeURIComponent(item.image) +'"></img>\
										</a>\
									</div>\
								</li>';
					}

					$('.lighting ul').html( html );
				}
			})
		},
		//获取财约你数据
		getCaiyueniData : function(){
			$.ajax({
				url: 'http://finance.qq.com/crossapi/finance/12243.html',
				dataType: 'jsonp',
				type: 'get',
				jsonpCallback: 'crossCallback_12243'
			}).done(function(result){
				//if( result.code==='0' ){
					var list = result.data,
						html = '';

					for(var i=0; i<4; i++){
						var cls = i==0 ? 'current' : '',
							item = list[i],
							title = decodeURIComponent(item.title),
							_title = title.length > 20 ? title.substr(0, 18)+'...' : title;
						html += '<li class="'+cls+'">\
									<h3 class="bg0">\
										<a href="'+ decodeURIComponent(item.url) +'" target="_blank" title="'+title+'">'+ _title +'</a>\
									</h3>\
									<div class="picbox">\
										<a href="'+ decodeURIComponent(item.url) +'" target="_blank">\
											<img alt="'+ decodeURIComponent(item.title) +'" src="'+ decodeURIComponent(item.image) +'"></img>\
										</a>\
									</div>\
								</li>';
					}

					$('.caiyueni ul').html( html );
				//}
			})
		},
		// 获取智图派数据
		getZhituData : function(){
			$.ajax({
				url: 'http://finance.qq.com/crossapi/finance/14132.html',
				dataType: 'jsonp',
				type: 'get',
				jsonpCallback: 'crossCallback_14132'
			}).done(function(result){
				var list = result.data,
					html = '',
					_len = list.length,
					len = _len>4 ? 4 : _len;

				for(var i=0; i<len; i++){
					var cls = i==0 ? 'current' : '',
						item = list[i],
						title = decodeURIComponent(item.title),
						image = decodeURIComponent(item.image),
						link = decodeURIComponent(item.url),
						_title = title.length > 20 ? title.substr(0, 18)+'...' : title;
					html += '<li class="'+cls+'">\
								<h3 class="bg0">\
									<a href="'+ link +'" target="_blank" title="'+title+'">'+ _title +'</a>\
								</h3>\
								<div class="picbox">\
									<a href="'+ link +'" target="_blank">\
										<img alt="'+ title +'" src="'+ image +'"></img>\
									</a>\
								</div>\
							</li>';
				}

				$('.zhitupai ul').html( html );
			})
		},

		// 获取视频数据
		getVideoData : function(){
			$.ajax({
				url: 'http://data.v.qq.com/videocms/getNewsvideoList.php?ref=pclient&appkey=6UkwV9DeHr9_PC&vsite=finance&ename=finance_jpjm',
				dataType: 'jsonp',
				type : 'get'
			}).done(function(result){
				if( result.response.code == 0 ){
					var content = result.data.content,
						html = '',
						len = 7;

					for(var i=0; i<len; i++){
						var item = content[i];
						if( i==0 ){
							html += '<li class="first">\
										<a href="'+item.link_addr+'" target="_blank">\
											<img src="'+item.pic_url+'" alt="'+item.subtitle+'">\
											<div class="overlay">\
												<div class="bg"></div>\
												<div class="cc">\
													<div class="icon"></div>\
													<div class="info">\
														<p class="t">'+item.title+'</p>\
														<p>'+item.subtitle+'</p>\
													</div>\
												</div>\
											</div>\
										</a>\
									</li>';
						}else{
							var play_num = item.play_times ? '<em>'+item.play_times.all+'</em>' : '';
							html += '<li>\
										<a href="'+item.link_addr+'" target="_blank" class="pic">\
											<img src="'+item.pic_url+'" alt="'+item.subtitle+'">\
											<span class="figure_mask">'+item.title+'</span>\
										</a>\
										<a href="'+item.link_addr+'" target="_blank" class="txt">'+item.subtitle+'</a>'+play_num+'\
									</li>';
						}
					}

					$('.video ul').html( html );
				}
			})
		},

		// 设置关键词
		setKeywords : function(){
			$('.keywords').each(function(){
				var text = $.trim( $(this).text() ),
					arr = text.split(';'),
					t=arr.length,
					max = t>3 ? 3 : t,
					html = '';

				for(var i=0; i<max; i++){
					var s = $.trim(arr[i]);
					if( s ){
						html += '<a href="http://finance.qq.com/dc_column_article/TagsList.htm?tags='+ encodeURIComponent(s) +'" target="_blank">'+s+'</a>';
					}
				}
				$(this).html( html ).show();
			})
		},

		// 腾讯微博的分享
		tqqShare : function(){
			$('.s_weibo').each(function( index ){
				$(this).prop('id', index+1);
			})
			$(".s_weibo").each(function(){ 
				_MI.ShareArticle2.build($(this).attr("id"));		
			});
		},

		// 设置时间格式并滚动
		hScroll : function(){
			$('.hscroll .con').hScroll();
		},
		
		// 设置右侧的广告位
		setSideAd : function(){
			var $focus_03 = $('#focus_03'),
				$l_qq_com = $('#focus_03 .l_qq_com'),
				$focus_02 = $('#focus_02');
				
			var timer = setInterval(function(){
				if( $l_qq_com.html() != "" ){
					$focus_02.html( $focus_03.html() );
					
					clearInterval( timer );
				}
			}, 100);
		},

		init : function(){
			this.hScroll();
			this.setZIndex();
			// this.getScrollData();
			this.setKeywords();
			this.getLightData();
			this.getZhituData();
			this.getCaiyueniData();
			this.getVideoData();
			this.tqqShare();
			this.setSideAd();

			$(".remenzhuanti .bd").mCustomScrollbar({
				theme:"minimal-dark",
				autoHideScrollbar : true
			});
		}
	}

	tool.init();

	// 分享按钮
	$('.list').on('mouseover', '.shareBtn', function(){
		$(this).addClass('shareBtnHover').children('.shareBtn16').show();
	}).on('mouseout', '.shareBtn', function(){
		$(this).removeClass('shareBtnHover').children('.shareBtn16').hide();
	}).on('click', '.more', function(e){
		// 点击查看更多

		var $this = $(this),
			$list = $this.parents('.list'),
			$med = $list.find('.med[ft=1]'),
			$med_0 = $list.find('.med[ft=0]'),
			$section = $med.find('.section'),
			$section_0 = $med_0.find('.section'),
			len = 3,
			page = parseInt( $this.attr('page') || 0 ),
			ftop = $list.offset().top;

		if( page<len-1 ){
			page++;

			for(var i=0; i<page; i++){
				$section_0.eq(i).hide();
			}
			$section_0.eq( page ).css({'height':'auto', 'overflow':'visible'});
			
			var $current = $section.eq(page-1),
				$next = $section.eq(page),
				height = $med.height();

			$current.css('overflow', 'hidden').animate({height:0}, 500, function(){
				$(this).hide();
			});
			$next.animate({height:height}, 500, function(){
				$(this).css('overflow', 'visible');
			});
			$this.attr('page', page);
			e.preventDefault();
			
			if(page==len-1){
				$this.prop('href', $this.attr('lazysrc')).text( $this.attr('lazytext') );
			}

		}
	}).on('click', '.backfirst', function(){
		var $this = $(this),
			$more = $this.next('.more'),
			$list = $this.parents('.list'),
			$section = $list.find('.section'),
			page = parseInt( $more.attr('page') || 0 );
			
		if( page > 0 ){
			$section.removeAttr('style');
			$more.attr('page', '0').text('点击查看更多');
			setTimeout(function(){
				$list.find('.section_0').css('overflow', 'visible');
			}, 20);
		}
	});
})


