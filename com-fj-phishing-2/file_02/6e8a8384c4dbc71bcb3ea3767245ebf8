		<script type="text/javascript">
			var flag = true;

			loadWideCss();

			function loadWideCss() {
				var $ww = jQuery(window).width();
				if($ww > 1280) {
					jQuery("body").addClass("w1240");
					$('.media_1240').show().attr('ft', 1);
					$('.media_1000').hide().attr('ft', 0);
				} else if($ww <= 1280) {
					jQuery("body").removeClass("w1240");
					$('.media_1000').show().attr('ft', 1);
					$('.media_1240').hide().attr('ft', 0);
				}
			}
		</script>

