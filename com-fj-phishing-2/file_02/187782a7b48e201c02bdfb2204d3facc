<script type="text/javascript">
    function loginWin(url,name,operator){
    if(operator){
        var win=window.open(url,name,'menubar=no,toolbar=no,location=no,directories=no,status=yes,scrollbars=yes,resizable=yes,width='+(window.screen.availWidth-8)+',height='+(window.screen.availHeight-50)+',top=0,left=0');
    }else{
        window.open(url,name);
    }
}
/*
function menuStyle() {
    var submenu = document.getElementById("sub_nav").getElementsByTagName("li");
    for(var i = submenu.length; --i >= 0;) {
        submenu[i].onmouseover = function () {
            this.style.backgroundColor = "#f0f0f0";
        };
        submenu[i].onmouseout = function () {
            this.style.backgroundColor = "#64a0ff";
        };
    }
}
*/
function closeWin() {
    window.opener = null;
    window.close();
}

function gogo(){
		var leftTr1=document.getElementById("leftTr1");
		var leftTr2=document.getElementById("leftTr2");
		
		var par1=document.getElementById("par1");
		var par2=document.getElementById("par2");
		
		//alert("**");
		if(par1!=null)
			par1.style.display='';
		if(par2!=null)	
			par2.style.display='none';
		if(leftTr1!=null){
			leftTr1.onmouseover = function () {
		    	  	this.style.cursor = "hand";
				 leftTr1.className="leftTr1";
				 leftTr2.className="leftTr2";
			
			  	  par1.style.display = ""; 
		   		  par2.style.display = "none";
		  	}
			leftTr1.onmouseout = function () {
				leftTr1.className="leftTr2";
				leftTr2.className="leftTr1";
				if(par1.style.display!= 'none')
				{
					leftTr1.className="leftTr1";
				 	leftTr2.className="leftTr2";
				}
				if(par2.style.display != 'none'){
					leftTr1.className="leftTr2";
					leftTr2.className="leftTr1";
				}
					
				
			}
		}
		if(leftTr2!=null){
			leftTr2.onmouseover = function () {
		    	  this.style.cursor = "hand";
				 leftTr1.className="leftTr2";
				 leftTr2.className="leftTr1";
			  	  par2.style.display = ""; 
		   		  par1.style.display = "none";
		  	}
			leftTr2.onmouseout = function () {
				leftTr1.className="leftTr1";
				 leftTr2.className="leftTr2";
				if(par1.style.display!= 'none')
				{
					leftTr1.className="leftTr1";
				 	leftTr2.className="leftTr2";
				}
				if(par2.style.display != 'none'){
					leftTr1.className="leftTr2";
					leftTr2.className="leftTr1";
				}
			}
		
		}		
	}	

</script>
</head>

<!--body onload="menuStyle();"-->
<body onload="gogo();">
