            function changeBcsearch(id) {
                switch (id) {
                    case 1:
                        $('#fl1').addClass('bcshover');
                        $('#fl2').removeClass('bcshover');
                        $('#list-bcform1').removeClass('hide');
                        $('#list-bcform2').addClass('hide');
                        break;
                    case 2:
                        $('#fl1').removeClass('bcshover');
                        $('#fl2').addClass('bcshover');
                        $('#list-bcform1').addClass('hide');
                        $('#list-bcform2').removeClass('hide');
                        break;
                }
            }
        </script>
         
