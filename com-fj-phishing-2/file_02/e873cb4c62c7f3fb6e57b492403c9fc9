	function setTab02Syn ( i )
	{
		selectTab02Syn(i);
	}
	
	function selectTab02Syn ( i )
	{
		switch(i){
			case 1:
			document.getElementById("TabTab02Con11").style.display="block";
			document.getElementById("TabTab02Con22").style.display="none";
			
			document.getElementById("font11").style.color="#ff0000";
			document.getElementById("font22").style.color="#000000";
			
			
			break;
			case 2:
			document.getElementById("TabTab02Con11").style.display="none";
			document.getElementById("TabTab02Con22").style.display="block";
			
			document.getElementById("font11").style.color="#000000";
			document.getElementById("font22").style.color="#ff0000";
			
			
			break;
			
		}
	}

</script>

