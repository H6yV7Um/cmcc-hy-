function searchPro(){
			if(document.getElementById('key').value==''){
				alert("请输入关键字");	
			}else{
				//window.open('http://so.jinmajia.com/tProcess.jsp?key='+document.getElementById('key').value+'&input3.x=35&input3.y=11&pageSize=40&orderFlag=0&select=项目名称',"国资项目搜索");
			var prourl = 'http://so.jinmajia.com/tProcess.jsp?';
			var qString ='mykeys='+document.getElementById('key').value+'&input3.x=35&input3.y=11&pageSize=40&orderFlag=0&select=项目名称'
			qString = encodeURI(qString);
			prourl = prourl + qString;
			document.getElementById('gzform').action=prourl;
			document.getElementById('gzform').submit();	
			return false;				
			}
}
</script>
 
 
