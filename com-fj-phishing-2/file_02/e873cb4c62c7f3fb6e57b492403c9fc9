	function setTab05Syn (n,url)
	{
		for(var i=1;i<=2;i++ ){
			document.getElementById("bjsgg"+i).style.display="none"; 
			document.getElementById("fontgg"+i).style.color="#000000";
			document.getElementById("bjsgg"+i).className="";
		}
		document.getElementById("bjsgg"+n).style.display="block";
		document.getElementById("fontgg"+n).style.color="#ffffff";
		document.getElementById("morebjsgg").href="http://www.cbex.com.cn/article/ggyts/"+url;
//ss		alter(document.getElementById("more").href);
		document.getElementById("gsgg").className="bjdt"+n;
		document.getElementById("bjsgg"+n).className="TabTab04Con";
	}
</script>
  
