	function setTab04Syn (n,url)
	{
		//默认进来时，全部div没有样式
		for(var i=1;i<=3;i++ ){
			document.getElementById("bjsdtdiv"+i).style.display="none"; 
			document.getElementById("font"+i).style.color="#000000";
			document.getElementById("bjsdtdiv"+i).className="";
		}
		//根据所传的参数，确定给某个div赋值
		document.getElementById("bjsdtdiv"+n).style.display="block";
		document.getElementById("font"+n).style.color="#ffffff";
		document.getElementById("morebjsdt").href="http://www.cbex.com.cn/article/xxpd/"+url;
		document.getElementById("bgdt").className="bjdt"+n;
		document.getElementById("bjsdtdiv"+n).className="TabTab04Con";
	}
</script>
  
