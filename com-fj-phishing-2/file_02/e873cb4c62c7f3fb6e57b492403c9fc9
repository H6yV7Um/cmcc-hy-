 <script>
function gofrnd(o)
{	
    var fObj = document.all.item(o);
	if(fObj==null)
		return;
	var fl = fObj.options[fObj.selectedIndex].value;
	if(fl!='0')
	{
		fl= fl;
		window.open(fl);
		}
	}
</script>



