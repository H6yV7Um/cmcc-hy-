					document.getElementById("xmfb"+i).className="tabContainer1";
				} 
				document.getElementById("lixmfb"+n).style.display="block";
				document.getElementById("xmfb"+n).className="tabContainer2";
				
			}
			var numxmfb = 0;
			//循环
			function loopXmfb(){
				numxmfb++;
				if(numxmfb==10){
					numxmfb=0;
				}
				showxmfb(numxmfb);
				
			}
        </script>
		<div class="xmfb"><img src="/images/xmfb.jpg" width="730" height="47" border="0"></div>
