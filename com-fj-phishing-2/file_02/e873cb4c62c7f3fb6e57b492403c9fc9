	function showZhuanlan(n){
		for(i=1;i<14;i++){
			document.getElementById("content"+i).style.display="none";
			document.getElementById("contenttd"+i).className="changebordercolor1";
		}
		document.getElementById("content"+n).style.display="block"
		document.getElementById("contenttd"+n).className="changebordercolor2";
		
	}

