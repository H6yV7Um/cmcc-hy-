	function setZptSyn(n,url){
		
		for(var i=1;i<=6;i++){
			document.getElementById("zpd0"+i).style.display="none";
		}
		document.getElementById("zpd0"+n).style.display="block";
		document.getElementById("zpd0"+n).className="zptprolist";
		document.getElementById("zptmore").href = url;
		document.getElementById('zpddhdiv').className='zptstyle'+n;
	
	}

</script>

