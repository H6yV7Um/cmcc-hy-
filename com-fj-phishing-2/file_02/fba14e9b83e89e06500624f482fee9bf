<script type="text/javascript">
	$(document).ready(function(){
		var userLoginId = getUserLoginId();
		srvMap.add("cart-count", "", "front/sh/shopCart!queryCountByCustId?custId=8888&uid=sc002");
	    srvMap.add("user-count", "../../assets/data/order/all.json","front/sh/order!querySubOrderCountForWeChat?uid=o004&odrStsCd=1042");
		if(userLoginId){
			Util.ajax.postJson(srvMap.get("cart-count"), "", function(data, flag){
				if(data && data.bean && data.bean.totalMcds){
					var count = data.bean.totalMcds;
					if(count>0){
						$('#J_cart_count').html(count).show();
					}
				}
			});
		}

		
		//生成一个11开头的19位的用户随机唯一标识
		var phoneNum = userLoginId || "11"+Math.random()*100000000000000000;
		var cityNm = "";//市名称
		var provinceCode = "";//省编码
		var province = "";//省名称
		var cityCode = "";//市编码
		var urlParams = "";//加密参数
		srvMap.add("onlineServiceUrlEncode", "", "front/sh/active!onlineServiceUrlEncode?uid=x001");//在线客服url参数加密接口
		srvMap.add("queryAddrByTel","","front/sh/login!queryAddrByTel?uid=w001");//获取省级代码和市名字
		Util.ajax.postJsonSync(srvMap.get("queryAddrByTel"), {"tel_num":phoneNum}, function(data, flag) {
			if(data.returnCode == 0){
				//成功
				province = data.bean.province;
				cityNm = data.bean.cityNm;
			} else {
				Util.Tips.warning("系统异常，请刷新后重试！", 'bottom', true);
				return false;
			}
		});
		Util.ajax.postJsonSync("/web85/assets_data/data/region/online-service-prov-code.json", "", function(data, flag) {//在线客服省代码json
			if(data){
				if(data[province]){
					provinceCode = data[province];
				}else{
					provinceCode = "000";//不存在写000
				}
			}			
		});
		Util.ajax.postJsonSync("/web85/assets_data/data/region/online-service-region-code.json", "", function(data, flag) {//在线客服市代码json
			if(data){
				if(data[cityNm]){
					cityCode = data[cityNm];
				}else{
					cityCode = "000";//如不存在 写000
				}
			}
		});
		var urlEncodeParams = {
			"phone" : phoneNum,
			"source" : "110407",
			"cc" : cityCode,
			"pc" : provinceCode
		}
		Util.ajax.postJsonSync(srvMap.get("onlineServiceUrlEncode"), urlEncodeParams, function(data, flag) {//获取加密参数
			if(data.bean){
				urlParams = data.bean.urlParams;
				$('#head_kefu').attr('href','http://kf.cmcc-cs.cn:20980/ngmmgw/channelapi/web/index/'+data.bean.urlParams);
				$('.borderNone a').attr('href','http://kf.cmcc-cs.cn:20980/ngmmgw/channelapi/web/index/'+data.bean.urlParams);
			}
		});
		

		//判断用户是否登陆，登陆后页面公共头部变化
	    if(userLoginId!=''){
	    	if($("#common_header").length>0){
		        $("#common_header").addClass("web_loginIn");
		        var headerHandle = Handlebars.compile($("#common_header_template").html());
		        userLoginId = userLoginId.substring(0,3)+"****"+userLoginId.substring(7,11);
		        var user = {
		            "userLoginId" : userLoginId
		        };
		        var common_header_content = headerHandle(user);
		        $(".web_index_top_r").html(common_header_content);
				//登陆成功 为公共头文件接入客服功能(已登陆情况下)
				$('#J_in_headkf').attr('href','http://kf.cmcc-cs.cn:20980/ngmmgw/channelapi/web/index/'+urlParams);
	    	}
	    }


	    //退出登陆功能
	    srvMap.add("quitseesion","","front/sh/login!initSession?uid=w001");
	    $("#quitBtn").click(function(){
	        Util.ajax.postJson(srvMap.get("quitseesion"),"uid=w001",quit);
	    });

	    function quit(data,flag){
	        if(data!=null){
	            if(data.state=='0'){
	                //alert('退出成功');
	                Util.Tips.success("退出成功", 'bottom', true);
	                //退出成功，清除用户记住密码localstorage中信息
	                localStorage.removeItem('cookieUserName');
	                localStorage.removeItem('encryPassword');
	                localStorage.removeItem('cookieUuid');
	                //清除sessionStorage中returnUrl
	                sessionStorage.removeItem('returnUrl');
	                document.location.href="../index/index.html";
	            }else{
	                //alert('退出失败，请重新退出');
	                Util.Tips.warning("退出失败，请重新退出", 'bottom', true);
	            }
	        }
	    }


	    //公共头部商品搜索
	    function globalsearch() {
	        var search = $("#J_search_input").val();
	        var type = GetQueryString('type');
	        var id = GetQueryString('id');
	        var param = "";
	        if(type!=null){
	            param += "&type="+type;
	        }
	        if(id!=null){
	            param += "&id="+id;
	        }
	        var searchName = $('#J_search_input').attr('placeholder');
	        console.log(searchName);
	        if(searchName=='商户入驻专区'){
	        	window.location.href = '../search/searchESB-filter.html?search=' + escape(search) + param;        	
	        }else{
	        	window.location.href = '../search/search-filter.html?search=' + escape(search) + param;	        	
	        }	        
	    }
	    //URL中搜索参数放入搜索输入框中
	    var searchContent = GetQueryString('search');
	    if(searchContent != null || searchContent != ''){
	    	JPlaceHolder.init(1); 
	    }
	    $('#J_search_input').val(searchContent)
	    $('#J_search_form').submit(function() {
	        globalsearch();
	        var searchName = $('#J_search_input').val();
	        if(searchName!==''&&searchName!==null&&searchName!==undefined){
	        	 Util.ajax.postJson('insertHotSearch',{
					searchName:searchName
					},function(data,flag){		
				})
	        }		       
	        return false;
	    });
	    $(".web_search_r input").click(function() {
	        globalsearch();
	    });

	    //头部-商品搜索
	    $(".web_search_l input").focus(function(){
	        $(".web_search_l span").show();
	    });
	    $(".web_search_l span").click(function(){
	        $(".web_search_l input").val("");
	        $(".web_search_l span").hide();
	    });
	    // 个人中心下拉
        $(".web_center,.web_center_list").mouseover(function(){
            $(".web_center_list").show();
            $(".web_center").css("border-color","#e0e0e0");
        });
        $(".web_center,.web_center_list").mouseout(function(){
            $(".web_center_list").hide();
            $(".web_center").css("border-color","#fff");
        });
	    
	    //产品块动画
	    var webAnimation = $(".web_proList ul li");
	    $(webAnimation).mouseover(function(){
	      $(this).addClass("web-active");
	    });
	    $(webAnimation).mouseout(function(){
	      $(this).removeClass("web-active");
	    });

	    // 产品鼠标经过显示购买按钮
	    $(".web_proList ul li").mousemove(function(){
	        $(this).children("p.hide_buy").show();
	        $(this).children("p.price").hide();
	    });
	    $(".web_proList ul li").mouseout(function(){
	        $(this).children("p.hide_buy").hide();
	        $(this).children("p.price").show();
	    });
		
		//个人中心左半部菜单增加样式
		var left_val = GetQueryString('left_menu');
		//console.log(left_val);
		if(left_val!=null){
			var left_menus=$(".web_Usercenter_left dl dd");
			//console.log(left_menus.length);
			$.each(left_menus,function(){
				if($(this).attr("id")==left_val)
				{
					$(this).addClass("center_cur");
				}
			});
		}
		
	 });
</script>
<!-- 底部导航 end -->
