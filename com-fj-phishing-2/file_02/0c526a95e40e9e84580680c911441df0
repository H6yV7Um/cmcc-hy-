                    padding-left: 5px;
                    line-height:20px;
                }
            </style>
         
           <script type="text/javascript">
                

                 $(function () {
                        $('#txtRobotQuestion').questionAutoComplete(
                            {   
                                delay:
                                    300,
                                source: 
                                    function( request, response) { 
				                        $.ajax(
                                            {
					                            url: '/ICBCDynamicSite/Robot/RobotSuggest.ashx',
					                            dataType: 'jsonp',
                                                timeout: 1000,
					                            data: 
                                                    {
						                                key: request.term,
                                                        wap: 1
					                                },

					                            success: 
                                                    function( data ) { 
                                                        var tips = data;                                                        
                                                        var ntips = {}; //tips.length=undefind
                                                        //最多返回10条,wap渠道最多显示6条
                                                        if (tips.length > 6) 
                                                        { 
                                                            for (var j = 0; j < 6; j++) {
                                                                ntips[j] = tips[j];
                                                                }
                                                        }
                                                        else {
                                                            ntips = tips;
                                                        }
                                                        if (ntips != null && tips.length > 0) {
                                                            response(ntips);
                                                        }else {
                                                            for(var i = 1;i < 99;i++) 
                                                            {
                                                                 $(('#ui-id-' + i)).hide();
                                                            }
                                                        }
                                                  }
                                              }
                                        );
			                        }
                            }
                        )
                     });
    window.onorientationchange=function(){
        for(var i = 1;i < 99;i++) {
            $(('#ui-id-' + i)).hide();
        }
    }

            </script>
        
            <div style="height:60px; width:240px;">  
                <table style="width: 100%;height:100%" cellpadding="0" cellspacing="0">
                    <tbody><tr>
                        <td style="width:80%" align="center">
                             <input id="txtRobotQuestion" style="font-size:12px;line-height:25px;
                        width: 160px; height: 60px;margin-left:0px;margin-top:0px;border:1px solid #D1D1D1;" value="我是工小智，请描述您的问题" onclick="robotAskBlurClearInput()" maxlength="30" class="ui-autocomplete-input" autocomplete="off"><span role="status" aria-live="polite" class="ui-helper-hidden-accessible"></span>
                        </td>
