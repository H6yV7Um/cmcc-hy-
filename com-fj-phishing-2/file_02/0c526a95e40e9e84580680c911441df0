<script type="text/javascript">
var widget = require('widget'), util = require('util'), d = document,
  ORIENTATION_CHANGE = 'onorientationchange' in window ? 'orientationchange' : 'resize';
function refreshSlides() {
    dWidth = $("#icbc_kjfw").width();
    new widget.OrientationReadyDetector(
    {
        clientWidth: dWidth,
        callback: function () {
            try {
                dWidth = $("#icbc_kjfw").width();
                for (var i = 0; i < slides.length; i++) {
                    dWidth = $("#icbc_kjfw").width();
                    if (dWidth > 1000) {
                        dWidth = dWidth / 2;
                    }
                    slides[i].refresh({
                        pageWidth: dWidth
                    });
                }
            } catch (e) {
            }

        }
    }
);
}
window.addEventListener(ORIENTATION_CHANGE, refreshSlides, false);

window.setInterval(refreshSlides, 500);

function move(silde, num) {
    try {
        slide = new widget.Slide({
            id: 'slide_' + num,
            pageWidth: dWidth,
            indicator: {
                id: 'slide_' + num + '_slot',
                activeClassName: 'msActive',
                controllable: true
            }
        });
        slides.push(slide);
    } catch (e) {

    }
}
var dBody = document.body,
slides = [];
var silde_01;
var silde_02;
var silde_03;
var silde_04;
$(document).ready(function () {
    // 快捷服务
    dWidth = $("#icbc_kjfw").width();
    move(silde_01, "01");
    // 其他区域……
    dWidth = $("#icbc_kjfw").width();
    if (dWidth > 1000) {
        dWidth = dWidth / 2;
    }
    move(silde_02, "02");
	move(silde_03, "03");
    move(silde_04, "04");
});
window.onunload = function () { };
</script>
<script type="text/javascript">
        var _touchstartX;
        var _touchstartY;
        var _touchMoveX;
        var _touchMoveY;
        var _touchTemp;
        /*手指每次点击在屏幕上的位置X,Y 多点触屏*/
        function _fnTouchX(e) {
            var touches = e.changedTouches,
            i = 0, l = touches.length, touch, tagX;
            for (; i < l; i++) {
                touch = touches[i];
                tagX = touch.pageX;
            }
            return tagX;
        }

        function _fnTouchY(e) {
            var touches = e.changedTouches,
            i = 0, l = touches.length, touch, tagY;
            for (; i < l; i++) {
                touch = touches[i];
                tagY = touch.pageY;
            }
            return tagY;
        }

        function menutouchstart(e) {
            _touchstartX = _fnTouchX(e);
            _touchstartY = _fnTouchY(e);
            _touchTemp = $('#slide_02_slot').position().left;
        }
        function menutouchMove(e) {

            _touchMoveX = _fnTouchX(e) - _touchstartX;
            _touchMoveY = _fnTouchY(e) - _touchstartY;

            /*这里是为了手指一定是横向滚动的*/
            if (Math.abs(_touchMoveY) < Math.abs(_touchMoveX)) {
                e.preventDefault();
                var moveX = _touchTemp + _touchMoveX;



                if (moveX > 0) {
                    moveX = 0;
                }

                var slide_02_slotwidth = document.getElementById('slide_02_slot').offsetWidth;
                var slide_02_slot_over_width = $("#slide_02_slot_over").width();
                var leftvalue = slide_02_slot_over_width - slide_02_slotwidth;
                if (moveX < leftvalue) {
                    moveX = left;
                }
                $('#slide_02_slot').css({ "left": moveX + "px" });
            }
        }
        function nextslide(slideid) {
            var slide_02_slotwidth = document.getElementById('slide_02_slot').offsetWidth;
            var slide_02_slot_over_width = $("#slide_02_slot_over").width();
            var leftvalue = slide_02_slot_over_width - slide_02_slotwidth;
            $('#slide_02_slot').css('left', leftvalue);
        }
        function prevslide(slideid) {
            $('#slide_02_slot').css('left', '0px');
        }
</script>
