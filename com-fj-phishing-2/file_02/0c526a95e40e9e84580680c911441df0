            <script type="text/javascript" src="http://www.icbc.com.cn/Portal_Resources/Common/jquery/jquery-ui-1.9.2.autocomplete.js"></script>
            <script type="text/javascript" src="http://www.icbc.com.cn/Portal_Resources/Robot/Ask/autocompletewap.js"></script>
            <link rel="stylesheet" href="http://www.icbc.com.cn/Portal_Resources/Robot/Ask/jquery.ui.core.css">
        
             <script type="text/javascript">
                    var robotAskLeft = 0;
                    var robotAskTop = 0;
                    var robotDefaultText = '';

                    function robotGotoAnswerPage(id, title) {
                          var newPage = true;
                          if (newPage) {
                              window.open('/ICBC/%E5%AE%A2%E6%88%B7%E6%9C%8D%E5%8A%A1/%E6%99%BA%E8%83%BD%E6%9C%8D%E5%8A%A1/default.htm?id=' + id + '&ct=' + title);

                              $('#robotAskTopTen').hide();
                          }
                          else {
                              window.location = '/ICBC/%E5%AE%A2%E6%88%B7%E6%9C%8D%E5%8A%A1/%E6%99%BA%E8%83%BD%E6%9C%8D%E5%8A%A1/default.htm?id=' + id + '&ct=' + title;
                          }
                    }

                    function searchAnswer() {
                        var newPage = true;
                        var key = $('#txtRobotQuestion').val();
                        
                        if (key =='我是工小智，请描述您的问题' || $.trim(key) == '' || key == robotDefaultText) {
                            return false;
                        }else {
                            key = encodeURIComponent(key);

                            if (newPage)
                                window.open('/ICBC/%E5%AE%A2%E6%88%B7%E6%9C%8D%E5%8A%A1/%E6%99%BA%E8%83%BD%E6%9C%8D%E5%8A%A1/default.htm?title=' + key.replace(/#/g, '%23'));
                            else
                                window.location = '/ICBC/%E5%AE%A2%E6%88%B7%E6%9C%8D%E5%8A%A1/%E6%99%BA%E8%83%BD%E6%9C%8D%E5%8A%A1/default.htm?title=' + key.replace(/#/g, '%23');
                        }
                    }                 


                    function robotAskBlurClearInput() {
                        var txt = document.getElementById('txtRobotQuestion');

                        if (txt.value == '我是工小智，请描述您的问题' || txt.value == '') {
                            txt.value = '';                          
                            
                        }   
                    }

                    $(document).ready(
                        function(){

                                $('#txtRobotQuestion').keydown(
                                function(event){
                                    if (event.keyCode == 13) {
                                        searchAnswer()
                                        
                                        return false;
                                    }
                                }
                            ); 
                            //回显关键字
                             var paras = window.location.search.replace('?', '').split('&');
                            if (paras != null && paras != '' && paras.length > 0) {
                                for (var i = 0; i < paras.length; i++) {
                                    var para = paras[i].split('=');

                                    if (para != null && para != '' && para.length == 2) {
                                        if (para[0] == 'title' || para[0] == 'ct') {
                                            var para1=decodeURIComponent(para[1]).replace(/%23/g,'#');
						                    $('#txtRobotQuestion').val(para1);
                                            robotDefaultText = para1;

                                            break;
                                        }
                                    }
                                }
                            }
                        }
                    );
           </script>
        
            <style type="text/css">        
                .ui-autocomplete
                {
                    list-style: none;
                    border: 1px solid #D1D1D1;
                    width: 252px;
                    background-color: #ffffff;
                    margin: 0px;
                    text-align: right;
                    padding-right: 0px;
  			        padding-left: 0px;
