<script type="text/javascript">

function dialogTip(content, options) {
    var opt = $.extend({
      skin: 'ui-toast',
      fixed: true
    }, options, {
      content: content
    })
    var d = dialog(opt).show();
    setTimeout(function() {
      d.close().remove();
    }, opt.time || 3000)
  }
function dialogConfirm(content, options, cb) {
    var opt = $.extend({
      fixed: true,
      padding: 40,
      okValue: '确定',
      ok: (cb && Object.prototype.toString.call(cb) === "[object Function]") ? cb : function() {},
      cancelValue: '取消',
      cancel: function() {}
    }, options, {
      content: content
    })
    dialog(opt).showModal();
  }
function dialogConfirm1(content, options, cb) {
    var opt = $.extend({
      
      fixed: true,
      title:"系统提示",
      okValue: "确定",
      ok: (cb && toString.call(cb) === "[object Function]") ? cb : function() {}
    }, 
      options, {
      content: content
    })
    dialog(opt).showModal();
  }
  function dialogConfirm2(content, options, cb) {
    var opt = $.extend({}, { 
      fixed: true,
      title:"消息确认",
      cancelValue:"取消",
      cancel: function() {},
      okValue: "确定",
      ok: (cb && Object.prototype.toString.call(cb) === "[object Function]") ? cb : function() {}
    }, 
      options || {}, {
      content:content
    });
    dialog(opt).showModal();
  };

</script>


