<script  type="text/javascript">
	//var isErrorMuch = "";
	
$(function(){
	$(document).keyup(function(event){
		if(event.keyCode == 13){
			if($("#keyword").is(":focus")){
				$("#keywordSearchBtn").trigger("click");
			}
		}
	});
});
/**
 * 验证
 */
function check() {
	if ($("#txtAccount").val() == ''
			|| '请输入登录账号' == $("#txtAccount").val()) {
		//alert('请填写邮箱/手机！');
		$("#loginWindow_error").show();
		$("#loginWindow_error").html("请填写邮箱/手机!");
		return false;
	}
	if ($("#txtPassword").val() == '') {
		//alert('请填写密码！');
		$("#loginWindow_error").show();
		$("#loginWindow_error").html("请填写密码！");
		return false;
	}
	if($("#j_captcha").val()==''){
		$("#loginWindow_error").show();
		$("#loginWindow_error").html("请填写验证码！");
		return false;
	}
	return true;

}
//登陆
function login() {
	if (!check()) {
		return;
	}
	var varAccount = $("#txtAccount").val();
	var varPassword = $.md5($("#txtPassword").val());
    var jCaptcha=$("#j_captcha").val();
	$.ajax({
		type : "POST",
		url : "/view/login/loginvalidate2",
		data : {
			'account' : varAccount,
			"password" : varPassword,
			"jCaptcha" : jCaptcha
		},
		dataType : "JSON",
		cache : false,
		async : false,
		success : function(data) {
			if (data.code == 1000) {
				user=data.msg;
				$(".bgBox").hide();
				$(".loginWindow").hide();
				
				$.ajaxDiv({
					url: "/view/to-nonavtop",
					divId:"topBar",
					success:function(data){
					}
				});
				if(golobalOldUrl != undefined && golobalOldUrl !=""){
					$.ajax({
	    				url:golobalOldUrl,
	    				dataType:golobalOldDataType,
	    				data:golobalOldData,
	    				success:function(data2, textStatus2, jqXHR2){
	    					if(golobalOldSuccess != undefined){
	    						golobalOldSuccess(data2, textStatus2, jqXHR2);
	    		        	}
	    				},
	    				error:function(data){
	    					
	    				}
	    			});
				}
			} else if (data.code == 500) {//密码错误三次
				//isErrorMuch = 1;
				//$('#jCaptchaDiv').show();
				$("#loginWindow_error").show();
				$("#loginWindow_error").html(data.msg);
			} else if (data.code == 250) {
				//验证码输入错误
				$('#ajaxLoginCheckCodeImg').click();
				$("#loginWindow_error").show();
				$("#loginWindow_error").html(data.msg);
			}else {
				$("#loginWindow_error").show();
				$("#loginWindow_error").html(data.msg);
			}
		}
	});
}
</script>-->
<script>
$(function(){
    $("#tabT").find("li").on('click', function() {
        var index = $(this).index();
        $(this).siblings().removeClass("selected").end().addClass("selected");
        //$("#tabC").children("div").slideUp().end().children("div:eq("+index+")").slideDown();
        $($("#tabC").children("div")[index]).show().siblings().hide();
    });

    var $windowLogin = $("#loginWindow"); 
   	
    $('.ajaxLoginShowBtn').on('click', function() {
       	$.ajax({
			url:"/after/order/islogin",
			success:function(data){
				if(data.success){
					window.location.reload();
				}else{
					Dialog.alert(data.msg);
				}
			},
			error:function(data){
				
			}
		});
   	});
   
   	$(".ajaxLoginCloseBtn").on("click",function(){
   		$windowLogin.hide();
   	});

})

	
</script>



