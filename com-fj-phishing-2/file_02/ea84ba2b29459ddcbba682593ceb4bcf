<script>

  $(function(){

    $('body').on('mouseover', '.homeNav li.cat', function() {
      $('.homeNav li.cat').removeClass('hover');
      $(this).addClass('hover');
    })
    $('body').on('mouseout', '.homeNav li.cat', function() {
      $(this).removeClass('hover');
    })

	   $(".gg").each(function(){
			var self = $(this);
			var $type = $(this).data("type");
			var $width =  $(this).data("width");
		          var $height =  $(this).data("height");
		          var $obj = $(this).data("json");		     
		          var $adgg = $(this).find(".adgg");
           function generateRandomAlphaNum(len) {
	          var rdmString = "";
	          for (; rdmString.length < len; rdmString += Math.random().toString(36).substr(2));
	          return rdmString.substr(0, len);
	        }
	        var id = generateRandomAlphaNum(12);      
			if($type==1){
			
			    var $html = '<div class="slideBox clearfix" id="slideBox'+ id +'" style="height:'+$height+'"><div class="slide" style="position:relative;"><div class="slideImg" style="height:'+$height+';width:'+$width+';overflow:hidden;"><ul>'
			    var $htmlLast= '</ul></div><div class="smallImgBox"><ul class="smallImg" style="display: inline-block;overflow: hidden;"></ul></div></div></div>'
				for(var i = 0; i < $obj.length ; i++){
					$html += '<li style="height:'+$height+'"><a target="_blank" href='+$obj[i]["link"]+' onclick="javascript:adClickLog('+$obj[i]["id"]+');" style="background:url('+$obj[i]["img"]+') no-repeat center center;height:'+$height+';display:block;"></a></li>';
			    }
			    var $htmlAll = $html+$htmlLast;
			    self.html($htmlAll);
          self.css('height', $height);
			    var $id = "#slideBox" + id ;
			    $($id + " " + ".smallImgBox").css({"width":$width,"text-align":"center","position":"absolute","bottom":"20px"});
				$($id + " " + ".slideImg li").soChange({
					thumbObjParent:$id + " " + ".smallImg",
					thumbObj:$id + " " + ".smallImg li",
					thumbNowClass:'select'
				});
			}
			else if($type==2){
				
				if($obj["url"] === "null"){
					$obj["url"] = null;
				}
				var $url = $obj["url"] || "javascript:;";
				self.html('<a target="_blank" href="'+ $url+'" onclick="javascript:adClickLog('+$obj["id"]+');" style="width:'+$width+';height:'+$height+';overflow:hidden;"><img src='+ $obj["img"]+' alt="" /></a>');
        self.css('height', $height);
			}
			else if($type==3){
				var richTextAdId;
				$(".adgg").each(function(){
					richTextAdId = $(this).data("adid");
					return false;
				});
				self.html('<div onclick="javascript:adClickLog('+richTextAdId+');" style="width:'+$width+';height:'+$height+';border:#dedede solid 1px;">'+$(this).html()+'</div>');
        self.css('height', $height);
			}
			//记录广告展示日志
		   $adgg.each(function(){
		   	   adShowLog($(this).data("adid"));
		   });
			
		});
	   $(".gg").show();
   });
   function adClickLog(id){
	   if(!!id){
			$.ajax({
				type : "POST",
				url : "/view/adlog/click",
				data : {
					'adId' : id
				},
				dataType : "JSON",
				async:true,
				cache : false,
				success : function(data) {
				}
			});
	   }
	}
	
	function adShowLog(id){
		if(!!id){
			$.ajax({
				type : "POST",
				url : "/view/adlog/show",
				data : {
					'adId' : id
				},
				dataType : "JSON",
				async:true,
				cache : false,
				success : function(data) {
				}
			});
		}
	}
</script>




