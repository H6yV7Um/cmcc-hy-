  <script type="text/javascript">
  seajs.use(['alipay/xbox/1.1.1/xbox', 'arale/switchable/1.0.0/carousel', '$'], function(Xbox, Carousel, $) {
  	<!-- FD:111:mobileweb/index/scriptFile.vm:804:index/index.schema:index下载页:START -->
    var jQuery = $;
    $(document).ready(function() {
      var windowHeight = $(window).height() - 110;
      var windowWidth = $(window).width();

      /**
       *  banner区域交互，确保首屏出现并且图片和动画处理
       */
      function resetTopHeight() {
        var topHeight = $(window).height() - 110;
        if(topHeight > 500) {
          $("#J-head").css({
            'height': topHeight + 'px'
          });
        }
      }
      /* 随机获取一个banner图片 */
      var bannerArr = [
        'https://t.alipayobjects.com/tfscom/T1h6NfXcxcXXXXXXXX.png',
        'https://t.alipayobjects.com/tfscom/T14yXfXeXxXXXXXXXX.png',
        'https://t.alipayobjects.com/tfscom/T19i0fXn8jXXXXXXXX.png',
        'https://t.alipayobjects.com/tfscom/T1tONfXm0oXXXXXXXX.png'
      ];

      var img = new Image();
      var random = Math.floor(Math.random() * bannerArr.length);
      var imgWidth, imgHeight;
      img.src = bannerArr[random];

      img.onload = function() {
        var topHeight = $(window).height() - 110;
        var topWidth = $(window).width();
        var currWidth = parseInt(topHeight / img.height * img.width, 10);
        if(currWidth > topWidth / 2) {
          var rightWidth = topWidth / 2 - currWidth - 50;
          $("#J-head-img").css({
            'right': rightWidth + 'px'
          });
        }
        $("#J-head-img").attr('src',img.src);
        $("#J-head-img").addClass("head-img-show");
      }

      /* 重置banner的尺寸 */
      function resetImgWidth() {
        var imgWidth = $("#J-head-img").width();
        var clientWidth = $(window).width();
        if(imgWidth > clientWidth/2) {
          var currWidth = clientWidth / 2 - imgWidth - 50;
          $("#J-head-img").css({
            'right': currWidth + 'px'
          });
        }
      }
      resetTopHeight();
      /*  浏览器缩放事件 */
      $(window).resize(function() {
        windowHeight = $(window).height() - 110;
        windowWidth = $(window).width();
        resetTopHeight();
        resetImgWidth();
      });
      /*  banner中的logo动画效果 */
      $("#J-head-img").addClass("head-img-show");
      $("#J-head-logo").addClass("logo-show");


      /**
       *  下载浮出层功能
       */
      var downLoad = new Xbox({
        trigger: '.download-btn',
        content: $('#J-download-dom'),
        width: '655px',
        height: '390px'
      }).render();
      var downLoad = new Xbox({
        trigger: '#J-tipsBtn',
        content: $('#J-download-dom-tips'),
        width: '655px',
        height: '390px'
      }).render();


      /**
       *  切换功能介绍和历史版本
       */
      $("#J-choose-pro").on('click', function(e) {
        e.preventDefault();
        $(this).addClass("toggle-pro-current");
        $("#J-choose-his").removeClass("toggle-his-current");
        $("#J-channel-his").css({
          'display': 'none'
        });
        $("#J-channel-pro").css({
          'display': 'block'
        });
        /* 当有滚动条的时候，需要默认到介绍的顶部 */
        if($(window).scrollTop() > windowHeight) {
          $(window).scrollTop(windowHeight);
        }
      });

      $("#J-choose-his").on('click', function(e) {
        e.preventDefault();
        $(this).addClass("toggle-his-current");
        $("#J-choose-pro").removeClass("toggle-pro-current");
        $("#J-channel-his").css({
          'display': 'block'
        });
        $("#J-channel-pro").css({
          'display': 'none'
        });
        /* 当有滚动条的时候，需要默认到介绍的顶部 */
        if($(window).scrollTop() > windowHeight) {
          $(window).scrollTop(windowHeight);
        }
      });

      /**
       *  钱包共展示交互
       */
      /* 页面加载进来，处理滚动条事件，保证是看到首屏 */
      $(window).scrollTop(0);

      /* 定义静态文件 */
      var proAssets = {
        ".y-bg": "https://i.alipayobjects.com/e/201310/1Gmta9nvfj.png",
        ".y-qb": "https://i.alipayobjects.com/e/201310/1Gn1sYNX5D.png",
        ".y-jb": "https://i.alipayobjects.com/e/201310/1Gn8I1pEIB.png",
        ".s-bg": "https://i.alipayobjects.com/e/201310/1GnaMrzW87.png",
        ".s-kc": "https://i.alipayobjects.com/e/201310/1GnaDDMDaR.png",
