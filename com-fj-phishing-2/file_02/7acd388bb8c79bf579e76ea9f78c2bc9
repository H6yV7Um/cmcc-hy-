        ".pageitem-2":'https://zos.alipayobjects.com/cmsmng/cms/images/j1rjwc2x/69e772a6-6b76-4ca3-b6ba-8ac7efe3a425.png' ,
        
        ".pageitem-3":'https://zos.alipayobjects.com/cmsmng/cms/images/j1rjw3qr/bd3597a0-35cd-4b1e-84aa-965e093e24ce.png' ,
        
        ".pageitem-4":'https://zos.alipayobjects.com/cmsmng/cms/images/j1rjwc2x/69e772a6-6b76-4ca3-b6ba-8ac7efe3a425.png' 
        
      };

      /* 当pageLoad之后，预加载所有静态文件 */
      var preloader = html5Preloader();
      var setupPreloader = function(assets) {
        //添加图片加载进程
        jQuery.each(assets, function(assetsItem) {
          var assetsId = assetsItem.substr(1, assetsItem.length)+'*:';
          preloader.addFiles(assetsId+assets[assetsItem]);
        });
        //图片加载完处理事件
        preloader.on('finish', function() {
          jQuery.each(assets, function(assetsItem) {
            var assetBg = 'url('+assets[assetsItem]+')';
            $(assetsItem).css({'background': assetBg+' no-repeat'});
          });
        });
        //出错处理
        preloader.on('error', function(e) {});
      }
      /* pageLoad */
      var readyStateCheckInterval = setInterval(function() {
        if (document.readyState === "complete") {
          setupPreloader(proAssets);
          clearInterval(readyStateCheckInterval);
        }
      }, 10);
      /* 滚动条事件，则现实动画效果 */
      $(window).scroll(function() {
//        scrollAnim();
        if ($(window).scrollTop() > 500) {
          $("#J-tips").css({
            'display':'block'
          })
        } else {
                  $("#J-tips").css({
                    'display':'none'
                  })
                }
      });

      function scrollAnim() {
        if($(window).scrollTop() > 140) {
          $("#J-yuerbao").addClass("anim-yuerbao");
        }

        if($(window).scrollTop() > 580) {
          $("#J-shenghuo").addClass("anim-shenghuo");
        }

        if($(window).scrollTop() > 1020) {
          $("#J-danmianfu").addClass("anim-danmian");
        }

        if($(window).scrollTop() > 1460) {
          $("#J-appstore").addClass("anim-appstore");
        }

        if($(window).scrollTop() > 1900) {
          $("#J-cardticket").addClass("anim-ticket");
        }

        if($(window).scrollTop() > 2340) {
          $("#J-travel").addClass("anim-travel");
        }

        if($(window).scrollTop() > 2780) {
          $("#J-securite").addClass("anim-safe");
        }

        if($(window).scrollTop() > windowHeight) {
          $("#J-main-channel").addClass("anim-fixed");
          $("#J-main-channel").css({
            'width': windowWidth + 'px'
          })
        } else {
          $("#J-main-channel").removeClass("anim-fixed");
        }
      }
//      scrollAnim();

      /**
       *  历史版本区域交互，手机介绍功能的轮播
       */
      var carousel = new Carousel({
        element: '#J-phone-show',
        panels: '#J-phone-show .pageitem',
        hasTriggers: false,
        easing: 'easeOutStrong',
        effect: 'scrollx',
        step: 1,
        viewSize: [306],
        autoplay:true
      }).render();
    });
  });
