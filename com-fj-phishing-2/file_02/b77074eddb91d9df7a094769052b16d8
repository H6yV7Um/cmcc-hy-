<script type="text/javascript">
var src ='';

function checkMobiePhone(insertPhone){
	var reg2=/^13[4,5,6,7,8,9]\d{8}$|^14[7]\d{8}$|^15[0,1,2,7,8,9]\d{8}$|^18[7,8]\d{8}$/;
	   	if(reg2.test(insertPhone)) {
	   		return 0;
	   	}else {
	   		return 1;
	   	}
}

/**
 * 按格式分割字符串
 * 如：手机号13832940385 按  [ 3, 4, 4 ] 格式分割，
 * 分割成 138 3294 0385
 */
function segmentByFormat(str, segmentArr) {
	var segmentStr = '';
	segmentArr.unshift(0);// 为分割数组头部添加一个0，为循环
	for(var i=0,sum=0,len=segmentArr.length-1;i<len;i++) {
		sum += Number(segmentArr[i]);
		segmentStr += str.substring(sum, sum+segmentArr[i+1]) + ' ';
	}
	segmentStr = segmentStr.substring(0, segmentStr.length-1);
	return segmentStr;
}

function VerifyPermissions(htmlUrl){
	///console.log(htmlUrl)
	var _url = src+"/busicenter/membershipArticles/MembershipArticlesAction/verifyPermissions.action";
	$.ajax({
	  	url:_url,
	  	dataType:"json",
	  	cache:false,
	  	type: "post",	
		async: true,
	  	success:function(data) {
	  		if(data.code == '0000'){
	  			window.location.href=src+data.data+htmlUrl+".html";
        		//window.open(src+data.data+htmlUrl+".html");
        	}else if(data.code == '1'){
        		 showConfirm("此文章需要收费才能阅读，是否继续",function(){
        			 window.location.href=src+data.data;	
       			}); 
        	}else if(data.code == '2'){
        		window.location.href=src+data.data;
        	}else if(data.code == '-1'){
        		showError(data.msg);
        	}
	  	},
	  	error:function(XMLHttpRequest, textStatus, errorThrown) {
	    }
	});
}
</script>

