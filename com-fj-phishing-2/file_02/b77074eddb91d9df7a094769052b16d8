	function searchNum(){
		var g = $("#gsd");
		var telphone = $("#searchInput").val();
		if(telphone == ""){
			 g.html("手机号码为空");
		}else if(telphone.match(/^170\d{8}$/)){
			g.html("正在查询...");
			//alert("ffff");
			var _url="/busicenter/phoneNum/PhoneNumAction/searchPhone.action";
		   	$.ajax({
			  	url:_url,
			  	data: {phone:telphone},
			  	dataType:"json",
			  	cache:false,
			  	type: "post",	
				async: true,
			  	success:function(data) {
					var msg="";
					if(data.code=="1"){
						msg=data.msg.province+"  "+data.msg.city;
					}else{
						msg=data.msg;
					}
					g.html(msg);
			  	},
			  	error:function(XMLHttpRequest, textStatus, errorThrown) {
			        //document.getElementById("wfSubmit").disabled = false
			        if(textStatus == 'timeout') {
			        	g.html("查询失败!");
			        } else {
			        	g.html("查询失败!");
			        }
			    }
			});
		}else{
			g.html("号码格式错误");
		}
	}
	function newGuid()
	{
	    var guid = "";
	    for (var i = 1; i <= 32; i++){
	      var n = Math.floor(Math.random()*16.0).toString(16);
	      guid +=   n;
	      if((i==8)||(i==12)||(i==16)||(i==20))
	        guid += "-";
	    }
	    return guid;    
	}
	function setAutoSubmit(){
		document.documentElement.onkeydown = function(e){
			e = e||window.event;
			if(e.keyCode==13){
				loginFromIndex();
			}
		};
 	}
	function check2(phone,op){
		var flag = false ;
		$(op).html("");
		var _url="/busicenter/phoneInfo/PhoneInfoCheckAction/checkPhoneState.action";
	   	$.ajax({
		  	url:_url,
		  	data: {"phone":phone},
		  	dataType:"json",
		  	cache:false,
		  	type: "post",	
			async: false,
		  	success:function(data) {
				var msg="";
				if(data.code=="00"){
					flag = true;
				}else{
					showError(data.msg);
					flag = false;
				}
		  	},
		  	error:function(XMLHttpRequest, textStatus, errorThrown) {
		        //document.getElementById("wfSubmit").disabled = false
		        if(textStatus == 'timeout') {
		        	showError(errorThrown);
		        	flag = false;
		        } else {
		        	showError(errorThrown);
		        	flag = false;
		        }
		    }
		});
	   	return flag ;
	};
	
	
	
	function check(phone){
		var flag = false ;
		var money = $("#moneyNum").text();
		var guid = newGuid() + newGuid() + newGuid();
		if(phone == ""){
			$("#errormsg").html("号码不能为空");
			return false;
		}else if(phone.length != 11){
			$("#errormsg").html("号码长度不足11位");
			return false;
		}else if(phone.match(/^1[3-9][0-9]\d{4,8}$/)){
			$("#errormsg").html("");
			$("#prodid").val($(".select_current").attr("prodid"));
			var _url="/busicenter/phoneInfo/PhoneInfoCheckAction/checkPhoneState.action";
			
		   	$.ajax({
			  	url:_url,
			  	data: {"phone":phone},
			  	dataType:"json",
			  	cache:false,
			  	type: "post",	
				async: false,
			  	success:function(data) {
					var msg="";
					if(data.code=="00"){
						flag =  true;
					}else{
						showError(data.msg);
						flag =  false;
					}
			  	},
			  	error:function(XMLHttpRequest, textStatus, errorThrown) {
			        //document.getElementById("wfSubmit").disabled = false
			        if(textStatus == 'timeout') {
			        	showError(errorThrown);
			        	flag = false;
			        } else {
			        	showError(errorThrown);
			        	flag =  false;
			        }
			    }
			});
		}else{
			$("#errormsg").html("号码格式错误");
			flag = false;
		}
		return flag;
	};
	
</script>
