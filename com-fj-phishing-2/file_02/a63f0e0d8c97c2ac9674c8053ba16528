	<script>
seajs.use([
    '$',
    'appstore/sticky/1.4.1/sticky',
    'appstore/scrollspy/1.0.0/scrollspy',
    'hermes/qrcode/1.0.0/qrcode'
], function ($, sticky, t, Qrcode) {
    $('body').scrollspy({target: '#navbar'});
    var length = $('.sidebar li').length;
    $('.sidebar').css({
        'height': length * 50
    });
    $('.bg-div').css({
        'height': (length - 1) * 50
    });
    sticky('.sidebar', 10);
    $('.sidebar').on("click", "a", function (e) {
        e.preventDefault();
        var $el = $(this);
        $('body,html').animate({
            scrollTop: $($el.attr('href')).offset().top
        }, 200)
    });
    if ($(window.location.hash).length > 0) {
        $('body,html').animate({
            scrollTop: $(window.location.hash).offset().top
        }, 200);
    }
    var len = $('#ui-slider-view li').length;
    var index = 0;
    $('#ui-slider-trigger a').click(function (e) {
        e.preventDefault();
        var i = $(this).index();
        $("#ui-slider-trigger .current").removeClass('current');
        $(this).addClass('current');
        $('#ui-slider-view .current').css({
            'z-index': 0
        }).removeClass('current').fadeOut();
        $('#ui-slider-view li').eq(i).addClass('current').css({
            'z-index': 1
        }).fadeIn();
        index = i;
    });
    $('#ui-slider-trigger a').eq(0).trigger('click');
    setInterval(function () {
        $('#ui-slider-trigger a').eq(++index % len).trigger('click');
    }, 5000);

    $('p[data-content]').each(function () {
        var data = $(this).data('content');
        var $el = $(this);
        var html = '';
        if (jQuery.isArray(data)) {
            $(data).each(function () {
                html += '<i class="market-icon">' + this.str + '</i>'
            });
        }
        $el.html(html);
    });
    $('a[data-type=4]').click(function (e) {
        e.preventDefault();
    });
    $('a[data-type=4]').each(function () {
        var $el = $(this);
        var href = $el.data('href');
        $(this).one('mouseover',function(){
            new Qrcode({
                target: $('.qr-c', $el)[0],
                text: href,
                width: 100,
                height: 100,
                imgSrc: "https://t.alipayobjects.com/tfscom/T1Z5XfXdxmXXXXXXXX.png"
            });
        });
    });
    $('#J_mainMenu').delegate('.channel', 'click', function () {
        var target = $(this);
        target.addClass('hover').siblings().removeClass('hover');
    }).delegate('.close', 'click', function (e) {
        e.stopPropagation();
        var target = $(this);
        target.closest('.channel').removeClass('hover');
    });

    $(document).on('click', function (e) {
        var target = $(e.target);
        if (target.closest('.channel').length == 0) {
            $('#J_mainMenu .channel').removeClass('hover');
        }
    });
});
</script>










                                                                                                                                                                                                                                        
