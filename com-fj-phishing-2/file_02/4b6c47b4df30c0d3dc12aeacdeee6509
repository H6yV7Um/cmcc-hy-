    <script type="text/javascript">	
  	

   var idx = 0;		
   var curtent_idx = $(".banner_container .item").length - 1;		
   function picChange(idx){			
   $(".banner_container .item").hide().eq(idx).fadeIn(500);			
   $(".btnlist .on").removeClass("on");					
   $(".btnlist .b").eq(idx).addClass("on");		}		
   var newsRoll=setInterval(function(){			
   idx++;			
   if(idx > curtent_idx){				 
   idx = 0;			}			
   picChange(idx);		
   },4000);				
   $($(".banner_container .item").get(0)).show();		
   $(".btnlist .b").eq(0).addClass("on");		
   var bannerindex=0;    	
   $(".btnlist .b").click(function()		{			
   var index = $(this).index();			
   $(".banner_container .item").stop().hide();			
   if(index <= curtent_idx){				
   idx = index;			}else if(index >curtent_idx){				
   idx = 0;			}			
   if(bannerindex != $(this).index())			{								
   $($(".banner_container .item").get($(this).index())).stop().fadeIn(500);				
   bannerindex = $(this).index();			}			
   $(".btnlist .on").removeClass("on");			
   $(this).addClass("on");		});		
   	
    </script>

