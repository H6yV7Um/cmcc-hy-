            <script language="javascript" type="text/javascript">
        $(document).ready(function () {
            $(".menua ul li").eq(1).find("a").addClass("hov");
            $(".menua ul li").eq(0).next().addClass("hov");
            $(".m_nav ul li.mnavli").eq(3).find("a").addClass("hover1");
            $(".mbanner").addClass("m_banner");
            $(".mban").addClass("m_ban");
            //右侧选项卡样式
            //初始化
            $(".yx-hd ul.gz li").eq(0).find("a").addClass("hover1");
            $(".yx_bd").eq(0).show();

            //右侧主页子菜单切换
            $(".yx-hd ul.gz li").mouseover(function () {
                $(".yx-hd ul.gz li a").removeClass("hover1");

                var i = $(".yx-hd ul.gz li").index(this);
                $(".yx-hd ul.gz li").eq(i).find("a").addClass("hover1");

                $(".yx_bd").hide();
                $(".yx_bd").eq(i).show();
            });
        });
            </script>

