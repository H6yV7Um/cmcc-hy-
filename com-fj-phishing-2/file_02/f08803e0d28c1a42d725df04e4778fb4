					});
				}
			}
		});
		if(!ipstate){
			$(".area-nav").find("li").eq(0).addClass("hver");
		}
	});
	function ad_pend(){
		var width = $(".wrapin").width();
		if(width == 1200){
			len = 4;
		}
		
		for(var i=0; i<len; i++){
			var cur = sign;
			if(sign >= child.length){
				cur = sign%child.length;
			}
			$(".ad-t2").append(child.eq(cur).clone());
			sign = parseInt(cur + 1);
		}
	}
//删除对比
function delschoolid(sid){
	var newsids = "";
	var sids = Cookie.get("sid");
	var sidarr = sids.split(",");
	if (sidarr.length > 0) {
		for (var i = 0; i < sidarr.length; i++) {
			if (sidarr[i] != sid) {
				if (newsids == "") {
					newsids = sidarr[i]
				} else {
					newsids = newsids + "," + sidarr[i]
				}
			}
		}
	}
	Cookie.set("sid", newsids, 259200);
	if (newsids == "") {
		$("#duibi").html("<li><em>未添加对比</em></li>")
	} else {
		$("#schoolid" + sid).remove()
	}
}
function list_db(sid,sname){
	$(".school_db").show();
	var flag = true;  
	var sids = Cookie.get("sid");
	var str= '<span id=schoolid'+sid+'><li ><em><a href="http://mba.mbachina.com/index/detail/'+sid+'.html" target="_blank">'+sname.substring(0,7)+'</a> <a href="javascript:void(0)" onclick="delschoolid('+sid+')"><img src="http://www.mbachina.com/resources/images/closed.gif"/></a></em></li></span>';                                                                 
	if(sids){
		var arr = sids.split(",");
		if(arr.length > 0){
			for(var i=0;i<arr.length;i++){
				if(arr[i] == sid){
					flag = false;
				}
			}
			if(arr.length > 3){
				alert("你已经选择四个学校对比了！");
			}else{   
				if(!flag){
					alert("该学校已在对比中！");
				}else{   
					$("#duibi").append(str);
					var newsids = sids + "," + sid;
					Cookie.set("sid", newsids, 259200); 
				}
			}   
		}else{
			$("#duibi").html(str);
			Cookie.set("sid", sid, 259200);
		}
	}else{
		$("#duibi").html(str);
		Cookie.set("sid", sid, 259200);
	} 
}
//开始对比
function list_db_check(){
    var sids = Cookie.get("sid");
    if(sids == null){
        alert("请您点击想要对比的院校进入院校页面，然后点击“加入对比”（在线报名服务中心左上角），两个以上的院校才能进行对比哦！");
    }else{
		var arr= sids.split(",");
        if(arr.length > 1){
			window.open ("http://mba.mbachina.com/index/contrast?sids="+sids);
        }else{
			alert("至少要选择2个学校，才能对比！");   
		}
	}
}
function list_db_empty(){
	$(".school_db ul").html("<li><em>未添加对比</em></li>");
    Cookie.set("sid", ""); 
}
//-->
</script>
