            $(function(){
                var arr,reg=new RegExp("(^| )tipId=([^;]*)(;|$)");
                arr=document.cookie.match(reg);
                if(arr){
                    $("#topTip").remove()
                }else{
                    $("#topTip").delay(600).animate({"height":"100px"},500,function(){						
                        $("#topTip .img1").addClass("on");
                        setTimeout(function(){
                            $("#topTip .btn").addClass("animated bounce")
                        },3900);
                        setTimeout(function(){
                            $("#topTip .img1").removeClass("on")
                        },4100)
                        setTimeout(function(){
                            $("#topTip .img1").addClass("tada")
                        },8100)
                        
                    })
                }
                    
                $(".left5").click(function(){
                    $("#topTip").animate({"height":"0"},500,function(){
                        $("#topTip").hide();
                    });
                    document.cookie="tipId="+true;    
                    var exp = new Date(); 
                    exp.setTime(exp.getTime() + 24*60*60*1000);
                    document.cookie = "tipId="+true+ ";expires=" + exp.toGMTString();
                    return false;
                })
                            
            })
        </script>
