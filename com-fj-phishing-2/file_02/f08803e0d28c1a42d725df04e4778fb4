<script type="text/javascript">
<!--
	var sign = 0;
	var len = 5;
	var child = $("#adimgs").find("p");
	$(function(){
		var wordlist = $("#wordlist").find("a");
		$(".rd").find("li").each(function(i){
			$(this).append(wordlist.eq(i).clone());
		});
		$(".ad-t2").empty();
		ad_pend();
	
		//ip定位
		var ipstate = false;
		$.ajax({ 
			url:"http://api.go2map.com/engine/api/ipcity/json?cb=?", 
			dataType:"json", 
			success:function(data,status){ 
				if(data.status == "ok"){
					$(".area-nav").find("li").each(function(){
						if($(this).children("a").html() == data.response.city){
							ipstate = true;
							$(this).addClass("hver").siblings().removeClass("hver");
							index = $(".area-nav li").index(this);
							$("#cb >div").eq(index).show().siblings().hide();
