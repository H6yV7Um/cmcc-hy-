<script type="text/javascript">
jQuery(document).ready(function(){
	var qcloud={};
	$('[_t_nav]').hover(function(){
		var _nav = $(this).attr('_t_nav');
		clearTimeout( qcloud[ _nav + '_timer' ] );
		qcloud[ _nav + '_timer' ] = setTimeout(function(){
		$('[_t_nav]').each(function(){
		$(this)[ _nav == $(this).attr('_t_nav') ? 'addClass':'removeClass' ]('nav-up-selected');
		});
		$('#'+_nav).stop(true,true).slideDown(200);
		}, 150);
	},function(){
		var _nav = $(this).attr('_t_nav');
		clearTimeout( qcloud[ _nav + '_timer' ] );
		qcloud[ _nav + '_timer' ] = setTimeout(function(){
		$('[_t_nav]').removeClass('nav-up-selected');
		$('#'+_nav).stop(true,true).slideUp(200);
		}, 150);
	});
});

function nclick()
{
	if(document.newsearch.terms.value=="" || document.newsearch.terms.value=="请输入搜索内容")
	{
	  //alert("请输入搜索内容。ddd");
		//document.all.Class.focus();
		document.newsearch.action="http://so.ccf.com.cn/";
	}else{
	  document.newsearch.action="http://so.ccf.com.cn/Search?terms="+encodeURIComponent($('input[name=terms]').val());
	}
return true;
}
function spclick()
{
	
	if(document.newsearch.terms.value=="" || document.newsearch.terms.value=="请输入搜索内容")
	{
		
	//	alert("请输入搜索内容。");
		//document.all.Class.focus();
		document.newsearchsimple.action="/newscenter/simplesearch.php";
	}else{
	  document.newsearchsimple.action="/newscenter/simplesearch.php?simpleterms="+encodeURIComponent($('input[name=terms]').val());	  
	}
	
return true;
}

 
</script>
