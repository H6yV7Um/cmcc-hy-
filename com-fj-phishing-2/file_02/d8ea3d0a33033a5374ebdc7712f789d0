(function(){

    KISSY.config({
        packages: [
            {
                path : '//g.alicdn.com/tcc/traffic/6.0.4/',
                ignorePackageNameInUri:true,
                name : 'traffic',
                charset: 'utf-8',
                combine:true,
                debug: false
            }
        ]
    });

    var cfg = { container : '#chongzhi-traffic' };

    var TRAFFIC_TYPE_ALL = '全国流量';
    var TRAFFIC_TYPE_LOCAL = '本地流量';

    //选项管理开始
    cfg.ispConfig = [];

    cfg.ispConfig.push( (cfg.ispConfig["defualt"] = {
        applyTo: 'defualt',
        options: 'for_default',
        selectType : TRAFFIC_TYPE_ALL,
        selectFaceValue : '300M'
    }));
    cfg.ispConfig.push( (cfg.ispConfig["电信: *"] = {
        applyTo: '电信: *',
        options: 'tele_comman',
        selectType : TRAFFIC_TYPE_ALL,
        selectFaceValue : '300M'
    }));
    cfg.ispConfig.push( (cfg.ispConfig["联通: 浙江"] = {
        applyTo: '联通: 浙江',
        options: 'unicom_zj',
        selectType : TRAFFIC_TYPE_LOCAL,
        selectFaceValue : '100M'
    }));

    //选项管理结束

    //下拉列表管理
    cfg.options = {};

    if(!cfg.options["tele_comman"]) { cfg.options["tele_comman"] = []; }
    cfg.options["tele_comman"].push({
        name : 'tele_comman',
        type : TRAFFIC_TYPE_ALL,
