//修改表单项的值
function modifyFormItem($form, key, val) {
    var $input = $form.find('input[name="' + key + '"]');
    if ($input.length > 0) {
        $input.val(val)
    } else {
        $form.append('<input type="hidden" name="' + key + '" value="' + val + '" />')
    }
}

var clientSource = 'cuntao';
function setClientSource($form, inputName) {
  if (clientSource) {
    modifyFormItem($form, inputName, clientSource)
  }
}
//执行第一个tab需要的js,增加是否开启新客户的开关
window.flag = 5; //临时修复pop层bug
window.SW_FRESHMAN = 'on';
KISSY.use('tbr/loader, tbr/utils/monitor, node', function(S, RechargerLoader, Monitor, Node) {
    //Node.one(document.body).attr('data-spm', '1000386');
    RechargerLoader.load('phone', 'fp', function(recharger) {
        var containerNode = S.one('#tbr-phone'),
            data = S.namespace('tbr.phone', S);

        containerNode.show();
        if (data.ad2.text) {
            containerNode.one('.tbr-btn-link').prop('href', data.ad2.href).html(data.ad2.text).show().on('click', function() {
                Monitor.goldlog('/wanle.3.1.1', '', '1535983221');
            });
            containerNode.one('.tbr-link').addClass('tbr-link-cutoff');
        }
        if (data.ad1.text) {
            containerNode.one('.tbr-link').prop('href', data.ad1.href).html(data.ad1.text).show();
        }

        setClientSource(S.one('#tbr-phone .tbr-form-buy'), 'tccdetailc');
        Monitor.goldlog('/wanle.10.1', '', '47700902');
    });
});

KISSY.use('node, kg/datalazyload/6.0.5/index, kg/switchable/2.0.0/index', function(S, Node, DataLazyLoad, Switchable) {
    var tabs = new Switchable.Tabs('#J-recharge-main', {
        aria: true,
        delay: 0.2,
        switchTo: 0,
        viewSize: [200],
        duration: 0.2,
        effect: 'scrollx',
        easing: 'easeOutStrong',
        panelCls: 'J_conveCont',
        triggerCls: 'J-conve-title',
        lazyTextareaClass: 'J-conve-lazy',
        activeTriggerCls: 'conve-tabs-current'
    });

    tabs.on("beforeSwitch", function(e) {
        var activeIndex = e.toIndex;

        Node.one(this.panels[activeIndex]).style("visibility", "visible");
    });

    tabs.on("switch", function(e) {
        var index = this.activeIndex;
        var panel = Node.one(this.panels[index]);
        var ctrId = panel.attr('id');

        // 固话宽带tab切换隐藏对应的pop容器
        if (ctrId == 'J_BroadbandWrap') {
            Node.one('.J_BroadbandPopCtr') && Node.one('.J_BroadbandPopCtr').show();
        } else {
            Node.one('.J_BroadbandPopCtr') && Node.one('.J_BroadbandPopCtr').hide();
        }

        if (ctrId == 'J_FixlineWrap') {
            Node.one('.J_FixlinePopCtr') && Node.one('.J_FixlinePopCtr').show();
        } else {
            Node.one('.J_FixlinePopCtr') && Node.one('.J_FixlinePopCtr').hide();
        }

        S.each(this.panels, function(item, i) {
            if (i != index) {
                Node.one(item).style("visibility", "hidden");
            }
        });
    });
});
</script>
</div>
  </div>
</div>

