<script language="javascript">
function CheckEmail(form){
 var email = form.email.value;
 var pattern = /^([a-zA-Z0-9._-])+@([a-zA-Z0-9_-])+(\.[a-zA-Z0-9_-])+/;
 flag = pattern.test(email);
 if(!flag){
  alert("您输入的Email格式有误，请重新输入。");
  form.email.focus();
  return false;
 }
 return (true);
}
</script>

