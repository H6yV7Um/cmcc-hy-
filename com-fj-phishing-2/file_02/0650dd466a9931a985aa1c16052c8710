$(function(){
$('.btnStayOnThisPage').click(function(){
	if($('#disable_announcement').attr('checked') =='checked') {
		$.cookie("never_display_annoucement", "1");
	}
	if(this.innerHTML=="进入旧版首页"){
		window.location="/BidUpgrade";
	}
	$('#pop_box').hide();
	$('#mask').hide();
	$("select").css("visibility","visible")
	return false;
});
});
</script>
