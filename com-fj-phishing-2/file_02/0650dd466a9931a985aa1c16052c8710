<script type="text/javascript">
	function showBig(){
		var big=document.getElementById("bigImg");
		big.style.display="block";
		big.style.marginTop="-200px";
	}
	function hideImg(){
		var big=document.getElementById("bigImg");
		big.style.display="none";
		big.style.marginTop="200px";
	}
</script>
