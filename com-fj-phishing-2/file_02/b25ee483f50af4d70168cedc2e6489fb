<script>
    KISSY.namespace("App");KISSY.add("nav",function(d){var a=d.DOM,i=d.Event,g="current",h="current-sub",f="nav-select",e="nav-hover",c="hover";function b(l,k){var j=this;j.config=d.merge(b.Config,k||{});j.container=a.get(l);j._init()}b.Config={navLight:"",subnavLight:"",subnavId:"#sub-nav",curIndex:-1};d.augment(b,d.EventTarget,{_init:function(){var k=this,j=k.config;k._light();k.subnavArr=a.filter("#single-nav a",function(m){var n=m.id.match(/J_Nav_(\w+)/)?m.id.match(/J_Nav_(\w+)/)[1]:"",l=a.get("#J_Sub_"+n);if(l&&a.children(l).length!=0){return true}else{return false}});i.on(k.subnavArr,"mouseenter mouseleave",function(o){var n=this,p,m;if(a.hasClass(n,g)){return}p=n.id.match(/J_Nav_(\w+)/)[1];m=a.get("#J_Sub_"+p);if(o.type==="mouseenter"){a.addClass(n,c);if(a.css(k.subContainer,"display")=="block"){a.removeClass(k.subContainer,f)}d.each(a.query(".J_IsSubnav"),function(q){a.css(q,"display","none")});a.addClass(k.subContainer,e);a.css(m,"display","inline");k.currentSubnav&&a.hide(k.currentSubnav);i.on(m,"mouseleave",function(q){k._recoverNav(n,m);i.remove(this,"mouseleave",arguments.callee)})}else{var l=d.UA.ie?o.toElement:o.relatedTarget;if(!a.parent(l,".J_IsSubnav")&&!a.hasClass(l,"J_IsSubnav")){k._recoverNav(n,m)}}})},_light:function(){var k=this,j=k.config;k.headerWrap=a.parent(k.container);k.subContainer=a.get(j.subnavId);if(j.navLight.length>0){k.highLightLine=1;k.current=a.get("#"+j.navLight,k.container);k.currentSubnav=a.get("#J_Sub_"+j.navLight.match(/J_Nav_(\w+)/)[1]);if(!k.current){return}a.addClass(k.current,g);if(k.currentSubnav){k._showSubnav(k.currentSubnav);if(k.headerWrap){a.css(k.headerWrap,"margin-bottom","48px")}}}else{k.higLightLine=0}if(j.subnavLight.length>0){if(!k.subContainer){return}k.currentSub=a.get("#"+j.subnavLight,k.subContainer);if(!k.currentSub){return}a.addClass(k.currentSub,h)}},_showSubnav:function(j){var k=this;a.addClass(k.subContainer,f);a.css(k.subContainer,"display","block");a.css(j,"display","inline")},_recoverNav:function(l,j){var k=this;a.removeClass(l,c);a.removeClass(k.subContainer,e);if(a.css(k.subContainer,"display")=="block"){a.addClass(k.subContainer,f)}a.hide(j);k.currentSubnav&&a.show(k.currentSubnav)},countLeft:function(q,j){var n=document.documentElement.clientWidth>990?document.documentElement.clientWidth:990,o=(n-990)/2,k=a.width(q),m=a.width(j),p=a.offset(q).left,r=p-o+k/2,l=r-m/2;if(m/2>r||(r+m/2)>990){l=0}a.css(j,"margin-left",l+"px")}});d.App.Nav={init:function(){d.ready(function(){var l=a.get("#nav"),k=a.get("#J_NavLight"),j=a.get("#J_SubnavLight");if(!l){return}var m=new b(l,{navLight:k?k.value:"",subnavLight:j?j.value:""})})}};return b});KISSY.App.SlideAllChl=function(){var c=KISSY,e=c.DOM,d=c.Event;var f=e.get(".multi-nav"),b=e.get(".nav-trigger"),a=e.get(".category");d.on(f,"mouseover mouseout",function(i){var g=e.viewportWidth()<990?990:e.viewportWidth(),h=-1*(Math.floor((g-990)/2));e.css(a,{width:(g-1)+"px",left:h+"px"});if(i.type=="mouseover"){e.addClass(b,"nav-trigger-on");e.css(a,"display","block")}else{if(i.type=="mouseout"){e.removeClass(b,"nav-trigger-on");e.css(a,"display","none")}}})};KISSY.ready(function(a){a.App.SlideAllChl();a.App.Nav.init()});
</script>

