<!--index script-->
<script>
    //周讯
    function _switch(cssSelecter) {
        var sWidth = $(cssSelecter).width();
        var len = $(cssSelecter + " ul li").length;
        var index = len - 1;//倒排
        var picTimer;

        var btn = "</div><div class='preNext pre' ></div><div class='preNext next' style='display: none'></div>";
        $(cssSelecter).append(btn);
        showPics(index);//初始化最后一张显示

        $(cssSelecter + " .pre").click(function () {
            index -= 1;
            if (index == 0) {
                index = 0;
                $(cssSelecter + " .preNext.pre").hide();
            }
            $(cssSelecter + " .preNext.next").show();

            showPics(index);
        });

        $(cssSelecter + " .next").click(function () {
            index += 1;
            if (index == len - 1) {
                index = len - 1;
                $(cssSelecter + " .preNext.next").hide();
            }
            $(cssSelecter + " .preNext.pre").show();

            showPics(index);
        });


        $(cssSelecter + " ul").css("width", sWidth * (len));


        function showPics(index) {
            var nowLeft = -index * sWidth;
            $(cssSelecter + " ul").stop(true, false).animate({"left": nowLeft}, 300);
            $(cssSelecter + " .btn span").stop(true, false).animate({"opacity": "0.4"}, 300).eq(index).stop(true, false).animate({"opacity": "1"}, 300); //为当前的按钮切换到选中的效果
        }
    }
    $(document).ready(function () {
        _switch("#focus_1");
        _switch("#focus");
    });
    //周讯end


    //    轮播
    $(function () {
        new slide("#main-slide", "cur", 345, 233, 1);
    })

    //    轮播end


    //    二级导航
    $("#nav_1 .nav").hover(function (e) {
        var $navBar;
        $navBar = $(e.target).find(".navbar");
        $navBar.show(100);
        $(e.target).siblings().find(".navbar").hide();
    },function(e){
        var $navBar;
        $navBar = $(e.target).find(".navbar");
        $navBar.hide();
    });
    $("#nav_1 .navbar").hover(null,function(){
        $(this).hide();
    });


    //    二级导航end

</script>
