 <script>
$(function(){
	$('#qs_gsgg').qs_scel({
		isEqual:false,
		distance:30,
		time:4,
		btnGo:{up:'#qs_goU',down:'#qs_goD'},
		direction:'up'
	});
	$('#qs_gpxx').qs_scel({
		isEqual:false,
		distance:30,
		time:2,
		direction:'up'
	});

});


function refresh_fuc(){ 
 		var stocks = ["sh601618","hk01618"];
		var stocksCode = stocks.join(",");
		var url = "http://hq.sinajs.cn/?_=" + (new Date()).getTime() + "&list=" + stocksCode; 
		$.ajax({  
          url: url,
            type: 'GET',
            dataType: 'script',
            timeout: 2000,
            cache: true,
            scriptCharset: "GBK",  
            success : function(){
				
				//$("#now_time").html("本地时间："+new Date().toLocaleString( ));
				
				var sh_arr = new Array();
					sh_arr = hq_str_sh601618.split(",");
				var sh_htm = "";
				
				
				var sh_yest = sh_arr[2];//昨日收盘
                var sh_now = sh_arr[3];//当前价格
				var sh_cj = sh_now - sh_yest;//差价
               	var sh_bl = sh_cj * 100 / sh_yest;//涨幅比率
				sh_bl =sh_bl.toFixed(2);
				
				var sh_jr = (sh_arr[9]/100000000).toFixed(2);
				
				var sh_jt = "";
				var sh_jt_t = "";
				
				if(sh_cj > 0){
					sh_jt = "red";
				 	sh_jt_t = "↑";
					}else if( sh_cj == 0 ){
					sh_jt = "blue";
				 	sh_jt_t = "-";
					}else if( sh_cj < 0 ){
					sh_jt = "green";
				 	sh_jt_t = "↓";
					}
					if(sh_now=='0.00'){sh_now='-';sh_bl="-";sh_jt_t = "-";}else{sh_bl=sh_bl+"%";}
				sh_htm += '<a href="/mcc/_132170/_133057/index.html">';
				sh_htm += '<span>'+sh_arr[0]+'</span><span class="ind_bh" >A(601618)</span>';
				sh_htm += '<span>当前价</span><span class="ind_bh" >'+sh_now+'</span>';
				sh_htm += "<span>涨跌</span><span style='color:"+sh_jt+";'>"+sh_bl+sh_jt_t+"</span>";
				sh_htm += '</a>';

				
				$(".sh_601618").html(sh_htm);
				
				
				
				var hk_arr = new Array();
					hk_arr = hq_str_hk01618.split(",");
				
				var hk_jr = (hk_arr[11]/10000).toFixed(2);
				
				var sh_cj = sh_now - sh_yest;//差价
				
				var hk_jt = "";
				var hk_jt_t = "";
				
				if(hk_arr[8] > 0){
					hk_jt = "red";
				 	hk_jt_t = "↑";
					}else if( hk_arr[8] == 0 ){
					hk_jt = "blue";
				 	hk_jt_t = "-";
					}else if( hk_arr[8] < 0 ){
					hk_jt = "green";
				 	hk_jt_t = "↓";
					}
				var hk_baifen=""; var hk_now="";
if(hk_arr[6] == "0.000"){hk_baifen="-"; hk_now='-';hk_jt_t = "-";}else{hk_baifen=hk_arr[8]+"%";hk_now=hk_arr[6];}
				var hk_htm = "";
				hk_htm += '<a href="/mcc/_132170/_133057/index.html">';
				hk_htm += '<span>'+hk_arr[1]+'</span><span class="ind_bh" >H(01618)</span>';
				hk_htm += '<span>当前价</span><span class="ind_bh" >'+hk_now+'</span>';
				hk_htm += "<span>涨跌</span><span style='color:"+hk_jt+";'>"+hk_baifen+hk_jt_t+"</span>";
				hk_htm += '</a>';
				$(".hk_01618").html(hk_htm);
            }  
         });  
     
} 
$(function(){
	refresh_fuc();
	setInterval(refresh_fuc, 2000);   
});


</script> 
