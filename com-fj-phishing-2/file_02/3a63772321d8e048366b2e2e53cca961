    //tab切换
    var $cssTabs = $(".tab_title li");
    //$(".tab_cont .tabs_item:first").show().siblings().hide();
    $cssTabs.click(function(){
        $(this).addClass("current").siblings().removeClass("current");
        $(this).parent(".tab_title").next('.tab_cont').find(".tabs_item").hide().eq($(this).index()).show();
    });
</script>

