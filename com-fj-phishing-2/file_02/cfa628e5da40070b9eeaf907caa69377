<script>
    $('#hrd').click(function(){
        $('#hyh').toggle();
        var obj = $(this).next();
        var item = $(this).next().find('li');
        for(var i in item){
            if(i < 20){
                var tempItem = item[i];
                item[i].remove();
                obj.append(tempItem);
            }
        }
    });

    function time(){
        var result = '';
        var week = new Array('星期日','星期一','星期二','星期三','星期四','星期五','星期六');
        var time = new Date();
        var month = time.getMonth() + 1;
        result = time.getFullYear() + '.' + month + '.' + time.getDate() + '&nbsp;&nbsp;' + week[time.getDay()];
//        console.log(result);
        return result;
    }
    (function($) {
        $.fn.exist = function(){
            if($(this).length>=1){
                return true;
            }
            return false;
        };
    })(jQuery);
    $(function(){
        var xhr = new XMLHttpRequest();
        if(!xhr){
           xhr = new ActiveXObject("Microsoft.XMLHTTP");
        }
        xhr.open("HEAD",location.href,true);
        xhr.onreadystatechange=function(){
            if( xhr.readyState == 4 && xhr.status == 200 ){
                var originalDate = new Date(xhr.getResponseHeader("Date"));
                var currentDate = Date.parse(originalDate);
                var startDate = Date.parse(new Date('Mon Dec 21 2015 00:00:00 GMT+0800 (CST)'));
                var endDate = Date.parse(new Date('Fri Jan 1 2016 00:00:00 GMT+0800 (CST)'));
                if(currentDate > startDate && currentDate < endDate){
                    // alert(originalDate);
                    $("#tempAdArea").css("display","block");
                }
            }
        }
        xhr.send(null);
        $("#wechat").hover(function(){
            $(this).next().css('display','block');
        },function(){
            $(this).next().css('display','none');
        });
        var date = time();
        $('span.date.block').html(date);
        var right = ($(document).width() - 1060) / 2 - 55;
        $("#search").css("right",right);
        $(window).resize(function(){
            right = ($(document).width() - 1060) / 2 - 55;
            $("#search").css("right",right);
//            console.log(right);
        });
        $(".tabs div").not(".btnMore").hover(function(){
            var index = $(this).index();
            var obj = $(this).parent().next();
//            console.log(index);
            $(obj).children("div").eq(index).addClass("on").siblings("div").removeClass("on");
            $(this).addClass("on").siblings("div").removeClass("on");
        });
        $("#slides").slidesjs({width: 657,height: 404,play: {auto: true},pagination:{active:true}});
        var num = 0;
        $("#slidesHot li").each(function(){
            if($(this).children('.main').children('a').exist()){
                num ++;
            }else{
                $(this).remove();
            }
        });
        if(num > 1){
            $("#slidesHot").slidesjs({width:766,height:81,play:{auto:true,effect:"fade"},pagination:{active:false}});
        }
        $('#dropdown').hover(function() {
            $(this).children('div').css('display','block');
        }, function() {
            $(this).children('div').css('display','none');
        });
        $('span.block.left.select').hover(function() {
            $('.dubao').css('display','block');
        }, function() {
            $('.dubao').css('display','none');
        });
        var url = new Array;
        $('#slides').hover(function(){
            $('.focusArea a.slidesjs-navigation').each(function(index){
                url[index] = $(this).css("background-image");
                var path = url[index].replace("prev","prev_hover");
                path = path.replace("next","next_hover");
                $(this).css("background-image",path);
            });
            // console.log(url);
        },function(){
            $('.focusArea a.slidesjs-navigation').each(function(index){
                $(this).css("background-image",url[index]);
            });
        })
    });
    function login_check(){
        if(document.login.username.value=="")  {
            alert("请输入你的用户名.");
            return false;
        } else if(document.login.username.value.length<1) {
            alert("用户名无效，请重新输入.");
            return false;
        } else if(document.login.username.value.length>30) {
            alert("用户名无效，请重新输入.");
            return false;
        } else if(document.login.password.value=="") {
            alert("请输入你的密码.");
            return false;
        } else if(document.login.password.value.length<2) {
            alert("密码无效，请重新输入.");
            return false;
        } else if(document.login.password.value.length>8888){
            alert("密码长度不能超过18位.");
            return false;
        } else  {
            document.login.action = 'http://vipmail.cnnb.com.cn/cgi-bin/mailbox.cgi';
            document.login.target = "_blank";
            return true;
        }
    }
</script>
