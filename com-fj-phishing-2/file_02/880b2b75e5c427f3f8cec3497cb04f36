        $.getJSON("/News/GetHomeList", function (d) {
            var times = "";
            for (var i = 0; i < d.Result.Company.length; i++) {
                if (d.Result.Company[i].dTimeAdd == null) { times = "" } else { times = d.Result.Company[i].dTimeAdd.substring(0, 10) }
                $("#dynamic").append(' <li><span>' + times + '</span><a title="' + d.Result.Company[i].sTitle + '" href="/News/ViewNews/' + d.Result.Company[i].Autocode + '" target="_blank">' + d.Result.Company[i].sTitle + '</a></li>');
            }
            for (var i = 0; i < d.Result.Industry.length; i++) {
                if (d.Result.Industry[i].dTimeAdd == null) { times = "" } else { times = d.Result.Industry[i].dTimeAdd.substring(0, 10) }
                $("#industry").append(' <li><span>' + times + '</span><a title="' + d.Result.Industry[i].sTitle + '" href="/News/ViewNews/' + d.Result.Industry[i].Autocode + '" target="_blank">' + d.Result.Industry[i].sTitle + '</a></li>');
