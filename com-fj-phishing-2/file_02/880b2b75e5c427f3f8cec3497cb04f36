    function biao(d) {

        var mydate = new Date();
        var listContainer =$("<div></div>");

        for (var i = 0; i < d.Result.length; i++) {
            //html += '<div class="col-md-3">';
            var item = $('<div class="news-grid" ></div>');
            var html = '   <div class="m-t-20 color-xmbh fs-18 color-secondary topTitle" title="' + d.Result[i].Name + '">' + d.Result[i].Name + '</div>';
            html += '  <div class="color-9 fs-16 finnumber">';
            html += '       <div class="fdiv">   <span class="col">' + d.Result[i].JoinNum + '</span> /' + d.Result[i].Period + '人    <br>  <span>参与人数</span>  </div>';
            html += '<div class="ediv">' + d.Result[i].FinishNum + ' -' + d.Result[i].Period + '个月<br>       项目期限     </div>';
            //html += ' </div>';


            var ValidityDate = new Date(d.Result[i].CreateTime.replace("T", " "));  //生效时间
            var CreateTime = new Date(d.Result[i].CreateTime.replace("T", " "));  //开始时间
            var Seconds = parseFloat(ValidityDate.getTime()) - parseFloat(mydate.getTime());   //生效时间跟 当前时间比较 取出毫秒数
            if (isNaN(Seconds)) {
                ValidityDate = new Date(d.Result[i].ValidityDate);  //生效时间
                CreateTime = new Date(d.Result[i].CreateTime);  //开始时间
                Seconds = ValidityDate.getTime() - mydate.getTime();
            }
            var baifen = (parseFloat(d.Result[i].JoinNum) / parseFloat(d.Result[i].Period)) * 100;
            var shengyu = 100 - baifen;
            html += '<div class="color-9 fs-16 surplustime">';
            html += '<div class="jdt1" style=" width : ' + baifen + '%;"></div>';
            html += '<div class="jdt2" style=" width :' + shengyu + '%;"></div>';
            html += '</div>';
            if (Seconds > 0) {
                var Seconds1 = ValidityDate.getTime() - CreateTime.getTime();   //生效时间跟 开始时间比较 相当于总数

                //var baifen = (parseFloat(Seconds) / parseFloat(Seconds1)) * 100;
                //var shengyu = 100 - baifen;
                var days = Math.floor(parseFloat(Seconds) / 1000 / 60 / 60 / 24);
                var house = Math.floor(parseFloat(Seconds) / 1000 / 60 / 60) - parseFloat(days) * 24;
                var minute = Math.floor(parseFloat(Seconds) / 1000 / 60) - parseFloat(house) * 60 - parseFloat(days) * 24 * 60;

                html += '<div class="color-9 fs-16 "><span>剩余时间：</span><span>' + days + '日' + house + '时' + minute + '分</span></div>';
            }
            else {
                html += '<div class="color-9 fs-16 "><span>剩余时间：</span><span>0日0时0分</span></div>';
            }
            //<div class="col-xs-12 col-sm-2 col-md-2 col-lg-2 secondaryOne"><div><span class="secondary-top-blue">' + d.Result[i].PerMoney + '</span></div><div><span class="line-title">每期金额</span></div></div>'
            html += '<div class="m-l-20 m-t-20 color-9 t-a-l  fs-14"><span>项目总额：</span><span class="color-secondary">￥' + d.Result[i].TotalMoney + '   </span></div>';
            html += '<div class="m-l-20 m-t-5 color-9 t-a-l  fs-14"><span>项目金额：</span><span class="color-secondary">￥' + d.Result[i].PerMoney + '</span></div>';
            if (d.Result[i].BusinessId == '10') {
                html += '<div class="m-l-20 m-t-5 color-9 t-a-l  fs-14"><span>固定利息：</span><span class="color-secondary">￥' + d.Result[i].FixInterest + '</span></div>';
            }
            else {
                html += '<div class="m-l-20 m-t-5 color-9 t-a-l  fs-14"><span>底 标 价：</span><span class="color-secondary">￥' + d.Result[i].ApplyMin + '</span></div>';
                html += '<div class="m-l-20 m-t-5 color-9 t-a-l  fs-14"><span>顶 标 价：</span><span class="color-secondary">￥' + d.Result[i].ApplyMax + '</span></div>';

            }
            html += '<div class="m-l-20 m-t-5 color-9 t-a-l  fs-14"><span>缴 款 日：</span><span class="color-secondary">' + d.Result[i].PayDay + '日</span></div>';
            html += '<div class="m-l-20 m-t-5 color-9 t-a-l  fs-14"><span>竞标类型：</span><span class="color-secondary">' + d.Result[i].CFType + '   </span></div>';
          


            html += '<div>';
            html += '<div style=" text-align: center;">';



            if (d.Result[i].Status == 0) {
                if (d.Result[i].JoinNum == d.Result[i].Period)
                {
                    html += '<a cid="' + d.Result[i].Id + '"  class="btn   awarning2" onclick=" add(this)">满 员</a>';
                }
                else
                {
                    html += '<a cid="' + d.Result[i].Id + '"  class="btn  awarning" onclick=" add(this)">加 入</a>';
                }
            }
            else if (d.Result[i].Status == 1) {
                html += '<a cid="' + d.Result[i].Id + '" class="btn  awarning2"  onclick=" add(this)"  >进行中</a>';
            }
            else if (d.Result[i].Status == -1) {
                html += '<a cid="' + d.Result[i].Id + '" class="btn awarning2"   onclick=" add(this)"  >招募失败</a>';
            }
            else if (d.Result[i].Status == 2) {
                html += '<a cid="' + d.Result[i].Id + '" class="btn awarning2"   onclick=" add(this)"  >圆满成功</a>';
            }

            html += '</div>';
            html += '</div>';
            html += ' </div>';
            item.html(html);
            item[0].cf=d.Result[i];
            listContainer.append(item);
        }
        return listContainer;
    }

    function add(sender) {
        //if ("False" != "True") { window.location = "/Home/login?backUrl=/Home/Index"; return; }
        
        if (isApp) {
            if (!isLogin()) {
                CallAppLogin();
                return;
            }
        }
            var id = $(sender).attr('cid');
            goToUrl("/UserCenter/InvitationId?cache=0&id=" + id);
            //cf = $(sender).closest(".news-grid");
            // ShowPage("/UserCenter/JoinStatement");
    }

